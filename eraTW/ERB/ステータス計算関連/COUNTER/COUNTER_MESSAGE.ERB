@素質计算(ARG)
#FUNCTION
#DIM 气弱系
#DIM 强气系
#DIM 坦率系
#DIM 真面目系

气弱系 = 0
强气系 = 0
坦率系 = 0
真面目系 = 0

;胆怯
IF TALENT:ARG:胆量 == -1
	气弱系 += 5
;坚强
ELSEIF TALENT:ARG:胆量 == 1
	强气系 += 1
ENDIF

;坦率
IF TALENT:ARG:态度 == -1
	坦率系 += 2
;叛逆
ELSEIF TALENT:ARG:态度 == 1
	强气系 += 1
ENDIF

;老实
IF TALENT:ARG:回应 == -1
	气弱系 += 1
;傲慢
ELSEIF TALENT:ARG:回应 == 1
	强气系 += 1
ENDIF

;自尊心低
IF TALENT:ARG:自尊心 == -1
	气弱系 += 1
;自尊心高
ELSEIF TALENT:ARG:自尊心 == 1
	强气系 += 1
ENDIF

;傲娇
IF TALENT:ARG:傲娇
	IF TALENT:ARG:恋慕 || TALENT:ARG:淫乱 || TALENT:ARG:服从
		坦率系 += 1
	ELSE
		强气系 += 1
	ENDIF
ENDIF

;自制心
SIF TALENT:ARG:自制心
	真面目系 += 1

;保守的
IF TALENT:ARG:性的兴趣 == -1
	真面目系 += 1
;好奇心
ELSEIF TALENT:ARG:性的兴趣 == 1
	坦率系 += 1
ENDIF

;阴郁
IF TALENT:ARG:开朗／阴郁 == -1
	气弱系 += 1
;开朗
ELSEIF TALENT:ARG:开朗／阴郁 == 1
	坦率系 += 1
ENDIF

;不在乎贞操
IF TALENT:ARG:贞操 == -1
	坦率系 += 1
;贞操重视
ELSEIF TALENT:ARG:贞操 == 1
	真面目系 += 1
ENDIF

;圧抑
IF TALENT:ARG:自己爱 == -1
	气弱系 += 1
;解放
ELSEIF TALENT:ARG:自己爱 == 1
	坦率系 += 1
ENDIF

;不知羞耻
IF TALENT:ARG:羞耻心 == -1
	坦率系 += 1
;害羞
ELSEIF TALENT:ARG:羞耻心 == 1
	气弱系 += 1
ENDIF

;献身的
SIF TALENT:ARG:献身的
	真面目系 += 2

;否定快感
IF TALENT:ARG:快感应答 == -1
	真面目系 += 1
;接受快感
ELSEIF TALENT:ARG:快感应答 == 1
	坦率系 += 1
ENDIF

;施虐狂
SIF TALENT:ARG:施虐狂
	强气系 += 2
	
;受虐狂
SIF TALENT:ARG:受虐狂
	气弱系 += 2

;气弱系
IF 气弱系 > 强气系 && 气弱系 > 坦率系 && 气弱系 > 真面目系 && !(TALENT:ARG:性别嗜好 == 1 && TALENT:MASTER:性别 == 2) && !(TALENT:ARG:性别嗜好 == 2 && TALENT:MASTER:性别 == 1) && TALENT:ARG:性别嗜好 != 3
	RETURNF 1
;强气系
ELSEIF 强气系 > 坦率系 && 强气系 > 气弱系 && 强气系 > 真面目系 && !(TALENT:ARG:性别嗜好 == 1 && TALENT:MASTER:性别 == 2) && !(TALENT:ARG:性别嗜好 == 2 && TALENT:MASTER:性别 == 1) && TALENT:ARG:性别嗜好 != 3
	RETURNF 2
;坦率系
ELSEIF 坦率系 > 强气系 && 坦率系 > 气弱系 && 坦率系 > 真面目系 && !(TALENT:ARG:性别嗜好 == 1 && TALENT:MASTER:性别 == 2) && !(TALENT:ARG:性别嗜好 == 2 && TALENT:MASTER:性别 == 1) && TALENT:ARG:性别嗜好 != 3
	RETURNF 3
;真面目系
ELSEIF 真面目系 > 强气系 && 真面目系 > 气弱系 && 真面目系 > 坦率系 && !(TALENT:ARG:性别嗜好 == 1 && TALENT:MASTER:性别 == 2) && !(TALENT:ARG:性别嗜好 == 2 && TALENT:MASTER:性别 == 1) && TALENT:ARG:性别嗜好 != 3
	RETURNF 4
ELSE
;其他
	RETURNF 5
ENDIF

