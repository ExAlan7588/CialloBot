@EQUIP_INCERT_V(ARG)
#DIM 竿役

竿役 = TEQUIP:ARG:50
;-------------------------------------------------
;行为者
;-------------------------------------------------
TCVAR:ARG:116 = 竿役
;TCVAR:2 射精部位フラグ（-1=避孕套 1=膣内 2=肛门 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛门侍奉 10=道具 11=触手 12=Ａ调教 13=Ｖ调教）
;TCVAR:4 射精快感强度

CSTR:竿役:10 = @"/{ARG}/"
TCVAR:竿役:射精部位 = 1

CSTR:ARG:10 += @"{TEQUIP:ARG:50}/"
SIF TCVAR:ARG:射精部位 == 0 || TCVAR:ARG:射精部位 >= 8
	TCVAR:ARG:射精部位 = 13


IF ARG == MASTER
	PRINTFORML ＜%NAME_OUTPUT_TRANSLATION("CALLNAME", 竿役)%插入Ｖ中＞

	TCVAR:竿役:射精快感强度 = 850 + TEQUIP:ARG:子宫 * 100
	TCVAR:ARG:射精快感强度 = 500 + (100 * ABL:ARG:Ｖ感觉)

	DOWNBASE:竿役:体力 += 15
	DOWNBASE:竿役:气力 += 20
	DOWNBASE:ARG:体力 += 10
	DOWNBASE:ARG:气力 += 20
	SOURCE:ARG:快Ｖ += 200
	SOURCE:竿役:快Ｃ += 200
	SOURCE:竿役:情爱 += 25
	SOURCE:竿役:性行动 += 250
	SOURCE:竿役:达成 += 25
	SOURCE:竿役:征服 += 500
	SOURCE:竿役:偏离正轨 += 50

ELSE
	PRINTFORML ＜%NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%Ｖ被插入中＞

	TCVAR:竿役:射精快感强度 = 700 + TEQUIP:ARG:子宫 * 100
	TCVAR:ARG:射精快感强度 = 750
	DOWNBASE:竿役:体力 += 15
	DOWNBASE:竿役:气力 += 20
	;时间停止中
	IF FLAG:70
		SOURCE:ARG:快Ｖ += 150
		CALL 插入继续Ｖ苦痛处理(ARG)

	ELSE
		DOWNBASE:ARG:体力 += 10
		DOWNBASE:ARG:气力 += 20
		SIF TEQUIP:ARG:体位 == 6
			SOURCE:ARG:苦痛 += 300
		SIF TALENT:竿役:形状 == 5
			SOURCE:ARG:苦痛 += 200
		SOURCE:ARG:快Ｖ += 150
		SOURCE:ARG:情爱 += 75
		SOURCE:ARG:露出 += 25
		SIF TCVAR:ARG:强行
			SOURCE:ARG:反感 += 1000
		;体位BOUNS
		IF TEQUIP:ARG:体位 == CFLAG:ARG:喜欢的体位
			SOURCE:ARG:情爱 += 100
			SOURCE:ARG:恭顺 += 50
		ENDIF
		CALL 插入继续Ｖ苦痛处理(ARG)
	ENDIF

	IF CFLAG:ARG:诶嘿嘿 == 2
		SOURCE:ARG:快Ｖ += 150
		SOURCE:ARG:情爱 += 100
		TCVAR:竿役:射精快感强度 += 150
	ENDIF
ENDIF

;-------------------------------------------------
;経験の处理
;-------------------------------------------------
EXP:竿役:插入经验 ++
EXP:ARG:Ｖ经验 ++
SIF !FLAG:70 && !CFLAG:ARG:恶作剧
EXP:ARG:Ｖ性交经验 ++
SIF TEQUIP:ARG:体位 == 6
	EXP:ARG:Ｖ扩张经验 ++
SIF FLAG:70 == 1
	EXP:MASTER:时奸经验 += 1
IF CFLAG:MASTER:恶作剧 == 2
	EXP:MASTER:睡眠奸经验 += 1
	EXP:ARG:睡眠奸经验 += 1
ENDIF
RETURN 1


