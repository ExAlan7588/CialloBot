;射精确定后の处理
@EJAC_CHECK_AFTER(角色登录番号)
#DIM 角色登录番号
#DIM 射精回数
#DIM 过剰射精

VARSET 射精回数
VARSET 过剰射精
;角色登录番号 射精する人
IF !TCVAR:角色登录番号:要去了
	IF BASE:角色登录番号:射精 > MAXBASE:角色登录番号:射精
		IF BASE:角色登录番号:勃起 < 1000
			BASE:角色登录番号:射精 = MAXBASE:角色登录番号:射精 - 1
		ELSE
			TCVAR:角色登录番号:要去了 = 1
		ENDIF
	ENDIF
	RETURN 0
ELSEIF TEQUIP:角色登录番号:挑逗模式 && 角色登录番号 != MASTER
	TCVAR:角色登录番号:挑逗回数 += 2
	RETURN 0
ENDIF

;射精(绝顶)回数
射精回数 = MIN(BASE:角色登录番号:射精 / MAXBASE:角色登录番号:射精, (100 + BASE:角色登录番号:精力) / 100)




NOWEX:角色登录番号:射精量 = BASE:角色登录番号:射精 * 10 / (ABL:角色登录番号:Ｃ感觉 / 2 + 10) / 100
IF SHOOTINGSTAR == 0
	NOWEX:角色登录番号:射精量 = NOWEX:角色登录番号:射精量 * 10 / (ABL:角色登录番号:Ｃ感觉 / 2 + 10)
ELSE
	NOWEX:角色登录番号:射精量 = NOWEX:角色登录番号:射精量 * 10 / (ABL:角色登录番号:Ｃ感觉 / 2 + 10) * SHOOTINGSTAR_NUM
ENDIF

IF NOWEX:角色登录番号:射精量 > BASE:角色登录番号:精力
	过剰射精 = NOWEX:角色登录番号:射精量 - BASE:角色登录番号:精力
ENDIF

IF 过剰射精
	IF TFLAG:剧毒身躯
		DOWNBASE:角色登录番号:体力 += 过剰射精 * 1
	ELSE
		DOWNBASE:角色登录番号:体力 += 过剰射精 * 1 / 2
	ENDIF
ENDIF

BASE:角色登录番号:精力 = MAX(0, BASE:角色登录番号:精力 - NOWEX:角色登录番号:射精量)
TCVAR:角色登录番号:要去了 = 0
STAIN:角色登录番号:Ｐ |= 4

;魔改射精后增加属性上限
IF DEVELOPMENT == 1
	BASE:角色登录番号:体力 += 200
	BASE:角色登录番号:气力 += 200
	MAXBASE:角色登录番号:体力 += HEALTH_DEVELOP
	MAXBASE:角色登录番号:气力 += STAIN_DEVELOP
	MAXBASE:角色登录番号:精力 += SPERM_DEVELOP
	SIF MAXBASE:MASTER:TSP >= 6666
			MAXBASE:MASTER:TSP += TSP_DEVELOP
ELSEIF DEVELOPMENT == 2 && 角色登录番号 == MASTER
	BASE:角色登录番号:体力 += 200
	BASE:角色登录番号:气力 += 200
	MAXBASE:角色登录番号:体力 += HEALTH_DEVELOP
	MAXBASE:角色登录番号:气力 += STAIN_DEVELOP
	MAXBASE:角色登录番号:精力 += SPERM_DEVELOP
	SIF MAXBASE:MASTER:TSP >= 6666
			MAXBASE:MASTER:TSP += TSP_DEVELOP
ENDIF

;BASEの处理
BASE:角色登录番号:射精 = 0

CSTR:角色登录番号:射精后处理用 = %CSTR:角色登录番号:射精判定用%
TCVAR:角色登录番号:已射精部位FLAG = TCVAR:角色登录番号:射精部位

;异常经验、射精经验、精液经验
;女性器がある场合？
SIF !EXP:角色登录番号:射精经验 && TALENT:角色登录番号:性别 & 1
	EXP:角色登录番号:异常经验 += 1

EXP:角色登录番号:绝顶经验 ++ 
EXP:角色登录番号:射精经验 += 射精回数

NOWEX:角色登录番号:射精 += 射精回数
EX:角色登录番号:射精 += 射精回数
TCVAR:角色登录番号:挑逗回数 = 0
TEQUIP:角色登录番号:挑逗模式 = 0
SIF AT_HOME(角色登录番号)
	YOGORE:(CFLAG:角色登录番号:当前位置) += 射精回数 * 50

IF TFLAG:剧毒身躯
	DOWNBASE:角色登录番号:体力 += LIMIT(NOWEX:角色登录番号:射精量 / 4, 30, 300)
	DOWNBASE:角色登录番号:气力 += LIMIT(NOWEX:角色登录番号:射精量 / 2, 40, 400)
ELSE
	DOWNBASE:角色登录番号:体力 += LIMIT(NOWEX:角色登录番号:射精量 / 8, 10, 100)
	DOWNBASE:角色登录番号:气力 += LIMIT(NOWEX:角色登录番号:射精量 / 4, 20, 200)
ENDIF

;MASTERでなければBASEやSOURCE等の处理を入れる
SIF 角色登录番号 == MASTER
	RETURN 0

IF EXP:角色登录番号:射精经验 < EXPLV:1
	SOURCE:角色登录番号:露出 += 5000 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:屈从 += 2500 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:不洁 += 200 * (射精回数 + 1) / 2
ELSEIF EXP:角色登录番号:射精经验 < EXPLV:2
	SOURCE:角色登录番号:露出 += 2500 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:屈从 += 2000 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:不洁 += 150 * (射精回数 + 1) / 2
ELSEIF EXP:角色登录番号:射精经验 < EXPLV:3
	SOURCE:角色登录番号:露出 += 1000 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:屈从 += 800 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:不洁 += 100 * (射精回数 + 1) / 2
ELSEIF EXP:角色登录番号:射精经验 < EXPLV:4
	SOURCE:角色登录番号:露出 += 800 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:屈从 += 600 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:不洁 += 50 * (射精回数 + 1) / 2
ELSEIF EXP:角色登录番号:射精经验 < EXPLV:5
	SOURCE:角色登录番号:露出 += 400 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:屈从 += 400 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:不洁 += 10 * (射精回数 + 1) / 2
ELSE
	SOURCE:角色登录番号:露出 += 200 * (射精回数 + 1) / 2
	SOURCE:角色登录番号:屈从 += 250 * (射精回数 + 1) / 2
ENDIF

;-------------------------------------------------
;调教对象の射精チェック
;-------------------------------------------------
@TARGET_EJAC_CHECK(角色登录番号)
#DIM 角色登录番号

;男性器がなければ返回
SIF !HAS_PENIS(角色登录番号)
	RETURN 0

LOCAL = (CUP:角色登录番号:快Ｃ + CUP:角色登录番号:快Ｖ + CUP:角色登录番号:快Ａ + CUP:角色登录番号:快Ｂ) / 4

;自制心
SIF TALENT:角色登录番号:自制心
	LOCAL /= 2

;接受快感
SIF TALENT:角色登录番号:快感应答 > 0
	TIMES LOCAL , 1.20

;否定快感（淫乱、娼妇で无效）
SIF TALENT:角色登录番号:快感应答 < 0 && !TALENT:角色登录番号:淫乱
	TIMES LOCAL , 0.80

;媚药
SIF (TCVAR:角色登录番号:媚药 || IsInsenceValid(角色登录番号)) && 角色登录番号 != MASTER
	LOCAL *= 2

;仙香玉兔
SIF IsInsenceValid(角色登录番号)
	LOCAL *= 2

;利尿剂
SIF TCVAR:角色登录番号:利尿剂
	LOCAL /= 2
;睡眠中
SIF CFLAG:角色登录番号:睡眠
	LOCAL /= 2
BASE:角色登录番号:射精 += LOCAL
;SIF BASE:角色登录番号:射精 > MAXBASE:角色登录番号:射精
;	BASE:角色登录番号:射精 = MAXBASE:角色登录番号:射精 - 1

;射精处理の角色别ターミナル
@SAMEN_SHOOT(射精した角色番号)
#DIM 射精した角色番号
#DIM 射精された角色番号

FOR 射精された角色番号, 0, CHARANUM
	SIF SAMEN_DIRECTION2(射精した角色番号, 射精された角色番号)
		CALL SAMEN_SHOOT2(射精した角色番号, 射精された角色番号)
NEXT

;部位别处理
@SAMEN_SHOOT2(SENDER, RECEIVER)
#DIM SENDER
#DIM RECEIVER
#DIM 强行反感
#DIM 强行恐怖
#DIM 信赖减少
#DIM 好感度减少
#DIM 欲求不满解消系数
#DIM 中毒BOUNS
#DIM 路人子用

CFLAG:RECEIVER:(SENDER+2500) += NOWEX:SENDER:射精量

中毒BOUNS = 0
IF SENDER == MASTER && CFLAG:RECEIVER:诶嘿嘿 == 2
	SOURCE:RECEIVER:达成 += NOWEX:SENDER:射精 * 150
	SOURCE:RECEIVER:征服 += NOWEX:SENDER:射精 * 150
	SIF !(NOWEX:RECEIVER:Ｃ绝顶 + NOWEX:RECEIVER:Ｖ绝顶 + NOWEX:RECEIVER:Ａ绝顶 + NOWEX:RECEIVER:Ｂ绝顶 + NOWEX:RECEIVER:Ｍ绝顶)
		SOURCE:RECEIVER:征服 += NOWEX:SENDER:射精 * 100
ENDIF
IF SENDER == MASTER && TALENT:RECEIVER:路人 == 1
	路人子用 = MIN(NOWEX:SENDER:射精,3)
	SELECTCASE TCVAR:SENDER:已射精部位FLAG
		CASE 1
			SIF ABL:RECEIVER:膣 < 路人子用
				CALL ChangeABL(RECEIVER, "膣", 1)
		CASE 2
			SIF ABL:RECEIVER:肛门 < 路人子用
				CALL ChangeABL(RECEIVER, "肛门", 1)
		CASE 3
			SIF ABL:RECEIVER:指 < 路人子用
				CALL ChangeABL(RECEIVER, "指", 1)
		CASE 4
			SIF ABL:RECEIVER:舌 < 路人子用
				CALL ChangeABL(RECEIVER, "舌", 1)
		CASE 4
			SIF ABL:RECEIVER:胸 < 路人子用
				CALL ChangeABL(RECEIVER, "胸", 1)
	ENDSELECT
ENDIF

IF TEQUIP:SENDER:避孕套 && TEQUIP:TARGET:50 == PLAYER
	;ゴムを外す
	TEQUIP:SENDER:避孕套 = 0
	IF RECEIVER
		SELECTCASE TCVAR:SENDER:已射精部位FLAG
			CASE 1
				CFLAG:RECEIVER:积攒度 += ABL:RECEIVER:膣射中毒 * NOWEX:SENDER:射精 * 15
			CASE 2
				CFLAG:RECEIVER:积攒度 += ABL:RECEIVER:肛射中毒 * NOWEX:SENDER:射精 * 15
		ENDSELECT
	ENDIF
	;接触のリセット
	CALL 插入解除(RECEIVER)
	TFLAG:套套内 = NOWEX:SENDER:射精量
	IF CFLAG:RECEIVER:自动套套 && ITEM:避孕套 && SENDER == MASTER
		ITEM:避孕套 --
		TEQUIP:SENDER:避孕套 = 1
	ENDIF
ELSEIF TEQUIP:SENDER:避孕套 && TALENT:TARGET:性别 == 3
	;ゴムを外す
	TEQUIP:TARGET:避孕套 = 0
	IF RECEIVER
		SELECTCASE TCVAR:SENDER:已射精部位FLAG
			CASE 1
				CFLAG:RECEIVER:积攒度 += ABL:RECEIVER:膣射中毒 * NOWEX:SENDER:射精 * 15
			CASE 2
				CFLAG:RECEIVER:积攒度 += ABL:RECEIVER:肛射中毒 * NOWEX:SENDER:射精 * 15
		ENDSELECT
	ENDIF
	;接触のリセット
	CALL 插入解除(TARGET)
	TFLAG:套套内 = NOWEX:TARGET:射精量
	IF CFLAG:RECEIVER:自动套套 && ITEM:避孕套 && !(SENDER == MASTER)
		ITEM:避孕套 --
		TEQUIP:TARGET:避孕套 = 1
	ENDIF
ELSE
;TCVAR:12 射精部位フラグ（1=膣内 2=肛门 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛门侍奉 10=道具 11=触手）
SELECTCASE TCVAR:SENDER:已射精部位FLAG
	;膣内
	CASE 1
		EXP:RECEIVER:精液经验 += NOWEX:SENDER:射精 * 2
		EXP:RECEIVER:膣射经验 += NOWEX:SENDER:射精 * 2
		STAIN:RECEIVER:Ｖ |= 4
		STAIN:RECEIVER:膣内 |= 4
		TCVAR:RECEIVER:V被中出FLAG = 2
		TCVAR:RECEIVER:Vに中出しした角色の番号 = SENDER
		IF FLAG:时间停止 == 1
			CFLAG:RECEIVER:膣内射精 = 2
		ELSE
			CFLAG:RECEIVER:膣内射精 = 1
		ENDIF
		NOWEX:RECEIVER:膣内精液 = NOWEX:SENDER:射精量
		EX:RECEIVER:膣内精液 += NOWEX:RECEIVER:膣内精液
		欲求不满解消系数 = 800 / MIN(100 + ABL:RECEIVER:膣射中毒 * 10,200)
		SIF NOWEX:RECEIVER:膣内精液 > 0
			CFLAG:RECEIVER:积攒度 -= SQRT(NOWEX:RECEIVER:膣内精液) * 欲求不满解消系数
		CFLAG:RECEIVER:累计膣内精液 += NOWEX:RECEIVER:膣内精液
		CFLAG:SENDER:累计膣内射精 += NOWEX:RECEIVER:膣内精液
		CFLAG:RECEIVER:(SENDER+3000) += NOWEX:RECEIVER:膣内精液
		CALL POISONBODY(RECEIVER, SENDER, 1)
		;SIF !TCVAR:RECEIVER:口服避孕药
		;PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", SENDER)%が%NAME_OUTPUT_TRANSLATION("CALLNAME", RECEIVER)%に{NOWEX:RECEIVER:膣内精液}出した
		CALL SAMEN_SHOOT3(NOWEX:RECEIVER:膣内精液 , SENDER, RECEIVER)
		IF SHIRAHU(RECEIVER)
			TCVAR:RECEIVER:种付けられ自觉 = 1
			CFLAG:RECEIVER:种付けられ自觉 = 1
		ENDIF
		IF SHIRAHU(SENDER) && SENDER != MASTER
			TCVAR:SENDER:种付け自觉 = 1
		ENDIF
		IF TCVAR:RECEIVER:强行
			IF TALENT:RECEIVER:路人 == 1
				CSTR:RECEIVER:路人子PROFILE２ = %REPLACE(@"%CSTR:RECEIVER:路人子PROFILE２%","现在已经老实了","、被膣内射精了")%
				SIF !STRCOUNT(CSTR:RECEIVER:路人子PROFILE２, "膣内射精了")
					CSTR:RECEIVER:路人子PROFILE２ = %REPLACE(@"%CSTR:RECEIVER:路人子PROFILE２%","处女被夺走了","处女被夺走之后、被膣内射精了")%
				IF EX:RECEIVER:膣内精液 >= 300 && !STRCOUNT(CSTR:RECEIVER:路人子PROFILE２, "充分")
					CSTR:RECEIVER:路人子PROFILE２ = %REPLACE(@"%CSTR:RECEIVER:路人子PROFILE２%","膣内射精了","注込了充分的精液")%
				ENDIF
				IF 路人子初期Ｖ性交经验 + 10 <= EXP:RECEIVER:Ｖ性交经验
					IF !STRCOUNT(CSTR:RECEIVER:路人子PROFILE２, "打倒在地然后")
						CSTR:RECEIVER:路人子PROFILE２ = %REPLACE(@"%CSTR:RECEIVER:路人子PROFILE２%","膣内射精了", "打倒在地然后膣内射精了")%
					ENDIF
					IF 路人子初期Ｖ性交经验 + 15 <= EXP:RECEIVER:Ｖ性交经验 && !STRCOUNT(CSTR:RECEIVER:路人子PROFILE２, "散々")
						CSTR:RECEIVER:路人子PROFILE２ = %REPLACE(@"%CSTR:RECEIVER:路人子PROFILE２%","打倒在地然后", "蛮横地打倒在地然后")%
					ENDIF
				ENDIF
				IF MARK:RECEIVER:快乐刻印 && !STRCOUNT(CSTR:RECEIVER:路人子PROFILE２, "绝顶")
					CSTR:RECEIVER:路人子PROFILE２ = %REPLACE(@"%CSTR:RECEIVER:路人子PROFILE２%","打倒在地然后", "打倒在地然后绝顶的同时被")%
				ENDIF
				IF MARK:RECEIVER:快乐刻印 && !STRCOUNT(CSTR:RECEIVER:路人子PROFILE２, "就那样狂乱地")
					CSTR:RECEIVER:路人子PROFILE２ = %REPLACE(@"%CSTR:RECEIVER:路人子PROFILE２%","绝顶的同时", "就那样狂乱绝顶的同时")%
				ENDIF
			ENDIF
			SELECTCASE ABL:RECEIVER:膣射中毒
				CASE IS < 3
					强行反感 = 100000
					好感度减少 = NOWEX:SENDER:射精 * RAND:50 + NOWEX:SENDER:射精 * RAND:50
					信赖减少 = NOWEX:SENDER:射精 * RAND:20 + NOWEX:SENDER:射精 * RAND:20
					强行恐怖 = NOWEX:RECEIVER:膣内精液 * 5
				CASE 3 TO 5
					强行反感 = 50000
					好感度减少 = NOWEX:SENDER:射精 * RAND:30 + NOWEX:SENDER:射精 * RAND:30
					信赖减少 = NOWEX:SENDER:射精 * RAND:15 + NOWEX:SENDER:射精 * RAND:15
					强行恐怖 = NOWEX:RECEIVER:膣内精液 * 3
				CASEELSE
					强行反感 = 10000
					好感度减少 = NOWEX:SENDER:射精 * RAND:10 + NOWEX:SENDER:射精 * RAND:10
					信赖减少 = NOWEX:SENDER:射精 * RAND:10 + NOWEX:SENDER:射精 * RAND:10
					强行恐怖 = NOWEX:RECEIVER:膣内精液
			ENDSELECT
			IF CFLAG:RECEIVER:生理周期 == 7
				强行反感 *= 2
				强行恐怖 *= 2
				好感度减少 *= 2
				信赖减少 *= 2
			ENDIF
			IF CFLAG:RECEIVER:强行内射 > 1
				强行反感 *= 2
				强行恐怖 *= 2
				好感度减少 *= 2
				信赖减少 *= 2
			ENDIF
			SOURCE:RECEIVER:反感 += 强行反感
			IF CFLAG:RECEIVER:强行内射 > 1
				CFLAG:RECEIVER:强行内射 = 3
			ELSE
				CFLAG:RECEIVER:强行内射 = 1
			ENDIF
			TFLAG:好感度管理 -= 好感度减少
			CALL CHANGE_CFLAG(4,RECEIVER,信赖减少 * (-1))
		ELSEIF TCVAR:RECEIVER:外に出すから
			强行反感 = 5000
			强行恐怖 = NOWEX:RECEIVER:膣内精液
			好感度减少 = NOWEX:SENDER:射精 * RAND:10 + NOWEX:SENDER:射精 * RAND:10
			信赖减少 = NOWEX:SENDER:射精 * RAND:10 + NOWEX:SENDER:射精 * RAND:10
			;危险日
			IF CFLAG:RECEIVER:生理周期 == 7
				强行反感 *= 10
				强行恐怖 *= 10
				好感度减少 *= 3
				信赖减少 *= 3
			;排卵日
			ELSEIF CFLAG:RECEIVER:生理周期 == 8
				强行反感 *= 5
				强行恐怖 *= 5
				好感度减少 *= 2
				信赖减少 *= 2
			ELSE
				强行反感 *= 5
				强行恐怖 *= 5
				好感度减少 *= 1
				信赖减少 *= 1
			ENDIF
			
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", SENDER)%原本允诺会拔出来,才同意不戴套的,结果反悔了...这让人气愤。
			PRINTW 
			
			SOURCE:RECEIVER:反感 += 强行反感
			TFLAG:好感度管理 -= 好感度减少
			CALL CHANGE_CFLAG(4,RECEIVER,信赖减少 * (-1))
		ENDIF

	;肛门
	CASE 2
		EXP:RECEIVER:精液经验 += NOWEX:SENDER:射精 * 2
		EXP:RECEIVER:肛射经验 += NOWEX:SENDER:射精 * 2
		STAIN:RECEIVER:Ａ |= 4
		STAIN:RECEIVER:肠内 |= 4
		;中出し表示用
		TCVAR:RECEIVER:A被中出FLAG = 2
		TCVAR:RECEIVER:Aに中出しした角色の番号 = SENDER
		IF FLAG:时间停止 == 1
			CFLAG:RECEIVER:肛门射精 = 2
		ELSE
			CFLAG:RECEIVER:肛门射精 = 1
		ENDIF
		NOWEX:RECEIVER:肛门内精液 = NOWEX:SENDER:射精量
		EX:RECEIVER:肛门内精液 += NOWEX:SENDER:射精量
		欲求不满解消系数 = 800 / MIN(100 + ABL:RECEIVER:肛射中毒 * 10,200)
		CFLAG:RECEIVER:积攒度 -= SQRT(NOWEX:RECEIVER:肛门内精液) * 欲求不满解消系数
		CFLAG:RECEIVER:累计肛门精液 += NOWEX:SENDER:射精量
		CFLAG:SENDER:累计肛门射精 += NOWEX:SENDER:射精量
		CFLAG:RECEIVER:(SENDER+3500) += NOWEX:RECEIVER:肛门内精液
		CALL POISONBODY(RECEIVER, SENDER, 2)
	;手淫
	CASE 3
		EXP:RECEIVER:精液经验 += 1 + NOWEX:SENDER:射精
		STAIN:RECEIVER:手 |= 4
		IF FLAG:时间停止 == 1
			CFLAG:RECEIVER:手上精液 = 2
		ELSE
			CFLAG:RECEIVER:手上精液 = 1
		ENDIF
		IF ABL:RECEIVER:精液中毒 > 2 && ABL:RECEIVER:技巧 > 2
			EXP:RECEIVER:精饮经验 ++
			CFLAG:RECEIVER:累计精饮 += NOWEX:SENDER:射精量 / 2
			CFLAG:RECEIVER:积攒度 -= NOWEX:SENDER:射精量 * ABL:RECEIVER:精液中毒 / 10 * (TALENT:RECEIVER:精爱味觉 + 1)
			CALL POISONBODY(RECEIVER, SENDER, 31)
		ELSE
			CALL POISONBODY(RECEIVER, SENDER, 3)
		ENDIF
		;接触のリセット
		TEQUIP:SENDER:指接触部位 = 0
	;口淫
	CASE 4
		EXP:RECEIVER:精液经验 += NOWEX:SENDER:射精 * 3
		STAIN:RECEIVER:口 |= 4
		IF FLAG:时间停止 == 1
			CFLAG:RECEIVER:口内精液 = 2
		ELSE
			CFLAG:RECEIVER:口内精液 = 1
		ENDIF
		;深喉插入
		欲求不满解消系数 = 800 / MIN(100 + ABL:RECEIVER:精液中毒 * 10,200)
		IF TALENT:RECEIVER:精爱味觉
			SOURCE:RECEIVER:快Ｍ += NOWEX:SENDER:射精 * 10
			欲求不满解消系数 += 1
		ENDIF
		IF SELECTCOM == 140
			EXP:RECEIVER:精饮经验 += NOWEX:SENDER:射精 * 3
			CFLAG:RECEIVER:累计精饮 += NOWEX:SENDER:射精量
			CFLAG:SENDER:累计口内射精 += NOWEX:SENDER:射精量
		ELSEIF  RAND:(MIN(60, EXP:RECEIVER:精饮经验 + 1)) > 10 + RAND:30
			EXP:RECEIVER:精饮经验 += NOWEX:SENDER:射精 * 3
			CFLAG:RECEIVER:累计精饮 += NOWEX:SENDER:射精量
			CFLAG:SENDER:累计口内射精 += NOWEX:SENDER:射精量
		ELSE
			EXP:RECEIVER:精饮经验 += NOWEX:SENDER:射精
			CFLAG:RECEIVER:累计精饮 += NOWEX:SENDER:射精量 / 3
			CFLAG:SENDER:累计口内射精 += NOWEX:SENDER:射精量
			欲求不满解消系数 /= 2
		ENDIF
		CALL POISONBODY(RECEIVER, SENDER, 4)
		CFLAG:RECEIVER:积攒度 -= SQRT(NOWEX:SENDER:射精量) * 欲求不满解消系数 / 4

		;接触のリセット
		TEQUIP:SENDER:口接触部位 = 0
	;乳交
	CASE 5
		EXP:RECEIVER:精液经验 += NOWEX:SENDER:射精 * 3
		CFLAG:RECEIVER:累计精浴 += NOWEX:SENDER:射精量
		CFLAG:SENDER:累计精浴射精 += NOWEX:SENDER:射精量
		CALL POISONBODY(RECEIVER, SENDER, 5)
		STAIN:RECEIVER:5 |= 4
		IF FLAG:时间停止 == 1
			CFLAG:RECEIVER:脸上精液 = 2
		ELSE
			CFLAG:RECEIVER:脸上精液 = 1
		ENDIF
		;TFLAG:特殊COM = 12 に该当するものがCSVに书いてない
		;(1=六九式 2=岩清水 3=G点刺激 4=乱牡丹 5=手淫口交 6= 7= 8= 六九式乳交 13=交互插入 )
		;TFLAGS.txtと书いてあることが违う（TFLAG:12 逆强奸继续フラグ（VA共通））
		;	→嫌な予感
		SIF TFLAG:特殊COM == 12
			STAIN:RECEIVER:口 |= 4
		;接触のリセット
		TEQUIP:SENDER:Ｂ接触部位 = 0
	;素股
	CASE 6
		EXP:RECEIVER:精液经验 += NOWEX:SENDER:射精
		CFLAG:RECEIVER:累计精浴 += NOWEX:SENDER:射精量
		CFLAG:SENDER:累计精浴射精 += NOWEX:SENDER:射精量
		CALL POISONBODY(RECEIVER, SENDER, 6)
		STAIN:RECEIVER:Ｖ |= 4
		;接触のリセット
		TEQUIP:SENDER:Ｃ接触部位 = 0
	;足交
	CASE 7
		EXP:RECEIVER:精液经验 += NOWEX:SENDER:射精
		CFLAG:RECEIVER:累计精浴 += NOWEX:SENDER:射精量
		CFLAG:SENDER:累计精浴射精 += NOWEX:SENDER:射精量
		CALL POISONBODY(RECEIVER, SENDER, 7)
	;体表
	CASE 8
		EXP:RECEIVER:精液经验 += NOWEX:SENDER:射精 * 2
		CFLAG:RECEIVER:累计精浴 += NOWEX:SENDER:射精量
		CFLAG:SENDER:累计精浴射精 += NOWEX:SENDER:射精量
		CALL POISONBODY(RECEIVER, SENDER, 8)
ENDSELECT
SIF SHIRAHU(RECEIVER)
	CALL ADDICTION_SOURCE(RECEIVER, SENDER)
ENDIF
CFLAG:RECEIVER:积攒度 = LIMIT(CFLAG:RECEIVER:积攒度, 0, 1000)
CFLAG:SENDER:积攒度 = MAX(CFLAG:SENDER:积攒度 - NOWEX:SENDER:射精 * 50, 0)

;射精された精子を子宫内へ送る处理
@SAMEN_SHOOT3(射精した精液量, 射精した角色, 射精された角色)
#DIM 射精した精液量
#DIM 射精された角色
#DIM 减算补正值
#DIM 加算值
#DIM 射精した角色のＣ感觉

;使われてない……
#DIM 射精した角色

;73,插入子宫口玩弄
;从后面将上书きされてるけどこれ意味有る？
IF SELECTCOM == 73
	减算补正值 = 100
ELSE
	SELECTCASE TEQUIP:射精された角色:子宫
	CASE 0
		减算补正值 = 150
	CASE 1
		减算补正值 = 120
	CASE 2
		减算补正值 = 100
ENDSELECT
ENDIF

;だいしゅきホールド
SIF TCVAR:射精された角色:Counter行动的派生 == 2 && (TCVAR:射精された角色:Counter行动 == 500 || TCVAR:射精された角色:Counter行动 == 5001)
	减算补正值 -= 30

射精した角色のＣ感觉 = ABL:射精した角色:Ｃ感觉
IF 射精した角色のＣ感觉 >= 17
	减算补正值 -= 100
ELSEIF 射精した角色のＣ感觉 >= 13
	减算补正值 -= 75
ELSEIF 射精した角色のＣ感觉 >= 9
	减算补正值 -= 50
ELSEIF 射精した角色のＣ感觉 >= 5
	减算补正值 -= 25
ENDIF

IF 减算补正值 < 0
	减算补正值 = 0
ENDIF

加算值 = 射精した精液量 - 减算补正值

IF !TALENT:射精された角色:路人
	SELECTCASE EXP:射精された角色:子宫口开发经验
		CASE IS >= 300
		CASE IS > 100
			加算值 = 加算值 * 2 / 3
		CASEELSE
			加算值 /= 2 
	ENDSELECT
ENDIF

;计算结果が负なら加算しない
IF 加算值 < 0
	RETURN 0
ENDIF
TCVAR:射精した角色:种付け量 += 加算值
EX:射精された角色:子宫内精液 += 加算值
CFLAG:射精された角色:(射精した角色+2000) += 加算值

;通常の指令外で你がＶに出した时の处理を批量でやる函数
;基本的に自慰暴露ックス用
@ADD_SAMEN_V(ARG, 射精した精液量,无自觉)
#DIM 射精した精液量
