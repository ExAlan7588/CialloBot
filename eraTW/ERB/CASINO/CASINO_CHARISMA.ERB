;红魔赌场用初期设定
@SET_CHARISMA(TYPE)
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS TYPE
#DIM LCOUNT
SELECTCASE TYPE
;まだ筹码を设定していないデータ用
CASE "NEW"
	FLAG:筹码人气 = 100
	FLAG:筹码变动状况 = 5
	FLAG:￥人气 = 100
	FLAG:￥变动状况 = 5
;すでに筹码を设定している周回用
;要するに红魔赌场の初期化指令だがカモられ指数は初期化しない
CASE "RESET"
	FLAG:筹码流通 = 0
	FLAG:筹码人气 = 100
	FLAG:筹码变动状况 = 5
	FLAG:筹码变动累积 = 0

	FLAG:￥流通 = 0
	FLAG:￥人气 = 100
	FLAG:￥变动状况 = 5
	FLAG:￥变动累积 = 0
ENDSELECT

@SET_THISMONTHCHARISMA
#DIM 筹码变动累积
#DIM 变动幅

筹码变动累积 = 0

;初日は150~250
CHARISMA_RATE:0 = 150 + RAND:100
CHARISMA_TENDENCY:0 = 5
FOR LOCAL,1,32
	IF 筹码变动累积 >= 10
		;适正レートだと安定しやすい
		IF INRANGE(CHARISMA_TENDENCY:(LOCAL - 1),150,250) && !RAND:2
			CHARISMA_TENDENCY:LOCAL = 5
		ELSEIF CHARISMA_TENDENCY:(LOCAL - 1) == 5
			;安定からは极端な变动をしない
			CHARISMA_TENDENCY:LOCAL = RAND(3,8)
		ELSEIF CHARISMA_RATE:(LOCAL -1) > 250
			CHARISMA_TENDENCY:LOCAL = RAND(2, 8)
		ELSEIF CHARISMA_RATE:(LOCAL -1) < 150
			CHARISMA_TENDENCY:LOCAL = RAND(3, 9)
		ELSE
			CHARISMA_TENDENCY:LOCAL = RAND(2, 9)
		ENDIF
		筹码变动累积 = 0
	ELSE
		;变动しないなら前期と同じ
		CHARISMA_TENDENCY:LOCAL = CHARISMA_TENDENCY:(LOCAL -1)
	ENDIF
	;前期筹码价格に応じて补正
	SELECTCASE CHARISMA_RATE:(LOCAL -1)
		CASE IS < 50
			IF !RAND:4
				CHARISMA_TENDENCY:LOCAL = 9
				筹码变动累积 = 0
			ENDIF
		CASE 50 TO 100
			IF !RAND:5
				CHARISMA_TENDENCY:LOCAL = 8
				筹码变动累积 = 0
			ENDIF
		CASE 250 TO 300
			IF !RAND:5
				CHARISMA_TENDENCY:LOCAL = 2
				筹码变动累积 = 0
			ENDIF
		CASE IS > 300
			IF !RAND:4
				CHARISMA_TENDENCY:LOCAL = 1
				筹码变动累积 = 0
			ENDIF
	ENDSELECT

	SELECTCASE CHARISMA_TENDENCY:LOCAL
		CASE 1
			变动幅 = - RAND:30 - 100
			筹码变动累积 += 3 + RAND:2 + RAND:3
		CASE 2
			变动幅 = RAND:20 - 50
			筹码变动累积 += 2 + RAND:2 + RAND:2
		CASE 3
			变动幅 = RAND:10 - 30
			筹码变动累积 += 1 + RAND:3 + RAND:2
		CASE 4
			变动幅 = -RAND:5 - 5
			筹码变动累积 += 1 + RAND:3 + RAND:2
		CASE 5
			变动幅 = RAND:10 - 5
			筹码变动累积 += 1 + RAND:3
		CASE 6
			变动幅 = RAND:5 + 5
			筹码变动累积 += 1 + RAND:3 + RAND:2
		CASE 7
			变动幅 = 30 - RAND:10
			筹码变动累积 += 1 + RAND:3 + RAND:3
		CASE 8
			变动幅 = RAND:20 + 50
			筹码变动累积 += 2 + RAND:2 + RAND:2
		CASE 9
			变动幅 = RAND:30 + 100
			筹码变动累积 += 3 + RAND:2 + RAND:3
	ENDSELECT

	CHARISMA_RATE:LOCAL = CHARISMA_RATE:(LOCAL -1) + 变动幅
	CHARISMA_RATE:LOCAL = LIMIT(CHARISMA_RATE:LOCAL,1,400)
	SIF CHARISMA_RATE:LOCAL == 1 || CHARISMA_RATE:LOCAL == 400
		筹码变动累积 = 10
NEXT

