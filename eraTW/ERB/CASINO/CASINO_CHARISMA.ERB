@CASINO_TRADE
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM LCOUNT
#DIM C_RATE
#DIM C_MAX
#DIM C_PRICE
#DIM Y_RATE
#DIM Y_MAX
#DIM Y_PRICE
C_RATE = CHARISMA_NOW()
;Y_RATEはC_RATEの1/100だが精度の关系で不使用
;Y_RATE = FUNC_CASINO("TRADE") / 100
;C购入取引に使用筹码入力の上限
C_MAX = MIN(CM_LIMIT - MONEY:2, MONEY / C_RATE)

PRINTFORML 妖精女仆
PRINTFORML 　欢迎光临
PRINTFORML 　这里可以用￥来交换筹码
IF C_RATE < 100
	PRINTFORML 　非常抱歉
	PRINTFORML 　现在无法购买筹码。
ENDIF
PRINTFORML 你の保有￥　　　　　　%金额表示(MONEY), 13, RIGHT%
PRINTFORML 你拥有的筹码　　　%三桁区切(MONEY:2), 13, RIGHT% / %三桁区切(CM_LIMIT)%
PRINTFORMW 现在的￥/筹码兑换率 　%金额表示(C_RATE), 13, RIGHT%
DRAWLINE
CALL ASK_M(@"购入筹码",C_RATE > 100 && C_MAX > 0, @"卖出筹码",FLAG:会员ランク,"放弃",1)
SELECTCASE RESULT
	CASE 0
		IF CM_LIMIT <= MONEY:2
			PRINTFORML 对不起、客人你的筹码已经达到上限了、请使用后再购买
			PRINTFORML 或提高会员等级、欢迎再次光临
			RETURN
		ENDIF
		PRINTFORML 妖精女仆
		PRINTFORML 　请输入要买的筹码数量（上限%三桁区切(C_MAX)%）
		FOR LCOUNT, 0, LOG10(C_MAX) + 1
			LOCAL = POWER(10, LCOUNT)
			PRINTFORML [{LOCAL, LOG10(C_MAX) + 1}] - 购入{LOCAL}筹码
		NEXT
		SIF C_MAX >= 4
			PRINTFORML [{C_MAX / 2, LOG10(C_MAX) + 1}] - 购入最大值的一半
		PRINTFORML [{C_MAX, LOG10(C_MAX) + 1}] - 购入最大值
		PRINTFORML [{0, LOG10(C_MAX) + 1}] - 放弃
		INPUT
		IF INRANGE(RESULT, 1, C_MAX)
			C_PRICE = RESULT
			Y_PRICE = C_PRICE * C_RATE
			CALL CASINO_MONEY("￥", -Y_PRICE)
			CALL CASINO_MONEY("筹码", C_PRICE)
			PRINTFORML 妖精女仆
			PRINTFORML 　感谢您的使用
			PRINTFORMW 　购买%三桁区切(C_PRICE)%个筹码花费了%金额表示(Y_PRICE)%
		ENDIF
	CASE 1
		CALL CM_BALANCE
	CASE 2
		RETURN
ENDSELECT

;为替相场の变动
@MOENY_HENDOU
#LOCALSIZE 1
#LOCALSSIZE 1
IF FLAG:筹码变动累积 >= 10
	FLAG:筹码变动状况 = RAND(1, 10)
	SELECTCASE FLAG:筹码变动状况
		CASE 9
			CALL SHOW_REMILLIA_INTERVIEW
		CASE 1
			CALL SHOW_CHARISMA_GUARD
	ENDSELECT
	FLAG:筹码变动累积 = 0
ENDIF

;极端な状况の补正
IF FLAG:筹码人气 >= 300
	SIF RAND:(FLAG:筹码人气 / 50)
		FLAG:筹码变动状况 = MAX(1, FLAG:筹码变动状况 - 2)
ELSEIF FLAG:筹码人气 <= 50 && INRANGE(FLAG:筹码变动状况, 1, 4)
	SIF RAND:(6 - FLAG:筹码变动状况)
		FLAG:筹码变动状况 += 2
ENDIF

SELECTCASE FLAG:筹码变动状况
CASE 1
	LOCAL = 60 + RAND:10
CASE 2
	LOCAL = 70 + RAND:10
CASE 3
	LOCAL = 80 + RAND:10
CASE 4
	LOCAL = 95 + RAND:5
CASE 5
	IF FLAG:筹码人气 <= 85
		FLAG:筹码人气 += RAND(1, 10)
	ELSEIF FLAG:筹码人气 >= 150
		FLAG:筹码人气 -= RAND(1, 10)
	ELSEIF RAND:2
		FLAG:筹码人气 += RAND(1, 10)
	ELSE
		FLAG:筹码人气 -= RAND(1, 10)
	ENDIF
CASE 6
	LOCAL = 105 + RAND:5
CASE 7
	LOCAL = 125 + RAND:10
CASE 8
	LOCAL = 150 + RAND:50
CASE 9
	LOCAL = 200 + RAND:100
ENDSELECT

IF FLAG:筹码变动状况 == 5
	FLAG:筹码变动累积 += 5
ELSE
	FLAG:筹码人气 = FLAG:筹码人气 * LOCAL / 100
	FLAG:筹码变动累积 += ABS(5 - FLAG:筹码变动状况) + RAND:2
ENDIF

FLAG:筹码人气 = MAX(10, FLAG:筹码人气)
TIMES FLAG:筹码流通, 0.98



