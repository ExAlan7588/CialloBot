@MESSAGE_COM60_1
;-------------------------------------------------
;60,正常位（シラフ）
;-------------------------------------------------
IF TFLAG:193
	;抜かず
	IF TCVAR:V插入管理FLAG == PLAYER && TARGET == TFLAG:103
		IF NOWEX:MASTER:射精
			PRINTFORML \@ OPENPLACE(CFLAG:MASTER:当前位置) == 2 ? 紧紧搂住%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的腰 # 把%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%按在墻上 \@激烈的动着腰、
			PRINTFORML 在射精的瞬间将%TINKO(PLAYER)%深深突入了膣内最深处…
		ELSEIF TCVAR:102
			PRINTFORML 在%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的膣内射入精液之后、
			PRINTFORML 趁着%TINKO(PLAYER)%尚未失去硬度再次开始动起了腰…
		ELSEIF TEQUIP:体位２ == 1
			;调教者についてる
			IF TALENT:PLAYER:2 & 2
				PRINTFORML 在品味着%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%膣内的触感的同时、再次开始动起了腰…
			;穿戴式假阴茎を使用している场合
			ELSE
				PRINTFORML 没有从%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的膣内拔出、再次开始动起了腰…
			ENDIF
		ELSE
			IF TEQUIP:体位２ == 1
				PRINTFORML 不从%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的膣内拔出%TINKO(PLAYER)%并再次开始动起了腰…
			;背面立位から
			ELSE
				PRINTFORML 暂时从%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%体内拔出%TINKO(PLAYER)%并让其转向这边
				PRINTFORML 抬起%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的一只脚插入进去再次开始动起了腰…
			ENDIF
		ENDIF
	ELSEIF TALENT:处女 == 1
		PRINTFORML 抬起%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的一只脚%SLIT(TARGET)%
		PRINTFORML 将龟头一口气捅了进去…
	ELSE
		IF MASTER_POSE(SET_V,SET_P,1) && MASTER_POSE(SET_V,SET_P,1) != TARGET
			PRINTFORML 从%NAME_OUTPUT_TRANSLATION("CALLNAME", (MASTER_POSE(SET_V,SET_P,1)))%体内拔出%TINKO(PLAYER)%的同时
			PRINTFORML 插入了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%…
		ELSE
			PRINTFORM 抬起%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的一只脚
			SIF TEQUIP:0 & 2
				PRINTFORM 拨开%SHOW_BOTTOM(TARGET,2)%、
			PRINTFORML 用%TINKO(PLAYER)%抵住%SLIT(TARGET)%并捅了进去…
		ENDIF
		IF TALENT:恋慕
			IF TALENT:处女
				PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%一边忍耐着痛楚一边紧紧抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%…
			ENDIF
		ENDIF
	ENDIF
;正常位
ELSE
	;抜かず
	IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
		IF NOWEX:MASTER:射精
			PRINTFORML 抱住仰面躺着的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%并动起了腰、
			PRINTFORML 在射精的瞬间将%TINKO(PLAYER)%深深突入了膣内最深处…
		ELSEIF TCVAR:102
			PRINTFORML 在%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的膣内射入精液之后、
			PRINTFORML 趁着%TINKO(PLAYER)%尚未失去硬度再次开始动起了腰…
		ELSEIF TEQUIP:体位２ == 1
			;调教者についてる
			IF TALENT:PLAYER:2 & 2
				PRINTFORML 在品味着%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%膣内的触感的同时、再次开始动起了腰…
			;穿戴式假阴茎を使用している场合
			ELSE
				PRINTFORML 没有从%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的膣内拔出、再次开始动起了腰…
			ENDIF
		ELSE
			PRINTFORML 不从膣内拔出并让%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%仰面躺下、再次开始动起了腰…
		ENDIF
	ELSE
		IF TALENT:处女 == 1
			PRINTFORML 打开仰面躺着的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的腿将龟头浅浅的插进去之后、
			PRINTFORML 贯穿了%SLIT(TARGET)%…
		ELSEIF MASTER_POSE(SET_V,SET_P,1) && MASTER_POSE(SET_V,SET_P,1) != TARGET
			PRINTFORML 从%NAME_OUTPUT_TRANSLATION("CALLNAME", (MASTER_POSE(SET_V,SET_P,1)))%体内拔出%TINKO(PLAYER)%的同时
			PRINTFORML 插入了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%…
		ELSE
			PRINTFORM 对仰面躺着的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%
			SIF TEQUIP:0 & 2
				PRINTFORM 拨开%SHOW_BOTTOM(TARGET,2)%、
			PRINTFORML 用%TINKO(PLAYER)%抵住%SLIT(TARGET)%并捅了进去…
		ENDIF
		IF TALENT:恋慕
			IF TALENT:处女 && TALENT:处女 != -1
				PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%一边忍耐着痛楚一边紧紧抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%…
			ENDIF
		ENDIF
	ENDIF
ENDIF

