;-------------------------------------------------
;67,对面座位
;-------------------------------------------------
@MESSAGE_COM67
IF FLAG:70 == 1
	IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
		IF NOWEX:MASTER:射精
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%抱住时停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%、
			PRINTFORML 在感受着%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的气味的同时、顺从着上涌的冲动向上耸动起了腰部…
		ELSEIF TEQUIP:体位２ == 4
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%吻向了时停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%、在舌头交织在一起的同时再次开始摇晃起了腰身…
		ELSE
			PRINTFORM 没有从膣内拔出%TINKO(PLAYER)%并搂住了时停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
			PRINTFORML 一边品尝着口内的滋味一边再次开始摇晃起腰身…
		ENDIF
	ELSE
		PRINTFORM 抱起时停中的%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
		PRINTFORM 用%TINKO(PLAYER)%对准
		SIF TEQUIP:0 & 2
			PRINTFORM 从拨开的%SHOW_BOTTOM(TARGET,2)%的空隙中露出的
		;润滑LV4以上
		SIF PALAM:润滑 >= PALAMLV:4
			PRINT 充分湿润的
		;矮小体型
		IF TALENT:体型 < 0
			PRINT 幼小肉穴
		ELSE
			PRINT 膣穴
		ENDIF
		PRINTFORM 插入进去、
		;口球の有无
		SIF TEQUIP:口球 == 0
			PRINT 一边接吻一边
		PRINTFORML 把%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%当做飞机杯如同在自慰一般摇动起腰身…
	ENDIF
ELSE
	IF TCVAR:100 == PLAYER && TARGET == TFLAG:103
		IF NOWEX:MASTER:射精
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%抱住%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%、
			PRINTFORML 顺从着上涌的冲动向上耸动起了腰部…
		ELSEIF TEQUIP:体位２ == 4
			PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%亲吻着%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%、在舌头交织在一起的同时再次开始摇晃起了腰身…
		ELSE
			PRINTFORM 没有从膣内拔出%TINKO(PLAYER)%并紧紧地抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
			PRINTFORML 在舌头交织在一起的同时再次开始摇晃起了腰身…
		ENDIF
	ELSE
		PRINTFORM 抱起%NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%的身体、
		PRINTFORM 慢慢放下的同时插入了
		SIF TEQUIP:0 & 2
			PRINTFORM 从拨开的%SHOW_BOTTOM(TARGET,2)%的空隙中露出的
		PRINTFORM %SLIT(, 1)%插入进去、
		;口球の有无
		SIF TEQUIP:口球 == 0
			PRINT 一边接吻一边
		PRINTL 摇动起了腰身…
	ENDIF
	;SIF TALENT:恋慕
	;	PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", TARGET)%用手环抱住了%NAME_OUTPUT_TRANSLATION("CALLNAME", PLAYER)%的脖子…
ENDIF
