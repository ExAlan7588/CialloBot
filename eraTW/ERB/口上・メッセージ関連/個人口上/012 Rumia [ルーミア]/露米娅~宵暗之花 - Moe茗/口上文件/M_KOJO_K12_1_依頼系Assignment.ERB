#DIM  IRAI_NO   ;抑制させる一般依赖ナンバー
;动きを抑制させたい场合は、」RETURN 0」に变更すること
SELECTCASE IRAI_NO
	;扫除を手传って（发生条件：TARGETがメインマップ在住）
	;帮忙做扫除（发生条件：TARGETがメインMap在住）
	CASE 1
		RETURN 1
	;给花浇水（发生条件：TARGETがMASTER侵入可能の自宅住まいかつ、メインMap在住かつ、亲密２以上）
	CASE 2
		RETURN 1
	;一起玩吧（发生条件：TARGETがTALENT:幼稚持ち）
	CASE 3
		RETURN 1
	;想要学习料理（发生条件：TARGETの料理技能がMASTER未满かつ、MASTERの料理技能が２以上かつ、亲密が３以上）
	CASE 4
		RETURN 1
	;关于特训（发生条件：TARGETの战斗能力がMASTER以下かつ、亲密が２以上かつ人里以外）
	CASE 5
		RETURN 1
	;谁来告诉我！（发生条件：TARGETの教养がMASTER以下）
	CASE 6
		RETURN 1
	;大扫除还没结束（发生条件：TARGETがメインMap在住かつ、冬の15日から31日までかつ、亲密が３以上）
	CASE 7
		RETURN 0
	;送信（发生条件：TARGETの信赖度が30以上かつ、TARGETに文通相手が设定されている（FUNC_IRAI.ERBの965目以降に存在）
	CASE 8
		RETURN 1
	;谁的遗失物（发生条件：TARGETの信赖度が30以上）
	CASE 9
		RETURN 1
	;怎么也找不到(そのまま落ちてる)（发生条件：TARGETの信赖度が30以上）
	CASE 10
		RETURN 1
	;怎么也找不到(谁かが拾ってる)（发生条件：TARGETの信赖度が30以上）
	CASE 11
		RETURN 1
	;惩らしめて战斗训练！（发生条件：TARGETのCSVに战斗能力が１以上ある场合）
	CASE 12
		RETURN 1
	;好吃的鱼が食べたいなぁ（发生条件：若鹭姫以外）
	CASE 13
		RETURN 1
	;薪を切らしてる（未实装）
	CASE 14
		RETURN 0
	;想被你干）发生条件：炮友or爱欲かつ积攒度700以上 or ヤリマン）
	CASE 15
		RETURN 0
	;烦恼相谈（发生条件：MASTERの话术技能が1以上）
	CASE 16
		RETURN 1
	;[SKIPSTART]
	;现状无し
	CASEELSE
		RETURN 0
	;[SKIPEND]
ENDSELECT

