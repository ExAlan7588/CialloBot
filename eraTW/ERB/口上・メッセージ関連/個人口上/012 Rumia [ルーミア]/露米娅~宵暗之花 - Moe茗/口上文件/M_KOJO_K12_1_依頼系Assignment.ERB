	RETURN 0
CASE "依赖破弃时"
	RETURN 0
CASE "依赖实行直前"
	RETURN 0
CASE "依赖实行直后"
	IF TFLAG:193 == 1
		;正解の选择肢
	ELSEIF TFLAG:193 == 2
		;ハズレの选择肢だったが逆に正解
	ELSEIF TFLAG:193 == -1
		;ハズレの选择肢
	ELSEIF TFLAG:193 == -2
		;正解の选择肢だったが逆にハズレ
	ENDIF
	RETURN 0
CASE "成功报告时"
;报酬：野菜か果物
	RETURN 0
CASE "失败报告时"
	RETURN 0
;CASE "依赖报告不要"
;＜この依赖にはありません＞
;	RETURN 0
CASE "口上委托率"
;＜この依赖特有の分岐です。(RESULT:100の值)％の概率で"口上専用"が发生します＞
	RESULT:100 = 0
;＜RESULT:YYに0以外の数值を入れると、该当のお恼みを发生させなくできます（詳しくは一般依赖16_お恼み相谈参照）＞
;1,2:弹幕ごっこの恼み 3,4:晩酌の恼み 5,6:本の恼み 7,8:香料の恼み
;9,10:音乐の恼み 11,12:服の恼み 13:发型の恼み 14:参拝の恼み 15:ダイエットの恼み
;16,17:菜园の恼み 18:休日の恼み 19:人气の恼み 20:集中力の恼み 21:御三家の恼み 22:进化の恼み
	RESULT:1 = 0
	RESULT:2 = 0
	RETURN 1
CASE "口上専用"
;＜この依赖特有の分岐です。(口上委托率)％の概率で发生します＞
;16,お恼み相谈 の场合
;RESULT        发生判定,0だと发生しません
;RESULT:1      正解の选择肢（ただしRESULT:2が0だと、一定概率で逆にハズレる）
;RESULT:2      逆张り正解／ハズレ判定の消去,1だと逆张りが无くなります
;RESULTS:0     お恼み文章（自动で『』で囲まれます）
;RESULTS:1～3  选择肢１～３
;何も发生しない状态になると无限ループになるので注意してください
;●追记メモ
;RESULT:1を4～6にするとそれぞれ1～3がハズレ（残り2つが正解）になります
;RESULT:1を7にするとどれも正解になります
;RESULT:1を8にするとどれもハズレになります
;1～3以外の场合は逆张り判定が自动で无くなります
	RESULT = 0
;▼サンプル
[SKIPSTART]
	SELECTCASE RAND:5
	CASE 0
		;このお恼みは、恋慕で发生し、亲密によって正解が变わります
		SIF TALENT:12:3
			RESULT = 1
		RESULTS:0 = 赤ちゃん作るなら何人がいいかな？
		RESULTS:1 = ５人でバスケチームを作ろう
		RESULTS:2 = ９人で野球チームを作ろう
		RESULTS:3 = １１人でサッカーチームを作ろう
		IF ABL:9 >= 100
			RESULT:1 = 3
		ELSEIF ABL:9 >= 20
			RESULT:1 = 2
		ELSE
			RESULT:1 = 1
		ENDIF
	CASE 1
		;このお恼みは、无条件で发生し、正解は绝对に2番で固定化されます
		SIF 1
			RESULT = 1
		RESULTS:0 = どのジョブがいいかな？
		RESULTS:1 = かっこいしナイト
		RESULTS:2 = シーフが便利だよ
		RESULTS:3 = 青魔道士最强！
		RESULT:1 = 2
		RESULT:2 = 1
	CASE 2
		;このお恼みは、夏の月に发生し、正解がランダムで变わります
		SIF DAY:2 == 2
			RESULT = 1
		RESULTS:0 = どの水着を买おうかな？
		RESULTS:1 = かわいい连衣裙
		RESULTS:2 = クールなセパレート
		RESULTS:3 = 色っぽいビキニ
		RESULT:1 = RAND:3 + 1
	CASE 3
		;このお恼みは、无条件で发生し、1番と3番が正解になります（5 - 3 = 2番がハズレになります）
		SIF 1
			RESULT = 1
		RESULTS:0 = ワンちゃんの名前は何にしよう？
		RESULTS:1 = ボロンゴ
		RESULTS:2 = ゲレゲレ
		RESULTS:3 = プックル
		RESULT:1 = 5
	CASE 4
		;このお恼みは、无条件で发生し、どれを选んでも正解になります
		SIF 1
			RESULT = 1
		RESULTS:0 = 夕饭は何がいい？
		RESULTS:1 = 肉じゃが
		RESULTS:2 = コロッケ
		RESULTS:3 = 烧き鱼
		RESULT:1 = 7
	ENDSELECT
[SKIPEND]
	SIF !RESULT
		RESTART
	RETURN 1
ENDSELECT
