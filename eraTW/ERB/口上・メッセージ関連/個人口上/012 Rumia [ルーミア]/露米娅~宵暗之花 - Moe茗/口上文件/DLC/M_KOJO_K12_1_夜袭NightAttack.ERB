                经验增加量:9 += 正戏回数 * MIN(MAX(ABL:12:4 / 10,1) , 2) + CFLAG:12:K12C_1_自记录积攒度 / 200 
                经验增加量:25 += 正戏回数 * MIN(MAX(ABL:12:4 / 10,1) , 2) + ABL:12:4  + CFLAG:12:K12C_1_自记录积攒度 / 200           
                经验增加量:27 += 正戏回数 * MIN(MAX(ABL:12:4 / 10,1) , 2) + ABL:12:4 * 2  + CFLAG:12:K12C_1_自记录积攒度 / 150
            ENDIF 
        ENDIF 
        ;小豆豆❤
        IF ABL:12:0 > 10 
            经验增加量:5 += (正戏回数 * MIN(MAX((ABL:12:0  / 5) ,1 ) , 5) + CFLAG:12:K12C_1_自记录积攒度 / 100 + RAND:3)
            经验增加量:0 += (正戏回数 * MIN(MAX(ABL:12:0 / 10,1) , 5) + ABL:12:0 * 2 + ABL:12:11 + CFLAG:12:K12C_1_自记录积攒度 / 150 + RAND:3)
        ELSEIF  ABL:12:1 > 5
            经验增加量:5 += (正戏回数 * MIN(MAX((ABL:12:0  / 5) ,1 ) , 3) + CFLAG:12:K12C_1_自记录积攒度 / 200 + RAND:3)
            经验增加量:0 += (正戏回数 * MIN(MAX((ABL:12:0  / 5) ,1 ) , 3) + ABL:12:0 * 2 + ABL:12:11 + CFLAG:12:K12C_1_自记录积攒度 / 150 + RAND:3)
        ELSE
            经验增加量:5 += (正戏回数 * MIN(MAX((ABL:12:0  / 5) ,1 ) , 2) + CFLAG:12:K12C_1_自记录积攒度 / 300 + RAND:3)
            经验增加量:0 += (正戏回数 * MIN(MAX((ABL:12:0  / 3) ,1 ) , 2) + ABL:12:0 * 2 + ABL:12:11 + CFLAG:12:K12C_1_自记录积攒度 / 150 + RAND:3)
        ENDIF 
        ;小欧派❤
        IF ABL:12:3 > 10 
            经验增加量:8 += (正戏回数 * MIN(MAX((ABL:12:3  / 5) ,1 ) , 5) + CFLAG:12:K12C_1_自记录积攒度 / 100 + RAND:3)
            经验增加量:3 += (正戏回数 * MIN(MAX((ABL:12:3 / 5),1) , 5) + ABL:12:3 * 2 + ABL:12:11/ 5  + CFLAG:12:K12C_1_自记录积攒度 / 150 + RAND:3)
        ELSEIF  ABL:12:3 > 5
            经验增加量:8 += (正戏回数 * MIN(MAX((ABL:12:3  / 5) ,1 ) , 3) + CFLAG:12:K12C_1_自记录积攒度 / 200 + RAND:3)
            经验增加量:3 += (正戏回数 * MIN(MAX((ABL:12:3  / 5) ,1 ) , 3) + ABL:12:3 * 2 + ABL:12:11/ 3 + CFLAG:12:K12C_1_自记录积攒度 / 150 + RAND:3)
        ELSE
            经验增加量:8 += (正戏回数 * MIN(MAX((ABL:12:3  / 5) ,1 ) , 2) + CFLAG:12:K12C_1_自记录积攒度 / 300 + RAND:3)
            经验增加量:3 += (正戏回数 * MIN(MAX((ABL:12:3  / 3) ,1 ) , 2) + MIN(ABL:12:3,1) + ABL:12:11 / 3 + CFLAG:12:K12C_1_自记录积攒度 / 150 + RAND:3)
        ENDIF 
    ;抱住露米娅酱的方式有很多种……哪一种她最喜欢呢？猜猜看吧❤
    SELECTCASE K12_1_NIGHT_ATTACK:8
        ;正常位
        CASE 10 
                TIMES 注入量 , 1.05 
        ;后背位
        CASE 11
            IF K12_1_NIGHT_ATTACK:1
                IF  K12_1_NIGHT_ATTACK:1 == 2
                    TIMES 经验增加量:4 , 0.85
                    TIMES 经验增加量:9 ,0.85
                    TIMES 经验增加量:27 ,0.85
