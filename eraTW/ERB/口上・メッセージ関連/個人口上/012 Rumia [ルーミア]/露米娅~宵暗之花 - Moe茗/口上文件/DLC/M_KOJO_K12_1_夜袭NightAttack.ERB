                TIMES 经验增加量:5 ,0.90 
                TIMES 经验增加量:0 ,0.90 
                TIMES 经验增加量:8 ,0.85 
                TIMES 经验增加量:3 ,0.85 
                TIMES 经验增加量:1 ,1.10 
                TIMES 注入量 , 1.25
        ;骑乘位
        CASE 12
                TIMES 经验增加量:5 ,1.10 
                TIMES 经验增加量:0 ,1.10 
                TIMES 经验增加量:8 ,0.85 
                TIMES 经验增加量:3 ,0.85 
                TIMES 经验增加量:1 ,1.10 
        ;抱地藏
        CASE 13
            IF K12_1_NIGHT_ATTACK:1
                IF  K12_1_NIGHT_ATTACK:1 == 2
                    TIMES 经验增加量:4 ,1.00
                    TIMES 经验增加量:9 ,1.17
                    TIMES 经验增加量:27 ,1.17
                ELSE 
                    TIMES 经验增加量:4 ,1.07
                    TIMES 经验增加量:9 ,1.07
                    TIMES 经验增加量:27 ,1.07
                ENDIF 
            ENDIF 
                TIMES 经验增加量:5 ,1.14
                TIMES 经验增加量:0 ,1.14 
