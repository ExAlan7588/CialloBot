@M_KOJO_Moe_K12_NIGHT_ATTACK
#DIM 注入量
#DIM 正戏回数
#DIM 经验增加量,58
#DIM 好感度上升量
#DIM 信赖上升量
#DIMS 日记记述
VARSET 经验增加量
;位置的设定
IF CFLAG:12:311 == CFLAG:0:311 
    CFLAG:12:300 = CFLAG:12:311
ELSE 
    CFLAG:12:300 = CFLAG:0:311
ENDIF   
;去除睡眠
CFLAG:12:313 = 0 
;记录夜袭次数
CFLAG:12:K12C_1_夜袭总次数 += 1
;口上色的设定
SETCOLOR MOE_RUMIA
;反抗禁止，愤怒禁止
SIF MARK:12:3 && (TCVAR:12:132 || CFLAG:12:318)
RETURN 0 
CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("START")
;CALL M_Moe_K12_ASK_M("可以哟",1,"今晚就先睡觉吧",1)
IF !RESULT 
    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("PERMIT")
    ;M_Moe_K12_ASK_M("可以哟",1,"今晚就先睡觉吧",1)
    IF !RESULT 
    ELSE 
        CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("REFUSE_AFTER_KISS")
        CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("TRY_TO_SLEEP")
    ENDIF 
ELSE 
    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("REFUSE")
    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("TRY_TO_SLEEP")
ENDIF 
CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("TOUCH")
;CALL M_Moe_K12_ASK_M("让露米娅激烈地去",1,"温柔地爱抚",1)
IF !RESULT 
    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("ARDENT_CUM")
    ;去完就结束了
ELSE 
    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("SOFT_CUM")
    IF (TALENT:0:2 & 1)
        ;自机女就跳过所有
    ELSE 
        ;如果是恋慕就触发素股，没有的话就触发视奸）
        IF TALENT:12:3 
            CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("RUBBING")
        ELSE 
            CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("STALK")
        ENDIF 
        ;CALL M_Moe_K12_ASK_M("推倒露米娅",1,"继续享受",1)
        ;CALL M_Moe_K12_ASK_M("还要，做更过分的事情（处女丧失警告）",1,"我来帮你吧",1)
        IF !RESULT             ;推倒
            CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("PUSH_OVER")
            IF RESULT == -1 ;强制不允许的情况,妊娠
            ELSE 
                IF TALENT:12:0 && TALENT:12:3 
                    ;CALL M_Moe_K12_ASK_M("我来帮你穿衣服吧",1,"还要，做更过分的事情（处女丧失警告）",1)
                    ;这里反倒会出现穿衣服
