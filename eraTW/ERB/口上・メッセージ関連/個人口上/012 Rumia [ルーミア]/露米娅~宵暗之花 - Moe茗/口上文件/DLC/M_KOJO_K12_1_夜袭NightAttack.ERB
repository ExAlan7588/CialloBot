        经验增加量:0 += RAND:2 + 2
        经验增加量:5 +=  4
        经验增加量:10 += 12
        经验增加量:3 += RAND:2 + 2
        经验增加量:8 += 4
        IF ABL:12:1 > 10 
            经验增加量:6 +=  8
            经验增加量:10 += 8
        ELSEIF ABL:12:1 > 5 
            经验增加量:6 +=  4
            经验增加量:10 += 4
        ELSEIF ABL:12:1 > 1
            经验增加量:6 +=  3
            经验增加量:10 += 3
        ELSEIF ABL:12:2 > 10 
            经验增加量:7 +=  8
            经验增加量:10 += 8
        ELSEIF ABL:12:2 > 5 
            经验增加量:7 +=  4
            经验增加量:10 += 4
        ELSEIF ABL:12:2 > 1 
            经验增加量:7 +=  2
            经验增加量:10 += 2
        ENDIF 
        IF K12_1_NIGHT_ATTACK:1
            IF ABL:12:4 > 10
                经验增加量:4 += 5  
                经验增加量:9 += 5
                经验增加量:10 += 5      
            ELSEIF ABL:12:4 > 5
                经验增加量:4 += 2  
                经验增加量:9 += 2
                经验增加量:10 += 2      
            ELSEIF ABL:12:4 > 1 
                经验增加量:4 += 1
                经验增加量:9 += 1
                经验增加量:10 += 1
            ELSE 
                经验增加量:4 += 1
            ENDIF 
        ENDIF 
    ELSEIF K12_1_NIGHT_ATTACK:7 == 1 
        IF ABL:12:1 > 10 
            经验增加量:6 +=  4
            经验增加量:10 += 4
        ELSEIF ABL:12:1 > 5 
            经验增加量:6 +=  2
            经验增加量:10 += 2
        ELSEIF ABL:12:1 > 1
            经验增加量:6 +=  1
            经验增加量:10 += 1
        ELSEIF ABL:12:2 > 10 
            经验增加量:7 +=  4
            经验增加量:10 += 4
        ELSEIF ABL:12:2 > 5 
            经验增加量:7 +=  2
            经验增加量:10 += 2
        ELSEIF ABL:12:2 > 1 
            经验增加量:7 +=  1
            经验增加量:10 += 1
        ENDIF 
        IF K12_1_NIGHT_ATTACK:1
            IF ABL:12:4 > 10
                经验增加量:4 += 3
                经验增加量:9 += 3
                经验增加量:10 += 3      
            ELSEIF ABL:12:4 > 5
                经验增加量:4 += 1  
                经验增加量:9 += 1
                经验增加量:10 += 1      
            ELSE 
                经验增加量:4 += 1
            ENDIF 
        ENDIF 
    ENDIF 
ENDIF 
;最后……诶嘿嘿，对付半夜袭击人类的涩妖怪，就应该用肉棒狠狠地惩罚❤）
IF K12_1_NIGHT_ATTACK:8
    ;首先，露米娅酱到底想要，被中出多少次，注入多少白白的东西呢❤？
    IF ABL:12:11 > 20 
        正戏回数 = 10 + CFLAG:12:K12C_1_自记录积攒度 / 70 + (ABL:12:11 + 1) / 3 + RAND:7 + ABL:12:9 / 5
    ELSEIF ABL:12:11 > 10
        正戏回数 = 7 + CFLAG:12:K12C_1_自记录积攒度 / 110 + (ABL:12:11 + 1) / 3 + RAND:5 + ABL:12:9 / 5
    ELSEIF ABL:12:11 > 5
        正戏回数 = 4 + CFLAG:12:K12C_1_自记录积攒度 / 170 + (ABL:12:11 + 1) / 5 + RAND:4 + ABL:12:9 / 5
    ELSE
        正戏回数 = 2 + CFLAG:12:K12C_1_自记录积攒度 / 220 + (ABL:12:11 + 1) / 3 + RAND:3 
    ENDIF 
    EXP:MASTER:插入经验 += 正戏回数 * 4 
    EXP:MASTER:射精经验 += 正戏回数
    EXP:MASTER:绝顶经验 += 正戏回数
    注入量 = MAX(BASE:0:6,300,(ABL:12:1 * 20 + ABL:12:9 * 5 + ABL:12:10 * 5 + ABL:12:11 * 12 + ABL:12:12 * 50 + ABL:12:53 * 75 + ABL:12:54 * 125 +  CFLAG:12:K12C_1_自记录积攒度 / 5 )    ) 
    ;然后，因为拿走了露米娅酱的第一次，所以要好好回报呢❤
    SIF K12_1_NIGHT_ATTACK:2 
        TIMES 注入量, 1.1 
    SIF K12_1_NIGHT_ATTACK:9
        TIMES 注入量, 1.3
    SIF K12_1_NIGHT_ATTACK:8 == 11
        TIMES 注入量 , 1.05
    SIF TCVAR:12:142 ;发情了哦
        TIMES 注入量 , 1.12
    SIF K12_1_NIGHT_ATTACK:9
        注入量 += 100
    ;之后，露米娅酱粉嫩可爱的小穴，要尤其注意关照呢❤
        IF ABL:12:1 > 10 
            经验增加量:20 += 正戏回数 * 4 + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
            经验增加量:16 += 注入量 / 25 
            经验增加量:14 += 注入量 / 25 
            经验增加量:6 += 正戏回数 * 4 + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) +  MIN(ABL:12:1 / 2 ,10)
            经验增加量:1 += 正戏回数 * MIN(ABL:12:1 / 2 , 10)  + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
        ELSEIF  ABL:12:1 > 5
            经验增加量:20 += 正戏回数 * 2 + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
            经验增加量:16 += 注入量 / 25 
            经验增加量:6 += 正戏回数 * 2 + MIN(ABL:12:1 / 2 ,5) / 3 + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
            经验增加量:1 += 正戏回数 * MIN(ABL:12:1 / 2 ,4) + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
        ELSE
            经验增加量:20 += 正戏回数 * MIN(ABL:12:1,3) + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
            经验增加量:16 += 注入量 / 25 
            经验增加量:6 += 正戏回数 * 1 + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
            经验增加量:1 += 正戏回数 * MIN( ABL:12:1 + 1 , 2) + ( CFLAG:12:K12C_1_自记录积攒度 / 100 ) 
        ENDIF 
    ;再然后，露米娅酱水灵灵的嘴，小小的欧派，还有可爱的小豆豆，哪个都不能放过捏❤
        ;嘴巴
        IF K12_1_NIGHT_ATTACK:1
            IF  K12_1_NIGHT_ATTACK:1 == 2
                经验增加量:4 += 正戏回数 * MIN(MAX(ABL:12:4 / 5,1) , 4) + ABL:12:4 * 2 + CFLAG:12:K12C_1_自记录积攒度 / 200
                经验增加量:9 += 正戏回数 * MIN(MAX(ABL:12:4 / 10,1) , 4) + CFLAG:12:K12C_1_自记录积攒度 / 250
                经验增加量:25 += 正戏回数 * MIN(MAX(ABL:12:4 / 10,1) , 4) + ABL:12:4  + CFLAG:12:K12C_1_自记录积攒度 / 200           
