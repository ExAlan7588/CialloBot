                        CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("FALL_TO_SLEEPING")
                    ELSE 
                        CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("INSERT_CHOOSE")
                        ;直接选择，等于5就取消
                        IF RESULT == 5 
                            CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("FALL_TO_SLEEPING")
                        ELSE 
                            CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("INSERT_INTENSELY")
                        ENDIF 
                    ENDIF 
                ELSE 
                    ;直接选择，等于5就取消
                    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("INSERT_CHOOSE")
                    IF RESULT == 5 
                        CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("FALL_TO_SLEEPING")
                    ELSE 
                        CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("INSERT_INTENSELY")
                    ENDIF 
                ENDIF 
            ENDIF 
        ELSE 
            IF TALENT:12:3  ;恋慕继续享受，
                CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("RUBBING_CONTINUE")
                IF !RESULT  ;进入了
                    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("RUBBING_INSERT")
                    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("INSERT_INTENSELY")
                ELSE 
                    CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("RUBBING_CUM")
                ENDIF 
            ELSE    ;思慕就帮忙穿衣服
                CALL M_KOJO_Moe_K12_NIGHT_ATTACK_MESSAGE("FALL_TO_SLEEPING")
            ENDIF 
        ENDIF 
    ENDIF 
