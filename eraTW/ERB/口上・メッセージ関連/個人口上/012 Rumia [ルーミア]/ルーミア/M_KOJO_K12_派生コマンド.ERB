;==================================================
;7,插入子宫口玩弄
;==================================================
@M_KOJO_MESSAGE_SCOM_K12_7
CALL M_KOJO_MESSAGE_SCOM_K12_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K12_7_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
PRINTFORML 
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
	;	PRINTFORMW 「」
	;	GOTO _SIKYUUKOU
	;	RETURN 1
	;ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;射精1ターン后。射精するときの口上はNOWEX:MASTER:射精
	ELSEIF TCVAR:102 && !NOWEX:MASTER:射精
		IF (TALENT:恋慕 || TALENT:恋人) && CFLAG:允许无套 > 0
			SELECTCASE RAND:5
				CASE 1
					CALL HPH_PRINT("「用肉棒把子宫给堵住了…　就那么想让我怀孕吗？　呼呼HPH」","L")
				CASE 2
					CALL HPH_PRINT("「现在不能往里顶啊HPH　子宫正在欢欣雀跃哟…HPH」","L")
				CASE 3
					CALL HPH_PRINT("「这么深的地方被不停地欺负的话…　会开始想要小宝宝的哦HPH」","L")
				CASE 4
					CALL HPH_PRINT(@"「欸嘿嘿ーHPH　%CALLNAME:MASTER%真是美味啊。　请再来一次吧ーHPH」","L")
				CASEELSE
					CALL HPH_PRINT("「精液HPH　被挤进小腹深处了…HPH」","L")
			ENDSELECT
				
			SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 露米娅的肉膣紧紧地缠住肉棒、试图从%CALLNAME:MASTER%身上榨取更多的精液…
				CASEELSE
					PRINTFORMW 露米娅的子宫口为了吸尽%CALLNAME:MASTER%的精液而不断地吮吸着龟头…
			ENDSELECT
		ELSEIF CFLAG:既成事实 >= 1
			SELECTCASE RAND:5
				CASE 1
					PRINTFORMW 「今天明明一直在射个不停…　元气过头了啊」
				CASE 2
					PRINTFORMW 「刚刚才射进去的精液都溢出来了呢…　真是太H了♪」
				CASE 3
					PRINTFORMW 「呀ー嗯、在里面咕啾咕啾地响着…」
				CASE 4
					PRINTFORMW 「要一鼓作气、把精子射进最里面哦」
				CASEELSE
					PRINTFORMW 「还没有射够吗？　大色鬼…」
			ENDSELECT
		ENDIF
		RETURN 1
	ELSEIF TALENT:恋慕 || TALENT:恋人
		IF EXP:子宫口开发经验 >= 50
			SELECTCASE RAND:5
				CASE 1
					CALL HPH_PRINT(@"「好喜欢那里被搅来搅去的HPH　再多来几下HPH」","L")
				CASE 2
					CALL HPH_PRINT(@"「里面的嘴巴把%CALLNAME:MASTER%的鸡鸡吃掉了HPH　就这样满满地射出来哦HPH」","L")
				CASE 3
					IF TALENT:妊娠
						CALL HPH_PRINT("「子宫张开了…　宝宝要被精液盖住了…HPH」","L")
					ELSE
						CALL HPH_PRINT("「子宫张开了…　肉棒进来了哦…HPH」","L")
					ENDIF
				CASE 4
					CALL HPH_PRINT("「尽情往里挤吧HPH　直到能够到子宫的深处为止…HPH」","L")
				CASEELSE
					CALL HPH_PRINT(@"「小穴全部都被变成属于%CALLNAME:MASTER%的东西了HPH」","L")
					CALL HPH_PRINT("「在最深处进行的性交、真是舒服呢HPH」","L")
			ENDSELECT
				SELECTCASE RAND:4
					CASE 1
						PRINTFORMW 露米娅承受着%CALLNAME:MASTER%的突刺、脸上染上了快乐的色彩…
					CASE 2
						PRINTFORMW 露米娅被%CALLNAME:MASTER%的阴茎蹂躏着发出了甜美的娇喘声…
					CASE 3
						PRINTFORMW 每当露米娅被捅到最深处时她的穴肉就会温和地缠住%CALLNAME:MASTER%的阴茎…
					CASEELSE
						PRINTFORMW 露米娅的子宫口如同在欢迎阴茎的到来一样将其轻柔地包裹起来…
				ENDSELECT
					
		ELSE
			SELECTCASE RAND:5
				CASE 1
					PRINTFORML 「承受住了%CALLNAME:MASTER%的全部、好开心啊…♪」
				CASE 2
					PRINTFORML 「稍微有点紧呢ー。%CALLNAME:MASTER%没觉得痛吧？」
				CASE 3
					PRINTFORML 「%CALLNAME:MASTER%的真是大呢ー。都顶到肚脐的位置了哦」
				CASE 4
					PRINTFORML 「都因为%CALLNAME:MASTER%我的肚子才鼓鼓的。就好像怀孕了一样…♪」
				CASEELSE
					PRINTFORML 「虽然最深处还在疼、但是又有些酥酥麻麻的感觉…」
			ENDSELECT
				PRINTFORMW 露米娅的膣肉紧紧地缠住了突刺进去的阴茎…
		ENDIF
		RETURN 1
	ELSEIF CFLAG:既成事实 >= 1
		SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「小腹、好难受啊…」
			CASE 2
				PRINTFORMW 「我说啊到此为止不要再往里进了ー…」
			CASE 3
				PRINTFORMW 「呀ー、那里好疼ー」
			CASE 4
				PRINTFORMW 「啊啊…　被进到最里面了哦…」
			CASEELSE
				PRINTFORMW 「小宝宝的房间、要被压坏了…！」
		ENDSELECT
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
;$_SIKYUUKOU
