	;ELSE
	;	PRINTFORMW 「素敌なプレゼントを贳ちゃた」
	;ENDIF
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物をお气に入りにする（↑の大事にするから续き）（700点以上かつ最高得点）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 3
	;IF STRCOUNT(SENSE, "身につける/")
	;	PRINTFORMW 「明日からこれを付けて行こうと」
	;ELSE
	;	PRINTFORMW 「明日からこれを持て行こうと」
	;ENDIF
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物をとりあえず保管する（400～699点）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 4
	;PRINTFORMW 「とりあえず饰ておこう」
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物をこっそり质に入れる（400点未满）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 5
	;PRINTFORMW 「私これ嫌いなのよね～」
