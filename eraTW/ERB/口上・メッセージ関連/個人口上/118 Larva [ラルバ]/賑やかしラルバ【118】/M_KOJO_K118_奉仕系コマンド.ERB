;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K118_80
CALL M_KOJO_MESSAGE_COM_K118_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K118_80_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「这、这是在玩什么……？ 没有被人看到吧……？」
		PRINTFORML 「……但、但是这个，说不定有点像我的角………」
		PRINTFORMW 拉尔瓦下定决心，战战兢兢地碰了一下%CALLNAME:MASTER%的肉棒……。
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:时间停止
		PRINTFORMW %CALLNAME:MASTER%在无法动弹的拉尔瓦的腋下用肉棒摩擦着……。
		RETURN 1
	;それ以外
	ELSE
		IF NOWEX:PLAYER:射精
			PRINTFORML 「%CALLNAME:MASTER%，舒服吗……？」
			PRINTFORMW 拉尔瓦突然加快了手淫的速度……。
		ELSE
			PRINTFORML 「这个，总觉得握着很安心……」
			PRINTFORMW 拉尔瓦红着耳朵继续手淫……。
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_K118_81
CALL M_KOJO_MESSAGE_COM_K118_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K118_81_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「这、舔这个的游戏……？ 比我的触角还要奇怪的气味……」
		PRINTFORMW 拉尔瓦一边踌躇着一边开始啾噜啾噜地舔龟头……。
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORML 把肉棒插进说不了话的拉尔瓦的口内……。
		PRINTFORMW 被小小的牙齿嘎吱嘎吱地摩擦着感觉很好……。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「啾噜啾噜…… 感觉像是在吸蜜一样……」
		PRINTFORMW 拉尔瓦像吮吸着花蕊一样地持续着口交……。
		IF NOWEX:PLAYER:射精
				PRINTFORML 「！！…………嗯……%UNICODE(0x2764)%」
				PRINTFORMW 拉尔瓦似乎喜欢精液的苦味……。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

