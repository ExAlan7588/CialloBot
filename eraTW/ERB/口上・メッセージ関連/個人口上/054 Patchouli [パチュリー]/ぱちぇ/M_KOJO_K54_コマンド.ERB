;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
;初めて
IF FIRSTTIME(SELECTCOM)
	;无いとは思うが恋慕
	IF TALENT:恋慕
		PRINTL 「…啾……我超级开心呢」
		PRINTFORML 帕秋莉用绽放的笑容诉说着自己对于%CALLNAME:MASTER%的想法
		PRINTFORMW 「这种毫无杂质的吻……只是一个吻而已，却让我感到比至今以来做过的任何事情都要更加令人高兴呢」
	;告白が成功してる
	ELSEIF CFLAG:1 == 1
		PRINTL 「啊……对了呢、姑且是恋人的话接吻还是很常见的吧」
		PRINTFORML 帕秋莉对于接受了%CALLNAME:MASTER%的恋情有了实感的样子
		SIF ABL:9 >= 4
			PRINTL 「要再来一次……吗？」
		WAIT
	;把柄で被逆推て中
	ELSEIF CFLAG:1 == 2 && CFLAG:317
		SELECTCASE TCVAR:20
			;接吻
			CASE 30,60
				PRINTW 「嗯嗯～……明明是实验对象却很大胆呢」
			;ソフトな感じ
			CASE 31 TO 33
				PRINTW 「……啾 啊啊、这样感觉不错吗」
			;爱抚
			CASE 34,50
				PRINTW 「啾……我只是在做这样的事情而已就接吻不觉得很浪费吗？」
			CASEELSE
				PRINTW 「啊 唔啾……实验对象请不要乱动」
		ENDSELECT
	;告白无しで被逆推たことがある
	ELSEIF CFLAG:1 == 2
		PRINTL 「啊……那个……要做什么实验呢？」
		PRINTFORMW 帕秋莉和%CALLNAME:MASTER%重叠着嘴唇，露出了稍微有些困扰的表情，询问着
	;告白成功で被逆推中
	ELSEIF CFLAG:1 == 3 && CFLAG:317
		SELECTCASE TCVAR:20
			;接吻
			CASE 30,60
				PRINTW 「嗯嗯～……嗯接吻真是温暖啊」
			;ソフトな感じ
			CASE 31 TO 33
				PRINTW 「……啾 啊、接吻也很不错呢」
			;爱抚
			CASE 34,50
				PRINTFORMW 「啾……能和%CALLNAME:MASTER%接吻什么的真奢侈呢」
			CASEELSE
				PRINTW 「啊 啾……嗯、接吻也不错呢」
		ENDSELECT
	;告白成功で诶嘿嘿に突入したことがある
	ELSEIF CFLAG:1 == 3
		;接吻经验あり
		IF EXP:27
			PRINTL 「说起来像这样的完全没有邪念的接吻以前从来没试过呢……」
			PRINTFORML %CALLNAME:MASTER%回忆了一下，表示赞同
			PRINTW 「……作为纪念让我们再来一次吧」
		ELSE
			PRINTL 「这是我的初吻哦……」
			PRINTFORML 帕秋莉的脸上泛起了红色、不敢看向%CALLNAME:MASTER%这么说道
			PRINTL 「真是抱歉呢、明明活了100年以上连接吻都没做过」
			PRINTFORMW 但是能被%CALLNAME:MASTER%追求真是太好了、帕秋莉开心地继续说道
		ENDIF
	;告白してもいないけど亲密だったので思わず…
	ELSEIF ABL:9 >= 7
		PRINTL 「嗯！？　……那个……我、还是第一次……」
		PRINTFORML %CALLNAME:MASTER%做出了看着帕秋莉的时候不知何时就亲上去了这样连借口都算不上的辩解
		IF BASE:情绪 >= 800
			PRINTW 「……算了、原谅你啦」
		ELSE
			PRINTW 「哈啊……希望你能再多考虑一下我的情绪呢」
		ENDIF
	;其他
	ELSE
		PRINTL 「……你是太激动了？这种事情麻烦你去找别人吧」
		PRINTFORMW 帕秋莉虽然短暂地瞥了%CALLNAME:MASTER%一眼又把注意力移回到了书上
	ENDIF
;恋慕
ELSEIF TALENT:恋慕
	;被逆推中
	IF CFLAG:317
		SELECTCASE TCVAR:20
			;接吻
			CASE 30,60
				IF RAND:2
					PRINTW 「啊姆啾……嗯啊 最喜欢了」
				ELSE
					PRINTW 「嗯嗯嗯……唔啊姆」
				ENDIF
			;从后面将抱きつかれた
			CASE 31
				PRINTFORML %CALLNAME:MASTER%从肩膀上探出头去看帕秋莉的时候，帕秋莉转过头来吻了上去
				PRINTW 「啊呼……嗯姆……」
			;前から
			CASE 32
				PRINTFORML %CALLNAME:MASTER%把嘴凑近了帕秋莉的脸颊时，被帕秋莉给吻了。
				PRINTW 「……哈啾……嗯啾……」
			;胸を揉ませる
			CASE 33
				IF RAND:2
					PRINTW 「呼呀…嗯嗯…欧派和嘴巴感觉都要融化了」
				ELSE
					PRINTFORMW 「啊呼啊……嗯啊唔…%CALLNAME:MASTER%的手指和嘴巴都好喜欢…」
				ENDIF
			;爱抚系
			CASE 34,50
				PRINTFORMW 「唔啾…啊呼呼、接吻太舒服了我都在颤抖了」
			CASEELSE
				PRINTFORMW 「啾姆……不管什么时候和%CALLNAME:MASTER%接吻都很开心哦」
		ENDSELECT
	;大成功
	ELSEIF TFLAG:193 == 1
		PRINTL 「嗯…啪啾…唔呲……」
		PRINTFORML %CALLNAME:MASTER%和帕秋莉像是索求着对方的唾液一般深吻着
		PRINTW 「更多……唔唔唔嗯—ー」
	;失败
	ELSEIF TFLAG:193 == -1
		PRINTFORML %CALLNAME:MASTER%和帕秋莉拼命摩擦着嘴唇时突然混入了血的味道
		PRINTW 「啊唔…啊、啊哈哈…嘴唇被磨破了呢」
	ELSEIF !RAND:4
		PRINTL 「啾哔…嗯啊…啾……」
		PRINTFORMW %CALLNAME:MASTER%和帕秋莉相互用舌头确认着对方的嘴唇。
	ELSEIF !RAND:3
		PRINTL 「…哈姆……啊姆啊姆…」
		PRINTFORML 帕秋莉和%CALLNAME:MASTER%相互温柔地舔舐着对方的嘴唇、感受着对方舌头的快感
		PRINTW 「姆啾……」
	ELSEIF RAND:2
		PRINTL 「嗯、唇齿相依是幸福的证明呢」
		PRINTFORMW 帕秋莉一边被%CALLNAME:MASTER%抱住一边和他深吻着
	ELSE
		PRINTFORML 「啾…%CALLNAME:MASTER%…嗯…%CALLNAME:MASTER%…最喜欢了」
		PRINTFORML 帕秋莉将无法用语言表达出来的心情通过接吻传达给了%CALLNAME:MASTER%
		PRINTW 「啾牧……亲我……姆啾啊」
	ENDIF
;诶嘿嘿中(と言うか被逆推てる)
ELSEIF CFLAG:317
	SELECTCASE TCVAR:20
		;接吻
		CASE 30,60
			PRINTW 「嗯啊……接吻、很喜欢呢」
		;ソフトなの
		CASE 31 TO 33
			PRINTW 「啊呼……嗯啾……」
		;爱抚系
		CASE 34,50
			PRINTW 「嗯……你很擅长用嘴巴取悦别人呢……」
		CASEELSE
			PRINTFORMW 「和%CALLNAME:MASTER%的接吻……我好像已经上瘾了呢」
	ENDSELECT
;诶嘿嘿解禁状态
ELSEIF CFLAG:6 >= 2
	;大成功
	IF TFLAG:193 == 1
		PRINTL 「嗯哈……嗯 果然是这样呢」
		PRINTL 帕秋莉松开嘴唇说道
		PRINTFORMW 「和%CALLNAME:MASTER%的接吻就像是麻药一样呢」
	;失败
	ELSEIF TFLAG:193 == -1
		PRINTL 「唔咕、你有点太用力了啦」
		PRINTFORMW 推开%CALLNAME:MASTER%的胸口、松开嘴唇的帕秋莉好像有点皱眉
	ELSEIF !RAND:4
		PRINTFORML 「嗯、%CALLNAME:MASTER%……」
		PRINTFORMW 帕秋莉稍微尖起嘴巴、催促着%CALLNAME:MASTER%的吻
	ELSEIF !RAND:3
		PRINTL 「啾……嗯是不是有点太贪心了呢……？」
		PRINTFORMW %CALLNAME:MASTER%说那不是挺平常呢么，帕秋莉也说“是那样么？”点了点头。两人依然保持着亲密的姿势
	ELSEIF !RAND:2
		PRINTFORML 帕秋莉从%CALLNAME:MASTER%的嘴唇上移开之后微笑着说道
		PRINTW 「稍微有点强硬呢……能再温柔一些吗」
	ELSE
		PRINTL 「啾……啪、啊呜……」
		PRINTFORML 帕秋莉带着稍显恍惚的眼神看着%CALLNAME:MASTER%
		PRINTW 「明明只是嘴唇的贴合……很有快感呢」
	ENDIF
;それ以外
ELSE
	;大成功
	IF TFLAG:193 == 1
		PRINTL 「啊呼……！？ 不、不对哦、我只是稍微觉得有点舒服而已啦」
		PRINTL 帕秋莉对于自己发出的娇声进行了不能算是辩解的辩解
		PRINTW 「别在那边偷笑啦！」
	;失败
	ELSEIF TFLAG:193 == -1
		PRINTL 「嗯咕……咳、咳」
		PRINTFORMW 帕秋莉被%CALLNAME:MASTER%突然的吻给吓了一跳、咳嗽了起来
	;亲密4以上
	ELSEIF ABL:9 >= 4
		IF RAND:2
			PRINTL 「……啾、啊呜」
			PRINTFORMW 帕秋莉低下头去、不想让%CALLNAME:MASTER%看到自己滚烫的脸
		ELSE
			PRINTL 「想接吻吗？……这样啊、啊 姆……」
			PRINTFORMW %CALLNAME:MASTER%将想说些什么的帕秋莉的嘴巴给堵住了
		ENDIF
	;其他(たぶんこの边は出ないような气がする)
	ELSE
		IF RAND:2
			PRINTFORML 「咕……%CALLNAME:MASTER%……你也好歹找个别的人吧」
			PRINTFORMW 帕秋莉怒上心头、瞪着突然亲上来的%CALLNAME:MASTER%
		ELSE
			PRINTL 「唔姆…………哈啊」
			PRINTFORMW 帕秋莉瞪了一眼%CALLNAME:MASTER%之后像是失去了兴趣一样又开始读起了手边的书本
		ENDIF
	ENDIF
ENDIF
ENDIF
RETURN 1
