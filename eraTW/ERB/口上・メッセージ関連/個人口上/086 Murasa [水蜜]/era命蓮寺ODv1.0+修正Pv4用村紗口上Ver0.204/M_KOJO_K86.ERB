;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K86_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SIF TCVAR:353 == 1
		TCVAR:353 = 0
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同栖かつ相手が先に起きてる
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕时陪睡中就寝时のみの筈
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕无しでも入ってくる可能性有り
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;同栖かつ相手が起きている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;同栖かつ相手が寝ている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝时TARGET起床はこちら（TARGET衰弱or陪睡中TARGET睡魔负け后そのままMASTERが起き续けた上で相手が起床した场合限定
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同栖
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF CFLAG:MASTER:陪睡中
				PRINTFORML 「…嗯…… 呼…哈……啊…」
				PRINTFORMW 「啊…… 早上好%CALLNAME:MASTER%,呼呼」
				;村纱私室以外的房间を私室に设定している(条件的に有り得ず
				;SIF CFLAG:MASTER:311 != 14
					;PRINTFORMW 「%CALLNAME:MASTER%来了的话… 是不是该早点起来比较好呐」
				PRINTL
				PRINTFORMW %CALLNAME:MASTER%和%CALLNAME:TARGET%舍不得温暖的被窝久久不肯出来…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同栖中
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF CFLAG:MASTER:陪睡中
				PRINTFORML 「…嗯…… 呼…哈……啊…」
				PRINTFORMW 「啊…… 早上好%CALLNAME:MASTER%,呼呼」
				;村纱私室以外的房间を私室に设定している(条件的に有り得ず
				;SIF CFLAG:MASTER:311 != 14
					PRINTFORMW 「%CALLNAME:MASTER%来了的话… 是不是该早点起来比较好呐」
				PRINTL
				PRINTFORMW %CALLNAME:MASTER%和%CALLNAME:TARGET%舍不得温暖的被窝久久不肯出来…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;其他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K86_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入スイッチ
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に回去（定时になって寝る）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF BASE:TARGET:体力 <= 500
			IF CFLAG:诶嘿嘿
				PRINTFORML 「哈啊… 哈啊… 已经,不行了……」
				PRINTFORMW %CALLNAME:TARGET%脱力的靠在%CALLNAME:MASTER%身上,肩膀随着急促的呼吸上下晃动着…
				PRINTL
				PRINTFORML 「居然是我先累了,啊… 呼呼… 太棒了,%CALLNAME:MASTER%」
				PRINTFORMW 这样说着%CALLNAME:TARGET%向着%CALLNAME:MASTER%嘴唇温柔的吻了过来…
				PRINTL
			ELSE
				PRINTFORMW 「嗯…果然还是很累」
			ENDIF
			PRINTFORMW 「……我要回房间休息了,晚安」
		ELSE
			PRINTFORMW 「嗯…已经这个时间了。我先回去休息了。」
			PRINTFORMW 「%CALLNAME:MASTER%也不要熬夜哦？呼呼,晚安」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:陪睡中
			PRINTL
			PRINTL
			PRINTFORMW 「呼呼……」
		ELSE
			PRINTFORMW 「嗯… 呼啊…啊……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会话というよりは寝言？）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTL
		PRINTFORMW 「Zzz… Zzz… 嗯唔…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;恶作剧中
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「嗯… 嗯唔…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;衰弱中（恋慕时、房间まで运ぶ）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:诶嘿嘿
			PRINTFORMW 「哈啊… 哈啊… 已经,不行了……」
			PRINTFORMW %CALLNAME:TARGET%脱力的靠在%CALLNAME:MASTER%身上,肩膀随着急促的呼吸上下晃动着…
			PRINTL
			PRINTFORMW 「呼呼… 真是,太棒了。感受到了这么多%CALLNAME:MASTER%的事,真是太幸福了」
			PRINTFORMW 这样说着%CALLNAME:TARGET%向着%CALLNAME:MASTER%嘴唇温柔的吻了过来…
			PRINTFORMW 就这样，暂时享受了一阵余韵之后,%CALLNAME:MASTER%将%CALLNAME:TARGET%送回了房间…
		ELSE
			PRINTFORMW 「嗯…果然还是很累」
			PRINTFORMW 「……你会送我回去吗？呼呼… 谢谢」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（房间まで运ばれる）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「啊啦… 已经想休息了吗」
		PRINTFORMW 「真没办法了,我来送你回房间吧」
		PRINTFORMW 这样说着的%CALLNAME:TARGET%意外的不费力抱起了%CALLNAME:MASTER%…
		PRINTL
		PRINTFORMW 「呼呼… 很吃惊吗？别看我这样,还是挺有力气的哦。」
		PRINTFORMW 「……那么，晚安。祝你有个好梦」
		PRINTW
		RETURN 1
	ENDIF
	;其他
	PRINTFORML  
ENDIF
RETURN 1

