;==================================================
;情事を目击した
;==================================================
@M_KOJO_EVENT_K86_6(ARG,ARG:1)
;口上主はTARGET
;ARGに情事相手
;--------------------------------------------------
;记入スイッチ
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;情事を目击した（亲热度:低）
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;负けじと参加（亲热度:高）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 2
		;白莲が居ない
		IF CFLAG:MASTER:当前位置 != CFLAG:55:当前位置
			PRINTFORMW 「…很享受呐？」
			PRINTL
			PRINTFORMW 白皙的手指搭在了%CALLNAME:ARG%的后肩上
			PRINTL
			PRINTFORML 「呼呼… 别一脸那么吃惊的样子,惹人疼爱的声音早就被我听到了哦？」
			PRINTFORML 「感觉又甜蜜又急促… 又非常舒服的声音。听到那种声音之后我都感觉要高潮了」
			PRINTFORMW 「所以，我也可以加入你们吗？呐%CALLNAME:MASTER%…」
			PRINTL
			PRINTFORMW 这样说着的%CALLNAME:TARGET%搭着%CALLNAME:ARG%的肩膀对%CALLNAME:MASTER%妖艳的微笑起来…
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;气まずそうに立ち去る
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 3
		PRINTFORML 「……打扰你了吧？」
		PRINTFORMW 「是呢,那我就告辞了,请您慢慢享受吧」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERに诱われて参加
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 4
		;白莲が居る
		IF CFLAG:MASTER:当前位置 == CFLAG:55:当前位置
			;村纱が恋人
			IF TALENT:TARGET:恋人
				PRINTFORML 「呼呼… 能和你一起还真是荣幸呐，圣…」
				PRINTL
				PRINTFORMW 这么说着的%CALLNAME:TARGET%亲吻了白莲的手背…
			ELSE
				PRINTFORMW 「我也要一起…？」
				PRINTL
				PRINTFORMW %CALLNAME:TARGET%露出困惑的表情看着白莲
				PRINTL
				PRINTFORMW 「圣… 不介意吗？」
				PRINTL
				PRINTFORML 「……这样阿,那样的话我很乐意」
				PRINTFORMW 「能和您一起真是荣幸,这么说出来还真感觉有点怪」
				PRINTL
				PRINTFORMW 这么说着的%CALLNAME:TARGET%露出了开朗的微笑…
