				RETURN 1
			ELSE
				PRINTFORMW 「在人家的膣内满满的射出来了呢…%UNICODE(0x2665) *1%」
				RETURN 1
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「嗯…%UNICODE(0x2665) *1%//」
				PRINTFORMW 「…色色的液体灌进来了…%UNICODE(0x2665) *1%」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「真是…大色狼…%UNICODE(0x2665) *1%」
				RETURN 1
			ELSE
				PRINTFORMW 「明明是付丧神…要怀上人类的孩子了…%UNICODE(0x2665) *1%」
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

