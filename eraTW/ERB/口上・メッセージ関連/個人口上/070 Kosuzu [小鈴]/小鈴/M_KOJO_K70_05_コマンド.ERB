;-------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_C_K70
SIF FLAG:时间停止
	RETURN 1
SIF !M_KOJO_K70_UFUFU()
	RETURN 1
;处女丧失
IF TCVAR:破瓜
	;正真正铭处女丧失
	IF TCVAR:破瓜 == 1
		PRINTL 
		PRINTFORMW 「啊……我很开心呢……❤」
		PRINTFORMW 「这下我也是个成熟的女性了吧……♪」
		;日记を书く
		CALL M_KOJO_DIARYSETTING_K70(47)
	;再生处女丧失
	ELSEIF TCVAR:破瓜 == 2
	;无自觉非处女丧失
	ELSEIF TCVAR:破瓜 == -1
	ENDIF
ENDIF

;Ａ处女丧失
IF TCVAR:Ａ破瓜
	;正真正铭Ａ处女丧失
	IF TCVAR:Ａ破瓜 == 1
		PRINTL 
		PRINTFORMW 「嗯嗯……有点痛……%CALLNAME:MASTER%先生太过分了……」
	;无自觉Ａ处女丧失
	ELSEIF TCVAR:Ａ破瓜 == -1
	ENDIF
ENDIF

