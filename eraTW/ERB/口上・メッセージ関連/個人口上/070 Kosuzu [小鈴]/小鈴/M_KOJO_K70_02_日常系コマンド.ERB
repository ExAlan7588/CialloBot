            CASE 7
                PRINTFORMW 「呵呵,不用勉强聊天,我也很开心的」  
            CASEELSE
                PRINTFORMW 「%CALLNAME:MASTER%先生,这个话题我们刚才不是才聊过吗?」
            ENDSELECT
        ELSE
            SELECTCASE RAND:3
            CASE 1
                PRINTFORMW 「是的,不过……」
            CASE 2
                PRINTFORMW 「嗯~嗯,这个是……」
            CASEELSE
                PRINTFORMW 「啊哈哈……这个话题我们刚才不是才聊过吗?」
            ENDSELECT
		ENDIF
	;失败
	ELSEIF TFLAG:SELECTCOM的分岐 == -1
		IF TALENT:70:恋慕 || TALENT:70:思慕
			SELECTCASE RAND:4
			CASE 1
                PRINTFORMW 「欸?呃……在说什么呢?」
            CASE 2
                PRINTFORML 「唔~……」
                PRINTFORMW 「%CALLNAME:MASTER%先生的话有时候很难懂啊……」
            CASE 3
                PRINTFORML 「%CALLNAME:MASTER%先生!你刚才有在听我说话吗!?」
                PRINTFORMW 「请不要胡乱敷衍我!」
            CASEELSE
                PRINTFORML 「嗯……?」
                PRINTFORMW 「总觉得有点模糊呢……」
            ENDSELECT
        ELSE
            SELECTCASE RAND:4
            CASE 1
                PRINTFORMW 「对不起,那个话题有点……」
            CASE 2
                PRINTFORMW 「唔~不太明白……」
            CASE 3
                PRINTFORMW 「呃~嗯……是、是的……」
            CASEELSE
                PRINTFORMW 「哈……是这样吗……」
            ENDSELECT
		ENDIF
	;ゲストキャラと同室だと各キャラ1日1回ずつゲスト会话が发生（茶とは别）
	;4=小铃が恋慕(同行中だと最优先),3=ゲストと同行中in铃奈庵,2=ゲストと约会中orゲストが恋慕で同行中,1=それ以外
	ELSEIF GUESTS != ""
		CALL M_KOJO_K70_画像表示(GUEST:1, "通常", "通常")
		CALL M_KOJO_K70_TALKSET("ゲスト", GUEST:1, GUEST, "会话")
		;一日一回
		TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(GUEST:1)) = 1
		;デート同行会话と被る场合があるのでこちらを优先させ发生させないようにする
		SETBIT TCVAR:70:360, 0
;试しに散策侧に移行してみる,
[SKIPSTART]
	;约会会话
	ELSEIF ABL:[[小铃]]:亲密 >= 7 && FLAG:约会的对象 == [[小铃]] && DATENAME_SPOT() != "道中" && !约会会话:(CFLAG:70:当前位置 / 10)
		POINT = CFLAG:70:当前位置 / 10
		;こんなにこだわるつもりはなかったのに、某美人妻がたく先生说话のに钓られてつい
