			PRINTFORML 「还要更多吗…？好的…❤」
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「用舌头…❤ %M_KOJO_K70_AUTO("キス：前", "…❤")% %M_KOJO_K70_AUTO("キス：后", "❤")% %M_KOJO_K70_AUTO("キス：后", "…❤")%」
			CASE 2
				PRINTFORMW 「尽情吧❤ %M_KOJO_K70_AUTO("キス：前", "…❤")% %M_KOJO_K70_AUTO("キス：后", "❤")% %M_KOJO_K70_AUTO("キス：后", "…❤")%」
			CASEELSE
				PRINTFORMW 「你喜欢吻吗…？ %M_KOJO_K70_AUTO("キス：前", "…❤")% %M_KOJO_K70_AUTO("キス：后", "❤")% %M_KOJO_K70_AUTO("キス：后", "…❤")%」
			ENDSELECT
		ELSE
			PRINTFORML 「嗯嗯…！ %M_KOJO_K70_AUTO("キス：前", "…❤")% %M_KOJO_K70_AUTO("キス：后", "…❤")%」
		ENDIF
	ENDIF
ENDIF
RETURN 1
