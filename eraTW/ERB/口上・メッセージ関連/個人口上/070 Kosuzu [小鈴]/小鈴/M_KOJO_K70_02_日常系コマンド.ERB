;403,休憩
;CFLAG:TARGET:睡眠(1=TARGETが寝てる隣で休憩
;==================================================
@M_KOJO_MESSAGE_COM_K70_403
CALL M_KOJO_MESSAGE_COM_K70_403_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K70_403_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K70
	IF CFLAG:70:睡眠
		PRINTFORMW 「嗯……」
	;醉ってる
	ELSEIF ALCOHOL_FACE(70) == "醉酒" || ALCOHOL_FACE(70) == "不省人事"
		CALL M_KOJO_K70_DRUNK("休憩")
	;发情
	ELSEIF TCVAR:[[小铃]]:发情
		CALL M_KOJO_K70_HATSUJO("休憩")
	;仕事中
	ELSEIF CFLAG:70:行动 == 5
		PRINTFORML 「哎呀，辛苦了吗？」
		PRINTFORMW 「请慢慢休息吧」
	;事后
	ELSEIF TCVAR:[[小铃]]:355
		PRINTFORML 「刚刚真的好舒服啊…❤」
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「稍微休息一下吧……」
		CASE 2
			PRINTFORMW 「再来一次好吗…❤」
		CASEELSE
			PRINTFORMW 「没想到会这么激烈啊…❤」
		ENDSELECT
	;おつかれ
	ELSEIF BASE:[[小铃]]:气力 < MAXBASE:[[小铃]]:气力 / 2 || BASE:[[小铃]]:体力 < 1000
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「好累啊～…我也要休息一下～……」
		CASE 2
			PRINTFORMW 「我也好累啊～…呼～……」
		CASEELSE
			PRINTFORMW 「也请您让我休息一下……呼～……」
		ENDSELECT
	ELSE
		PRINTFORML 「哎呀，辛苦了吗？」
		PRINTFORMW 「那就稍微休息一下吧」
	ENDIF
ENDIF
RETURN 1
