@M_KOJO_MESSAGE_COM_K70_300_1
#DIM GUEST, 2
#DIMS GUESTS
#DIM POINT
#DIM 约会会话, 120
#DIM 会话日
#DIM 会话回数
;日が变わってたら约会会话リセット
SIF !TCVAR:70:352
	VARSET 约会会话
IF 会话日 != DAY
	会话回数 = 0
	会话日 = DAY
ENDIF
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	RESETCOLOR
	会话回数 ++
	;ゲスト角色判定
	CALL M_KOJO_K70_GUEST("会话")
	GUEST = RESULT
	GUEST:1 = RESULT:1
	GUESTS = %RESULTS%
	;ここに口上书いて下さい
	IF ALCOHOL_FACE(70) == "醉酒" || ALCOHOL_FACE(70) == "不省人事"
		PRINTFORML %CALLNAME:PLAYER%试着和%CALLNAME:TARGET%搭话、但是%CALLNAME:TARGET%只是哼哼了两句、听不懂在说什么
	ELSEIF TCVAR:[[小铃]]:发情
		PRINTFORML %CALLNAME:PLAYER%尝试与表现异常的%CALLNAME:TARGET%交谈
	;会话不能の地の文は恋慕でない场合は省略できず、恋慕の时は表示されないので、ここではどちらの场合も同じように表示されるようにする
	ELSEIF TCVAR:70:会话不能
		IF TALENT:70:恋慕
			PRINTFORML 想跟%CALLNAME:TARGET%搭话却不知道该说什么
			PRINTFORMW 周围充满着尴尬的空气…
		ENDIF
	ELSEIF GUESTS != ""
		CLEARLINE 2
		PRINTFORML 三个人会话
		DRAWLINE
		;文章上だけであってソース等は变わりません
		PRINTFORML %CALLNAME:PLAYER%和%CALLNAME:TARGET%、%GUESTS%三个人交替着说话
	ELSE
		;约会会话
		IF TALENT:70:恋慕 && FLAG:约会的对象 == 70 && DATENAME_SPOT() != "道中" && !约会会话:(CFLAG:70:当前位置 / 10)
			CLEARLINE 2
			PRINTFORML 去%DATENAME_SPOT()%约会
			DRAWLINE
		ENDIF
		PRINTFORM %CALLNAME:PLAYER%和%CALLNAME:TARGET%
		IF TALENT:70:恋慕
			PRINTDATA
				DATAFORM 坐一起挨着聊天
				DATAFORM 有点害羞地聊了会
				DATAFORM 微笑着交谈
				DATAFORM 在融洽的气氛中交谈
				DATAFORM 热情地聊了起来
				DATAFORM 边嬉戏边聊
			ENDDATA
		ELSE
			PRINTDATA
				DATAFORM 聊了些无聊的话题
				DATAFORM 聊了些琐事
				DATAFORM 拉了会家常
				DATAFORM 聊了些普通的话题
				DATAFORM 聊了些常见的话题
				DATAFORM 稍微聊了一会儿
				DATAFORM 讲了个笑话
			ENDDATA
		ENDIF
		IF TFLAG:SELECTCOM的分岐 > 0
			PRINTFORML ，聊得热火朝天
		ELSEIF TFLAG:SELECTCOM的分岐 < 0
			PRINTFORML ，但话聊到一半断掉了
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
	PRINTW 
	CALL M_KOJO_COLOR_K70
	;陪睡中
	IF CFLAG:[[小铃]]:陪睡中
		IF TALENT:[[小铃]]:恋慕
		    SELECTCASE RAND:8
		    CASE 1
		        PRINTFORMW 「嗯嗯~……这样下去……安然入睡……」
		    CASE 2
		        PRINTFORMW 「明天见……我们再聊聊吧……」
		    CASE 3
		        PRINTFORMW 「嗯嗯，嗯嗯……」
		    CASE 4
		        PRINTFORMW 「呵呵……感觉很舒服呢……」
		    CASE 5
		        PRINTFORMW 「咻咻～……好暖和啊……」
		    CASE 6
		        PRINTFORMW 「诶嘿嘿……让我们更贴近一点吧…❤」
		    CASE 7
		        PRINTFORMW 「呼啊……感到安心呢……」
		    CASEELSE
		        PRINTFORMW 「噗啊～…好困啊～……」
		    ENDSELECT
		ELSE
		    SELECTCASE RAND:3
		    CASE 1
		        PRINTFORMW 「嗯嗯~……就睡觉吧～…」
			CASE 2
		        PRINTFORMW 「嗯嗯，嗯嗯……」
		    CASEELSE
		        PRINTFORMW 「好的……好的……」
		    ENDSELECT
		ENDIF
