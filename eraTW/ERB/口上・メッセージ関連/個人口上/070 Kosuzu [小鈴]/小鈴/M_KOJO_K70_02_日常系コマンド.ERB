@M_KOJO_MESSAGE_COM_K70_352_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K70
	;した
	IF TFLAG:SELECTCOM的分岐
		;初めて
		IF FIRSTTIME("告白")
			IF TALENT:70:恋慕
				PRINTFORMW 「嗯……好开心～～！」
				PRINTL 
				PRINTFORMDL 　紧紧的抱住
				PRINTFORMDL 小铃在欢喜的泪水中抱住%CALLNAME:MASTER%
				PRINTW 
				PRINTFORMW 「我也一直喜欢%CALLNAME:MASTER%先生……！」
				PRINTFORMW 「但是，因为害怕说出口……担心关系会变质……」
				PRINTFORMW 「所以现在非常高兴……现在，真的很幸福……❤」
				;写日记
				CALL M_KOJO_DIARYSETTING_K70(43)
			ELSE
				PRINTFORMW 「诶……是、是吗！？」
				PRINTFORMW 「真、真的吗……？」
				PRINTFORMW 「这么、这么突然地说出来……不过……」
				PRINTFORMW 「不过……好、好啊……%CALLNAME:MASTER%先生的话……」
				PRINTFORMW 「虽然有点不安，但我相信%CALLNAME:MASTER%先生会珍惜我……」
				PRINTFORMW 「我也会慢慢喜欢上%CALLNAME:MASTER%先生的……所以……」
				;写日记
				CALL M_KOJO_DIARYSETTING_K70(44)
			ENDIF
		;一度拒绝，果然是个方便的家伙！
		ELSE
			;和告白一样，这里只有单纯的喜欢，应该是的
			PRINTFORMW 「真的吗……？ 嗯、好啊……？」
			PRINTFORMW 「呜、好高兴……太好了……」
			PRINTFORMW 「我一直都在担心会不会被讨厌……」
			PRINTFORMW 「不过，我坚持下去真的是对的……%CALLNAME:MASTER%先生接受了我……真的太好了……」
			;写日记
			CALL M_KOJO_DIARYSETTING_K70(45)
		ENDIF
		IF TALENT:70:恋慕
			PRINTL 
			PRINTFORMDL 实现了爱情的两人静静地依偎在一起，被甜蜜的空气包围，不知不觉中交换了亲吻……
			PRINTW 
			EXP:70:接吻经验 += 1
			EXP:70:Ｍ经验 += 1
			EXP:MASTER:Ｍ经验 += 1
			EXP:MASTER:接吻经验 += 1
			IF FIRSTTIME("初吻")
				CALL SET_FIRSTKISS(70)
				;［告白する］の条件が二人きり
				IF M_KOJO_K70_SUZUNAAN(CFLAG:70:当前位置) == 1
					CSTR:70:初吻丧失履历 = %PRINT_DATE_F(DAY)%，在静寂的铃奈庵中，%CALLNAME:MASTER%对她的告白深深地爱恋，两人在幸福的氛围中交换了承诺的第一吻。
				ELSEIF M_KOJO_K70_SUZUNAAN(CFLAG:70:当前位置) == 2
					CSTR:70:初吻丧失履历 = %PRINT_DATE_F(DAY)%，在自己的房间里，%CALLNAME:MASTER%对她的告白深深地爱恋，两人在幸福的氛围中交换了承诺的第一吻。
				ELSEIF FLAG:约会的对象 == 70
					CSTR:70:初吻丧失履历 = %PRINT_DATE_F(DAY)%，在%DATENAME_PLACE()%的约会中，%CALLNAME:MASTER%对她的告白深深地爱恋，两人在幸福的氛围中交换了承诺的第一吻。
				ELSE
					CSTR:70:初吻丧失履历 = %PRINT_DATE_F(DAY)%，在%NAME_FROM_PLACE(CFLAG:70:当前位置)%的地方，%CALLNAME:MASTER%对她的告白深深地爱恋，两人在幸福的氛围中交换了承诺的第一吻。
				ENDIF
			ENDIF
			CALL M_KOJO_COLOR_K70
			PRINTFORMW 「呵呵……❤ 虽然未来有很多未知，但是――」
			PRINTFORMW 「我们一直在一起吧♪ %CALLNAME:MASTER%先生❤」
		ENDIF
	;说不出话
	ELSE
		PRINTFORML 「？」
		PRINTFORMW 「%CALLNAME:MASTER%先生，发生了什么吗？」
	ENDIF
ENDIF
RETURN 1
