;他者介入で情绪变化
;==================================================
@M_KOJO_EVENT_K70_5(ARG,ARG:1)
;口上主はTARGET
;ARGに入ってきたキャラ（人数が膨大なので、自己把握自己责任で
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;灵梦が入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == [[灵梦]]
		PRINTFORMW 「啊，灵梦小姐，%M_KOJO_K70_AISATSU()%～」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;魔理沙が入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == [[魔理沙]]
		PRINTFORMW 「啊，魔理沙小姐，%M_KOJO_K70_AISATSU()%～」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;紫が入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == [[紫]]
		PRINTFORMW 「啊！ 喂，紫小姐……%M_KOJO_K70_AISATSU()%……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;文が入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == [[文]]
		PRINTFORMW 「啊！ 喂，文小姐！ 这个是，那个……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;猯藏が入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == [[猯藏]]
	    PRINTFORMW 「啊！ 猯藏小姐！ 这个是，那个……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;阿求が入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == [[阿求]]
		SELECTCASE M_KOJO_K70_BALANCE([[小铃]], [[阿求]])
		CASE "独占", "信赖"
			PRINTFORMW 「阿求!别插手啦,正好到兴头上呢!」
		CASE "悲恋", "揶揄"
			;阿求是恋人却在这里么……?
			PRINTFORMW 「阿求!抱、抱歉呢……啊哈」
		CASE "敌对", "恋慕"
			PRINTFORMW 「阿求!别插手啦,正好到兴头上呢!你去别处玩去!」
		CASEELSE
			PRINTFORMW 「怎么是阿求呢,看看氛围好不好」
		ENDSELECT
		RETURN 1
	ENDIF
	;其他
	SELECTCASE RAND:6
	CASE 1
		PRINTFORML 「啊,来人了呢……」
	CASE 2
		PRINTFORML 「唔~……正好到好处呢……」
	CASE 3
		PRINTFORML 「啊,来人了……咳嗯」
	CASE 4
		PRINTFORML 「对、对不起,有点太近了吧……」
	CASE 5
		PRINTFORML 「啊~……我们保持点距离吧……」
	CASEELSE
		PRINTFORML 「唔唔……就到这里吧……」
	ENDSELECT
ENDIF
RETURN 1

