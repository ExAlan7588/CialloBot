			CASE 2
				PRINTFORMW 「该回去了呢……」
			CASEELSE
				PRINTFORMW 「已经全黑了呢……」
			ENDSELECT
			;おやすみセット
			CALL M_KOJO_EVENT_K70_3_OYASUMI
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている（～は眠そうにしている）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
