@M_KOJO_EVENT_K70_26(ARG,ARG:1)
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;-------------------------------------------------
		;见られた瞬间
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF FIRSTTIME("自慰暴露")
				PRINTFORMW 「啊……%CALLNAME:MASTER%桑……你、你从什么时候在那里……」
				PRINTFORMW 「我、我不要啊ーーーー!!\/\/\/ 嫁不出去啦ーーーー!!\/\/\/」
				TCVAR:70:350 += 1
				TALENT:70:心情 = -1
				TCVAR:70:356 = 1
			ELSEIF TALENT:70:恋慕 && GETBIT(CFLAG:70:既成事实, 1)
				;20日以上会ってない
				IF DAY - CFLAG:70:1002 >= 20
					PRINTFORML 「啊……%CALLNAME:MASTER%桑……!」
					PRINTFORMW 「这个是……那个,因为……」
					PRINTFORMW 「因为~最近都见不到,很寂寞的说~……」
					PRINTFORMW 「都是%CALLNAME:MASTER%桑的错~……呜呜……」
					TCVAR:70:356 = 4
				ELSE
					PRINTFORML 「啊……%CALLNAME:MASTER%桑……!」
					PRINTFORMW 「这个是……那个,读了一本那种内容的书……稍微有点……」
					PRINTFORMW 「一想到%CALLNAME:MASTER%桑就忍不住了……\/\/\/」
					TCVAR:70:356 = 3
				ENDIF
			ELSE
				PRINTFORML 「啊……%CALLNAME:MASTER%桑……!」  
				PRINTFORMW 「你、你为什么在这里啊ーー!!\/\/\/ 笨蛋ーーー!!\/\/\/」
				TCVAR:70:356 = 2
			ENDIF
			RETURN 1
		ENDIF
	CASE 1
		;-------------------------------------------------
		;何もしない
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			;初回、恋慕なしor合意まだ
			IF TCVAR:70:356 == 1 || TCVAR:70:356 == 2
				PRINTFORMW 「请出去ーーーー!!\/\/\/」
			;恋慕+合意  
			ELSEIF TCVAR:70:356 == 3
				PRINTFORMW 「啊!等一下…啊……」
			;久しぶり
			ELSEIF TCVAR:70:356 == 4
				PRINTFORMW 「啊!不要这样……呜……」
			ENDIF
			RETURN 1
		ENDIF
	CASE 2
		;-------------------------------------------------
		;そのまま续行
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			;初回,恋慕なしor合意まだ
			IF TCVAR:70:356 == 1 || TCVAR:70:356 == 2
				PRINTFORMW 「不要看哦~~\/\/\/」
			;恋慕+合意,久しぶ里 
			ELSEIF TCVAR:70:356 == 3 || TCVAR:70:356 == 4
				;自慰口上を表示
				IF EXP:70:调教自慰经验 >= 30
					SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「啊……❤ 嗯❤ 感觉好舒服…❤」
						PRINTFORMW 「再看着我吧……❤ 看,一想到%CALLNAME:MASTER%桑我就会……❤」
					CASE 2 
						PRINTFORMW 「哈~❤ 被%CALLNAME:MASTER%桑看着自慰❤ 变成了习惯❤」
						PRINTFORMW 「呀……❤ 没办法停下来呢……%M_KOJO_K70_AUTO("喘ぎ：小", "❤")%」
					CASEELSE
						PRINTFORMW 「%CALLNAME:MASTER%桑……❤ %M_KOJO_K70_AUTO("喘ぎ：小", "❤")% %M_KOJO_K70_AUTO("喘ぎ：小", "❤")%」
						PRINTFORMW 「来、摸摸我吧……❤ 呜……」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「%CALLNAME:MASTER%桑,我好寂寞……❤ %M_KOJO_K70_AUTO("喘ぎ：小", "❤")%」  
						PRINTFORMW 「因为这样的话……❤ 只有我一个人……嗯❤」
					CASEELSE
						PRINTFORMW 「平、平时都是……一边想着%CALLNAME:MASTER%桑……❤」
						PRINTFORMW 「%M_KOJO_K70_AUTO("喘ぎ：小", "❤ ", 1)%哈啊……!」
					ENDSELECT
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
	CASE 3
		;-------------------------------------------------
		;追い出し
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL 
			PRINTFORMW 「%CALLNAME:MASTER%桑笨蛋啊ーーー！！\/\/\/」
			TCVAR:70:350 = 3
			TALENT:70:心情 = -1
			BASE:70:愤怒 += MAXBASE:70:愤怒
			CALL M_KOJO_K70_YAMETEKUDASAI(70, 3, 1)
			RETURN 1
		ENDIF
	CASE 4
		;-------------------------------------------------
		;やっちゃう
		;记入チェック（=0, 非表示、1, 表示）
