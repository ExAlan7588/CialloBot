;关系性比较函数,小铃と阿求にしか使ってないけど一応泛用できるように
@M_KOJO_K70_BALANCE(ARG:1, ARG:2)
#FUNCTIONS
;ARG:1	主对象
;ARG:2	比较对象
IF TALENT:(ARG:1):恋人 && TALENT:(ARG:2):恋人
	RETURNF "信赖"
ELSEIF TALENT:(ARG:1):恋人
	RETURNF "独占"
ELSEIF TALENT:(ARG:2):恋人 && TALENT:(ARG:1):恋慕
	RETURNF "悲恋"
ELSEIF TALENT:(ARG:2):恋人
	RETURNF "揶揄"
ELSEIF TALENT:(ARG:1):恋慕 && (TALENT:(ARG:2):恋慕 || TALENT:(ARG:2):思慕) && CFLAG:(ARG:1):信赖度> 1500
	RETURNF "信赖"
ELSEIF TALENT:(ARG:1):恋慕 && (TALENT:(ARG:2):恋慕 || TALENT:(ARG:2):思慕)
	RETURNF "敌对"
ELSEIF TALENT:(ARG:1):恋慕
	RETURNF "恋慕"
ELSEIF TALENT:(ARG:2):恋慕
	RETURNF "応援"
ELSE
	RETURNF "平穏"
ENDIF

