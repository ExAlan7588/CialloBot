		IF LOCAL:1
			PRINTL
			PRINTFORMW 「嗯……❤ 那样的话……❤ 但是……❤」
			PRINTFORMW 「啊嗯❤ 嘿嘿❤ 一直在等待您❤❤」
			PRINTFORMW 「请对我温柔一点❤ 多多疼爱我吧……❤」
			RETURN 1
		ENDIF
	CASE 5
		;-------------------------------------------------
		;事后
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = M_KOJO_K70_UFUFU()
		;-------------------------------------------------
		IF LOCAL:1
			PRINTL
			PRINTFORML 「哈啊哈啊……舒服……❤」
			PRINTFORMW 「但、但是敲门之类的……还是要做一下的呢……」
			RETURN 1
		ENDIF
	CASE 6
		;-------------------------------------------------
		;事后お泊り
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = M_KOJO_K70_UFUFU()
