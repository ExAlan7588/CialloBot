;==================================================
;一日の结束时(AFTERTRAニーの前)、认识がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K70_31(ARG,ARG:1)
#DIM 前回のブーム
#DIM 今回のブーム
;口上主はTARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:自慰暴露で分岐入れてもよさそう
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;※口上はないけどフラグの管理だけする
	;マイブームが无い场合、概率で发生
	IF (!CFLAG:[[小铃]]:1080 || CFLAG:[[小铃]]:1080 > 10)
		IF !RAND:24
			前回のブーム = CFLAG:[[小铃]]:1080 % 10
			CFLAG:[[小铃]]:1080 = 0
			WHILE !CFLAG:[[小铃]]:1080
				;种类数を取得
				CALLF M_KOJO_K70_KOSUZUBOOM()
				今回のブーム = RAND:RESULT + 1
				SIF 今回のブーム != 前回のブーム
					CFLAG:[[小铃]]:1080 = 今回のブーム
			WEND
			;残り日数は40～80日
			CFLAG:[[小铃]]:1081 = 40 + RAND:41
			CFLAG:[[小铃]]:1082 = 0
			[IF_DEBUG]
				PRINTFORML ※小铃口上DEBUG用
				PRINTFORML ！マイブーム发生
			[ENDIF]
		ENDIF
	;残り日数が无くなったらマイブーム结束,
	ELSEIF CFLAG:[[小铃]]:1081 <= 0
		CFLAG:[[小铃]]:1080 += 10
		CFLAG:[[小铃]]:1081 = 0
		CFLAG:[[小铃]]:1082 = 0
	ELSE
		;マイブームカウントダウン
		CFLAG:[[小铃]]:1081 --
	ENDIF
	[IF_DEBUG]
		CALL EVENTEND_TSUBUYAKI
		PRINTFORML ※小铃口上DEBUG用
		PRINTFORML マイブーム：%M_KOJO_K70_KOSUZUBOOM()%（あと {CFLAG:[[小铃]]:1081}日）
		PRINTFORML 数值：{CFLAG:[[小铃]]:1080}, 回数：{CFLAG:[[小铃]]:1082}
	[ENDIF]
;	;-------------------------------------------------
;	;小分岐
;	;-------------------------------------------------
;	;今日会っていない
;	;记入チェック（=0, 非表示、1, 表示）
;	LOCAL:1 = 0
;	;-------------------------------------------------
;	IF LOCAL:1 && ARG
;		RETURN 1
;	ENDIF
;	;-------------------------------------------------
;	;今日会っている
;	;记入チェック（=0, 非表示、1, 表示）
;	LOCAL:1 = 0
;	;-------------------------------------------------
;	IF LOCAL:1 && ARG == 0
;		RETURN 1
;	ENDIF
ENDIF
RETURN 1

