;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
;通常の仕事以外（宴会）では发生しない
IF LOCAL && TCVAR:70:357 != TIME && GROUPMATCH(CFLAG:70:职业种类, 40, 47)
	;连打防止
	TCVAR:70:357 = TIME
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;仕事开始（职场へ移动する）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;在铃的房间
		IF M_KOJO_K70_SUZUNAAN(CFLAG:MASTER:当前位置) == 2
			PRINTFORMW 「对不起%CALLNAME:MASTER%桑,我还有工作要做,所以差不多到此为止吧……」
			SIF TALENT:70:恋慕 && PALAM:70:情欲
				PRINTFORMW 「接下来再继续吧……❤」
		ELSEIF CFLAG:MASTER:当前位置 == CFLAG:[[小铃]]:当前位置
			IF TALENT:70:恋慕
				SELECTCASE RAND:4
				CASE 1
					PRINTFORML 「啊,工作时间到了!」
					PRINTFORMW 「虽然还想继续聊天,但是失陪了~」
				CASE 2
					PRINTFORML 「该去工作了」
					PRINTFORMW 「聊天之后再继续吧」
				CASE 3
					PRINTFORML 「对不起,我要到此为止了……」
					PRINTFORMW 「去工作了~」
				CASEELSE
					PRINTFORML 「我要回家工作去了」
					PRINTFORMW (啊,真想再多待一会儿呢)
				ENDSELECT
			ELSE
				PRINTFORMW 「啊,这个时候,我该回家了」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;业务中
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;店番
		IF CFLAG:70:职业种类 == 47
			SELECTCASE RAND:10
			CASE 1
				PRINTFORML 「%M_KOJO_K70_AISATSU()%~」
			CASE 2
				PRINTFORML 「这个书是——」
			CASE 3
				PRINTFORML 「欢迎光临~」
			CASE 4
				PRINTFORML 「谢谢您的归还~」
			CASE 5
				PRINTFORML 「如果那里没有的话,那就没有了」
			;半分はまったり
			CASEELSE
				PRINTFORML 「♪～～」
			ENDSELECT
		;藏书整理
		ELSEIF CFLAG:70:职业种类 == 40
			;特に要らない气がした
			;WAIT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;仕事结束
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;小铃の仕事って活动时间外に终わるからデート诱えないのよねー
	IF LOCAL:1 && ARG == 3
		;店番
		IF CFLAG:70:职业种类 == 47
			;仕事手传った
			IF GETBIT(TCVAR:70:360, 1)
				IF TALENT:70:恋慕
					PRINTFORML 「呼、好累呀～」
					;同栖中
					IF CFLAG:MASTER:初始位置 == CFLAG:70:初始位置
						SELECTCASE RAND:3
						CASE 1
							PRINTFORMW 「能和%CALLNAME:MASTER%桑一起工作真是太棒了♪」
							PRINTFORMW 「如果%CALLNAME:MASTER%桑愿意的话,就这样一直……啊,不不,没什么❤」
						CASE 2
							;老爸会登场吗?
							PRINTFORMW 「多亏了%CALLNAME:MASTER%桑,最近销售额有所提高!」
							PRINTFORMW 「老爸也一直感谢您帮了大忙哦~」
						CASEELSE
							PRINTFORMW 「有%CALLNAME:MASTER%桑在真是帮大忙了~」
							PRINTFORMW 「之后要吃饭吗?洗澡吗?还是……❤」
						ENDSELECT
					ELSE
						PRINTFORMW 「今天辛苦您了!」
						SELECTCASE RAND:5
						CASE 1
							;并不是特别的事件,只是催促搬家
							PRINTFORML 「如果%CALLNAME:MASTER%桑愿意的话,可以正式在我们店里工作哦ー」
							PRINTFORMW 「这样的话,可以住在我们家,或者我的房间里面…同、同居之类的……❤」 
						CASEELSE
							PRINTFORMW 「嘻嘻,%CALLNAME:MASTER%桑一起工作真开心♪」
						ENDSELECT
					ENDIF
				ELSE
					PRINTFORML 「唷,辛苦了」
					PRINTFORMW 「报酬之后再给您吧」
				ENDIF
			ELSE
				IF TALENT:70:恋慕
					PRINTFORML 「呼,累死了~」
					PRINTFORMW 「如果%CALLNAME:MASTER%桑能帮忙的话会很感激哦,怎、怎么样?」
				ELSE
					PRINTFORMW 「呼,累死了~」 
				ENDIF
			ENDIF
		;藏书整理
		ELSEIF CFLAG:70:职业种类 == 40
			;仕事手传った
			IF GETBIT(TCVAR:70:360, 1)
				IF TALENT:70:恋慕
					PRINTFORML 「唷,辛苦您了~」
					PRINTFORMW 「有%CALLNAME:MASTER%桑帮忙的话,可以更快结束,很感谢♪」
				ELSE
					PRINTFORML 「唷,辛苦您了」
					PRINTFORML 「多亏您的帮助,提早完成了~」
					PRINTFORMW 「太感谢您了ー!」
				ENDIF
			ELSE
				PRINTFORMW 「呼,终于完成了~」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

