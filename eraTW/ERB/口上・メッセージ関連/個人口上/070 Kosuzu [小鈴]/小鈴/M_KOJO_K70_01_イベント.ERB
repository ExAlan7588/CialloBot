;铃奈庵判定
;1=铃奈庵,2=自分房间（引っ越した后も考虑）,3=房间追い出し
@M_KOJO_K70_SUZUNAAN(ARG)
#FUNCTION
IF MAIN_MAP == 2
	IF ARG == P213铃奈庵
		RETURNF 1
	ELSEIF ARG == P226小铃私室
		;连れ出す函数ができてました＆特定条件で连れ出せるので对応しました
		IF COM_351_CONDITION(70)
			RETURNF 2
		;不法侵入,恋慕でも思慕でもなく阿求と一绪でもなく连れ出すこともできない,要するに时止めて侵入した（一応阿求と一绪に入って阿求が出て行くパターンもある）
		ELSEIF !TALENT:[[小铃]]:恋慕 && !TALENT:[[小铃]]:思慕 && CFLAG:MASTER:当前位置 != CFLAG:[[阿求]]:当前位置 && COM_351_CONDITION([[小铃]]) == 0
			RETURNF 3
		ELSE
			RETURNF 2
		ENDIF
	ENDIF
ELSE
	IF ARG == 商业街
		RETURNF 1
	ELSEIF CFLAG:[[小铃]]:当前位置 == OMANEKIBEYA() || CFLAG:[[小铃]]:当前位置 == CFLAG:[[小铃]]:初始位置
		RETURNF 2
	ENDIF
ENDIF

