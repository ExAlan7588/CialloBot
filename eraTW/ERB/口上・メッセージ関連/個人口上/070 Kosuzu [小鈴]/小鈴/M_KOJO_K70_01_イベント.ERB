;分岐用のゲストキャラ判定
@M_KOJO_K70_GUEST(ARGS)
#DIM CHARA
;ARGS=呼び出したコマンド
;RESUTS=ゲストキャラの名前
;RESULT=分岐用の值
;RESULT:1=キャラ番号
;RESULTS:1=ゲストキャラの呼び方
;会话,茶以外は一日一回を无视する（现在は日常爱抚のみ）
VARSET RESULT
VARSET RESULTS
;阿求(80)は最优先,RETURNは1で固定
IF CFLAG:[[阿求]]:当前位置 == CFLAG:[[小铃]]:当前位置 && M_KOJO_K70_GUEST_ABLE([[阿求]])
	IF !GROUPMATCH(ARGS, "会话", "茶") || !TCVAR:[[小铃]]:(M_KOJO_K70_GUEST_TCVAR([[阿求]]))
		RESULTS = 阿求
		RESULTS:1 = 阿求
		RESULT:1 = [[阿求]]
		RETURN 1
	ENDIF
ENDIF
FOR LOCAL, 1, 10
	;灵梦(1)、魔理沙(11)、紫(26)、文(29)、猯藏(69)
	CHARA = TARGET:LOCAL
	IF GROUPMATCH(CHARA, [[灵梦]], [[魔理沙]], [[紫]], [[文]], [[猯藏]]) && M_KOJO_K70_GUEST_ABLE(CHARA)
		IF !GROUPMATCH(ARGS, "会话", "茶") || !TCVAR:[[小铃]]:(M_KOJO_K70_GUEST_TCVAR(CHARA))
			RESULTS = %CALLNAME:CHARA%
			RESULTS:1 = %CALLNAME:CHARA%桑
			RESULT:1 = CHARA
			;台词を先に考えてたらよくわからん分岐になってしまった
			;恋慕の小铃と同行中（デート含む）
			IF CFLAG:[[小铃]]:同行 && TALENT:[[小铃]]:恋慕
				RETURN 4
			;铃奈庵でゲストと同行中
			ELSEIF CFLAG:CHARA:同行 && M_KOJO_K70_SUZUNAAN(CFLAG:[[小铃]]:当前位置) == 1
				RETURN 3
			;ゲストと约会中
			ELSEIF CFLAG:CHARA:同行 && FLAG:约会的对象 == CHARA
				RETURN 2
			;恋慕のゲストと同行中
			ELSEIF CFLAG:CHARA:同行 && TALENT:CHARA:恋慕
				RETURN 2
			;小铃が恋慕
			ELSEIF TALENT:[[小铃]]:恋慕
				RETURN 4
			ELSE
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
NEXT

