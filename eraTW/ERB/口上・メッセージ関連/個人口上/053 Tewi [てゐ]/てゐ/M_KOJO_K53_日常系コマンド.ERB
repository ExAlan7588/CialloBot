;300,会话
;TFLAG:193(1=大成功 0=成功 -1=失败 -2=大失败（TARGETがMASTERより教养が4以上高い)
;TCVAR:302(0=会话可能 1>=非恋慕时会话不能)
;MARK:反抗刻印(3以上=会话不能)(4以上はバグ
;TCVAR:烂醉(1=通常成功or大失败)
;==================================================
@M_KOJO_MESSAGE_COM_K53_300
;指令共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～说着话。」とかが消えます。また、削除した场合は自动で口上色が付きません。
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K53_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K53_300_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TCVAR:烂醉
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「呼喵～～～…」
			CASE 1
				PRINTFORML 「呜～嗯…呜～嗯…」
		ENDSELECT
	ELSEIF CFLAG:勃然大怒
		PRINTFORML 「…太烦人的话、会惹怒因幡们哟…」
	ELSEIF IsHeat(TARGET)
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「嗯…嗯、呼…、看、看什么啊、什么事都没有哟…」
			CASE 1
				PRINTFORML 「嗯…呼………、哦…？……啊、对不起、…那个、你说了什么…」
			CASE 2
				PRINTFORML 「哈啊…哇呼…、…诶？听、听到了啦……好啦、你继续说下去…」
		ENDSELECT
		PRINTFORML 帝不时漏出甜蜜的声音、和%CALLNAME:PLAYER%的会话还在继续…
	ELSE
		;お话
		SELECTCASE RAND:4
			;身内の话
			CASE 0
				SELECTCASE RAND:4
					;因幡
					CASE 0
						PRINTFORML 「这么说来之前因幡们～…」
					;铃仙
					CASE 1
						PRINTFORML 「和铃仙好好相处吧」
						PRINTFORML 「那孩子虽然很胆小，但是戏弄她还是很开心的」
					;永琳
					CASE 2
						PRINTFORML 「永琳是我的师匠」
						PRINTFORML 「只是稍微恶作剧一下就生气有点受伤呢～」
					;辉夜
					CASE 3
						PRINTFORML 「公主虽然总是一直待在家里」
						PRINTFORML 「但是最近偶尔也会到人里去～」
				ENDSELECT
			;あることないこと
			CASE 1
				SELECTCASE RAND:4
					CASE 0
						PRINTFORML 「我要是发生了什么事，幻想乡中的１亿只因幡不会保持沉默的说」
						PRINTFORML 「明白了的话，你还不快来跪拜我」
					CASE 1
						PRINTFORML 「铃仙总是保持一副很正常的清纯模样」
						PRINTFORML 「但是你没看见她穿着什么内衣哦…」
					CASE 2
						PRINTFORML 「实际上在迷途森林里有隐藏的宝藏在沉眠哟」
						PRINTFORML 「而记载了宝藏位置的地图就在师匠放内衣的柜子里…」
					CASE 3
						PRINTFORML 「你知道公主每天深夜都在偷偷干什么么？」
						PRINTFORML 「是啊，说晚上做的事的确不大好…」
				ENDSELECT
			;商谈
			CASE 2
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「喂、不买点药么？现在有铃仙穿过的胖次附送哟？」
					CASE 1
						PRINTFORML 「意中人对你不感兴趣？那么我向你推荐这款商品！」
					CASE 2
						PRINTFORML 「拿着的话就会有好事发生！幸运的护符『开运大纹』！」
						PRINTFORML 「现在买的话就算你便宜一些哟」
				ENDSELECT
			;你の话を闻く
			CASEELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「呼～嗯，原来是那样啊～…」
					CASE 1
						PRINTFORML 「诶～、那之后怎么样了？」
					CASE 2
						PRINTFORML 「啊ー、原来如此呢～」
				ENDSELECT
				;样子
				SELECTCASE TFLAG:193
					CASE 0
						PRINTFORML 帝坐在一个舒适的地方，啪嗒啪嗒的甩着脚…
					CASE 1
						PRINTFORML 帝被%CALLNAME:PLAYER%逗弄着，库斯库斯的笑着…
					CASE -1
						PRINTFORML 帝听了%CALLNAME:PLAYER%的话打着哈欠，一副无聊的样子…
				ENDSELECT
		ENDSELECT
		
	ENDIF
	
	WAIT
	
ENDIF
RETURN 1
