;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K53_80
CALL M_KOJO_MESSAGE_COM_K53_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K53_80_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;２４时间以上澡堂に入っていない场合咎められる
		IF DAY * 1440 + TIME - EX:MASTER:上次的入浴时间 > 1440
			PRINTFORMW 「呜哇！等下这个、这个好好洗过了没？让我握住这样的东西真是低级呢ー」
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		CALL K53_AUTO_PVOICE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

