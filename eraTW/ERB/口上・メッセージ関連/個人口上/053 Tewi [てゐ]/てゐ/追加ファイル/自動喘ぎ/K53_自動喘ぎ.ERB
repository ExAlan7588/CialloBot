;*********************************************************************************
;K53_PLIST_NUM			喘ぎ声列表数			喘ぎ声パターンを追加したらこの数をいじる
;K53_PVOICE_FRAG			使用可能な喘ぎ声列表		(=0 使用不可　=1 使用可
;		0:对象部位　1:你部位　2:快感　3:苦痛　4:反感　5:接吻　6:口交　7:吐息　8:深喉插入　9:声我慢
;
;K53_台词使用可			台词系は使用一回まで		(=0 使用不可　=1 使用可
;RAND:K53_台词发生概率
;*********************************************************************************
@K53_AUTO_PVOICE

;初期化・初期设定
VARSET LOCALS

K53_NUM = 40
K53_台词发生概率 = 3

;使用喘ぎ列表初期化
FOR LOCAL, 0, K53_PLIST_NUM
	K53_PVOICE_FRAG:LOCAL = 0
NEXT
K53_台词使用可 = 1


;*********************************************************************************
;合计快感量、快感から接触部位を判定
;总PALAMLVとってるけどたぶん不使用かも
;*********************************************************************************
;快感总量判定
K53_快感 = CUP:快Ｃ + CUP:快Ｖ + CUP:快Ａ + CUP:快Ｂ + CUP:快Ｍ

;快感を与えられている部位の总PALAMLVと
;快感を与えられている部位を検索
;検索する快感0:快Ｃ 1:快Ｖ 2:快Ａ 3:快Ｂ 4:快Ｍ
K53_总PALAMLV = 0
K53_接触部位 = 
FOR LOCAL,0, 5
	;快感が与えられている
	IF CUP:TARGET:LOCAL > 0
		;总PALAMLV增加
		K53_总PALAMLV += GETPALAMLV(PALAM:TARGET:LOCAL,4)
		
		;接触部位记忆
		SELECTCASE LOCAL
			CASE 0
				K53_接触部位 += "Ｃ"
			CASE 1
				K53_接触部位 += "Ｖ"
			CASE 2
				K53_接触部位 += "Ａ"
			CASE 3
				K53_接触部位 += "Ｂ"
			CASE 4
				K53_接触部位 += "Ｍ"
		ENDSELECT

	ENDIF
	
NEXT
;*********************************************************************************
;使用可能喘ぎ声列表判定
;*********************************************************************************
;深喉插入中
IF SELECTCOM == 140
	;イマラチオ
	K53_PVOICE_FRAG:8 = 1
	IF STRCOUNT(K53_接触部位, "Ｃ") || STRCOUNT(K53_接触部位, "Ｖ") || STRCOUNT(K53_接触部位, "Ａ") || STRCOUNT(K53_接触部位, "Ｂ")
		;あえぎ声
		IF K53_刺激LV("反感") >= 1
			;反感
			K53_PVOICE_FRAG:4 = 1
		;强い快感
		ELSEIF K53_刺激LV("快感") >= 2
			;口ふさぎ　部位　快感
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:2 = 1
			K53_PVOICE_FRAG:9 = 1
		ENDIF
	ELSE
		;深喉插入嫌
		IF K53_刺激LV("反感") >= 1
			;反感
			K53_PVOICE_FRAG:4 = 1
		ENDIF
	ENDIF
ELSEIF STRCOUNT(K53_接触部位, "Ｍ")
	;口交中
	IF TEQUIP:TARGET:口接触部位 == 1
		;Ｍ以外の接触部位がある
		IF STRCOUNT(K53_接触部位, "Ｃ") || STRCOUNT(K53_接触部位, "Ｖ") || STRCOUNT(K53_接触部位, "Ａ") || STRCOUNT(K53_接触部位, "Ｂ")
			;强い快感
			IF K53_刺激LV("快感") >= 2
				;快感が强い场合口ふさぎ强快感で代用、口交喘ぎ无し
				K53_PVOICE_FRAG:2 = 1
				K53_PVOICE_FRAG:9 = 1
			ELSE
				;口交　快感　部位
				K53_PVOICE_FRAG = 1
				K53_PVOICE_FRAG:1 = 1
				K53_PVOICE_FRAG:2 = 1
				K53_PVOICE_FRAG:6 = 1
			ENDIF
			
		;口交系のみ
		ELSE
			;口交
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:6 = 1
		ENDIF
		
	;接吻or爱抚or继续接吻中の行为
	ELSE
		;Ｍ以外の接触部位がある
		IF STRCOUNT(K53_接触部位, "Ｃ") || STRCOUNT(K53_接触部位, "Ｖ") || STRCOUNT(K53_接触部位, "Ａ") || STRCOUNT(K53_接触部位, "Ｂ")
			;チュパ
			;强い快感の场合口ふさぎで代用
			IF K53_刺激LV("快感") >= 2
				K53_PVOICE_FRAG:9 = 1
			ELSE
				K53_PVOICE_FRAG:5 = 1
			ENDIF
			;接吻中に反抗の付きかねない指令实行
			IF K53_刺激LV("苦痛") >= 1 || K53_刺激LV("反感") >= 1
				;继续接吻中に指插入处女膜丧失、子宫口责め等
				
				IF K53_刺激LV("苦痛") >= 1
					;苦痛
					K53_PVOICE_FRAG = 1
					K53_PVOICE_FRAG:1 = 1
					K53_PVOICE_FRAG:3 = 1
				ELSE
					;反感
					K53_PVOICE_FRAG = 1
					K53_PVOICE_FRAG:1 = 1
					K53_PVOICE_FRAG:4 = 1
				ENDIF
				
			;接吻＋喘ぎ
			ELSE
				;チュパ　快感　部位
				K53_PVOICE_FRAG = 1
				K53_PVOICE_FRAG:1 = 1
				K53_PVOICE_FRAG:2 = 1
			ENDIF
			
		;接吻
		ELSE
			;接吻嫌		この分岐は发生しないかもしれない
			IF K53_刺激LV("反感") >= 1
				;チュパ　反感
				K53_PVOICE_FRAG:4 = 1
				K53_PVOICE_FRAG:5 = 1
				
			;接吻、继续接吻
			ELSE
				;チュパ
				K53_PVOICE_FRAG:5 = 1
			ENDIF
		ENDIF
	ENDIF

;侍奉系		侍奉快乐判定
;骑乘位、逆强奸系も含まれる
ELSEIF TCVAR:108 == 1
	;喘ぎ声のみ
	IF K53_刺激LV("快感") >= 2
		;快感　部位
		K53_PVOICE_FRAG = 1
		K53_PVOICE_FRAG:2 = 1
		
	;吐息＋喘ぎ
	ELSEIF STRCOUNT(K53_接触部位, "Ｃ") || STRCOUNT(K53_接触部位, "Ｖ") || STRCOUNT(K53_接触部位, "Ａ") || STRCOUNT(K53_接触部位, "Ｂ")
		;侍奉　快感
		K53_PVOICE_FRAG = 1
		K53_PVOICE_FRAG:1 = 1
		K53_PVOICE_FRAG:2 = 1
		K53_PVOICE_FRAG:7 = 1
		
	;吐息のみ
	ELSE
		;吐息
		K53_PVOICE_FRAG:1 = 1
		K53_PVOICE_FRAG:7 = 1
	ENDIF
;
ELSE
	;苦痛
	;痛み目安	处女:1250 子宫口责め:～500 打屁股:450+幼稚350 肛门爱抚:100 正常位肛交:800
	IF K53_刺激LV("苦痛") >= 1
		;快感も强い
		IF K53_刺激LV("快感") >= 2
			;苦痛　快感
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:2 = 1
			K53_PVOICE_FRAG:3 = 1
			
		;悲鸣のみ
		ELSE
			;苦痛　部位
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:3 = 1
		ENDIF
		
	;反感・恐怖
	;反感目安	后背位:600 插入Ｇスポット・子宫口责め:500+贞操重视1000
	ELSEIF K53_刺激LV("反感") >= 1 || K53_刺激LV("恐怖") >= 1
		;快感も强い		插入Ｇスポット等	2000くらい？
		IF K53_刺激LV("快感") >= 2
			;反感　部位　快感
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:2 = 1
			K53_PVOICE_FRAG:4 = 1
			
		;悲鸣のみ
		ELSE
			;反感　你部位　
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:4 = 1
		ENDIF
		
	;快感
	ELSEIF STRCOUNT(K53_接触部位, "Ｃ") || STRCOUNT(K53_接触部位, "Ｖ") || STRCOUNT(K53_接触部位, "Ａ") || STRCOUNT(K53_接触部位, "Ｂ")
		;快感　部位　你部位
		K53_PVOICE_FRAG = 1
		K53_PVOICE_FRAG:1 = 1
		K53_PVOICE_FRAG:2 = 1
	
	;其他
	ELSE
		;药系・道具不使用の什么也不做、等
		;吐息
		K53_PVOICE_FRAG:7 = 1
	ENDIF
ENDIF

;修正
;睡奸中
IF CFLAG:睡眠
	;台词系无し
	K53_PVOICE_FRAG = 0
	K53_PVOICE_FRAG:1 = 0
	
	;苦痛、恐怖、反感无
	K53_PVOICE_FRAG:3 = 0
	K53_PVOICE_FRAG:4 = 0
	
	;快感を吐息へ
	IF STRCOUNT(K53_接触部位, "Ｃ") || STRCOUNT(K53_接触部位, "Ｖ") || STRCOUNT(K53_接触部位, "Ａ") || STRCOUNT(K53_接触部位, "Ｂ")
		K53_PVOICE_FRAG:2 = 0
		K53_PVOICE_FRAG:7 = 1
	ENDIF

ELSE
	[SKIPSTART]
	;喘ぎ声我慢（作りかけ　地の文が无いため、よくわからないことになっている
	
	;Ｍ以外の接触部位がある
	IF STRCOUNT(K53_接触部位, "Ｃ") || STRCOUNT(K53_接触部位, "Ｖ") || STRCOUNT(K53_接触部位, "Ａ") || STRCOUNT(K53_接触部位, "Ｂ")
		;素質
		IF TALENT:自尊心 || TALENT:自制心 || TALENT:贞操 || TALENT:羞耻心 || TALENT:快感应答 == -1
			K53_PVOICE_FRAG:9 = 1
			;确立で完全がまん
			SELECTCASE RAND:4
				CASE 0
					K53_PVOICE_FRAG:2 = 0
			ENDSELECT
		;状况
		;ベッドルームで后背位
		ELSEIF BEDROOM() && TEQUIP:体位 == 2
			K53_PVOICE_FRAG:9 = 1
			;确立で完全がまん
			SELECTCASE RAND:4
				CASE 0
					K53_PVOICE_FRAG:2 = 0
			ENDSELECT
		ENDIF
	ENDIF
	[SKIPEND]
ENDIF


;被逆推
IF CFLAG:诶嘿嘿 == 2

ENDIF
;*********************************************************************************
;K53_部位台词设定
;责められている部位への台词
;*********************************************************************************
K53_部位台词 =
IF STRCOUNT(K53_接触部位, "Ｃ")
	IF TEQUIP:阴蒂夹
		SELECTCASE RAND:2
			CASE 0
				K53_部位台词 = 咕、咦咦咦咦咦咦咦、呀、呀啊
			CASE 1
				K53_部位台词 = 咕、唔嗯
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 0
				K53_部位台词 = 咕哩
			CASE 1
				K53_部位台词 = 小穴
			CASE 2
				K53_部位台词 = 豆豆
		ENDSELECT
	ENDIF
ELSEIF STRCOUNT(K53_接触部位, "Ｖ")
	IF TEQUIP:振动棒
		K53_部位台词 = 呀、呀啊、肚、肚子…、…咿、咿咿咿咿、啊…
	ELSE
		SELECTCASE RAND:3
			CASE 0
				K53_部位台词 = 要坏了
			CASE 1
				K53_部位台词 = 呐
			CASE 2
				K53_部位台词 = 小穴
		ENDSELECT
	ENDIF
ELSEIF STRCOUNT(K53_接触部位, "Ａ")
	IF TEQUIP:肛用振动棒
		K53_部位台词 = 呜、呼呜咿、屁屁、呼、咿…、…卟呜呜呜、呼呀…
	ELSE
		SELECTCASE RAND:3
			CASE 0
				K53_部位台词 = 屁屁
			CASE 1
				K53_部位台词 = 菊穴
			CASE 2
				K53_部位台词 = 屁股
		ENDSELECT
	ENDIF
ELSEIF STRCOUNT(K53_接触部位, "Ｂ")
	SELECTCASE RAND:3
		CASE 0
			K53_部位台词 = 姆呐
		CASE 1
			K53_部位台词 = 胸部
		CASE 2
			K53_部位台词 = 乳头
	ENDSELECT
ELSE
	K53_PVOICE_FRAG = 0
ENDIF

;*********************************************************************************
;指令台词设定
;指令に对する台词
;*********************************************************************************
K53_指令台词 =
SELECTCASE SELECTCOM
	;爱抚
	CASE 0
		IF TEQUIP:乳头夹 || TEQUIP:阴蒂夹
			SELECTCASE RAND:2
				CASE 0
					K53_指令台词 =不行 、那个、不行、啊哈啊啊啊啊啊～～～
				CASE 1
					K53_指令台词 = 咿、哇、好痛苦、不要戴上那个、不要、咿呀呀呀
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					K53_指令台词 = 呀、那里…不要一直欺负那里…
				CASE 1
					K53_指令台词 = 这样的、才不算什么、才一点也不…舒服……
				CASE 2
					K53_指令台词 = 手指、一直进进出出、好、好厉害…
			ENDSELECT
		ENDIF
	;舐阴
	CASE 1
		IF TCVAR:102
			K53_指令台词 = 你、把我弄脏了、要好好清理干净…
		ELSE
			SELECTCASE RAND:2
				CASE 0
					K53_指令台词 = 舔舔
				CASE 1
					K53_指令台词 = 舌
					SELECTCASE RAND:2
						CASE 0
							K53_指令台词 += "啧啧"
						CASE 1
							K53_指令台词 += "舔"
					ENDSELECT
			ENDSELECT
		ENDIF
	;指插入
	CASE 3
		IF EX:膣内精液 > 300
			K53_指令台词 = 精液…%K53_ハート付与()%里面搞得乱七八糟的…
		ELSE
			IF TEQUIP:振动棒
				K53_指令台词 = 不、不行、那个、咿呀呀呀、呜嗯、啊啊啊啊啊～～～
			ELSE
				SELECTCASE RAND:2
					CASE 0
						K53_指令台词 = 手指
					CASE 1
						K53_指令台词 = 手指鸡鸡
				ENDSELECT
			ENDIF
		ENDIF
	;舐肛
	CASE 4
		SELECTCASE RAND:2
			CASE 0
				K53_指令台词 = 舔舔
			CASE 1
				K53_指令台词 = 呀啊、那里、那里不行
		ENDSELECT
	;肛门爱抚
	CASE 5
		IF TEQUIP:肛用振动棒
			K53_指令台词 = 哦、哦啊、麻、麻麻的、那、那个、不、不要
		ELSE
			SELECTCASE RAND:3
				CASE 0
					K53_指令台词 = 屁屁麻麻的、奇怪的、感觉…
				CASE 1
					K53_指令台词 = 屁屁要坏掉了、拔、拔出来…
				CASE 2
					K53_指令台词 = 呀啊、呜、呜咿、火辣辣的、呀呀呀呀啊…
			ENDSELECT
		ENDIF
	;胸爱抚系
	CASE 6, 7, 12
		IF TEQUIP:乳头夹
			K53_指令台词 = 呀啊、乳头要坏掉了、不要、快拿下来
		ELSE
			SELECTCASE RAND:2
				CASE 0
					K53_指令台词 = 乳头、要掉下来了…、呀啊…
				CASE 1
					K53_指令台词 = 呀、呀啊啊…、乳头、不要用手指弹啊…
			ENDSELECT
		ENDIF
	;吸乳头
	CASE 11
		SELECTCASE RAND:3
			CASE 0
				K53_指令台词 = 呀…不要吸乳头…呀啊…
			CASE 1
				K53_指令台词 = 咿呀呀呀！？不、不要用牙齿、禁止用牙咬…
			CASE 2
				K53_指令台词 = 啊、喂、一直吸乳头的话…
		ENDSELECT
	;秘贝・张开肛门
	CASE 8, 13
		;列表操作:反感闭じる　吐息开放
		K53_PVOICE_FRAG:4 = 0
		K53_PVOICE_FRAG:7 = 1
		;中出しされた
		IF TCVAR:102
			K53_指令台词 = 啊ー啊，被萝莉控的鸡鸡玷污了，要不要去跟师匠说呢～
		;ELSEIF TCVAR:104
		ELSE
			K53_指令台词 = …怎样、这样满足了么？……HENTAI
		ENDIF
	;自慰
	CASE 9
		;列表操作
		K53_PVOICE_FRAG = 0
		K53_PVOICE_FRAG:4 = 0
		;IF ABL:露出癖 >= 3
		;TEQUIP:振动棒 && TEQUIP:肛用振动棒
		;IF TEQUIP:振动棒
		;IF TEQUIP:肛用振动棒
		;IF TEQUIP:振动棒 && TEQUIP:肛用振动棒 && ABL:Ｖ感觉 >= 5 && ABL:Ａ感觉 >= 5 && ABL:Ｃ感觉 >= 5 && ABL:Ｂ感觉 >= 5 && PALAM:情欲 >= PALAMLV:4
		;ELSEIF TEQUIP:振动棒 && ABL:Ｖ感觉 >= 5 && ABL:Ｃ感觉 >= 5 && ABL:Ｂ感觉 >= 5 && PALAM:情欲 >= PALAMLV:4
		;ELSEIF TEQUIP:肛用振动棒 && ABL:Ａ感觉 >= 5 && ABL:Ｃ感觉 >= 5 && ABL:Ｂ感觉 >= 5 && PALAM:情欲 >= PALAMLV:4
		;ELSEIF ABL:自慰中毒 >= 3 && ABL:Ｃ感觉 >= 5 && ABL:Ｂ感觉 >= 5 && PALAM:情欲 >= PALAMLV:4
			
		;ELSE
			K53_指令台词 = 不、不要看…！HENTAI…HENTAI～…呀
		;ENDIF
	;性交Ｖ系
	CASE 60, 61, 64, 65, 67, 68, 71
		K53_指令台词 = %K53_TINPO()%
	;责弄Ｇ点
	CASE 72
		K53_指令台词 = 呀、那里%K53_ハート付与()%好厉害
	;子宫责め
	CASE 73
		SELECTCASE EXP:TARGET:子宫口开发经验
			CASE IS < 25
				K53_指令台词 = 哦、哦啊…%K53_ハート付与()%呼呼、呼呜…咿咿……！呼、呜
			CASE 25 TO 50
				SELECTCASE RAND:3
					CASE 0
						K53_指令台词 = 呀啊、那里…%K53_ハート付与()%我、我说…、不要碰……
					CASE 1
						K53_指令台词 = 啊、顶到头了…%K53_ハート付与()%滑滑的…来、来了……
					CASE 2
						K53_指令台词 = 那里、不要咕嗯、放开我%K53_ハート付与()%鸡鸡进来了～～
				ENDSELECT
			CASE IS > 50
				SELECTCASE RAND:3
					CASE 0
						K53_指令台词 = 最、最里面…快进来、快……%K53_ハート付与()%哦
					CASE 1
						K53_指令台词 = 不、不要%K53_ハート付与()%鸡鸡、那里不行、咿呀……
					CASE 2
						K53_指令台词 = 嗯、咕啊%K53_ハート付与()%太粗鲁了、坏掉了、坏掉了、要坏掉了、咿呀啊啊啊…
				ENDSELECT
		ENDSELECT
	;性交Ａ系
	CASE 62, 63, 66, 69, 70, 71, 74, 75
		SELECTCASE RAND:3
			CASE 0
				K53_指令台词 = 呼呜呜呜、屁、屁屁…、好痛、苦……
			CASE 1
				K53_指令台词 = 呀啊、再慢一点…再慢一点动…
			CASE 2
				K53_指令台词 = 要、要坏掉了、不行了、不…不要…
		ENDSELECT
	;手淫
	CASE 80
		SELECTCASE RAND:2
			CASE 0
				K53_指令台词 = 居然要我握住这个…、真的是、太差劲了ー…
			CASE 1
				K53_指令台词 = 唔唔～…%K53_TINPO()%出来好多汁･･･
		ENDSELECT
	;口交・真空口交
	CASE 81, 88
		IF GETPALAMLV(PALAM:情欲, 15) >= 9
			SELECTCASE RAND:3
				CASE 0
					K53_指令台词 = 真、真是的…、%K53_TINPO()%真、真的好多…
