							LOCALS += K53_ハート付与()
					ENDSELECT
				ENDIF
			ENDIF
		;深喉插入
		CASE 8
			IF K53_PVOICE_FRAG:8
				SELECTCASE RAND:9
					CASE 0
						LOCALS += "哦卟呜"
					CASE 1
						LOCALS += "卟"
					CASE 2
						LOCALS += "嗯卟"
					CASE 3
						LOCALS += "呜卟"
					CASE 4
						LOCALS += "呜卟呜"
					CASE 5
						LOCALS += "姆"
					CASE 6
						LOCALS += "姆呜"
					CASE 7
						LOCALS += "姆卟"
					CASE 8
						LOCALS += "哦卟"
				ENDSELECT
				IF CFLAG:睡眠
					SELECTCASE RAND:5
						CASE 0
							LOCALS += "？"
						CASE 1
							LOCALS += "…"
						CASEELSE
							LOCALS += K53_ハート付与()
					ENDSELECT
				ELSE
					SELECTCASE RAND:5
						CASE 0
							LOCALS += "！"
						CASE 1
							LOCALS += "！？"
						CASEELSE
							LOCALS += K53_ハート付与()
					ENDSELECT
				ENDIF
			ENDIF
		;声がまん・口ふさぎ
		CASE 9
			IF K53_PVOICE_FRAG:9
				;刺激が大きい场合接吻感觉外处理
				IF K53_刺激LV("快感") >= 2 || K53_刺激LV("苦痛") >= 1 || K53_刺激LV("反感") >= 1
					SELECTCASE RAND:9
						CASE 0
							LOCALS += "嗯嗯嗯"
						CASE 1
							LOCALS += "嗯呢"
						CASE 2
							LOCALS += "嗯姆～～！？"
						CASE 3
							LOCALS += "嗯嗯！"
						CASE 4
							LOCALS += "嗯呜呜呜～～"
						CASE 5
							LOCALS += "嗯嗯嗯呜！？"
						CASE 6
							LOCALS += "卟"
						CASE 7
							LOCALS += "嗯姆呜"
						CASE 8
							LOCALS += "～～～"	
					ENDSELECT
					SELECTCASE RAND:2
						CASE 0
							LOCALS += K53_ハート付与()
					ENDSELECT
				ELSEIF K53_刺激LV("快感") >= 0
					SELECTCASE RAND:9
						CASE 0
							LOCALS += "嗯"
						CASE 1
							LOCALS += "嗯呢"
						CASE 2
							LOCALS += "嗯～"
						CASE 3
							LOCALS += "嗯姆"
						CASE 4
							LOCALS += "呼"
						CASE 5
							LOCALS += "呼唔"
						CASE 6
							LOCALS += "唔"
						CASE 7
							LOCALS += "呼"
						CASE 8
							LOCALS += "呜呜呜"	
					ENDSELECT
					SELECTCASE RAND:2
						CASE 0
							LOCALS += K53_ハート付与()
						CASE 1
							LOCALS += "…"
					ENDSELECT
				ENDIF
			ENDIF
	ENDSELECT
	
	;文字数が一定に达したら结束
	SIF STRLENSU(LOCALS) > K53_NUM
		BREAK
REND

;*********************************************************************************
;あえぎ声描画
PRINTFORML 「%LOCALS%」
PRINTW


