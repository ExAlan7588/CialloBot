	;-------------------------------------------------
	;妊娠中
	;IF TALENT:妊娠
	;	PRINTFORML 「」
	;	RETURN 1
	;ENDIF
	;精液ボテ
	;IF !TALENT:妊娠 && EX:TARGET:膣内精液 >= 1000
	;	PRINTFORML 「」
	;	RETURN 1
	;ENDIF
	;减少になってもいいさと考えるんだ
	;IF !CFLAG:1502 && CFLAG:信赖度 >= 500 && ABL:教养 > 2 && MONEY >= 300000
	;IF !CFLAG:1502 && CFLAG:信赖度 >= 500 && ABL:教养 > 2
	;	TRYCALLFORM M_KOJO_MESSAGE_COM_K50_进学
	;	RETURN 1
	;ENDIF
	
	IF 家庭菜园可(CFLAG:MASTER:当前位置) && !GETBIT (TCVAR:350,1)
		SETBIT TCVAR:350,1
		;SELECTCASE PLANT_TYPE_0
		SELECTCASE PLANT_TYPE
			CASE 151
				PRINTFORMW 「啊，卷心菜田。」
				PRINTFORML 「竟然把家庭菜园弄成这样。%UNICODE(0x2661)%」
				PRINTFORMW 「哥哥，是想要孩子吗？」
				IF CFLAG:允许无套 <= 2 && TALENT:恋人
					IF CFLAG:16:信赖度 < 500
						PRINTFORML 「但是姐姐大人说不行哦」
					ELSE
						PRINTFORML 
						PRINTFORML 「好啊」
						PRINTFORMW 「因为%CALLNAME:TARGET%也想要哥哥的宝宝」
						PRINTFORMW 「诶嘿嘿！来啊哥哥大人！在卷心菜田里抓住我！」
						CFLAG:允许无套 = 2
					ENDIF
				ELSE
					PRINTDATAL
						DATALIST
							DATAFORM 「%CALLNAME:TARGET%觉得为时过早」
							;ばっちゃに语呂を合わせぱっちぇ呼称
							DATAFORM 「如果不来红豆饭的话，那就不可能了。帕秋莉是这么说的」
						ENDLIST
						DATALIST
							DATAFORM 「哥哥大人、拜托慢慢来吧」
							DATAFORM 「不要勉强啊，诶嘿嘿♪」
						ENDLIST
						DATALIST
							DATAFORM 「想・要・吧・？兄・长・大・人？」
							DATAFORM 「被%CALLNAME:TARGET%看穿了」
							DATAFORM 「诶嘿嘿。现在还不行%UNICODE(0x2661)%」
						ENDLIST
					ENDDATA
				ENDIF
			CASE 163
				PRINTFORML 「姐姐大人以前也是蓝色的吗」
			CASE 164
				PRINTFORML 「西瓜先生～西瓜先生～好大啊ー西瓜先生～」
				PRINTFORML 「大家一起吃西瓜先生吧！」
				PRINTFORMDW %CALLNAME:TARGET%突然笑了
			CASE 177
				PRINTFORML 「把这葡萄摘下来就做红酒吧」
				PRINTFORML 「%CALLNAME:TARGET%呢。想要去踩葡萄」
				PRINTFORML 「诶嘿嘿。好期待啊♪」
				PRINTFORMD %CALLNAME:TARGET%把裙子卷了起来
				
				IF TALENT:MASTER:受虐狂
					PRINTFORMDL 于是、%CALLNAME:MASTER%把目光移到踩踏的大腿之间，股间隆起了
				ELSE
					PRINTFORMDL 一副欢快的样子
				ENDIF
			CASE 187
				PRINTFORML 「百合～♪小百合～♪」
				PRINTFORML 「呐呐兄长大人。百合的根部可以吃哟！」
				SIF TALENT:MASTER:性别 == 3
					PRINTFORMDL %CALLNAME:TARGET%看着%CALLNAME:MASTER%的胯下的眼神很奇怪…
			CASE 188
				IF TCVAR:媚药
					PRINTFORML 「呐呐哥哥大人」
					PRINTFORML 「给胡萝卜浇上柠檬就会成为媚药！！但是媚药是什么啊？」
				ELSE
					PRINTFORML 「胡萝卜先生♪胡萝卜先生♪香菇先生♪牛蒡先生♪啊啊～空空的莲藕先生♪」
					PRINTFORML 「木瓜已经经过了♪」
				ENDIF
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;以下日常 とりあえず随机三框
	;-------------------------------------------------
	
	IF TFLAG:193 == 1
		PRINTFORMDW %CALLNAME:TARGET%对%CALLNAME:MASTER%的话赞同似的用力地点点头
		RETURN 1
	ELSEIF TFLAG:193 == -1
		PRINTFORMDW %CALLNAME:TARGET%对%CALLNAME:MASTER%的话感到了小小的疑惑，微微歪着脑袋
		RETURN 1
	ENDIF
	;恋慕 后期
	IF TALENT:恋慕
		IF !RAND:3
			;PRINTFORML 「」
		ELSEIF !RAND:2
			;PRINTFORML 「」
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	;思慕 中期
	ELSEIF TALENT:思慕
		IF !RAND:3
			;PRINTFORML 「」
		ELSEIF !RAND:2
			;PRINTFORML 「」
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	;それ以外
