				RETURN 1
			;Ｂ＆Ｍ绝顶
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ绝顶
		;强绝顶Ｃ
		IF NOWEX:Ｃ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ绝顶
		;强绝顶Ｖ
		IF NOWEX:Ｖ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｖ
		ELSE
