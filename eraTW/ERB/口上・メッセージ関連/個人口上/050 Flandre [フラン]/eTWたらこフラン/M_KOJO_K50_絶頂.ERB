;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SIF !SHIRAHU(TARGET)
		RETURN 0
	IF 充填率(TARGET,1) >= 100
		;筹码は株だからおNISAまは优遇されるの
		PRINTFORML 「哥哥、%CALLNAME:TARGET%已经肚子满满了哟」
		PRINTFORMW 「变得不能吃饭了哟」
	ELSE
		PRINTFORMW 「嗯……\@ TALENT:恋慕 ? %UNICODE(0x2661)% #  \@」
		PRINTFORML 「哥哥的小鸡鸡在肚子里头，咚咚咚做着」
		IF GETBIT(CFLAG:1500,1)
			PRINTFORMW 「在%CALLNAME:TARGET%中射精着呢」
		ENDIF
		IF ABL:Ｖ感觉 > 2
			PRINTFORMW 「诶嘿嘿%UNICODE(0x2661)%在咚咚咚做的时候，每次哥哥的小鸡鸡闹腾，%CALLNAME:TARGET%也很舒服哟」
		ELSE
			PRINTFORMW 「哥哥好像很舒服」
		ENDIF
		IF GETBIT (CFLAG:1500,3)
			PRINTFORMW 「哥哥高兴吗？在%CALLNAME:TARGET%肚子里\@ GETBIT (CFLAG:1500,1) ? 射精 # 咚咚地做着 \@开心吗？」
			SIF 充填率(TARGET,1) < 50
				PRINTFORMW 「继续做也可以哦」
		ENDIF
	ENDIF
ENDIF
