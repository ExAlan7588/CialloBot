;==================================================
;MASTERが推倒た
;==================================================
@M_KOJO_EVENT_K50_15(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;未实装（推倒指令に统合？
	;MASTERが推倒た（ウフフ开始时）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF FIRSTTIME(SELECTCOM)
			PRINTFORML 「哥哥大人。你在做什么？？」
			PRINTFORMW 「职业摔角游戏？柔道游戏？还・是・说・啊…」
			PRINTFORMW 「弹幕游戏？」
			RETURN 1
		ENDIF
		;死に分岐
		;まとまりが恶い。
		IF GETBIT (CFLAG:1500,4)
			IF 充填率(TARGET,1)
				PRINTFORML 「又想让我怀孕吗？」
			ELSE
				PRINTFORML 「兄长大人。想要让我怀孕吗？」
			ENDIF
		ELSEIF GETBIT (CFLAG:1500,3)
			IF CFLAG:TARGET:积攒度 / 10 > 750
				PRINTFORML 「%CALLNAME:TARGET%、中出游戏也可以哟」
			ELSE
				PRINTFORML 「又要玩中出游戏吗？」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;逢濑结束（ウフフ结束时）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;膣内射精经验がある
		IF GETBIT (CFLAG:1500,4)
			IF 充填率(TARGET,1)
			ELSE
			ENDIF
		;はじめてのなかだし
		ELSEIF 充填率(TARGET,1)
			PRINTFORMW 「呐哥哥。『怀孕』是什么意思？」
			PRINTFORMW 「喜欢？是恋人的那种喜欢吗？」
			PRINTFORML 
			PRINTFORMW 「呼嗯」
			PRINTFORMW 「和帕秋莉说的好像不一样呢」
			PRINTFORML 
			PRINTFORMW 「哥哥大人」
			PRINTFORMW 「%CALLNAME:TARGET%的喜欢是恋人的喜欢%UNICODE(0x2661)%」
			SETBIT CFLAG:1500,4
			RETURN 1
		;膣内射精した事无い分岐
		ELSE
			PRINTFORMW 「已经可以了吗？哥哥」
			PRINTFORMW 「哥哥大人。%CALLNAME:TARGET%的话是不够的」
		ENDIF
	ENDIF
	;其他
	PRINTFORM 
ENDIF
RETURN 1

