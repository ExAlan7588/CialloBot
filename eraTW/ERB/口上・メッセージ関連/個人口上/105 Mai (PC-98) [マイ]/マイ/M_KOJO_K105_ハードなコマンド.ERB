;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K105_140
CALL M_KOJO_MESSAGE_COM_K105_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_140_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状况
	IF FLAG:70 ;时奸
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		PRINTFORML 
		RETURN 1
	;通常
	ELSE
		IF ABL:受虐属性 < 2
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「嗯、呜！？　咕、嗯……嗯……」
				PRINTFORMDW 舞对突然的激烈行为投以责备的目光，但并没有特别抵抗的样子。
			ELSE
				PRINTFORML 「嗯咕！　嗯～！」
				PRINTFORMDW 舞试图扭开脸，挣扎着……
			ENDIF
		ELSE
			IF TALENT:恋人 || TALENT:恋慕
				IF CFLAG:105:1006
					PRINTFORMDL 舞将脸贴近%CALLNAME:MASTER%的阴茎，仿佛要吞下一样，开始用喉咙进行侍奉。
					PRINTFORMDL 虽然她发出了痛苦的呻吟声，但声音中掺杂着甜美的音色，似乎有些高兴的样子...
					PRINTFORML 「嗯呜呜、嗯、哦诶%UNICODE(0x2665) *1%　咕、咕呼……嗯%UNICODE(0x2665) *1%」
				ELSE
					PRINTFORML 「嗯、咕……%UNICODE(0x2665) *1%　呕，噗……%UNICODE(0x2665) *1%」
					PRINTFORMDW ​舞虽然看起来很痛苦，但她还是积极地动着脸和舌头，主动地进行侍奉。
				ENDIF
			ELSE
				PRINTFORMDL 舞本能地表现出些许抵抗，但很快就安静地接受了。
				PRINTFORMW 「咕、嗯……嗯布，噢咕……」
				SIF TALENT:思慕
					PRINTFORMW （痛苦……好痛苦，但是……这个，意外的……）
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

@M_KOJO_EXTRASOURCE_COM_K105_140
IF TALENT:恋人 || TALENT:恋慕
	SELECTCASE CFLAG:105:1006
	CASE 0
		SOURCE:反感 = 300
	CASE 1
		SOURCE:屈从 += ABL:105:受虐属性 * 150
		SOURCE:反感 = 0
		SOURCE:快Ｍ += ABL:105:受虐属性 * 100
		TIMES DOWNBASE:体力 , 0.50
		TIMES DOWNBASE:气力 , 0.50
	CASE 2
		SOURCE:屈从 += ABL:105:受虐属性 * 300
		SOURCE:反感 = 0
		SOURCE:快Ｍ += ABL:105:受虐属性 * 200
		TIMES DOWNBASE:体力 , 0.30
		TIMES DOWNBASE:气力 , 0.30
	CASE 3
		SOURCE:屈从 += ABL:105:受虐属性 * 500
		SOURCE:反感 = 0
		SOURCE:快Ｍ += ABL:105:受虐属性 * 300
		SOURCE:快Ｖ += ABL:105:受虐属性 * 30
		TIMES DOWNBASE:体力 , 0.10
		TIMES DOWNBASE:气力 , 0.10
	CASEELSE
	ENDSELECT
ENDIF
RETURN 1
