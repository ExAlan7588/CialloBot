;-------------------------------------------------
;奴隷の绝顶时の口上
;NOWEX:Ｃ绝顶～4 CVABM绝顶
;NOWEX:二重绝顶 二重绝顶
;NOWEX:三重绝顶 三重绝顶
;NOWEX:四重绝顶 四重绝顶
;NOWEX:五重绝顶 五重绝顶
;NOWEX:喷乳 喷乳
;NOWEX:射精 射精
;NOWEX:放尿 放尿
;随意组み合わせて使ってください
;-------------------------------------------------
@M_KOJO_吧友火焔猫燐制作_MESSAGE_PALAMCNG_B_K36
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;喷乳
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:喷乳
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;绝顶
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;五重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:五重绝顶
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:四重绝顶
		;部位を指定せず简易的に书く场合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの间に书いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:四重绝顶
			;Ｃ＆Ｖ＆Ａ＆Ｂ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ａ＆Ｍ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ＆Ｍ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ＆Ｂ＆Ｍ绝顶
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;三重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:三重绝顶
		;部位を指定せず简易的に书く场合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの间に书いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:三重绝顶
			;Ｃ＆Ｖ＆Ａ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ａ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ａ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｍ绝顶
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ｍ绝顶
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｖ＆Ｍ绝顶
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ｍ绝顶
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ＆Ｍ绝顶
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｍ绝顶
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;二重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:二重绝顶
		;部位を指定せず简易的に书く场合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの间に书いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:二重绝顶
			;Ｃ＆Ｖ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｂ绝顶
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ绝顶
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｍ绝顶
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｍ绝顶
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ绝顶
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ绝顶
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ绝顶
		;强绝顶Ｃ
		IF NOWEX:Ｃ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ绝顶
		;强绝顶Ｖ
		IF NOWEX:Ｖ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ａ绝顶
		;强绝顶Ａ
		IF NOWEX:Ａ绝顶 == 2
			PRINTFORMW 
		;绝顶Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ绝顶
		;强绝顶Ｂ
		IF NOWEX:Ｂ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｂ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｍ绝顶
		;强绝顶Ｍ
		IF NOWEX:Ｍ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｍ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


