			PRINTFORMW
			RETURN 1
		ENDIF
	;PLAYERだけ射精
	ELSE
		;TARGETについてる
		IF TALENT:2 & 2
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

