;==================================================
@M_KOJO_EVENT_K19_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF FLAG:时间停止
		ELSEIF CFLAG:318 == 1
			PRINTFORML  
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				;CASE 1
					;PRINTFORML 
					;PRINTFORMW 
				CASE 0
					PRINTFORML 「哼、就算%CALLNAME:MASTER%来了也没什么可高兴的嘛！」
					PRINTFORMW …把脸扭向一边的莉莉的翅膀、却在很开心地飘扬拍打着。
				CASEELSE
					PRINTFORML 「等等等等下！我还没做好心理准备别突然就出现啊！」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「嗯、能够消磨时间呢」
					PRINTFORMW 
				CASE 0
					PRINTFORML 「看起来很忙呢。嘛、和我没什么关系都是了」
					PRINTFORMW 
				CASEELSE
					PRINTFORML 「这不是%CALLNAME:MASTER%嘛。很闲的话就陪我吧」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「嗯？在哪里见过的脸呢」
