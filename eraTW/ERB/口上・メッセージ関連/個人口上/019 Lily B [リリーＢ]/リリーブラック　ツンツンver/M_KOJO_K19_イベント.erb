						;PRINTFORML 
						;PRINTFORMW 
					CASE 0
						PRINTFORML 「端、端茶什么的也不是不会给你做啦」
						PRINTFORML 「很偶然得还剩下了一个很好吃的包子。没错、是偶然的噢！」
					CASEELSE
						PRINTFORML 「想要来见我的话要好好地事先告诉我啊！」
						PRINTFORML 「就算%CALLNAME:MASTER%觉得很好、我这边也是要做好各种各样的准备才行啊！」
				ENDSELECT
			ELSEIF TALENT:思慕
				SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「就算你来这里也没有什么有趣的东西噢」
					PRINTFORML 
				CASE 0
					PRINTFORML 「是来见姐姐的吗？还是说来见我的？」
				CASEELSE
					PRINTFORML 「嗯、有什么事吗？」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「真亏你能知道这里是我们的家啊」
						PRINTFORML 「我觉得他人很难理解这里是妖精的家的啊」
					CASE 0
						PRINTFORML 「我可没打算招待你呢」
					CASEELSE
						PRINTFORML 「你进去别人房间里时候至少要敲下门吧？」
				ENDSELECT
			ENDIF
		ENDIF
		IF FLAG:时间停止
		ELSEIF CFLAG:318 == 1
			PRINTFORML  
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 莉莉意识到自己脸上啪得露出笑容后立刻就摆出了严肃的表情
					PRINTFORML 「这、这只是回想起美好的记忆才笑的呢！」
				CASE 0
					PRINTFORML (我、我的春天来啦）
					PRINTFORML 
				CASEELSE
					PRINTFORML  (…难、难道说是来找我之类的吗？)
					PRINTFORML 
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「嗯、是来邀请我去玩的吗？」
				CASE 0
					PRINTFORML 「会不会发生些有趣的事情呢…」
				CASEELSE
					PRINTFORML 「啊咧、这不是%CALLNAME:MASTER%嘛」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「妖精可是为所欲为出现在各种地方的存在哟」
				CASE 0
					PRINTFORML 「嗯ー、也不在这里吗」
				CASEELSE
					PRINTFORML 「什么呀。就这么在意我在这里这件事么？」
			ENDSELECT
