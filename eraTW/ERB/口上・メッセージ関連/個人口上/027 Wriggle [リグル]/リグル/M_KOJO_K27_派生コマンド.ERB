					CALL HPH_PRINT,@"「那里HPHHPH　%TEXTR(@"%CALLNAME:MASTER%/你")%的精子HPH　充满注入吧HPHHPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「想怀孕HPH　想怀上%TEXTR(@"%CALLNAME:MASTER%/你")%的宝宝HPH」","L"
					CALL HPH_PRINT,@"「可以在里面射吗HPH　想要在最深处射出来HPH」","L"
				CASE 2
					CALL HPH_PRINT,@"「把那里打开HPH　那里HPH　是我最珍贵的地方HPH」","L"
					CALL HPH_PRINT,@"「给我HPH　生个强壮的孩子HPHHPH」","L"
				CASE 3
					CALL HPH_PRINT,@"「啊HPH　啊HPH　要出来了HPH　最深处HPH　那里HPHHPH」","L"
					CALL HPH_PRINT,@"「%TEXTR("性器/阴茎")%直接HPH　碰到那里了HPH」","L"
				CASE 4
					CALL HPH_PRINT,@"「来了HPH　来了HPH　来了HPH　来了HPHHPH」","L"
					CALL HPH_PRINT,@"「最深处HPH　有点酸酸的地方HPH　送到了HPHHPH」","L"
				CASE 5
					CALL HPH_PRINT,@"「孩子HPH　在哪儿生HPH　%TEXTR("生殖器/阴茎")%里HPH　接吻HPH　好的HPH」","L"
				CASE 6
					CALL HPH_PRINT,@"「那、那么、这么粗暴地、戳进去的话」","L"
					CALL HPH_PRINT,@"「要射出来了HPH　\@ CFLAG:1006 >= 1 ? %TEXTR("子宫/宝宝的房间/子袋")% # 子袋 \@要掉出来了哦HPHHPH」","L"
				CASE 7
					CALL HPH_PRINT,@"「那里HPH　那里、很好呢HPH」","L"
					CALL HPH_PRINT,@"「积攒的精液HPH　全部注入到我最深处HPH　放出去吧HPHHPH」","L"
				CASE 8
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%…HPHHPH」","L"
					CALL HPH_PRINT,@"「快点…HPH　\@ CFLAG:1006 >= 1 ? %TEXTR("子宫/宝宝的房间/子袋/膣内/阴户")% # 中 \@射进来…HPH　嘿…HPH」","L"
				CASE 9
					CALL HPH_PRINT,@"「厉害啊…哎HPH　%TEXTR(@"%CALLNAME:MASTER%/你")%的%TEXTR("生殖器/阴茎")%…HPH」","L"
					CALL HPH_PRINT,@"「肚子里，变得糊糊的了…HPH」","L"
				CASE 10
					CALL HPH_PRINT,@"「啊ーーHPH　啊ーーHPH　啊ーーHPH　啊ーーHPH」","L"
				ENDSELECT
				PRINTW 
			;开始裂缝
			CASE 25 TO 50
				SELECTCASE RAND:6
				CASE 0
					CALL HPH_PRINT,@"「喔HPH　喔HPH　呜HPH」","L"
					CALL HPH_PRINT,@"「虽然很痛苦HPH　但这个HPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「\@ CFLAG:1006 >= 1 ? %TEXTR("子宫/宝宝的房间/子袋")% # 肚子 \@HPH　要瘪了HPH」","L"
				CASE 2
					CALL HPH_PRINT,@"「那里HPH　%TEXTR(@"%CALLNAME:MASTER%/你")%的好刺激呀HPH」","L"
				CASE 3
					CALL HPH_PRINT,@"「啊HPH　喔HPH　喔HPH　啊HPH　啊HPH　啊HPH　啊啊HPH」","L"
					PRINTFORML 在子宫口被一再刺激的过程中，莉格露发出了尖叫声，全身颤抖…
				CASE 4
					CALL HPH_PRINT,@"「直到肚子里HPH 到达肚子里HPH」","L"
					CALL HPH_PRINT,@"「%TEXTR(@"%CALLNAME:MASTER%/你")% 的到达HPH 已经到达了HPH」","L"
				CASE 5
					CALL HPH_PRINT,@"「不行HPH 不行HPH」","L"
					CALL HPH_PRINT,@"「精液HPH 想要HPH \@ CFLAG:1006 >= 1 ? %TEXTR("子宫/宝宝的房间/子宫袋/阴道/阴户")% # 里 \@想要HPHHPH」","L"
				ENDSELECT
				PRINTW 
			;异物感
			CASEELSE
				SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「来了HPH 感到了HPH」","L"
					CALL HPH_PRINT,@"「虽然很痛苦HPH 但是这个HPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「\@ CFLAG:1006 >= 1 ? %TEXTR("子宫/宝宝的房间/子袋")% # 肚子里\@HPH 压扁HPH」","L"
				CASE 2
					CALL HPH_PRINT,@"「不行HPH 不行HPH」","L"
				ENDSELECT
				PRINTW 
			ENDSELECT
		ELSE
			SELECTCASE EXP:子宫口开发经验
			;开发完全完成
			CASE IS > 50
				IF TALENT:恋慕 || TALENT:恋人
					SELECTCASE RAND:7
					CASE 0
						PRINTFORML 「来吧…%TEXTR(@"%CALLNAME:MASTER%/你")% 如果是你的话，就算强硬一点也没关系…♥」
					CASE 1
						PRINTFORML 「在肚子里，回荡着，啊♥」
						PRINTFORML 莉格露似乎不仅仅受到异物感的困扰，偶尔还会被深处回荡的快感所摆弄…
					CASE 2
						PRINTFORML 「嗯啊，啊♥　那个，被温柔地敲打，我或许并不讨厌…♥」
						PRINTFORML 「嗯啊，好的，好的，啊♥」
					CASE 3
						PRINTFORML 「再多敲打一点♥　敲打一点♥」
						PRINTFORML 「进去了♥　啊♥　进去了♥」
					CASE 4
						PRINTFORML 「%TEXTR(@"%CALLNAME:MASTER%/你")%的%TEXTR("生殖器/阴茎")%和我的紧密接触着呢♥」
					CASE 5
						PRINTFORML 「有些痛苦♥　但是，感觉，舒服♥」
						PRINTFORML 「为什么，呢♥　因为是%TEXTR(@"%CALLNAME:MASTER%/你")%嘛♥　大概♥」
					CASE 6
						PRINTFORML 「不行♥　却♥」
						PRINTFORML 「%TEXTR("扭动/摩擦/")%喜欢的♥」
					ENDSELECT
				ELSEIF TALENT:爱欲 || TALENT:炮友
					SELECTCASE RAND:7
					CASE 0
						PRINTFORML 「最深处♥　成为了%TEXTR(@"%CALLNAME:MASTER%/你")%专属的肉穴了♥」
					CASE 1
						PRINTFORML 「这个♥　太棒了♥　被深深戳中了的感觉，陷进去了♥」
					CASE 2
						PRINTFORML 「重重地来♥　到最深处♥　慢慢地♥」
					CASE 3
						PRINTFORML 「击中了♥　被压迫着♥　噗呲♥　噗呲♥」
					CASE 4
						PRINTFORML 「啊♥　嗯♥　最深处♥　被压扁了♥」
					CASE 5
						PRINTFORML 「\@ CFLAG:1006 >= 1 ? %TEXTR("子宫/宝宝的房间/子袋")% # \@进一步戳击♥」
					CASE 6
						PRINTFORML 「\@ CFLAG:1006 >= 1 ? %TEXTR("子宫/宝宝的房间/子袋")% # \@被击中了♥　噢噢♥」
					ENDSELECT	
				ELSEIF TALENT:思慕
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「呜呜呜、呜呜呜、呜呜」
						SELECTCASE RAND:3
						CASE 0
							PRINTFORML （深处，传来沉甸甸的感觉…）
						CASE 1
							PRINTFORML （我的内部，好像被%CALLNAME:MASTER%的生殖器形状填满了）
						CASE 2
							PRINTFORML （这是，撞到了小宝宝的房间吗！？ 不、不行啊…♥）
						ENDSELECT
					CASE 1
						PRINTFORML 「呼！？♥　不，不舒服啊…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「啊啊啊！！？♥　进来了！？ 进来了啊！！」
					CASE 1
						PRINTFORML 「不、不行啊！ 深深的戳进去，会变得奇怪的！」
					ENDSELECT
				ENDIF
				PRINTW 
			;破裂开始
			CASE 25 TO 50
				IF TALENT:恋慕 || TALENT:恋人
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「过来吧…如果是%TEXTR(@"%CALLNAME:MASTER%/你")%的话，我能接受的…」
					CASE 1
						PRINTFORML 「嗯、嗯、嗯、嗯、嗯呜！？♥」
						PRINTFORML 莉格露似乎除了异物感之外，还时而受到深处回响的快感的影响…
					CASE 2
						PRINTFORML 「哈啊、啊、那个，被轻轻拍打的感觉，我不讨厌，可能…♥」
						PRINTFORML 「嗯呐、好的、好的，啊、啊啊♥」
					ENDSELECT
				ELSEIF TALENT:爱欲 || TALENT:炮友
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「这样深深地戳进去的话啊啊啊♥　%TEXTR(@"%CALLNAME:MASTER%/你")%专用的洞会变成的啊♥」
					CASE 1
						PRINTFORML 「这样♥　危险啊♥　会卡住的可能性很大啊♥」
					CASE 2
						PRINTFORML 「沉重的，来了啊♥　深处，嗡嗡地…♥」
					ENDSELECT
				ELSEIF TALENT:思慕
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「呜呜呜、呜呜呜、呜呜」
						SELECTCASE RAND:3
						CASE 0
							PRINTFORML （深处，传来沉甸甸的感觉…）
						CASE 1
							PRINTFORML （我的内部，好像被%CALLNAME:MASTER%的生殖器形状填满了）
						CASE 2
							PRINTFORML （这是，撞到了小宝宝的房间吗！？ 不、不行啊…♥）
						ENDSELECT
					CASE 1
						PRINTFORML 「呼！？♥　不对，不舒服啊…」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「呜呜呜，深深的，进来了…！」
					CASE 1
						PRINTFORML 「最深处，被击中了啊…！」
					ENDSELECT
				ENDIF
				PRINTW 
			;异物感
			CASEELSE
				IF TALENT:恋慕 || TALENT:恋人
					SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「求求你…就算戳进来，也请温柔一点…」
					CASE 1
						PRINTFORML 莉格露咬紧了嘴唇，忍耐着疼痛和异物感，表现得坚忍而可爱…
					CASE 2
						PRINTFORML 「那里…激烈地戳进去，是不行的哦」
