;621,钓りをする
;TFLAG:193(0=钓れない 1～4=鱼 11～15=うまい鱼 610=木材 131=守矢の守札 21=跳蛋 609=冰の鳞 606=龙の爪)
;TFLAG:194(1000以上ならTARGETが钓った)
;==================================================
@M_KOJO_MESSAGE_COM_K143_621
CALL M_KOJO_MESSAGE_COM_K143_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_621_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;あなたが钓った
	IF TFLAG:194 < 1000
		SELECTCASE TFLAG:193
		;钓れない
		CASE 0
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「对不起啊、我太蠢了没看到你看到的大鱼」
			CASE 1
				PRINTFORML 「桀桀。一定是很大的吧」
				PRINTFORML 「不是很常说吗？　『逃掉的鱼往往都是大鱼』」
			ENDSELECt
		;鱼
		CASE 1 TO 4
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「哦、做得好啊」
			CASE 1
				PRINTFORML 「竟然比我先钓到了啊」
			CASE 2
				PRINTFORML 「哦哦、很厉害啊」
				IF K143C_刚欲同盟 == 1
					PRINTFORML 「我现在任命你为我们的钓鱼小隊长。感到荣幸吧」
				ELSE
					PRINTFORML 「你想成为我们的专属钓手吗？　我们保证提供好待遇的喔」
				ENDIF
			ENDSELECT
		;鱼
		CASE 11 TO 15
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……」
				PRINTFORML 尤魔羡慕地看着%CALLNAME:MASTER%钓到的鱼
			CASE 1
				PRINTFORML 「呐、%CALLNAME:MASTER%」
				PRINTFORML 「我们彼此关系很好吧」
				PRINTFORML 「为了以后维持良好的关系、会考虑将那条鱼让给我吗？　嗯？」
			CASE 2
				PRINTFORML 「哦、这条鱼看起来很好吃啊」
				PRINTFORML 「我的胃现在正饿得像条鱼。你要为此负责、做出些好吃的东西来吃！」
			ENDSELECT
		;跳蛋
		CASE 21
			IF K143C_性に贪欲 > 0 && K143C_性に贪欲 != 2
				PRINTFORML 「…不用了啦」
				PRINTFORML 「就算我再喜欢Ｈ的事物、也不会要一个在河边捡到的家伙啦」
			ELSE
				PRINTFORML 「有人会把这种东西扔到河里吗…」
				PRINTFORML 「或者、是河里的妖怪不小心弄丢的东西吧…」
			ENDIF
		;守矢の守札
		CASE 131
			PRINTFORML 「在这种地方捡到东西。也许会有出乎意料的好处哦」
			PRINTFORML 说这话的人似乎一点都不认真
		;龙の爪
		CASE 606
			PRINTFORML 「龙之爪啊…真的存在过啊」
			PRINTFORML 「这是珍贵的物品。不要浪费。可以当药品或武器使用」
			PRINTFORML 「最好的办法是交给我保管。桀桀桀、开玩笑的啦」
			PRINTW 
		;冰の鳞
		CASE 609
			PRINTFORML 「哦、真漂亮啊」
			PRINTFORML 「有什么用途呢」
		;木材
		CASE 610
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「桀桀桀。真是个大东西啊」
			CASE 1
				PRINTFORML 「鱼形状的木材...？　 把弄起来真是精细呢」
			CASE 2
				PRINTFORML 「熊？　的摆设吧。口中还含有鲑鱼呢」
			ENDSELECT
		;龙涎香
		CASE 643
			PRINTFORML 「哦、能闻到甜味呢」
			PRINTFORML 「感觉可以卖给那些喜欢收藏的人呢」
		;カエル
		CASE 706
			PRINTFORML 「是青蛙啊」
			PRINTFORML 「就算弄错了也不要生吃喔、我倒是没关系啦」
			PRINTW 
		ENDSELECT
	;尤魔が钓った
	ELSE
		SELECTCASE TFLAG:193
		;钓れない
		CASE 0
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「只、只是巧合啦、只是巧合。今天身体状况不太好啦」
				PRINTFORML 虽然什么都还没说、但尤魔已经在自作主张得开始解释了
			CASE 1
				PRINTFORML 「...什么事、有话你就直说」
				PRINTFORML 尤魔瞪了一眼
			ENDSELECt
		;鱼
		CASE 1 TO 4
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「嘛嘛、还算可以吧」
			CASE 1
				PRINTFORML 「怎么样、看看我的钓鱼技术如何」
			CASE 2
				PRINTFORML 「桀桀桀。今天晚餐的菜就确定了呢」
			ENDSELECT
		;鱼
		CASE 11 TO 15
			SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「桀桀桀。今天要大吃一顿啊」
				PRINTFORML 拿着一条大鱼、尤魔很高兴
			CASE 1
				PRINTFORM 「这家伙看起来就很好吃。回家后也让同盟的家伙们也尝尝吧」
			CASE 2
				PRINTFORML 「桀桀桀。怎么样、看吧。这家伙肯定很好吃」
				PRINTFORML 「如果我来烹饪的话、这东西会更加美味。是不是也想尝看看了？」
			CASE 3
				PRINTFORML 「桀桀桀。怎么样、看吧。这是条好鱼呢」
				PRINTFORML 得到了美味的鱼、尤魔笑得很开心
			ENDSELECT
		;跳蛋
		CASE 21
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「...这种东西为什么可以在河里钓到呢？」
			CASE 1
				PRINTFORML 「难道河童们都在水里...自慰吗？」
			CASE 2
				PRINTFORML 「...这是什么啊。我不用这个」
			ENDSELECT
		;守矢の守札
		CASE 131
			PRINTFORML 「这是什么。纸钱？」
			PRINTFORML 「破破烂烂的。反而会被詛咒吧」
			PRINTW 
		;龙の爪
		CASE 606
			PRINTFORML 「诶、这是什么。爪？」
			PRINTFORML 「这么大、是龙爪之类的吗？　好稀有啊！」
			PRINTW 
		;冰の鳞
		CASE 609
			PRINTFORML 「好漂亮ー。可以看穿到另一边呢」
			PRINTFORML 「这东西会不会融化吧。为了保险起见、放在冷处保存吧」
		;木材
		CASE 610
			SELECTCASE RAND:3
			CASE 0
				IF K143C_鬼杰组 == 1
					PRINTFORML 「…别在笑了ー啊。你跟你老大一样性格差啊」
				ELSEIF K143C_劲牙组 == 1
					PRINTFORML 「你啊、就因为不是自己家的老大就放肆的大笑吗…」
				ELSEIF K143C_刚欲同盟 == 1
					PRINTFORML 「可恶…等等我会让你知道的」
					PRINTFORML 「我会把你讨厌的东西全都放进你的饭里面的」
				ELSE
					PRINTFORML 「可恶、我还以为中大奖了呢」
					PRINTFORML 「呜、吵死人了。谁能想到会钓到木头呢」
				ENDIF
			CASE 1
				PRINTFORML 「啥？　怎么会钓到木头啊。可恶、好重啊」
				PRINTFORML 「嘿、别光笑不帮忙、来帮忙啊。啊啊、真是的」
			CASE 2
				PRINTFORML 「…别摆出一副像压扁的馒头一样的脸、想笑就笑出来啊」
			ENDSELECT
		;カエル
		CASE 706
			PRINTFORML 「是青蛙啊。嘛、可以做成菜来吃吧」
			PRINTFORML 「什么啊。青蛙很好吃的啊。尤其是炸过的唐扬鸡」
			PRINTFORML 「嘛、不过也不是什么东西都能吃啦…特别是你们人类」
			PRINTW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
