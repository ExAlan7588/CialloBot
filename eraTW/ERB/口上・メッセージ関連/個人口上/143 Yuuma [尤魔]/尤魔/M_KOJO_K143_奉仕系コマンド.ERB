;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K143_80
CALL M_KOJO_MESSAGE_COM_K143_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_80_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;初めての阴茎
		IF CFLAG:1003 == 0
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「！！！」
				PRINTFORML 「哇、这是、什么啊…」
				PRINTFORML 「这比我的手臂还粗耶？」
				IF BASE:MASTER:勃起 >= 1000
					PRINTFORML 「血管都浮现出来了…这个看起来很有嚼劲啊」
					PRINTFORML 「桀桀桀、开玩笑啦、开玩笑的。应该只需要用手套弄就好了吧？」
				ELSE
					PRINTFORML 「明明还没变硬、但就有这样的大小吗…」
					PRINTFORML 「这个、应该还会变更大吧？　我一定要看看」
				ENDIF
			CASE 1,2
				PRINTFORML 「哦哦、这个真的很可爱呢」
				PRINTFORML 「不用感到羞耻啊、这对我的身体来说、是刚刚好的啊」
				PRINTFORML 「好了、先来好好摸摸它吧」
			CASE 5
				PRINTFORML 「桀桀桀、一根长的十分凶恶的鸡鸡啊」
				PRINTFORML 「这就是你使女人哭泣的方式吗。而现在、你也想让我这样哭泣吗？」
				IF TALENT:MASTER:非童贞 == 0
					PRINTFORML 「什么…从来没有这样的经验…？」
					PRINTFORML 「……」
					PRINTFORML 「嗯…那个…对不起啊」
				ELSE
					PRINTFORML 「真期待啊。不过、先来点前戏吧」
					PRINTFORML 「……」
					PRINTFORML 「但这个、好像不太好握紧啊...」
				ENDIF
			CASE 4
				PRINTFORML 「…嘿、听着」
				PRINTFORML 「这个太耀眼了、怎么办啊？」
				PRINTFORML 「都被分心了、我都没有性欲了啊...」
			CASEELSE
				PRINTFORML 「嗯...这就是你的鸡鸡吗」
				PRINTFORML 「看起来像香肠一样美味啊...不知道能不能吃啊？」
				PRINTFORML 「桀桀桀、开玩笑、开玩笑的」
				PRINTL 
			ENDSELECT
		;阴茎2回目以降
		ELSE
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「像往常一样、还是很大一根啊…」
				IF TALENT:130:恋人
					PRINTFORML 「你用这个粗大的东西、让吉吊高潮的吗？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「这种大小恰好是骊驹喜爱的样子」
				ENDIF
				IF BASE:MASTER:勃起 >= 1000
					PRINTFORML 「血管都浮现出来了…这个看起来很有嚼劲啊」
					PRINTFORML 「桀桀桀、开玩笑的啦、别当真。来、我帮你打一枪、老老实实躺好吧」
				ELSE
					PRINTFORML 「竟然还能再变的更大…真是惊人啊」
					PRINTFORML 「来吧、快点勃起起来让我看看」
				ENDIF
				PRINTL 
			CASE 1,2
				PRINTFORML 「桀桀桀、你的东西还是一如既往地长得很可爱啊」
				IF TALENT:130:恋人
					PRINTFORML 「吉吊是喜欢这种类型的人吗？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「这么小、居然还能满足骊驹？　真的很厉害」
				ENDIF
				PRINTFORML 「好了好了、那么硬度和持久力是怎么样的呢…？」
				PRINTL 
			CASE 5
				PRINTFORML 「桀桀桀、一如既往的很凶恶啊」
				IF TALENT:130:恋人
					PRINTFORML 「竟然还对这样的东西产生兴奋、吉吊的口味是好还是坏呢？」
				ELSEIF TALENT:133:恋人
					PRINTFORML 「骊驹是不是认为、普通的东西已经无法满足她了吗？」
				ENDIF
				PRINTL 
			CASE 4
				PRINTFORML 「…还是一如既往地耀眼呢」
				PRINTFORML 「太难做了、难以忍受」
				PRINTL 
			CASEELSE
				PRINTFORML 「桀桀桀、外表看起来还是很美味啊」
				PRINTFORML 「只吃一点点也不行嘛？　只吃前端也行啊」
				PRINTL 
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		IF K143C_鬼杰组 == 1
		ELSEIF K143C_劲牙组 == 1
		ELSEIF K143C_刚欲同盟 == 1
		ELSE
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_K143_81
CALL M_KOJO_MESSAGE_COM_K143_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K143_81_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;初めてのペニス
		IF CFLAG:1003 == 0
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
				PRINTFORML 「!!!」
				PRINTFORML 「诶,这是什么...」
				PRINTFORML 「比我的胳膊粗了吧?这个」
				IF BASE:MASTER:勃起 >= 1000
			        PRINTFORML 「血管都凸起来了...感觉会很有食感」
			        PRINTFORML 「嘿嘿,开玩笑的,开玩笑。我就该吸吮这个对吧?」
				ELSE
			        PRINTFORML 「没硬起来就已经这么大了...」
			        PRINTFORML 「硬起来会变得更大吧?我好想亲眼见识一下」
				ENDIF
			CASE 1,2
				PRINTFORML 「哦哦,真是可爱呢」
				PRINTFORML 「来来,不用害羞。对我的身体来说刚刚好」
				PRINTFORML 「来,用我的嘴好好吸吮它吧」 
			CASE 5
			    PRINTFORML 「嘿嘿,摆出了相当凶残的表情呢」
			    PRINTFORML 「就是用这个让女人们痛哭流涕的吗?还想对我也这么做吧?」
			    IF TALENT:MASTER:非童贞 == 0
			        PRINTFORML 「什么...一次也没有这种经验吗...?」
			        PRINTFORML 「......」
			        PRINTFORML 「抱歉...我用嘴帮你做,所以不要哭了」
			    ELSE
			        PRINTFORML 「很期待呢。不过先从前戏和调情开始吧」
			        PRINTFORML 「......」
			        PRINTFORML 「然而这个,感觉有点不好吸吮...」
			    ENDIF
			CASE 4
			    PRINTFORML 「...喂」
			    PRINTFORML 「这么刺眼能做点什么吗?」
 			    PRINTFORML 「分心了,兴致全无...」
			CASEELSE
			    PRINTFORML 「哦...这就是你的阴茎吗」
			    PRINTFORML 「看起来像香肠,感觉会很美味...可以吃吗?」
			    PRINTFORML 「嘿嘿,开玩笑的,开玩笑」
			    PRINTL
			ENDSELECT
		;ペニス2回目以降
		ELSE
			SELECTCASE TALENT:MASTER:形状
			CASE 6,10
			    PRINTFORML 「和以前一样,大小真夸张...」
			    IF TALENT:130:恋人
			        PRINTFORML 「是用这个粗壮的东西满足着吉吊吗?」
				ELSEIF TALENT:133:恋人
			        PRINTFORML 「看起来很符合骊驹的喜好呢」
			    ENDIF
			    IF BASE:MASTER:勃起 >= 1000
			        PRINTFORML 「血管都凸起来了...感觉会很有食感」
			        PRINTFORML 「嘿嘿,开玩笑的,开玩笑。来,我给你口交,你别动」
			    ELSE
			        PRINTFORML 「说这还能变得更大...真是令人震惊」
			        PRINTFORML 「来来,快让它勃起吧」
			    ENDIF
			    PRINTL
			CASE 1,2
			    PRINTFORML 「嘿嘿,外表和以前一样可爱呢」
			    IF TALENT:130:恋人
			        PRINTFORML 「吉吊喜欢这种类型吗?」
			    ELSEIF TALENT:133:恋人
			        PRINTFORML 「虽然这么小,但也能满足骊驹吗?真厉害」
			    ENDIF
			    PRINTFORML 「那硬度和持久力怎么样呢...?」
			    PRINTL 
			CASE 5
			    PRINTFORML 「嘿嘿,和以前一样凶残的表情呢」
			    IF TALENT:130:恋人
			        PRINTFORML 「被这样的粗物插入,不知道吉吊的品味是好是坏」
			    ELSEIF TALENT:133:恋人
			        PRINTFORML 「骊驹没法被普通的东西满足吗?」
			    ENDIF
			    PRINTL
			CASE 4
			    PRINTFORML 「...一如既往的刺眼啊」
			    PRINTFORML 「好难做,做不了」
			    PRINTL
			CASEELSE
			    PRINTFORML 「嘿嘿,看起来和以前一样美味」
			    PRINTFORML 「就吃一点头部可以吗?没关系的,就吃一点头部」
			    PRINTL 
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

