;NOWEX:7 三重绝顶
;NOWEX:8 四重绝顶
;NOWEX:9 五重绝顶
;NOWEX:10 喷乳
;NOWEX:11 射精
;NOWEX:12 放尿
;适当に组み合わせて使ってください
;-------------------------------------------------
@M_KOJO_NK_MESSAGE_PALAMCNG_B_K38
IF FLAG:70
	RETURN 1
ENDIF
;-------------------------------------------------
;喷乳
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:喷乳
	PRINTDATA
		DATAFORM 「呜——胸部，乳汁要溢出来了！」
		DATAFORM 「啊啊，想要喝一口的话，也可以哦%UNICODE(0x2764)%」
		DATAFORM 「呜啊——！居然直接喷了出来……」
	ENDDATA
ENDIF
;-------------------------------------------------
;射精
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTDATA
		DATAFORM 「咕噫——人家居然就这样射了……」
		DATAFORM 「呜——哈啊…哈啊……可惜不能让%CALLNAME:MASTER%怀孕呢%UNICODE(0x2764)%」
		DATAFORM 「呜……看到女孩子这个样子……就这么高兴吗%UNICODE(0x2764)%」
	ENDDATA
ENDIF
;-------------------------------------------------
;放尿
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORML 
ENDIF
;-------------------------------------------------
;绝顶
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;五重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:五重绝顶
		PRINTFORML 「啊……啊……%UNICODE(0x2764)%」
		PRINTFORMDW   少女在绝顶的余韵中身体微微痉挛着，似乎已经说不出话了。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:四重绝顶
		;部位を指定せず简易的に书く场合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの间に书いてください
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「啊——！受不了啦！要死掉啦——！%UNICODE(0x2764)%」
				PRINTFORMDW   少女被玩弄得乱七八糟，被快感的浪潮吞没了。
			CASE 1
				PRINTFORML 「啊啊……已经……完全……%UNICODE(0x2764)%」
				PRINTFORMDW   脱力的少女，在高潮的余韵中沦陷着。
			CASE 2
				PRINTFORML 「呜——！%UNICODE(0x2764)%咕噫——！」
				PRINTFORMDW   少女用全身的颤抖，传达着那种难以言喻的欢愉。
		ENDSELECT
		[SKIPSTART]
		SELECTCASE NOWEX:四重绝顶
			;Ｃ＆Ｖ＆Ａ＆Ｂ绝顶
			CASE 1
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ｖ＆Ａ＆Ｍ绝顶
			CASE 2
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ＆Ｍ绝顶
			CASE 3
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ绝顶
			CASE 4
				PRINTFORML 
				RETURN 1
			;Ｖ＆Ａ＆Ｂ＆Ｍ绝顶
			CASE 5
				PRINTFORML 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;三重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
