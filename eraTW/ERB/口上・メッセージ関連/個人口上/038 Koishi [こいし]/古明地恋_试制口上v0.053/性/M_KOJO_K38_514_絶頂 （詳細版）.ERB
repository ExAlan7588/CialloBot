			CASE 5
				PRINTFORML 
				RETURN 1
			;Ｂ＆Ｖ＆Ｍ绝顶
			CASE 6
				PRINTFORML 
				RETURN 1
			;Ａ＆Ｖ＆Ｍ绝顶
			CASE 7
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ｂ＆Ｍ绝顶
			CASE 8
				PRINTFORML 
				RETURN 1
			;Ａ＆Ｂ＆Ｍ绝顶
			CASE 9
				PRINTFORML 
