	ENDIF
	;-------------------------------------------------
	;二重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:二重绝顶
		;部位を指定せず简易的に书く场合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの间に书いてください
		PRINTFORML 「哈啊……完全没想到呢%UNICODE(0x2764)%」
		PRINTFORML 「%CALLNAME:MASTER%居然能做到这样……」
		PRINTFORMDW   少女对%CALLNAME:MASTER%的熟练程度感到佩服。
		[SKIPSTART]
		SELECTCASE NOWEX:二重绝顶
			;Ｃ＆Ｖ绝顶
			CASE 1
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ａ绝顶
			CASE 2
				PRINTFORML 
				RETURN 1
			;Ｖ＆Ａ绝顶
			CASE 3
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ｂ绝顶
			CASE 4
				PRINTFORML 
				RETURN 1
			;Ｖ＆Ｂ绝顶
			CASE 5
				PRINTFORML 
				RETURN 1
			;Ａ＆Ｂ绝顶
			CASE 6
				PRINTFORML 
				RETURN 1
			;Ａ＆Ｍ绝顶
			CASE 7
				PRINTFORML 
				RETURN 1
			;Ｖ＆Ｍ绝顶
			CASE 8
				PRINTFORML 
				RETURN 1
			;Ｃ＆Ｍ绝顶
			CASE 9
				PRINTFORML 
				RETURN 1
			;Ｂ＆Ｍ绝顶
			CASE 10
				PRINTFORML 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ绝顶
		;超强绝顶Ｃ
		IF NOWEX:Ｃ绝顶 == 3
			PRINTFORML 「哇啊……意识……要飘走了……%UNICODE(0x2764)%」
		;强绝顶Ｃ
		ELSEIF NOWEX:Ｃ绝顶 == 2
			PRINTFORML 「啊啊啊%UNICODE(0x2764)%\@GETBIT(TALENT:性别,1)?肉棒#秘豆\@那里，酥酥麻麻的%UNICODE(0x2764)%」
		;绝顶Ｃ
		ELSE
			PRINTFORML 「呜哇…好厉害%UNICODE(0x2764)%」
		ENDIF
	ENDIF
	IF NOWEX:Ｖ绝顶
		;超强绝顶Ｖ
		IF NOWEX:Ｖ绝顶 == 3
			PRINTFORML 
		;强绝顶Ｖ
		ELSEIF NOWEX:Ｖ绝顶 == 2
			PRINTFORML 
		;绝顶Ｖ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
	IF NOWEX:Ａ绝顶
		;超强绝顶Ａ
		IF NOWEX:Ａ绝顶 == 3
			PRINTFORML 
		;强绝顶Ａ
		ELSEIF NOWEX:Ａ绝顶 == 2
			PRINTFORML 
		;绝顶Ａ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ绝顶
		;强绝顶Ｂ
		IF NOWEX:Ｂ绝顶 == 3
			PRINTFORML 
		;强绝顶Ｂ
		ELSEIF NOWEX:Ｂ绝顶 == 2
			PRINTFORML 
		;绝顶Ｂ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
	IF NOWEX:Ｍ绝顶
		;超强绝顶Ｍ
		IF NOWEX:Ｍ绝顶 == 3
			PRINTFORML 
		;强绝顶Ｍ
		ELSEIF NOWEX:Ｍ绝顶 == 2
			PRINTFORML 
		;绝顶Ｍ
		ELSE
			PRINTFORML 
		ENDIF
	ENDIF
ENDIF
