			CALL HPH_PRINT, @"「啊啊啊HPH　不，不要！　屁股，难受…好难受啊HPH」", "L"
			CALL HPH_PRINT, @"「%K30_C_NAME(MASTER)%！　做吧HPH　快点、捅进来啊HPHHPH」", "L"
		ENDIF
		PRINTFORMDW 刺激得敏感的屁股被爱抚，\@ !TEQUIP:50 ? 深深含住肉棒的 # 湿漉漉的 \@蜜穴溢出了新的蜜液…
	;Ａ插入:感觉-高
	ELSE
