;まぐろ喘ぎ表示
@K30_GASPING_RAND(ARG)
;--------------------------------------------------
#DIM TEMP2
TEMP2 = ARG
SELECTCASE TEMP2
;ノーマル时は肯定否定半々
	CASE 0
		PRINTFORM %TEXTR("啊啊/啊嗯/噫唔/不要/不行了")%
		SIF PALAM:30:情欲 > PALAMLV:8 && RAND:2 == 1
			PRINTFORM 
		SIF PALAM:30:情欲 > PALAMLV:5 && RAND:2 == 1
			PRINTFORM ♥　
;接吻・口交はちゅぱ音
	CASE 1
		PRINTFORM %TEXTR("啾呋/叻咯/哈姆/啾/唔姆")%
		SIF PALAM:30:情欲 > PALAMLV:7 && RAND:2 == 1
			PRINTFORM ♥　
	CASE 2
		PRINTFORM %TEXTR("唔噗/啾啵/啾噜/滋噜/啾噗")%
		SIF PALAM:30:情欲 > PALAMLV:7 && RAND:2 == 1
			PRINTFORM ♥　
;Ａ弱点なのでハートマシマシ
	CASE 3
		PRINTFORM %TEXTR("啊嗯/不行/那里是/啊哈啊/不要啊/屁股")%
		IF PALAM:30:情欲 > PALAMLV:7 && RAND:2 == 1
			CALL HPH_PRINT(@"HPH　")
			SIF PALAM:30:情欲 > PALAMLV:9
				CALL HPH_PRINT(@"HPH　")
		ELSEIF PALAM:30:情欲 > PALAMLV:4 && RAND:2 == 1
			PRINTFORM ♥　
		ENDIF
;炮友、烂醉时は肯定のみ、ハートマシマシ
	CASE 4, 5
		PRINTFORM %TEXTR("呀啊/啊唔/噫啊/唔啊/噫唔/唔唔/呀唔/啊啊/啊哈啊")%
		SIF PALAM:30:情欲 > PALAMLV:4 && !RAND:3 == 1
			PRINTFORM 
		SIF PALAM:30:情欲 > PALAMLV:7 && RAND:2 == 1
			CALL HPH_PRINT(@"HPH　")
		SIF PALAM:30:情欲 > PALAMLV:9
			CALL HPH_PRINT(@"HPH　")
;药时＠こことか个别の方がどう考えてもいいよね。また今度やる
	CASE 6
		IF TCVAR:烂醉 || CFLAG:睡眠
		ELSE
			SELECTCASE SELECTCOM
				CASE 50, 51
					IF TCVAR:强行
					ELSE
						PRINTFORM %TEXTR("谢谢/安心了/拜托了")%
					ENDIF
				CASEELSE
					PRINTFORM %TEXTR("你要做什么/不要用奇怪的东西/感觉…有什么怪怪的…")%
			ENDSELECT
		ENDIF
;ＳＭ时＠ここも个别だ。また今度
	CASE 7
		;打屁股
		IF SELECTCOM == 100
			IF TCVAR:烂醉
				PRINTFORML %TEXTR("好疼……/不～要～/呀唔…/住…手…")%……」
			ELSE
				PRINTFORML %TEXTR("疼/不要/好疼")%！ ……%TEXTR("你要做什么/求你…不要这样/不要…不要打我")%………」
			ENDIF
		;それ以外
		ELSE
			IF TCVAR:烂醉
				PRINTFORML %TEXTR("嗯嗯/嗯～/呜～嗯")%……」
			ELSE
				PRINTFORML %TEXTR("好怕…/你要做什么/不要做…那么可怕的事")%………」
			ENDIF
		ENDIF
;侍奉(ちゅぱ音无し)时
	CASE 8
		IF TCVAR:烂醉
			PRINTFORM %TEXTR("嗯～？/干什么呀～？/硬…硬硬的？/滑…滑滑的？")%
		ELSEIF CFLAG:睡眠
			PRINTFORM %TEXTR("嗯嗯/嗯ー？/呜嗯/嗯嗯嗯")%
		ELSE
			PRINTDATA
				DATAFORM 这样就可以了么？
				DATAFORM 这样就满足了么？
				DATAFORM 这样做就行了么？
				DATAFORM 像这样动就可以了么？
				DATAFORM 是这种感觉么？
			ENDDATA
		ENDIF
ENDSELECT
RETURN 0
