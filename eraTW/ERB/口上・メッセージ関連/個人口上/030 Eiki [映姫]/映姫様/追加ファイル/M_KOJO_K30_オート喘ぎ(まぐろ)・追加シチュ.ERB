;自动喘息声口上（まぐろ）
;口上どうしても思い浮かばないのでAUTO_AEGI改变函数
;部位・感度等无视して矛盾しない喘ぎ声をつくるのを目标に
@K30_GASPING_MAGURO
;--------------------------------------------------
#DIM 状况
#DIM 心神丧失
#DIM ひどい
#DIM TEMP
#DIM TEMP2
心神丧失 = 0
ひどい = 0
TEMP = 0
TEMP2 = 0
SIF !CFLAG:30:诶嘿嘿
	RETURN 0
;ノーマルまぐろ
状况 = 0 
;炮友
SIF TALENT:爱欲 || TALENT:炮友 || TALENT:风骚
	状况 = 4
;烂醉まぐろ
IF TCVAR:烂醉
	状况 = 5
	心神丧失 =1
ENDIF
;睡眠まぐろ
IF CFLAG:睡眠
	状况 = 9
	心神丧失 =1
ENDIF
;接吻时
SIF GROUPMATCH(SELECTCOM, 19, 20)
	状况 = 1
;口交时
SIF GROUPMATCH(SELECTCOM, 81, 88, 89, 140, 501, 505, 508, 509, 512)
	状况 = 2
;个别处理时(药
SIF GROUPMATCH(SELECTCOM, 50, 51, 180, 181, 182, 192, 490)
	状况 = 6
;SM系(SOURCE入らない)时
SIF GROUPMATCH(SELECTCOM, 100, 104, 105, 107)
	状况 = 7
;侍奉(ちゅぱ音无し)时
SIF GROUPMATCH(SELECTCOM, 80, 82, 83, 84, 85, 510, 511)
	状况 = 8
;Ａ关联
SIF GROUPMATCH(SELECTCOM, 4, 5, 13, 45, 62, 63, 66, 69, 70, 71, 74, 75, 514, 515)
	状况 = 3
PRINTFORM 「
SIF TCVAR:强行
	PRINTFORM %TEXTR("不要/不行/住手/不要")%！　…
;炮友、心神丧失、药、ＳＭは必ず最初に一言
IF (RAND:2 == 0 || GROUPMATCH(状况, 4, 6, 7) || 心神丧失) && !TCVAR:强行 
	CALL K30_GASPING_RAND(状况)
	;ＳＭは一言で处理结束
	SIF 状况 == 7
		RETURN 1
	TEMP = 1
ENDIF
;行中の处理
REPEAT 5 + RAND:2
	SELECTCASE 状况
	;接吻・口交はちゅぱ音
	;心神丧失・强行时は频度半分
		CASE 1, 2
			IF (RAND:2 == 1 && !(心神丧失 || TCVAR:强行)) || (RAND:4 == 1 && (心神丧失 || TCVAR:强行))
				IF 状况 == 1
					PRINTFORM …%TEXTR("嗯唔/呸咯/哈姆/啾噜/唔姆")%
				ELSE
					PRINTFORM …%TEXTR("唔噗/啾啵/啾噜/滋噜/啾噗")%
				ENDIF
				SIF PALAM:30:情欲 > PALAMLV:7 && !TCVAR:强行 && RAND:3 != 1
					PRINTFORM ♥　
			ELSE
				PRINT …
			ENDIF
	;通常は拒否っぽい声	、准强制わいせつは寝言っぽいのを、炮友・恋人同伴は肯定をそれぞれ2回まで
		CASE 0, 4, 5, 9
			IF TEMP2 < 3 && !TCVAR:强行
				IF RAND:3 == 1
					TEMP2 ++
					IF TCVAR:烂醉
						PRINTFORM ……%TEXTR("唔呼呼/唔嗯～/啊唔/唔唔/咿咕")%
					ELSEIF CFLAG:睡眠
						PRINTFORM ……%TEXTR("呼喵/啊呼/唔/啊嗯/嗯唔")%
					ELSEIF 状况 == 4 || TALENT:恋慕
						PRINTFORM ……%TEXTR("好舒服/那里/啊啊/好棒/啊嗯/呀唔")%
						IF (PALAM:30:情欲 > PALAMLV:3 && RAND:3 == 1) || (PALAM:30:情欲 > PALAMLV:5 && RAND:3 != 1)
							PRINTFORM 
						ENDIF
						IF PALAM:30:情欲 > PALAMLV:6
							SIF RAND:2 == 1
								CALL HPH_PRINT(@"HPH　")
						ELSEIF PALAM:30:情欲 > PALAMLV:3 && RAND:3 != 1
							PRINTFORM ♥　
						ENDIF
					ELSE
						PRINTFORM ……%TEXTR("这样不行/不行了/感觉好奇怪/感觉有什么……/不要/要变奇怪了")%
						IF (PALAM:30:情欲 > PALAMLV:2 && RAND:3 == 1) || PALAM:30:情欲 > PALAMLV:6
							PRINTFORM 
						ENDIF
						IF PALAM:30:情欲 > PALAMLV:9
							SIF RAND:4 == 1
								CALL HPH_PRINT(@"HPH　")
						ELSEIF PALAM:30:情欲 > PALAMLV:5 && RAND:3 == 1
							PRINTFORM ♥　
						ENDIF
					ENDIF
				ELSE
					PRINT …
				ENDIF
			ELSE
				PRINT …
			ENDIF
	;他は无言
		CASEELSE
			PRINT …
	ENDSELECT
REND
IF TEMP == 0
	;强行・药时は语尾に一言だけ、后はどっちかに一言だけ TCVAR:30:386 强行口上反映フラグ
	IF TCVAR:强行
		TCVAR:30:386 = 1
	;强行时に避孕
		SIF (SELECTCOM == 51 || TEQUIP:PLAYER:避孕套) && TCVAR:30:不耐烦 == 1
			PRINTFORM %TEXTR("你想干什么…？/根本搞不懂你想怎样…/别开玩笑了…/你到底在想什么呢…")%
		PRINTFORM %TEXTR("…够了…住手/…太过分了/…已经够了吧/…明明这么信任着你/…给我适可而止/…不要碰我")%
	ELSE
		CALL K30_GASPING_RAND(状况)
	ENDIF
ENDIF
PRINTFORMW 」
PRINTFORML 
;MASTER射精直前
IF NOWEX:MASTER:11 && !心神丧失 
	;强行避孕无しで中出し
	IF TCVAR:强行 && !TCVAR:30:口服避孕药 && TEQUIP:PLAYER:避孕套 == 0 && TCVAR:PLAYER:2 == 1
		;阎魔样の罚は准备中
		SETBIT CFLAG:1800, 0
		ひどい ++
		PRINTDATA
			DATAFORM 「等等…拜托了……
			DATAFORM 「不要，不要啊……
			DATAFORM 「不要！！ 拔出去啊！！ ……
			DATAFORM 「里面！ 里面不行！！ ……
		ENDDATA
		SELECTCASE ひどい
			CASE 1
				PRINTFORM %TEXTR("不要…不要了/会怀上的…不要这样/真的…不要这样")%………
			CASEELSE
				PRINTFORM %TEXTR("不要…饶了我吧…/…不要…不要了…/求你了…不要这样…")%………
		ENDSELECT
		PRINTFORML %TEXTR("不要啊啊啊啊/不要啊啊啊啊！！/不行啊啊啊啊！！/啊啊啊啊啊！")%！」
	ELSEIF 状况 == 2
		SIF EXP:30:精饮经验 < 20 && !TALENT:恋慕
			PRINTFORMW （%TEXTR("等会吐出来吧…/真不想咽下去啊…/射在外面啊…")%）
	ENDIF
;绝顶直前
ELSEIF (NOWEX:Ｃ绝顶 || NOWEX:Ｖ绝顶 || NOWEX:Ａ绝顶 || NOWEX:Ｂ绝顶 ) && !心神丧失 && !GROUPMATCH(状况, 1, 2, 8) 
	PRINTFORMDW 映姫为了忍住突如其来的绝顶感,%TEXTR("身体颤抖了起来/不停地摇着头/拼命捂住了嘴巴/紧紧地搂住了自己的胸口/紧紧闭上了眼睛")%……
	;炮友にはなんとなく悔し(ryな煽りを入れる
	IF 状况 == 4
		PRINTDATADW
			DATAFORM 然而,早已沉溺于肉欲的身体却轻易的就到达了高潮……
			DATAFORM 但是,早已在身体上铭记的那份雌性的天性,使得她的抵抗毫无任何作用……
			DATAFORM 无法抗拒的快感像是要嘲笑这份毫无意义的抵抗似的,从映姬的内心涌了出来……
			DATAFORM 但是,在这绝顶的时刻,映姬已经被更加甜美的快感所迷住了……
			DATAFORM 但是这也只不过只是一时的挣扎,反而,因为一直压抑而变得膨胀的快感向映姬袭了过来……
		ENDDATA
	ENDIF
ENDIF
RETURN 1
