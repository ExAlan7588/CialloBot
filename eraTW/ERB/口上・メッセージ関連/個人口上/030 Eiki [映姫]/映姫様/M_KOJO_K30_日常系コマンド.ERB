;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K30_300
RETURN 1

@M_KOJO_MESSAGE_COM_K30_300
;TRAIN_MESSAGEを消す条件并べる。TFLAG:193を借りてフラグ立てる
;初回or变Ｔで初对面日
IF !STRCOUNT(CSTR:30:0, "300/") || (CFLAG:30:衣物暂时更换 == 62 && STRCOUNT(CSTR:30:1, "初对面"))
	TFLAG:193 = 4
;口交抜き
ELSEIF TFLAG:193 == -999
;变Ｔ
ELSEIF K30_CHECK_HEN_T() && !GROUPMATCH(CFLAG:当前位置, 情侣酒店, 情人旅馆, OMANEKIBEYA())
	TFLAG:193 = 2
;振动棒约会中
ELSEIF CHK_DATENOW(CFLAG:30:约会中) && TCVAR:30:364 > 0
	TFLAG:193 = 5
;事后の会话
ELSEIF TIME_PROGRESS(CFLAG:1504) <= 10 && TALENT:恋慕 && !K30_BE_SEEN() && TALENT:心情 > -1 && !MARK:反抗刻印 && !K30_CHECK_HEN_T()
	CALLF ONCE("会话＠纪念日")
	TFLAG:193 = 6
;裁判中
ELSEIF (仕事中チェック(30) && BETWEENTIME(540, 719) && (CFLAG:30:当前位置 == CFLAG:30:职场) && CFLAG:30:职业种类 == 43) 
;接吻魔取得时概率
ELSEIF TALENT:接吻魔 && TALENT:恋慕 && RAND:(50 - TALENT:心情 * 25 - (CFLAG:积攒度 / 50)) == 0 && TALENT:心情 != -1 && MARK:30:反抗刻印 == 0 && (K30_BE_SEEN() == 0 || RAND:2 == 1)
	TFLAG:193 = 3
;他は通常どおり
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K30_300_1
	RETURN RESULT
ENDIF
;TRAIN_MESSAGE抜きで口上メイン呼出
CALL K30_D_LINE
CALL M_KOJO_MESSAGE_COM_K30_300_1
RETURN RESULT

