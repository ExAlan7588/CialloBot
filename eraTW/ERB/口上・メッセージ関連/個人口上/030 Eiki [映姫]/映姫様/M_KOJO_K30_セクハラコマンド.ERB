IF CFLAG:诶嘿嘿 || FLAG:70 || EQUIP:裤子
	RETURN 0
ELSE
	CALL K30_D_LINE
	CALL K30_COM330_TRAIN
	RETURN 1
ENDIF

@M_KOJO_MESSAGE_COM_K30_330
TCVAR:30:356 = (TFLAG:193 < 0)
SIF K30_CHECK_HEN_T() && TALENT:心情 > -1
	RETURN 1
CALL M_KOJO_MESSAGE_COM_K30_330_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K30_330_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		CALL K30_PRINT_BUTT_IMAGE(SELECTCOM)
	ELSEIF CFLAG:诶嘿嘿 == 2
		IF TEQUIP:TARGET:50 == MASTER
			IF RAND:2 == 1
				PRINTFORMW 「嗯♥　能好好地，看见吧……」
				PRINTFORMW 「我的，这里…把你的，紧紧地吞进去了哦…♥」
			ELSE
				PRINTFORMW 「哈，哈啊…♥　%K30_C_NAME(MASTER)%，再多看看…」
				PRINTFORMW 「舒服的地方♥　进去了哦，看看嘛♥」
			ENDIF
		ELSEIF TEQUIP:TARGET:51 == MASTER
			IF RAND:2 == 1
				CALL HPH_PRINT, @"「呵呵呵　连接的地方，藏起来了…在意吗？」", "L"
				CALL HPH_PRINT, @"「怎么样，用我的屁股…把你的东西，如此下流地，吞进去了哦」", "W"
			ELSE
				CALL HPH_PRINT, @"「怎么样，能好好地…看见吗…」", "L"
				CALL HPH_PRINT, @"「屁股…变得这么，张开了…甚至发出了下流的声音」", "W"
			ENDIF
		ELSEIF !EQUIP:下半身内衣２
			IF PALAM:润滑 < PALAMLV:7
				PRINTFORMW 「怎么样？　开始兴奋了吗……？」
			ELSE
				PRINTFORMW 「能看清吧……我的，这里」
				PRINTFORMW 「变得这么…黏糊糊的…这么下流了…♥」
			ENDIF
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「我的%PANTSNAME(EQUIP:下半身内衣２, TARGET)%，现在正被盯着…」
				PRINTFORMW 「想脱掉，吗？」
			;关系ないけど、随机でたまにとんでもないやつ是てるよね
			ELSE
				PRINTFORMW 「这个%PANTSNAME(EQUIP:下半身内衣２, TARGET)%…」
				PRINTFORMW 「和我，相配吗…♥」
			ENDIF
		ENDIF
		RETURN 1
	ELSE
		SELECTCASE TFLAG:193
			;ビンタ
			CASE -5
				PRINTDATAL
					DATAFORM 「……这样稍微冷静下来了吗」
					DATAFORM 「……你在开玩笑吗！？」
					DATAFORM 「……你到底在想什么？」
				ENDDATA
				PRINTDATAW
					DATAFORM 「给我知道羞耻！　羞耻！」
					DATAFORM 「啊啊…真是的，想说的话像山一样多了」
					DATAFORM 「居然…敢对阎魔做出这种事情」
				ENDDATA
				CALL K30_LECTURE_CHECK(30 + RAND:10, 1)
			;つねられる
			CASE -3
				PRINTDATAW
					DATAFORM 「……你是儿童吗」
					DATAFORM 「……擅自行动的这只手不好吗？」\n「还是说…是主人的头脑不好呢？」
					DATAFORM 「偷东西的手会被砍掉」\n「好像有过这样的惩罚」\n「对于这只无法自制的手…什么样的惩罚才适合呢？」
				ENDDATA
				CALL K30_LECTURE_CHECK(25 + RAND:15, 1)
			;嫌がる
			CASE -1
				PRINTDATAW
					DATAFORM 「任凭欲望摆布地活着肯定会后悔的哦」
					DATAFORM 「积累的善行也……失去只是一瞬间哦？」
					DATAFORM 「特意做别人讨厌的事情，是不是搞错了什么？」
					DATAFORM 「未经允许就被做这种事，谁都会讨厌的吧？」
				ENDDATA
				CALL K30_LECTURE_CHECK(25 + RAND:10, 1)
			;呆れられる
			CASE 0
				PRINTDATAW
					DATAFORM 「啊啊，真是的…是这样啊」\n「你这个人…就是这样的人呢…」
					DATAFORM 「按照自己的方式活着是很棒的事情，但是」\n「我觉得被欲望吞噬地活着就不怎么样了哦」
					DATAFORM 「%K30_C_NAME(MASTER)%也有很多优点，但是」\n「光凭这一点就全部抵消了哦…」
				ENDDATA
			;成功（裤子用
			;人前で主动裤子下ろすのもたいがいだと思う
			CASE 1
				IF K30_BE_SEEN() == 0
					IF ABL:露出癖 > 3
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「虽然很不好意思……但如果是你的话……」
							CASEELSE
								PRINTFORMW 「只想给你看……只想给%K30_C_NAME(MASTER)%你一人看啊…」
						ENDSELECT
						TFLAG:193 += 20
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「就算只有我们两人……也还是很害羞…」
							CASEELSE
								PRINTFORMW 「啊……！　呜，呜呜，好害羞……」
						ENDSELECT
					ENDIF
				ELSE
					IF ABL:露出癖 > 5
						TFLAG:193 += 20
						;ぱんつ是てる
						IF TEQUIP:0 > 1
							PRINTFORMW 「因为是%K30_C_NAME(MASTER)%的请求…即使是这种事情，如果是你的话…♥」
						;没穿内裤でも平气？でたくし上げ
						ELSE
							IF RAND:2 == 1
								PRINTFORMW 「如果%K30_C_NAME(MASTER)%能看着我，我就什么都…♥」
							ELSE
								PRINTFORMW 「我，被看着……被看到这种样子…我……♥」
							ENDIF
						ENDIF
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「因为关系…阎王的威严…就到此为止吧…」
							CASEELSE
								PRINTFORMW 「……不行哦……%K30_C_NAME(K30_FIND_AROUND(),2)%…在看着呢…」
						ENDSELECT
					ENDIF
				ENDIF
			;成功（裙子用
			CASE 2
				PRINTFORMDL 映姬慢慢地向这边回头…用%TEXTR("啪地/轻轻地")%拍打%CALLNAME:MASTER%的头
				PRINTFORMDL 因为惊讶和难为情，脸颊染成了红色…
				PRINTFORMW 
				IF K30_BE_SEEN()
					PRINTDATAW
						DATAFORM 「那个…至少，在没有人的地方…」\n「%K30_C_NAME(K30_FIND_AROUND(), 2)%，不是在吗…」
						DATAFORM 「这种事情…能不能在人前稍微控制一下…？」\n「对我来说威严和体面是很重要的…」
						DATAFORM 「退一万步说，只是给你看的话还勉强可以」\n「在%K30_C_NAME(K30_FIND_AROUND(), 1)%在的地方稍微…」
					ENDDATA
				ELSE
					PRINTDATAW
						DATAFORM 「啊啊，真是的…」\n「对你做的事情，一一追究也没有意义」\n「我知道啦…」
						DATAFORM 「真是的…%K30_C_NAME(MASTER)%又不是儿童了」\n「稍微冷静一点啊…」
						DATAFORM 「即使是给可以给看的人看也是」\n「这种事情是需要心理准备，或者说觉悟的」\n「…明白吗？」
					ENDDATA
				ENDIF
			;振动棒约会
			;约会中は裙子めくれないのでOMANEKIBEYA専用
			CASE 3
				PRINTFORMDL 在持续不断的快感中滴落淫水的同时，映姬无意识地摇晃着屁股
				PRINTFORMDL 似乎正在焦急地等待着被%CALLNAME:MASTER%的手玩弄…
				PRINTFORMW 
				STRDATA LOCALS
					DATAFORM 「%K30_C_NAME(MASTER)%…快点，在这里…给我」\n「现在立刻，也可以哦…」
					DATAFORM 「只是，看着…？」\n「想要，屁股…我已经…忍耐不住了…」
					DATAFORM 「%K30_C_NAME(MASTER)%…是这里哦，这里」\n「快点，把这个拔出来…把你的…」
				ENDDATA
				CALL HPH_PRINT, LOCALS, "L"
		ENDSELECT
		;日记のフラグ
		SIF DIARY:30:15 == 0
			CALL K30_WRITE_DIARY(15)
	ENDIF
ENDIF
RETURN 1
;掀裙子地の文
@K30_COM330_TRAIN
IF TCVAR:30:364 > 0
	TFLAG:193 = 3
ELSEIF TFLAG:193 == 1
	TFLAG:193 = 2
ENDIF
PRINTFORMDL %CALLNAME:MASTER%从映姬的背后掀起了%SHOW_BOTTOM(TARGET,1)%！
PRINTFORMDL 突如其来的事情让映姬反应不过来，%UNDER_SKIRT_DESCRIPTION(TARGET)%显露了出来…
CALL K30_PRINT_BUTT_IMAGE(SELECTCOM)
IF K30_CHECK_HEN_T()
	PRINTFORML 「…………」
	PRINTFORMDL 映姬朝这边回头，但是什么都没说，只是看着这边。
	PRINTFORMDW 这样比被责骂或者敲打更让人有罪恶感…
	RETURN
ENDIF
SELECTCASE TFLAG:193
	;ビンタ
	CASE -5
		LOCAL = (CFLAG:勃然大怒 || MARK:反抗刻印 > 1 || (MARK:反抗刻印 && TALENT:心情 < 0))
		WAIT
		PRINTFORMDL 映姬迅速地回过头，以那股气势直接扇了一巴掌！！
		SIF LOCAL && RAND:2
			PRINTFORMDL 痛彻心扉的一击！
		CALL K30_QUAKE(2 + LOCAL * 2)
		SIF LOCAL
			BASE:MASTER:体力 -= 300 
		PRINTFORMDL 痛到爆炸！！！
		PRINTFORMW 
	;つねられる
	CASE -3
		PRINTFORMDL 痛痛痛痛…！
		PRINTFORMDL 回过头的映姬狠狠地拧了一下我的手背
		PRINTFORMDL 从那纤细的手指上传来了难以置信的力道…
		PRINTFORMW 
	;嫌がる
	CASE -1
		PRINTFORMDL 回过头的映姬露出了明显厌恶的表情
		PRINTFORMDL 果然她不是那种能听懂玩笑的人啊…
		PRINTFORMW 
	;呆れられる
	CASE 0
		PRINTFORMDL 回过头的映姬用不耐烦的眼神瞪着这边…
		PRINTFORMW 
	;MESSAGE_COMで表示
	CASE 2
	CASE 3
ENDSELECT
