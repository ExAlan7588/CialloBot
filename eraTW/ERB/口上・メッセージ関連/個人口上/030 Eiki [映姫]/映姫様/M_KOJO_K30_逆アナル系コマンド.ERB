				PRINTFORMDL 映姬舔舐到皱纹都发白，%CALLNAME:CHR%就对抗般地吞咽到喉咙深处
				PRINTFORMDW 两位美少女的下流服侍三明治强烈地满足了%CALLNAME:MASTER%的征服欲…
			ENDIF
	ENDSELECT
ENDIF
RETURN 1
;舐肛で施虐属性ではなく侍奉精神が上がる映姫样のEXTRASOURCE函数
@M_KOJO_EXTRASOURCE_COM_K30_91
#DIM CHR
;ソース变更处理
SOURCE:征服 = 0
SOURCE:情爱 = 0
SOURCE:达成 = 300
SOURCE:屈从 = 2500
SOURCE:偏离正轨 = 1200
SOURCE:反感 = 1200
TCVAR:MASTER:射精部位 = 9
;地の文分岐决定
SIF TCVAR:30:385 || TCVAR:30:强行 || TCVAR:30:烂醉
	RETURN
SOURCE:反感 /= 2
SIF !TALENT:30:恋慕 || ABL:30:侍奉精神 < 5 || ABL:30:Ａ感觉 < 2 || !STRCOUNT(CSTR:30:0, "/91/")
	RETURN
SOURCE:情爱 = 200
SOURCE:反感 /= 3
TFLAG:194 ++
SIF ABL:30:Ａ感觉 < 5 || ABL:30:侍奉精神 < 7 || PALAM:30:情欲 < PALAMLV:9 || !RAND:4
	RETURN
SOURCE:情爱 *= 2
SOURCE:达成 *= 2
SOURCE:反感 /= 2
TFLAG:194 ++
CHR = MASTER_POSE(0, 1)
;３Ｐ中插入or口交→TARGET变更舐肛で疑似タブル指令化
IF CHR && ((!TEQUIP:CHR:50 && ABL:CHR:Ｖ感觉 > 2) || (!TEQUIP:CHR:51 && ABL:CHR:Ａ感觉 > 2))
	TFLAG:193 = CHR
	TFLAG:194 = 4
ELSEIF CHR && TEQUIP:CHR:口接触部位 == 1 && EXP:CHR:精饮经验 > 19
	TFLAG:193 = CHR
	TFLAG:194 = 5
;你が射精直前でTINKOが空いてたら舐肛手コキ扱い
ELSEIF TCVAR:MASTER:要去了 && (!CHR || GROUPMATCH(MASTER_POSE(30, 1, 1), 3, 4))
	TFLAG:194 = 3
	CALL TOUCH_SET(SET_FROM_YUBI, SET_P, 30)
	TCVAR:MASTER:射精快感强度 = 700
	SOURCE:MASTER:快Ｃ += ABL:30:指 * 20
	EXP:30:手淫经验 ++
ENDIF
