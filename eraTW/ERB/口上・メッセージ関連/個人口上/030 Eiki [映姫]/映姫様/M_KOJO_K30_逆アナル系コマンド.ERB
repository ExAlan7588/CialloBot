	TCVAR:30:媚药 = TFLAG:192
	IF TALENT:PLAYER:调合知识
		DOWNBASE:体力 -= 150
		DOWNBASE:气力 -= 150
	ELSE
		DOWNBASE:体力 -= 300
		DOWNBASE:气力 -= 300
	ENDIF
	UP:情欲 = 0
	SOURCE:偏离正轨 = 0
	IF HETEROSEX(30, MASTER)
		EXP:(50 + HETEROSEX(30, MASTER)) -= 1
		EXP:PLAYER:(50 + HETEROSEX(30, MASTER)) -= 1
	ENDIF
	ITEM:媚药 ++
ENDIF

