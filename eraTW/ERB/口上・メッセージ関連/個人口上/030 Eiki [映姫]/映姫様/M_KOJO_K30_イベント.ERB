;### 口上基本フラグ设定 #############################
@M_KOJO_FLAGSETTING_K30
#DIM COM_TIME
#DIMS PREV_STR

;基本フラグ设定はここで行ってください
;共通の口上フラグは必要な部位のコメントアウトを外してご利用ください
;口上専用フラグはご自由に
;Xには角色番号を入れてください
;眠奸口上有、时间停止口上有、扮演に1を入れると、それぞれ时间停止时、眠奸时、扮演时に指令口上が读み込まれるように
;入れないと时间停止时、眠奸时、扮演时は一切喋りません
CFLAG:30:时间停止口上有 = 1
CFLAG:30:眠奸口上有 = 1
;破瓜中止口上有に1を入れると、@M_KOJO_EVENT_KX_LOST_VIRGIN_STOPで中止の成否を操作できます
;映姫样はお坚いので处女はなるべく守ろうとするよ
;推倒禁止に1を入れると、主动推倒てこなくなります
;映姫样はお坚いので恋慕か炮友がつくまで推倒てこないよ
IF CFLAG:30:1801 == 1
	CFLAG:30:破瓜中止口上有 = 1
	IF TALENT:30:恋慕 || TALENT:30:炮友 || TALENT:30:爱欲
		CFLAG:30:推倒禁止 = 0
	ELSE
		CFLAG:30:推倒禁止 = 1
	ENDIF
ELSE
	CFLAG:30:推倒禁止 = 0
	CFLAG:30:破瓜中止口上有 = 0
ENDIF

;口上内抱き寄せ判定_初回と口上内抱き寄せ判定_通常に1を入れると、M_KOJO_EVENT_KX_PERMISSIONで推倒の成否を操作できます
;CFLAG:X:口上内抱き寄せ判定_初回 = 0
;CFLAG:X:口上内抱き寄せ判定_通常 = 0

;映姫样は意识がちゃんとある时しか接吻合意を认めないよ
SIF CFLAG:30:1801 == 1 && CFLAG:30:1602 == 0
	CLEARBIT CFLAG:30:既成事实, 0

;映姫样は走光ガ—ド硬めだよ
;でも振动棒约会中はユルユルだよ
IF !CFLAG:30:诶嘿嘿 && TEQUIP:30:肛用振动棒
	CFLAG:30:めくれ补正 = 33
ELSEIF TCVAR:30:352 || CFLAG:30:没穿内裤
	CFLAG:30:めくれ补正 = -100
ELSE
	CFLAG:30:めくれ补正 = -10
ENDIF

;兔子之日でも映姫样は变Ｔから逃げられないよ
IF CFLAG:30:衣物暂时更换 == 55 && !CFLAG:30:诶嘿嘿
	LOCAL = DAY % 7 == 5 ? 62 # 17
	CALL CTRL_CLOTHES_SET(30, "现在衣装の变更_衣装セット", LOCAL)
	CALL CLOTHES_SETTING_TRAIN(30)
ENDIF

;映姫样はえっちした时间をしっかり觉えてるよ
IF TCVAR:30:383 && !CFLAG:30:诶嘿嘿
	TCVAR:30:383 = 0
	CFLAG:1504 = TIME:1
ENDIF

;タ—ン每复数恋人管理
CALL K30_DENY_POLYAMORY(0)

;媚药で搾り取り模式なら积攒度减らさない
SIF GETBIT(TCVAR:30:367, 3)
	CFLAG:30:积攒度 = MIN(CFLAG:30:积攒度, 666)
	
;媚药で搾り取り模式中は诶嘿嘿から逃さない
SIF COM_STR == "K30_搾り取り模式"
	CALL K30_KIMESEKU_PUSHDOWN

;COM_STR管理=口上独自で1タ—ンのみ继续するフラグを立てる
SIF STRFIND(PREV_STR, "K30_") > -1 && COM_STR == PREV_STR && COM_TIME < TIME:1
	COM_STR = 
PREV_STR '= COM_STR
COM_TIME = TIME:1

;无自觉孕ませの代偿
IF CFLAG:30:1805 > 1
	TALENT:30:心情 = MIN(TALENT:30:心情, 0)
	CFLAG:30:生子补正 = 0
	EX:30:膣内精液 = 0
	SIF FLAG:生子祈愿 == 30
		FLAG:生子祈愿 = 0
ENDIF

;ガ—タ—吊带描画フラグ
SIF !TCVAR:30:354
	TCVAR:30:354 = CFLAG:30:今日的胖次 == 22 ? 1 # -1

;オプション设定＆当前位置が映姫样と一绪の场合、指令待机时のアニメ描画优先度变更
SETANIMETIMER 32627
SIF CFLAG:MASTER:当前位置 == CFLAG:30:当前位置 && GETBIT(CFLAG:30:1500, 2)
	SETANIMETIMER 50

;振动棒约会フォロ—
SIF TCVAR:30:364 >= 0
	RETURN 1
SIF CFLAG:30:诶嘿嘿
	RETURN 1
TEQUIP:30:肛用振动棒 = 1
TCVAR:30:364 = 1
RETURN 1

;### 来访前フラグ设定 #############################
@K30_BEFORETRAIN
;变Ｔを着て朝から心情愉快とか无理
SIF DAY % 7 == 5 || FLAG:ファッション == 62
	TALENT:30:心情 = MIN(TALENT:30:心情, 0)

;母乳イベント用フラグ
CFLAG:30:1802 = (INRANGE(CFLAG:30:分娩经过日, 1, 24) || CFLAG:30:妊娠经过日数 >= 90)

;同栖中外泊朝归りチェック
SIF CFLAG:MASTER:初始位置 == CFLAG:30:初始位置 && CFLAG:MASTER:当前位置 != CFLAG:30:初始位置 && STRFIND(CSTR:30:1, "同居") == -1
	CFLAG:1605 = 0

;### 口上色判定 ###################################
@M_KOJO_COLOR_K30
SETCOLOR CFLAG:30:1501
RETURN RESULT

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"UPDATE判定"
;※※※※※※※※※※※※※※※※※※※※※※※※※※
@M_KOJO_UPDATE_K30
#DIM TEMP
#DIM R_TEMP
;口上独自のUPDATE用
;ENCOUNTER専用に独自分岐フラグを仕込んだ场合等にどうぞ
;UPDATE判定でCFLAG等の角色变量を使用场合、角色番号の指定が必要になります
VARSET LOCALS
SETBIT CFLAG:30:1803, 4
SIF TALENT:30:恋人 && !CFLAG:30:1505
	SETBIT CFLAG:30:1505, 0
IF CFLAG:30:认识 && MASTERNAME:30 == ""
	CFLAG:30:1501 = 0x337D5C
	PRINTFORML
	PRINTFORML 在UP约会中可以变更映姬大人对你的称呼了
	PRINTFORML 要从「%CALLNAME:MASTER%\@ TALENT:30:恋人 ? # 桑 \@」进行变更吗？
	CALL ASK_YN
	IF !RESULT
		CALL K30_SET_C_NAME(0)
		RESETCOLOR
	ELSE
		MASTERNAME:30 = %CALLNAME:MASTER%
		PRINTFORMW 已设置为「%CALLNAME:MASTER%\@ TALENT:30:恋人 ? # 桑 \@」
	ENDIF
ENDIF
SIF CFLAG:30:允许无套 == 2
	SETBIT CFLAG:30:1502, 0
IF CFLAG:30:认识 && GETBIT(CFLAG:30:1803, 3) == 0
	VARSET DIARY:30, 0, 0, 101
	VARSET PAGESET:30, 0, 0, 101
	DIARY:30:50 =1
	PAGESET:30:1 = 50
	IF CFLAG:30:1902
		CSTR:30:81 = [50:%TOSTR(CFLAG:30:1902)%/晴天/]
		CSTR:30:82 = [50:%MASTERNAME:30%/%CSTR:30:82%/]
	ELSE
		CFLAG:30:1900 = 3
		CFLAG:30:1901 = 1
		CSTR:30:81 = [50:3/晴天/]
		CSTR:30:82 = [50:%MASTERNAME:30%/博丽神社/]
	ENDIF
	SIF !TALENT:30:无接吻经验
			CALL K30_WRITE_DIARY(1, 1)
	SIF CSTR:30:处女丧失履历 != "" 
			CALL K30_WRITE_DIARY(2, 1)
	PRINTFORML 
	PRINTFORML 由于规格变更，日记已被重置。
	PRINTFORMW 继承数据中，日记的发生·显示可能会出现异常，请谅解。
	PRINTFORML 
	SETBIT CFLAG:30:1803, 3
ENDIF
$K30_GUARD_CHECK
IF GETBIT(CFLAG:30:1803, 0)
	RESETCOLOR
	PRINTFORML
	PRINTFORML 映姬大人是\@ CFLAG:30:1801 == 1 ? 慎重的阎魔大人 # 普通的幻想乡少女 \@
	PRINTFORML 要变更映姬大人的慎重程度吗？
	CALL ASK_M("进行",1 , "暂时保留" ,1 , "不再变更",1)
	SELECTCASE RESULT
		CASE 0
			CALL K30_SELECT_TYPE
			GOTO K30_GUARD_CHECK
		CASE 1
			PRINTFORMW 引继UPDATE时会再次出现选择项
		CASE 2
			CLEARBIT CFLAG:30:1803, 0
			PRINTFORMW 选择完毕
	ENDSELECT
ENDIF
PRINTFORML 
IF !GETBIT(CFLAG:30:1803, 6)
	SETBIT CFLAG:30:1803, 6
	IF !CFLAG:30:应用差分替换 && SPRITECREATED("30_bodyparts_00")
		PRINTFORML 将映姬大人的头像切换为推荐口上的其他头像。
		PRINTFORML 如果有不便之处，请从[显示设定]中将[差分]设为OFF。
		PRINTFORML 
		CFLAG:30:应用差分替换 = 1
	ENDIF
ENDIF
R_TEMP = CURRENTREDRAW()
REDRAW 0
PRINTL 映姬大人口上专用图像显示选项
PRINTL 
DO
	TEMP = LINECOUNT
	CALL PRINTBUTTON_EX, @"[0] - 口交动画　　%\@ GETBIT(CFLAG:30:1500, 0) ? 有 # 无 \@%　※有描画负荷", 0, 0, GETBIT(CFLAG:30:1500, 0)
	PRINTL
	CALL PRINTBUTTON_EX, @"[1] - 屁股图像　　　%\@ GETBIT(CFLAG:30:1500, 1) ? 有 # 无 \@%　※有少量挑人的表现", 1, 0, GETBIT(CFLAG:30:1500, 1)
	PRINTL
	CALL PRINTBUTTON_EX, @"[2] - 眨眼动画　%\@ GETBIT(CFLAG:30:1500, 2) ? 有 # 无 \@%　※实验要素-略微动作不稳定", 2, 0, GETBIT(CFLAG:30:1500, 2)
	PRINTFORML \n
	PRINTL [9] - 决定
	INPUT
	SELECTCASE RESULT
		CASE 0 TO 2
			INVERTBIT CFLAG:30:1500, RESULT
		CASE 9
			REDRAW R_TEMP
			BREAK
	ENDSELECT
	CLEARLINE LINECOUNT - TEMP
LOOP 1

IF !GETBIT(CFLAG:30:1803, 7)
	SETBIT CFLAG:30:1803, 7
	TALENT:30:痛觉 = -1
	PRINTFORML 
	CALL COLORMESSAGE, "根据说辞内容，赋予映姬大人【不怕痛】的素质", C_YELLOW, 1, 0
ENDIF

IF !GETBIT(CFLAG:30:1803, 8)
	SETBIT CFLAG:30:1803, 8
	CFLAG:30:1501 = 0x337D5C
	PRINTFORML 
	CALL M_KOJO_COLOR_K30
	LOCAL = 0x337D5C, 0x52C487, 0x7FD8A7, GETDEFCOLOR(), 0xFF93FB
	SIF !CFLAG:30:认识
		PRINTFORML 「那边的你……突然打扰非常抱歉。我叫四季映姬。」
	PRINTFORML 「最近我听到了有人说我的话太长、难以听懂的声音。」
	PRINTFORML 「话长是因为听者平日的所作所为不够好，虽然……」
	PRINTFORML 「难以听懂的话，应该是我这边的问题吧。」
	PRINTFORML 「……用什么样的声音说话，大家会觉得更容易理解呢？」
	PRINTFORMDL 
	PRINTL [0] - 用低沉冷静的声音
	SETCOLOR LOCAL:1
	PRINTL [1] - 不刻意地用普通的声音
	SETCOLOR LOCAL:2
	PRINTL [2] - 用稍微高一点的声音
	RESETCOLOR
	PRINTL [3] - 别太个性
	SETCOLOR LOCAL:4
	PRINTL [4] - 请用H的声音
	RESETCOLOR
	DO
		INPUT
		SIF INRANGE(RESULT, 0, 4)
			BREAK
		CLEARLINE 1
	LOOP 1
	CFLAG:30:1501 = LOCAL:RESULT
	CALL M_KOJO_COLOR_K30
	SELECTCASE RESULT
		CASE 0, 1
			PRINTFORML 「明白了。感谢您的配合。」
		CASE 2
			PRINTFORML 「这个……稍微有些不习惯呢。」
			PRINTFORML 「不过我会努力试试看的。感谢您的配合。」
		CASE 3
			PRINTFORML 「我的声音……有那么难听懂吗……？」
			PRINTFORML 「嗯……总之谢谢您的指正。」
		CASE 4
			PRINTFORML 「诶……？　用这样的声音……？」
			PRINTFORML 「………真的吗？」
	ENDSELECT
	RESETCOLOR
	PRINTFORML
	PRINTFORMW ※口上颜色可以在SHOP菜单中更改
ENDIF

