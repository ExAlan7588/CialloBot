	;日中は隅っこの方でじっとしている確率が5割
	IF CFLAG:今日的运势 < 500
		TCVAR:30:引力点 = HIDEOUT:MAIN_MAP
	;恋人あなたが一人暮らしならあなた自室に逃げ込む確率が3割
	ELSEIF CFLAG:今日的运势 > 699 && K30_FIND_LOVER() < 0 && (!PRIVATEROOM:(CFLAG:MASTER:初始位置 % 100))
		TCVAR:30:引力点 = CFLAG:MASTER:初始位置
	ENDIF
ENDIF
RETURN
