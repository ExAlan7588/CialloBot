@M_KOJO_ENCOUNTER_K30
;初对面、挨拶のタイミング
;TALENTの变更はここでどうぞ
CALL K30_SELECT_TYPE
SIF K30_CHECK_HEN_T()
	CALLF FIRSTTIME("变Ｔお披露目")
;BIT 0と3を同时にON
CFLAG:30:1803 = 9
;日记フラグ
CALL K30_WRITE_DIARY(50, 1)
CFLAG:30:1901 = 据点_家主
CALLF ONCE("初对面")
CALL M_KOJO_COLOR_K30
;白玉楼か是非曲直厅に房间持ち
IF MAIN_MAP == 6 && 据点_家主 != 26
	;变ＴVer.-冥界住まいで4日间映姫样を避け继续。たぶん死に分岐
	IF K30_CHECK_HEN_T()
		CFLAG:30:1900 = 4
		PRINTFORMW 「诶……！？」
		PRINTFORMDL 向声音的方向转过身，那里站着一个穿着奇特服装的少女。
