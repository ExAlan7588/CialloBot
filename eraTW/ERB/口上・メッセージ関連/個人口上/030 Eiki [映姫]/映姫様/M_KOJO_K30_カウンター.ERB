;-------------------------------------------------
;怀孕愿望Counter
;抱きつくばっかじゃなくてちゃんと妊活してよ、という杂なテコ入れ
;固定化されてもつまらないので基本概率はあんまり上げない
;本体侧が优秀なので随意数字いれても问题无し
@K30_COUNTER_WISH_PREGNANCY
#DIM WISH_VAL
;-------------------------------------------------
WISH_VAL = 10
;素股フラグ时
SIF TCVAR:30:362 == 1
	WISH_VAL += 999
;妊活に励む日
SIF ESTRUS_CYCLE(30) >= 70
	WISH_VAL += 15
;溜め込んでたらそりゃ狙われる
SIF TALENT:MASTER:浓厚精液
	WISH_VAL += 15
SIF WISH_VAL < RAND:100
	RETURN 1
;もうしてたら腰振りorだいしゅきホールド
IF TEQUIP:50 == PLAYER
	TCVAR:次回Counter = 88
	SIF BASE:MASTER:射精 > MAXBASE:MASTER:射精
		TCVAR:次回Counter = 86
;喜欢的体位で孕みたいとかえっちい感じがする
ELSEIF CFLAG:喜欢的体位 && RAND:2 == 1
	SELECTCASE CFLAG:喜欢的体位
		CASE 1, 4, 6, 9
			TCVAR:次回Counter = 41
		CASE 2, 5, 7, 10 
			TCVAR:次回Counter = 43
		CASE 3, 8
			TCVAR:次回Counter = 85
	ENDSELECT
;やる气まんまんの时はズブリといっちゃう
ELSEIF WISH_VAL + RAND:50 > 50
	TCVAR:次回Counter = 85
;ここからはもう适当
ELSE
	SELECTCASE RAND:3
		CASE 0
			TCVAR:次回Counter = 85
		CASE 1
			IF RAND:2 == 1
				TCVAR:次回Counter = 41
			ELSE
				TCVAR:次回Counter = 43
			ENDIF
		CASE 2
			IF RAND:2 == 1
				TCVAR:次回Counter = 51
			ELSE
				TCVAR:次回Counter = 44
			ENDIF
	ENDSELECT
ENDIF
RETURN 1

