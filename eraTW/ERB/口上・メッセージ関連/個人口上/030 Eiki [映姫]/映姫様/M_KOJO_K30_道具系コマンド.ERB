	RETURN
SIF !NOWEX:MASTER:射精 || STRFIND(CSTR:30:1, "おもらし") > -1 || !SUMARRAY(NOWEX:30:0, 7, 10) || TEQUIP:MASTER:避孕套
	RETURN
CALLF ONCE("おもらし")
PRINTFORML 
PRINTFORMDL 被突刺屁股而强烈兴奋的映姬突然改变了声调恳求起来
STRDATA LOCALS
	DATAFORM 「啊、啊HPH　等等！　等等HPH　等等啊啊HPH」
	DATAFORM 「啊HPH　停、停下！　停下啊！　一次、停下啊HPH」
	DATAFORM 「呀HPH　不要、不要！　真的、不要啊HPH」
ENDDATA
LOCALS = %LOCALS%\n「%TEXTR("要漏出来了！　要漏出来了啊/不要HPH　尿！　不要啊")%HPH」\n「饶了我吧！　饶了我吧HPHHPH」

CALL HPH_PRINT, LOCALS, "W"
PRINTFORMDL 似乎是因为所有羞耻的孔穴都被玩弄，尿意已经接近极限了…
RESETCOLOR
PRINTBUTTON "[放过]　　　", 0
PRINTBUTTON "[不放过]", 1
CALL M_KOJO_COLOR_K30
DO
	INPUT
	CLEARLINE 1
	IF !RESULT
		PRINTFORMDL …总觉得剥夺阎魔大人的尊严到那种程度不太好
		PRINTFORMDW %CALLNAME:MASTER%为了尽快迎来射精，开始专注于活动腰部
		RETURN 1
	ELSEIF RESULT == 1
		BREAK
	ENDIF
LOOP 1
TFLAG:194 = 045
TCVAR:30:374 = 1
NOWEX:放尿 = NOWEX:Ｃ绝顶
EX:放尿 ++
EXP:放尿经验 += EXP_UP(5, 30)
