		;场外に出て你が角色と冲突した际の台词
		;文字数制限はありません,泛用台词もありません
		;-------------------------------------------------
		RESULTS = うわあ!
	CASE "そこだ！"
		RESULTS = %TEXTR("…あ!目の前!/そこです!そこ!")%
	CASE "近い"
		RESULTS = %TEXTR("近いですね/近いですね/近づいてます")%
	CASE "远い"
		RESULTS = %TEXTR("远いですね/近づいては来たかも?")%
	CASE "かなり远い"
		RESULTS = %TEXTR("结构远いですね/3.14159 26535…")%
	CASE "前"
		RESULTS = %TEXTR("前方です/方位は正しいですね/いい感じですよー")%
	CASE "少し前"
		RESULTS = %TEXTR("近い!近いですよ!/もう少し!")%
	CASE "后ろ"
		RESULTS = %TEXTR("后ろですよ/反对かなー/そっちじゃないですー")%
	CASE "少し后ろ"
		RESULTS = %TEXTR("近いんだけど…近いけど…/うーん…")%
	CASE "右"
		RESULTS = %TEXTR("右です/左!…あ、右です/もっと右")%
	CASE "少し右"
		RESULTS = %TEXTR("稍微右/正面…いや右かな")%
	CASE "左"
		RESULTS = %TEXTR("左です/右!…いえ、左でした/もっと左")%
	CASE "少し左"
		RESULTS = %TEXTR("稍微左/うーん…左?/微妙だなー…左?")%
	CASE "结果"
		;-------------------------------------------------
		;结束后の一言
		;文字数制限はありません,泛用台词もありません
		;TFLAG:193,
		;   -1=失败
		;    0=成功
		;    1=大成功
		;    2=木刀で大成功（BOUNS点）
		;ARG
		;    0=正直者
		;    1=ウソつき
		;-------------------------------------------------
		SELECTCASE TFLAG:193
		CASE -1
			RESULTS = ま、そういう事もありますって
		CASE 0
			RESULTS = やりましたね
		CASE 1
			RESULTS = 一刀两断!って感じでしたよ
		CASE 2
			RESULTS = …やるじゃない
		ENDSELECT
	ENDSELECT
ENDIF
