	CASE 9
		RETURN 1
	;怎么也找不到(そのまま落ちてる)（发生条件：TARGETの信赖度が30以上）
	CASE 10
		RETURN 1
	;怎么也找不到(谁かが拾ってる)（发生条件：TARGETの信赖度が30以上）
	CASE 11
		RETURN 1
	;惩らしめて！（发生条件：TARGETのCSVに战斗能力が１以上ある场合）
	CASE 12
		RETURN 1
	;好吃的鱼が食べたいなぁ（发生条件：若鹭姫以外）
	CASE 13
		RETURN 1
	;柴火用完了（未实装）
	CASE 14
		RETURN 1
	;想被你抱（发生条件：炮友or爱欲かつ积攒度700以上 or 风骚）
	CASE 15
		RETURN 1
		;发生はするようにしてるけど、追加予定无
	;お恼み相谈（发生条件：MASTERの话术技能が1以上）
	CASE 16
		RETURN 1
	[SKIPSTART]
	;现状无し
	CASEELSE
		RETURN 1
	[SKIPEND]
ENDSELECT


;==================================================
;依赖口上・各依赖で共通の口上
;==================================================
@M_KOJO_长短发_IRAI_K59(ROLE, SCENE, IRAI_ID)
#DIMS ROLE     ;角色の立场、依赖者かそうでないか
#DIMS SCENE    ;セリフの表示されるタイミング
#DIM  IRAI_ID  ;依赖自体のID
#DIM  CLIENT_ID;依赖者IDだけ抜き出したもの
#DIM  CHK_ID   ;依赖IDだけ抜き出したもの
#DIMS IRAI_NAME;依赖名
CHK_ID    = IRAI_ID % 1000
CLIENT_ID = IRAI_ID_TO_CLIENT(IRAI_ID)
IRAI_NAME = STR_DATA_IRAI(IRAI_ID, "依赖名", CLIENT_ID)
RESULT = 0
IF CHK_ID > 100
	TRYCALLFORM M_KOJO_长短发_IRAI_%IRAI_NAME%_K59(ROLE, SCENE, CLIENT_ID)
ELSE
	TRYCALLFORM M_KOJO_长短发_IRAI_{CHK_ID}_K59(ROLE, SCENE, CLIENT_ID)
ENDIF
SIF RESULT
	RETURN 1
;--------------------------------------------------
;ここにあるのは各依赖で共通の口上です
;依赖ごとの口上を表示した场合は共通の口上は表示されません
;【】で囲ってあるのは口上前后の地の文や发生する处理です
;WAITをするか等の目安にしてください（L,WはそれぞれPRINTFORML,PRINTFORMWの略）
;--------------------------------------------------
SELECTCASE SCENE
CASE "依赖提示时"
;●依赖を持ち矿物碎片れる时
;【W どうやら［彼女］は［你］に赖み事があるようだ】
;【◆口上】
;【依赖データ表示】
;【INPUT（受托するかY/N）】
	
CASE "依赖非受托时"
;●内容だけ见て受托しなかった时
;【L ［你］は丁重にお断りした】
;【◆口上】
;【W ［彼女］は残念そうな颜をしている】
	
CASE "依赖受托时"
;●依赖を受托した时
;【W ［你］は［彼女］の依赖を受けた】
;【◆口上】
	
CASE "依赖确认时"
;●[366]で依赖内容を依赖人に确认した时
;【◆口上】
;【依赖データ表示】
;【INPUT（破弃するかY/N）】
	
CASE "依赖破弃时"
;●受托した依赖を破弃する时
;【◆口上】
;【W 依赖を破弃しました。】
	
CASE "依赖实行直前"
;●[367]で依赖を实行する时、实行处理の前に呼び出される
;※依赖によって状况が大きく异なるので共通口上は非推奖
;【◆口上】
;【实行时の地の文・ソース等】
;【▼下の口上に续く】
	IF ROLE == "CLIENT"
	;（共通）CLIENT：依赖实行の对象と依赖主（口上主）が同一の场合（主に特训系ですが一绪に捜索等をした场合も含まれます）
		
	ELSEIF ROLE == "TARGET"
	;（共通）TARGET：口上主が依赖实行の对象である场合（主に配达系ですが捜索等をした际、TARGETにしていた场合も含まれます）
		
	ENDIF
CASE "依赖实行直后"
;●[367]で依赖を实行する时、实行处理の后に呼び出される
;※依赖によって状况が大きく异なるので共通口上は非推奖
;【▲上の口上から续き】
;【◆口上】
;【达成时・CLIENT：▼下の口上に续く】
;【达成时・TARGET：忘れずに［依赖主］に报告しよう】
	IF ROLE == "CLIENT"
	;（共通）CLIENT：依赖实行の对象と依赖主（口上主）が同一の场合（主に特训系ですが一绪に捜索等をした场合も含まれます）
		SELECTCASE CFLAG:CLIENT_ID:依赖状况
		CASE 2
		;实行后未解决
			
		CASE 3
		;实行后达成
			
		CASE 6
		;实行后失败（现在「06_谁来告诉我！」のみ）
			
		ENDSELECT
	ELSEIF ROLE == "TARGET"
	;（共通）TARGET：口上主が依赖实行の对象である场合（主に配达系ですが捜索等をした际、TARGETにしていた场合も含まれます）
		SELECTCASE CFLAG:CLIENT_ID:依赖状况
		CASE 2
		;实行后未解决
			
		CASE 3
		;实行后达成
			
		CASE 6
		;实行后失败（现在「06_谁来告诉我！」のみ）
			
		ENDSELECT
	ENDIF
CASE "成功报告时"
;●报告时（成功）
;【依赖实行と报告が同时の时：▲上の口上から续き】
;【◆口上】
;【ソースの入手・道具等の报酬获得】
	
CASE "失败报告时"
;○直接失败报告は现在「06_谁来告诉我！」のみ
;●报告忘れ・期限切れになった际、遭遇时の最后に呼び出される
;（遭遇时）【挨拶等】→【心情の良し恶し】→【色欲察觉】→【祭日イベント】→【◆ココ！】（尚、依赖失败判定があると约会のお诱いは发生しません）
;【◆口上】
;○心情の变化や追加のペナルティ等は口上侧で处理してください
	
CASE "依赖报告不要"
;○一般依赖では现在「02_给花浇水」「09_谁的遗失物」のみ
;●起床时、报告不要依赖を达成していた场合に呼ばれる口上
;（起床时）【起床メッセージ】→【X时间寝た等】→【◆ココ！】→【神签・补充等】→【一日开始】
;○地の文はありません
;○口上主がその场にいなくても表示されます（MASTERが寝る时のイベント口上と同じタイプ）
	
ENDSELECT
RETURN 1

