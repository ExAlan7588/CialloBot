	IF CANNOT_DANMAKU() == 1
	;屋内のとき（胜负できない）
	;【◆口上】
	;【W 屋外で挑もう…】
		PRINTFORMW 「虽然我不太明白，但没办法，我来吧」
		PRINTFORMW 「…等等，你说在室内很危险？可是巫女来的时候我们就正常进行了啊？」
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 2
	;仕事中（胜负できない）
	;【◆口上】
	;【W お取り込み中だ…】
		PRINTFORMW 「弹幕对决？现在...你看不出来吗？」
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 3
	;你が疲劳中（胜负できない）
	;【◆口上】
	;【W 今は疲れている…】
		PRINTFORMW 「%MASTERNAME:59%...你，无论从哪个角度看都很疲劳啊」
		;关系性次第で、「贵方…你～」というセリフになるが气にして是けない
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 4
	;口上主が疲劳竜（胜负できない）
	;【◆口上】
	;【W 今挑むのはフェアじゃない…】
		PRINTFORMW 「嗯？弹幕对决？嘛，你先好好看看我的现状吧...」
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 5
	;既に今日一度弹幕胜负をしている（胜负できない）
	;【◆口上】
	;【W 今日はもう挑めない…】
		PRINTFORMW 「没意思」
		RETURN 1
	ELSE
	;【◆口上】
	;【W 胜负开始！！】
	;【以降はそのまま弹幕口上が呼ばれます】
		PRINTFORMW 「弹幕对决？而且还是代替某个人？为什么？」
		PRINTFORMW 「…好吧，没关系，我来当你的对手」
		RETURN 1
	ENDIF
	;【▼そのまま下の口上へ】
CASE "依赖实行直后"
;○この依赖では讨伐される侧の口上です
	IF CFLAG:CLIENT_ID:依赖状况 == 2
		IF CANNOT_DANMAKU() == 0
			;败犬
			;【▲上の口上から续き】
			;【L ［彼女］の讨伐に失败した……】
			;【W ……こうなったら后日出直すしかない】
			;【◆口上】
			PRINTFORMW 「你去重新准备两个小时!」
			PRINTFORMW 「…我只是想说一下这个而已」
			RETURN 1
		ELSE
			;挑めなかった场合
			;【▲上の口上から续き】
			;【◆口上】
			PRINTFORMW 「以后再说吧。以后」
			RETURN 1
		ENDIF
	ELSEIF CFLAG:CLIENT_ID:依赖状况 == 3
	;胜利
	;【▲上の口上から续き】
	;【W ［彼女］を惩らしめた！】
	;【◆口上】
		PRINTFORMW 「咕噜!」
		PRINTFORMW 「…%MASTERNAME:59%请转告给那个人，说『小恶魔倒下得很美丽』」
		RETURN 1
	ENDIF
CASE "成功报告时"
;报酬：赏金（时价）
	PRINTFORMW 「原来如此...是这样啊」
	PRINTFORMW 「这对我很有参考价值。非常感谢！」
	RETURN 1
CASE "失败报告时"
	PRINTFORMW 「没有获得成果...真遗憾」
	RETURN 1
;CASE "依赖报告不要"
;＜この依赖にはありません＞
;	RETURN 0
ENDSELECT

