@M_KOJO_MESSAGE_COM_K78_0_1
#DIM 腋フラグ
腋フラグ = 0
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;插入中
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V插入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A插入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;扶她
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	IF !TEQUIP:继续接吻 && !TEQUIP:吸乳头 && !K78_EXCESSIVE_STATURE_GAP() && !RAND:3
		;腋汗ジミあり
		IF K78_SWEATY_UNDERARMS()
			PRINTFORMDL \@ K78_ARMPIT_SWEAT() == 2 ?是因为兴奋起来了吗 #胳膊发痒的 \@、%CALLNAME:TARGET%的腋下已经被汗水浸湿。
			PRINTFORMDL %CALLNAME:MASTER%将脸凑了过去，享受着浓厚的香味。
			腋フラグ = 1
		;腋が露出してる
		ELSEIF K78_ARMPIT_EXPOSURE()
			PRINTFORMDL %CALLNAME:MASTER%爱抚%CALLNAME:TARGET%全身的同时、%K78_ARMPIT_HAIR_MODIFIER()%开始舔她的腋下。
			SELECTCASE CFLAG:1007
			CASE 0,1
				PRINTFORMDL \@ CFLAG:1007 == 0 ? 光滑 # 粗糙 \@\@ K78_ARMPIT_SWEAT() ? 的那里被带着甜香的汗水湿润着。 # 触感很舒适。 \@
			CASE 2,3
				PRINTFORMD \@ CFLAG:1007 == 2 ? 柔软的腋毛 # 茂盛的腋毛 \@
				IF K78_ARMPIT_SWEAT()
					PRINTFORMDL 汗水挥发着，散发着与平时的烤白薯一样的甜味不同的体臭。
				ELSE
					PRINTFORMDL 塞进脸，用舌头一边搅拌一边品味%CALLNAME:TARGET%的腋下的味道。。
				ENDIF
			ENDSELECT
			腋フラグ = 1
		ENDIF
	ENDIF
	IF K78_BASIC_CONDITION_IN_MATING()
		$PETTING_LOOP
		IF TALENT:恋慕 || TALENT:爱欲
			IF 腋フラグ
				SELECTCASE RAND:4
				CASE 1
					IF K78_ARMPIT_SWEAT()
						CALL K78_HPH_PRINT,@"「不行HPH 因为现在出了很多汗，不要闻啊！HPH」","L" 
					ELSE
						GOTO PETTING_LOOP
					ENDIF
				CASE 2
					CALL K78_HPH_PRINT,@"「呀啊HPH 腋下实在太羞耻了HPH」","L"
				CASE 3
					CALL K78_HPH_PRINT,@"「又在舔奇怪的地方HPH」","L"
				CASEELSE
					CALL K78_HPH_PRINT,@"「不要这么执着腋下啊HPH」","L" 
				ENDSELECT
			ELSE
				SELECTCASE RAND:5
				CASE 1
					IF EXP_UP(27,TARGET)
						CALL K78_HPH_PRINT,@"「啊呵、嗯HPH还想要kiss…HPH」","L" 
					ELSE
						GOTO PETTING_LOOP
					ENDIF
				CASE 2
					CALL K78_HPH_PRINT,@"「最喜欢被你的手指触摸了HPH」","L" 
				CASE 3
					CALL K78_HPH_PRINT,@"「嗯、好舒服……更多，更多HPH」","L" 
				CASE 4
					CALL K78_HPH_PRINT,@"「啊HPH胸，可以哦HPH哈啊…很温柔呢HPH」","L" 
				CASEELSE
					CALL K78_HPH_PRINT,@"「\@TALENT:性别 & 2 ? 小鸡鸡 # 小豆豆 \@ 好棒HPH 再多摸摸HPH」","L"
				ENDSELECT
			ENDIF
		ELSE
			IF 腋フラグ
				PRINTFORML 「等下！ 哇，腋下不行啦！」
			ELSEIF EXP_UP(27,TARGET) && RAND:2
				PRINTFORML 「哈姆……嗯……啾… 啊，哈」
			ELSE
				PRINTFORML 「啊… 嗯，嗯哈，哈 嗯啊」
			ENDIF
		ENDIF
	ENDIF
	SIF 腋フラグ || K78_BASIC_CONDITION_IN_MATING()
		WAIT
ENDIF
RETURN 1
