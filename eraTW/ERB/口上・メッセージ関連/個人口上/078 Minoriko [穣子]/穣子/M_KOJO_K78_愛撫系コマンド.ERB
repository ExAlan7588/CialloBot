	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF K78_BASIC_CONDITION_IN_MATING()
			PRINTFORMW 「等、等下！ 在舔哪里啊喂！」
			PRINTFORMDW 发现%CALLNAME:MASTER%在舔她不干净的那个洞的%CALLNAME:TARGET%挣扎叫喊了起来。
			PRINTFORMW 「那里不行，哈啊……哼……啊、哎、哎」
			PRINTFORMDW 压住了%CALLNAME:TARGET%的手腕持续舔了一会，慢慢的抵抗就变弱了，妖艳的娇喘声也随着而来。
		ENDIF
		RETURN 1
	ENDIF
	
	IF !K78_EXCESSIVE_STATURE_GAP()
		IF CFLAG:1002 && TCVAR:351 && RAND:2
			SELECTCASE RAND:9
			CASE 1 TO 4
				PRINTFORMDL 排便后的%TEXTR("肛门/菊花/菊门")%散发着强烈的臭气。。
			CASE 5 TO 8
				PRINTFORMDL 舔舐排泄物残渣这样的倒错性的行为使得%CALLNAME:MASTER%更加兴奋了
			CASEELSE
				PRINTFORMDL 未消化残留的%TEXTR("玉米皮/韭菜/番茄皮")%黏在了肛门上。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMDL 对侵入的舌头做出反应的%TEXTR("肛门/菊花/菊门")%咯吱咯吱地蠕动着。
			CASE 2
				PRINTFORMDL \@ CFLAG:1003 == 2 && RAND:3 ?%TEXTR("用舌头抚弄/舔舐")%被%TEXTR("不检点/下流/茂盛")%的毛发所围绕的 # \@%TEXTR("肛门/菊花/菊门")%，\@ CFLAG:1002 && !TCVAR:351 ?隐约感觉到大便的气味。 #%CALLNAME:TARGET%的娇喘突然变轻了。 \@
			CASEELSE
				SELECTCASE ABL:肛门
				CASE IS >= 4
					PRINTFORMD %TEXTR("解开/开发/使用了")%
				CASE 2,3
					PRINTFORMD %TEXTR("正在变成开发途中的/开始松散的/变成性器官的。")%
				CASE IS <= 1
					PRINTFORMD %TEXTR("突然缩小了的/紧闭的/还未开发的")%
				ENDSELECT
				PRINTFORMDL %CALLNAME:TARGET%的%TEXTR("肛门/菊花/菊门")%被唾液濡湿，发出光芒。
			ENDSELECT
		ENDIF
		IF K78_BASIC_CONDITION_IN_MATING()
			IF TALENT:恋慕 || TALENT:爱欲
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「讨厌，大便的地方不能呸喽呸喽的那样舔HPH」","L"
				CASE 2
					CALL K78_HPH_PRINT,@"「明明很脏的HPH不行的HPH呀，啊HPH呼，哈恩」","L"
				CASEELSE
					CALL K78_HPH_PRINT,@"「啊，HPH啊，虽然很舒服……但是果然这样的还是不行啊HPH」","L"
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「居然在舔那种地方、啊、啊、哈，这也太变态了」
				CASEELSE
					PRINTFORML 「都说了很脏了　真是的、快停下来　啊、嗯阿」
				ENDSELECT
			ENDIF
		ENDIF
		WAIT
	ENDIF
ENDIF
RETURN 1

