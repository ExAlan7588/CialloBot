						CALL K78_HPH_PRINT,@"「今天的内衣怎么样？HPH喜欢这样的吗HPH」","L"
					ELSE
						CALL K78_HPH_PRINT,@"「最好不要穿内衣？我知道了HPH你在这方面很执着呢HPH」","L"
					ENDIF
				ELSE
					PRINTFORML 「全部脱掉只剩内衣是吗？好吧，知道了」
				ENDIF
				;内衣姿になってはじめて腋が露出した、かつ腋汗ジミあり
				IF !K78_ARMPIT_EXPOSURE() && K78_ARMPIT_EXPOSURE(TARGET,1) && K78_SWEATY_UNDERARMS()
					PRINTL
					SELECTCASE CFLAG:1007
					CASE 0,1
