;==================================================
@M_KOJO_EVENT_K78_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	CALL K78_HAIR_GROWTH,78
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて一绪に入る
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF K78_CONSCIOUS_AND_TOLERANT()
			PRINTFORMW 「啊，%CALLNAME:MASTER%已经在里面了呀。呐，我可以进去一起洗么？」
		ENDIF
		SIF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて出て行った
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて追い出された
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他角色と约会中に遭遇した
	;ARG:1に约会的对象
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TCVAR:353 != ARG:1 && K78_CONSCIOUS_AND_TOLERANT()
			;静叶と约会
			IF ARG:1 == 77 && FLAG:扮演 != 77
				IF TALENT:恋人 || ( TALENT:恋慕 && !TALENT:77:恋人 )
					PRINTFORML 「呜～　姐姐，你黏得也太紧了！」
					PRINTFORMDW %CALLNAME:TARGET%暴露出了对%CALLNAME:77%的抗议。
				ELSE
					PRINTFORMW 「要和姐姐一起出门吗？要玩得开心点哟」
				ENDIF
			;さっきと违う相手と约会
			ELSEIF TCVAR:353 != 0
				IF TALENT:恋慕 || TALENT:恋人
					PRINTFORML 「又和别的女人…」
					PRINTFORMDW 被%CALLNAME:TARGET%紧紧的盯住了。
				ELSE
					PRINTFORML 「为什么约会的对象和刚才不一样了…」
					PRINTFORMDW %CALLNAME:TARGET%呆住了。
				ENDIF
			ELSE
				IF TALENT:恋慕 || TALENT:恋人
					PRINTFORML 「看起来关系很好呐……」
					PRINTFORMDW 看到带着女人的%CALLNAME:MASTER%，%CALLNAME:TARGET%露出似乎很有趣的表情。
				ELSEIF TALENT:思慕
					PRINTFORML 「哼。%CALLNAME:MASTER%真是个广爱的人呐…」
					PRINTFORMDW %CALLNAME:TARGET%一副满不在乎的样子向%CALLNAME:MASTER%和%CALLNAME:(ARG:1)%那里撇了一眼。
				ELSE
					PRINTFORM 「啊，%CALLNAME:MASTER%。
					SELECTCASE TIME:0
					CASE IS <600
						PRINTFORMW 早上好」
					CASE 600 TO 1139
						PRINTFORMW 你好」
					CASE IS >= 1140
						PRINTFORMW 晚上好」
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
		IF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
			TCVAR:78:353 = ARG:1
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


