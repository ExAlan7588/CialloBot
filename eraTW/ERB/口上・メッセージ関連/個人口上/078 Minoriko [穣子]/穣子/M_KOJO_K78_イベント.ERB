;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K78_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	SIF SHIRAHU(TARGET)
		TCVAR:78:352 = 1
	CALL K78_HAIR_GROWTH,78
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同栖かつ相手が先に起きてる
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕时陪睡中就寝时のみ？
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕无しでも入ってくる可能性有り
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF K78_CONSCIOUS_AND_TOLERANT()
				IF TALENT:恋慕 || TALENT:爱欲
					CALL K78_HPH_PRINT,@"「抱歉，把你吵醒了吧……只是一直特别想见到你HPH」","W"
				ELSEIF TALENT:思慕 || TALENT:炮友
					PRINTFORML 「啊，早上好」
					PRINTFORMDL 为什么%CALLNAME:TARGET%会在这里呢。
					PRINTFORMW 「只，只是顺路过来看看而已哦」
				ELSE
					PRINTFORMW 「醒了么？我来稍微借点东西」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同栖かつ相手が起きている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF K78_CONSCIOUS_AND_TOLERANT()
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「早上好%CALLNAME:MASTER%。那么，今天我们来打理一下田地吧」
			CASE 2
				PRINTFORMDL 一睁开眼睛%CALLNAME:TARGET%的脸庞就立刻出现在了%CALLNAME:MASTER%面前。
				CALL K78_HPH_PRINT,@"「埃嘿嘿♪看到%CALLNAME:MASTER%可爱的睡脸了HPH」","W"
			CASEELSE
				;TARGETと一绪に起きた
				IF TIME:0 == CFLAG:起床时间
					PRINTFORMW 「早上好。今天做什么好呢」
				;TARGETより遅く起きた
				ELSE
					PRINTFORMW 「终于起床了？睡了个懒觉呢」
				ENDIF
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同栖かつ相手が寝ている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝时TARGET起床はこちら（TARGET衰弱or陪睡中TARGET睡魔负け后そのままMASTERが起き续けた上で相手が起床した场合限定
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同栖
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同栖中
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;其他
	PRINTFORML  
ENDIF
RETURN 1

