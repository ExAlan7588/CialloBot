;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;绝顶
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_CONSCIOUS_AND_TOLERANT() && !TALENT:无知
		;-------------------------------------------------
		;五重绝顶
		;-------------------------------------------------
		IF NOWEX:五重绝顶
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「哦、嗯哦HPH 嗯哈啊HPH 啊啊、啊、啊啊啊啊HPH 咿、咿咕呜呜呜HPH」","L"
				PRINTFORMDL 在%CALLNAME:TARGET%大声娇喘之后，如同断了线的木偶一般全身松弛，呼哧呼哧地靠在肩上喘着气。
				PRINTFORMW 「呼、呼、呼 ……够…够了…饶了我吧…」
			CASE 2
				CALL K78_HPH_PRINT,@"「呀、啊、啊HPH 嗯啊啊HPH 嗯咿、嗯呜咿咿HPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%的脸上满是眼泪鼻涕和口水，全身无力地抽搐着。
				CALL K78_HPH_PRINT,@"「哈啊、哈、哈欸HPH 哦、呜啊、哈啊HPH 呼、呼HPH 嗯呜HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「要死了、要死掉了HPH 不行了HPH 不行HPH 嚯哦、嗯嚯哦哦哦HPH」","L"
				PRINTFORMDL 发出了象悲鸣一样的哭喊的%CALLNAME:TARGET%，筋疲力尽的靠在%CALLNAME:TARGET%身上。
				CALL K78_HPH_PRINT,@"「哈、哈……太，太厉害了HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;四重绝顶
		;-------------------------------------------------
		IF NOWEX:四重绝顶
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「啊啊，不行HPH 饶了我吧HPH 哈、恩啊啊啊HPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「已经不行了HPH太舒服了，要变成白痴了HPH脑袋都一片空白了HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「不要HPH 已经要去了HPH 太舒服了HPH 感觉要HPH 飞起来了HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;三重绝顶
		;-------------------------------------------------
		IF NOWEX:三重绝顶
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「啊啊HPH这个好厉害啊HPH啊啊啊啊HPH啊啊啊啊」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「恩、恩HPH不行啊HPH什么很厉害的要来了HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「嗯阿HPH 呜阿HPH 不行HPH 咿，咿咿～～HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;二重绝顶
		;-------------------------------------------------
		IF NOWEX:二重绝顶
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「啊，啊哈。哈HPH 要去了 去了啊啊HPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「哈啊、啊啊HPH 啊、啊、去了　要去了HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「嗯哈、哈啊、昂HPH %CALLNAME:MASTER%　好舒服啊HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;单部位の绝顶
		;-------------------------------------------------	
		IF NOWEX:Ｃ绝顶
			;超强绝顶Ｃ
			IF NOWEX:Ｃ绝顶 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「\@ TALENT:性别 & 2 ?小鸡鸡 #小豆豆 \@好舒服HPH 嗯HPH 嗯、嗯咿HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「嗯呼HPH 嗯嗯呜HPH 哦、\@ TALENT:性别 & 2 ?小鸡鸡 #小豆豆 \@要去了HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「\@ TALENT:性别 & 2 ?小鸡鸡 #小豆豆 \@好棒、好棒啊HPH 啊、哈、嗯呜HPH」","W"
				ENDSELECT
			;强绝顶Ｃ
			ELSEIF NOWEX:Ｃ绝顶 == 2
				CALL K78_HPH_PRINT,@"「呼啊　哈、咿、高潮了HPH」","W" 
			;绝顶Ｃ
			ELSE
				PRINTFORMW 「啊、嗯、哈那里、对、就是那里」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ｖ绝顶
			;超强绝顶Ｖ
			IF NOWEX:Ｖ绝顶 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「嗯阿、小穴好厉害HPH 不行了、啊啊HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「好厉害HPH小穴要去了HPH 嗯，库～～HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「好舒服HPH小穴咕啾咕啾的响着HPH」","W"
				ENDSELECT
			;强绝顶Ｖ
			ELSEIF NOWEX:Ｖ绝顶 == 2
				CALL K78_HPH_PRINT,@"「啊啊、啊、啊哈、里面、去了HPH」","W"
			;绝顶Ｖ
			ELSE
				PRINTFORMW 「嗯，啊，就是那里，要来了」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ａ绝顶
			;超强绝顶Ａ
			IF NOWEX:Ａ绝顶 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「嗯哦HPH 啊啊HPH 屁股，有什么厉害的东西，要来了！HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「恩，恩嘿HPH 屁穴HPH 屁穴要去了HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「啊，呀啊HPH 屁股洞要去了HPH 唔，啊啊啊！HPH」","W"
				ENDSELECT
			;强绝顶Ａ
			ELSEIF NOWEX:Ａ绝顶 == 2
				CALL K78_HPH_PRINT,@"「啊，屁股要，呜阿，啊哈HPH」","W"
			;绝顶Ａ
			ELSE
				PRINTFORMW 「啊、哼、哼、啊啊、啊啊」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ｂ绝顶
			;超强绝顶Ｂ
			IF NOWEX:Ｂ绝顶 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「啊哈HPH 好，好舒服HPH 胸部要去了HPH 」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「乳头酥酥麻麻的HPH 好厉害HPH 要去了，去了～～HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「胸部，胸部好热HPH 啊啊、胸部要高潮了HPH」","W"
				ENDSELECT
			;强绝顶Ｂ
			ELSEIF NOWEX:Ｂ绝顶 == 2
				CALL K78_HPH_PRINT,@"「唔唔唔　啊啊，胸部不行了，咿咿咿！HPH」","W"
			;绝顶Ｂ
			ELSE
				PRINTFORMW 「嗯，哈，嗯，啊嗯」
			ENDIF
			RETURN 1
		ENDIF
		[SKIPSTART]
		IF NOWEX:Ｍ绝顶
			;超强绝顶Ｍ
			IF NOWEX:Ｍ绝顶 == 3
				PRINTFORMW 
			;强绝顶Ｍ
			ELSEIF NOWEX:Ｍ绝顶 == 2
				PRINTFORMW 
			;绝顶Ｍ
			ELSE
				PRINTFORMW 
			ENDIF
		ENDIF
		[SKIPEND]
	ENDIF
ENDIF
RETURN 1
