				LOCALS:0 = 不变化
			CASE 1
				LOCALS:0 = 在生产前后会发生变化
			CASE 2
				LOCALS:0 = 无论是否怀孕，都会根据母乳体质发生变化
			CASEELSE
				LOCALS:0 = ※CFLAG:78:1001の值が不正※
			ENDSELECT

			SELECTCASE CFLAG:78:1002
			CASE 0
				LOCALS:1 = 无脱粪场景
			CASE 1
				LOCALS:1 = 有脱粪场景
			CASEELSE
				LOCALS:1 = ※CFLAG:78:1002の值が不正※
			ENDSELECT

			PRINTL
			PRINTFORMDL 现在的乳头变化的设定：%LOCALS:0%　　　现在的屎尿屁表现的设定：%LOCALS:1%
			PRINTL
			CALL ASK_M("变更乳头变化的设定",1,"变更屎尿屁表现的设定",1,"这样就好",1)
			SELECTCASE RESULT
			CASE 0
				IF CFLAG:78:1001 >= 2
					CFLAG:78:1001 = 0
				ELSE
					CFLAG:78:1001 += 1
				ENDIF
				PRINTL
				GOTO PRAYER_LOOP
			CASE 1
				CFLAG:78:1002 = !CFLAG:78:1002
				PRINTL
				GOTO PRAYER_LOOP
			CASE 2
				PRINTFORMDL 总觉得%CALLNAME:TARGET%的乳头%LOCALS:0%了。
				PRINTFORMD %CALLNAME:TARGET%
				IF CFLAG:78:1002 == 0
					PRINTFORMD 为了肛交，认真清洁屁股做好准备
				ELSE
					PRINTFORMD 即使在肛交前也保持屁股原样
				ENDIF
				PRINTFORMDW 了。
				PRINTL
			ENDSELECT
		ENDIF
		SETCOLOR LOCAL:1
	ENDIF
	IF K78_CONSCIOUS_AND_TOLERANT()
		SELECTCASE RAND:9
		CASE 1 TO 4
			PRINTFORMW 「许了什么愿望呢？」
		CASE 5 TO 8
			PRINTFORMW 「看起来很虔诚的样子呢」
		CASEELSE
			PRINTFORMW 「嗯唔… 你所拜的那个神，总感觉很可疑的样子…」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

