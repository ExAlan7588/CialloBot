				CALL K78_HPH_PRINT,@"「我也是女孩子呐，也是会在这方面努力的HPH」","W" 
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
			CASE 1
				SELECTCASE RAND:2
				CASE 1
					IF TALENT:思慕 || TALENT:炮友
						PRINTFORMDL 回过神来%CALLNAME:MASTER%和%CALLNAME:TARGET%的肩膀已经靠在一起了。
						PRINTFORMW 「不知不觉… 就，这么近了…」
					ELSE
						GOTO PHYSICAL_CONTACT_LOOP
					ENDIF
				CASEELSE
