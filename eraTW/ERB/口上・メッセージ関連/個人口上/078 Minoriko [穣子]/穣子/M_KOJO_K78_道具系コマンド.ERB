;45,肛用振动棒
;==================================================
@M_KOJO_MESSAGE_COM_K78_45
CALL M_KOJO_MESSAGE_COM_K78_45_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_45_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	IF K78_BASIC_CONDITION_IN_MATING()
		;装着
		IF TEQUIP:肛用振动棒
			IF TALENT:恋慕 || TALENT:爱欲
			
			ELSE
			
			ENDIF
		;解除
		ELSE
			IF 充填率(TARGET,2) >= 60 && !RAND:3
				IF CFLAG:1002
					IF ( TALENT:恋慕 || TALENT:爱欲 ) && !TCVAR:351 && !RAND:5
						PRINTFORMDL 额头上冒着汗忍受着便意的%CALLNAME:TARGET%终于到了界限，随着buchibuchi的激烈的声音喷出了精液混杂的粪便。
						CALL DIALOGUE_K78_DEFECATION_IN_MATING
					ELSEIF !TCVAR:351 && RAND:2
						PRINTFORMDL 或许是因为几乎溢出的精液流入了直肠、%CALLNAME:TARGET%的腹部咕噜咕噜的响着。
						PRINTFORMDL 虽然很想上厕所，但还是拼命忍耐着。
					ELSE
						PRINTFORMDL 大量的精液被灌进了%CALLNAME:TARGET%肚子里，和肠液交杂发出独特的气味的%TEXTR("精液/浓稠精液/子种汁/白浊液")%从%TEXTR("肛门/菊花/菊门")%垂了下来。
					ENDIF
				ELSE
					PRINTFORMDL 从%CALLNAME:TARGET%的肛门里将振动棒拔了出来，大量被灌进直肠里的%TEXTR("精液/浓稠精液/子种汁/白浊液")%慢慢地垂落下来。
				ENDIF
			ELSE
				IF CFLAG:1002 && ( TALENT:恋慕 || TALENT:爱欲 ) && !TCVAR:351 && !RAND:20
					PRINTFORMDL  %CALLNAME:TARGET%已经无法控制自己，伴随着巨大的屁声，从肛门里喷出了大量的粪物。
					CALL DIALOGUE_K78_DEFECATION_IN_MATING
				ELSEIF CFLAG:1002 && !RAND:5
					PRINTFORMDL 受到振动棒的振动刺激，肠道开始蠕动%CALLNAME:TARGET%%TEXTR("噗呲/噗呜/嘭/噗哔")%的放了个屁。
					CALL DIALOGUE_K78_FARTING_IN_MATING
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%的%TEXTR("肛门/菊花/菊门")%即使拔出\@ TALENT:MASTER:性别 & 2 ?%TEXTR("肉棒/阴茎/阴茎/肉棒")% # 道具 \@，仍然%TEXTR("张开着/一张一合/抽搐着")%，露出了%TEXTR("肛内的红肉/滑溜溜的粘膜")%。
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

