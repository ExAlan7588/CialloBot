		IF CFLAG:诶嘿嘿 == 2
			
		ELSEIF CHK_DATENOW(CFLAG:LOCAL:约会中) && CFLAG:MASTER:当前位置 != OMANEKIBEYA()
			
		ELSE
			IF BASE:情绪 >= 500 && BASE:理性 < 975
				;开心的回抱过来…
				IF TALENT:恋慕
					SELECTCASE RAND:2
					CASE 1
						CALL K78_HPH_PRINT,@"「我也会紧紧地抱着你HPH」","W"
					CASEELSE
						CALL K78_HPH_PRINT,@"「呵呵HPH总觉得安心下来了……HPH」","W"
					ENDSELECT
				;照れくさそうにしながらも、满更でもないようだ…
				ELSEIF TALENT:思慕
					SELECTCASE RAND:2
					CASE 1
						PRINTFORML 「唔，动不了了…」
						PRINTFORMDW 这么说着的%CALLNAME:TARGET%也轻轻抱了过来。
					CASEELSE
						PRINTFORMW 「又粘过来了… 哈，就没有一点羞耻感吗」
					ENDSELECT
				;有点害臊并ている…
				ELSE
