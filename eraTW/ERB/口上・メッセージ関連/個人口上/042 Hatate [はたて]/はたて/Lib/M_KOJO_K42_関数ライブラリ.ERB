;==================================================
;醉っぱらい度を数字で返す式中关数
;=0 清醒 =1 微醺 =2 醉酒 3=不省人事
@K42_DRUNK()
#FUNCTION
SIF BASE:42:酒气 <= 2
	RETURNF 0
SELECTCASE BASE:42:酒气
	;不省人事
	CASE IS > (MAXBASE:42:酒气 / 10) * 8
		RETURNF 3
	;醉酒
	CASE IS > (MAXBASE:42:酒气 / 10) * 5
		RETURNF 2
	;微醺
	CASE IS <= (MAXBASE:42:酒气 / 10) * 5
		RETURNF 1
ENDSELECT
