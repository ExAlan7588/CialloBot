;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K42_140
CALL M_KOJO_MESSAGE_COM_K42_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K42_140_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORML 「呃唔…呃、唔、嗯唔…！」
			PRINTFORML 「咳哈、咳咳、呕呃…」
			IF TALENT:恋慕 || TALENT:恋人
				PRINTFORML 「哈啊、哈啊…我说，别强迫我了…！」
				PRINTFORML 「用嘴巴帮你做也可以啊…」
				PRINTW 
			ELSE
				PRINTFORML 「稍微、别这么粗暴…！」
				PRINTW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		IF TALENT:恋慕 || TALENT:恋人
			IF ABL:Ｍ感觉 >= 5
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！　guo滋！　guo滋！
				PRINTFORML 「%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　mo%UNICODE(0x2665) *1%　mo%UNICODE(0x2665) *1%」
				PRINTFORML 即使蹭蹭地戳着极的喉咙深处、喉咙也很幸福地不断吸附着肉棒
				PRINTW 
			ELSEIF ABL:Ｍ感觉 >= 2
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢%UNICODE(0x2665) *1%　guo%UNICODE(0x2665) *1%」
				PRINTFORML （明明很粗暴、很难受的、却好舒服%UNICODE(0x2665) *1%　为什么！？）
				PRINTW 
			ELSE
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢啵！？　噢！？　噢！！　噢唉！」
				PRINTFORML 果拍打着大腿抗议，但是无视并直接顶到了她的喉咙深处
				PRINTW 
			ENDIF
			RETURN 1
		ELSEIF TALENT:思慕
			IF ABL:Ｍ感觉 >= 5
				PRINTFORML 啾啵！ 噗啾！ 噗啵！ 咕啵！ 咚！ 咚！
				PRINTFORML 「唔呃、咕唔、 呼~%UNICODE(0x2665) *1% 呼~%UNICODE(0x2665) *1%」
				PRINTFORML 果的表情痛苦和快乐交织在一起，紧紧地吸吮着肉棒
				PRINTW 
			ELSEIF ABL:Ｍ感觉 >= 2
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢、咕mo、嗯卟、嗯卟」
				PRINTFORML 极的表情因为痛苦而扭曲着、但是嘴角却微微地翘起了
				PRINTW 
			ELSE
				PRINTFORML 啾啵！　卟啾！　卟啵！　咕啵！
				PRINTFORML 「噢啵！？　噢！？　噢！！　噢唉！」
				PRINTFORML 果拍打着大腿抗议，但是无视并直接顶到了她的喉咙深处
				PRINTW 
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

