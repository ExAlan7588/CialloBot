;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ELSE
		ENDIF
	ENDIF
	;-------------------------------------------------
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;处女か无自觉非处女の场合
	IF LOCAL:1 && TFLAG:194
		;情欲が高い场合の分岐
		IF TFLAG:194 == 2
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;非处女
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
			RETURN 1
		;それ以外
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1