;==================================================
@M_KOJO_MESSAGE_SCOM_K42_1
CALL M_KOJO_MESSAGE_SCOM_K42_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K42_1_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:爱欲 || TALENT:炮友
		;强制舐阴←→舐阴は六九式目标对象外らしい
		;强制舐阴←→给对方口交も×
		;给对方口交←→口交はＯＫ
		;果扶她
		IF HAS_PENIS(TARGET) == 1
			;你もちんこついてる（男または扶她）
			IF HAS_PENIS(MASTER) == 1
				IF PREVCOM == 1;舐阴
					;果口交中
					IF TEQUIP:TARGET:口接触部位 == 1
						PRINTFORML 「呀呜！？　啊、真是的，现在舔的话，不行啦…」
						PRINTFORML 用舌头一口气从肉棒的根部到会阴舔来舔去，果的屁股颤抖了一下
						PRINTW 
					;果正在舔阴
					ELSE
						PRINTFORML 被眼前啪嗒啪嗒张合的雌穴所诱惑，悄悄地亲吻了上去
						PRINTFORML 「啊嗯～%UNICODE(0x2665) *1%　真是的，现在是轮到我了啦…%UNICODE(0x2665) *1%」
						PRINTW 
					ENDIF
				ELSEIF PREVCOM == 2;给对方口交
					;果口交中
					IF TEQUIP:TARGET:口接触部位 == 1
						SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「呀呜！？」
							PRINTFORML 咬住在眼前晃来晃去的果的肉棒，在口中颤抖了一下
							PRINTW 
						CASE 1
							PRINTFORM 在被果的口交技巧玩弄的时候，眼前
							SELECTCASE TALENT:TARGET:形状
							CASE 6,10;巨根、马并み
								PRINTFORM 出现了与那里格格不入的，大概有人类手腕那么粗的
							CASE 1,2;粗チン
								PRINTFORM 不知为何有些可爱的
							CASE 5;长疣的
								PRINTFORM 与主人截然相反的凶恶形状的
							CASEELSE
								PRINTFORM 与外表不符的健壮的
							ENDSELECT
							PRINTFORML 肉棒闯入了视线
							PRINTW 
							PRINTFORML 吧唧…
							PRINTFORML 「欸！？　真是的，你干什么啊，嗯嗯！！%UNICODE(0x2665) *1%」
							PRINTW 
							PRINTFORML 「要、要比比看谁先高潮吗…奉陪就是了」
							PRINTFORML 「我才不会输呢…%UNICODE(0x2665) *1%　哈姆、噗啵、噗啵…%UNICODE(0x2665) *1%」
							PRINTW 
						ENDSELECT
					;果正在舔阴
					ELSE
						PRINTFORM 在被果爱抚着雌穴的时候，眼前
						SELECTCASE TALENT:TARGET:形状
						CASE 6,10;巨根、马并み
							PRINTFORM 出现了与那里格格不入的，大概有人类手腕那么粗的
						CASE 1,2;粗チン
							PRINTFORM 不知为何有些可爱的
						CASE 5;长疣的
							PRINTFORM 与主人截然相反的凶恶形状的
						CASEELSE
							PRINTFORM 与外表不符的健壮的
						ENDSELECT
						PRINTFORML 肉棒闯入了视线
						PRINTW 
						PRINTFORML 噗噜…
						PRINTFORML 「呀！？　真是、真是的，顶端、我很敏感的啊，呜咿～%UNICODE(0x2665) *1%」
						PRINTFORML 「不要啦、好舒服～%UNICODE(0x2665) *1%　那样舔来舔去的，不要啊…%UNICODE(0x2665) *1%」
						PRINTW 
					ENDIF
				ELSEIF PREVCOM == 81;口交
					IF TEQUIP:MASTER:口接触部位 == 1
						PRINTFORML 「也让我舔舔你的啊…%UNICODE(0x2665) *1%　哈姆～%UNICODE(0x2665) *1%」
					ELSEIF TEQUIP:MASTER:口接触部位 == 6
						PRINTFORML 「不要这样，只舔那个地方嘛…%UNICODE(0x2665) *1%」
						PRINTFORML 「我也会做的…%UNICODE(0x2665) *1%　哈姆～%UNICODE(0x2665) *1%」
					ENDIF
					PRINTFORML 「嗯、嗯、嗯…%UNICODE(0x2665) *1%　硬硬的，好…%UNICODE(0x2665) *1%」
					PRINTW 
				ELSE
				ENDIF
			;你は女。对応指令がないよ！
			ELSE
			ENDIF
		;果女
		ELSE
			IF PREVCOM == 1;舐阴
				SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「啊、真是的，现在是我让你舒服啊…啊%UNICODE(0x2665) *1%　嗯%UNICODE(0x2665) *1%」
					PRINTFORML 「嗯啾%UNICODE(0x2665) *1%　哈啊%UNICODE(0x2665) *1%　啊啊%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 1
					PRINTFORML 嘟啾、啾啪、啾噜噜…
					PRINTFORML 「嗯%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼～～～～%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 2
					PRINTFORML 「啊、那样的话、就不能好好地、让你舒服了…嗯%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 3
					PRINTFORML 「啊、那种地方、不要扩张啊！」
					PRINTFORML （不要啊…被%MASTERNAME:42%全部看到了…%UNICODE(0x2665) *1%）
					PRINTW 
					PRINTFORML 「口正闲着，还不是你做了那种事情…嗯%UNICODE(0x2665) *1%　嗯嗯%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTFORML 「湿淋淋之类的、不用说出来啊…笨蛋%MASTERNAME:42%…%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 4
					PRINTFORML 在眼前，果肉感十足的屁股摇晃着…
					PRINTFORML 在那正中央，果那色情的肉穴，散发着雌性的气味，抽动着，像是渴望着什么一样地开合着
					PRINTW 
					PRINTFORML 「嗯嗯！？　啊、笨蛋、不要用手指、掰开啊、看到肉壶的话就糟了」
					PRINTFORML 用双手将阴唇扒开，随着“滋”的一声粘着声，小穴口暴露了出来
					PRINTFORML 果中断了口中的服侍，慌忙抗议
					PRINTW 
					PRINTFORML 被说了不要，当然不可能回答“我知道了”。无视抗议，将舌尖拧入小穴口
					PRINTFORML 「呀%UNICODE(0x2665) *1%　舌头、进来了～～～～%UNICODE(0x2665) *1%」
					PRINTW 
					PRINTFORML 「不要舔肉壶～～%UNICODE(0x2665) *1%　小弟弟、没法舔了、啊%UNICODE(0x2665) *1%　呀%UNICODE(0x2665) *1%」
					PRINTFORML 「你、你这家伙…哈姆～%UNICODE(0x2665) *1%　唔噗、唔噗～%UNICODE(0x2665) *1%　啾啵、啾啵、啾啵%UNICODE(0x2665) *1%」
					PRINTFORML 或许是燃烧起了对抗意识，果以比刚才更加热情的舌头，重新开始了口交
					PRINTW 
					PRINTFORML 这样下去，不快点让她高潮的话，我好像会先射出来…
					PRINTFORML （快点高潮，快点高潮啊～%UNICODE(0x2665) *1%　我已经、要高潮了哟～%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%）
					PRINTW 
				ENDSELECT
			ELSEIF PREVCOM == 81;口交
				SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「一直被你摆布的话可不行、我要报仇…%UNICODE(0x2665) *1%　嗯噗…%UNICODE(0x2665) *1%」
					PRINTFORML 「哈、嗯%UNICODE(0x2665) *1%　啾噗、啾噗…%UNICODE(0x2665) *1%　
				CASE 1
					PRINTFORML 嘟啾、啾啪、啾噜噜…
					PRINTFORML 「嗯%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼～～～～%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 2
					PRINTFORML 「哈、哈…%UNICODE(0x2665) *1%　嗯…你的、看起来很痛苦的样子…哈姆%UNICODE(0x2665) *1%」
					PRINTFORML 
					PRINTW 
				CASE 3
					PRINTFORML 「哈啊%UNICODE(0x2665) *1%　啊啊%UNICODE(0x2665) *1%」
					PRINTFORML 「狡猾，只有%MASTERNAME:42%…哈姆%UNICODE(0x2665) *1%」
					PRINTFORML 「嗯%UNICODE(0x2665) *1%　呼呜%UNICODE(0x2665) *1%　嗯嗯嗯%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
