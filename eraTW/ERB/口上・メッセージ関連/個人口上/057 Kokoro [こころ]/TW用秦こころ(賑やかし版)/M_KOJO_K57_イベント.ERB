	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF FLAG:时间停止
		ELSEIF CFLAG:318 == 1
			PRINTFORMW 
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%，正好想看看你～」
					PRINTFORMW 心露出开心的笑容说道
				CASE 0
					PRINTFORML 「喔喔，来的正好，%CALLNAME:MASTER%」
					PRINTFORMW 「正好现在没什么事」
				CASEELSE
					PRINTFORML 「喔喔～%CALLNAME:MASTER%，见到你我很开心」
					PRINTFORMW 「所以，开心时的面具…」
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「%CALLNAME:MASTER%，是来找我的吗？」
					PRINTFORMW 
				CASE 0
					PRINTFORML 「欢迎光临，见到你我很开心」
					PRINTFORMW 「那个，开心时的面具在…」
				CASEELSE
					PRINTFORMW 「啊，%CALLNAME:MASTER%，怎么了？」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「嗯？ 怎么了？」
					PRINTFORMW 
				CASE 0
					PRINTFORMW 「啊？ 怎么了吗？」
				CASEELSE
					PRINTFORMW 「喔喔，意料之外的偶遇呢——」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
