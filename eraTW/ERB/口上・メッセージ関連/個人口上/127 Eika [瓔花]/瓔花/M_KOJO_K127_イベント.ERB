				ENDIF
			PRINTFORMW
			;许可しない
			CASEELSE
				CFLAG:127:1002 = -1
				;PRINTFORML 变更されません
				PRINTFORML 未变更
				PRINTFORMW
			ENDSELECT
ENDIF

RETURN 1

