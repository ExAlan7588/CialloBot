;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな变量などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:Ｃ绝顶 NOWEX:Ｖ绝顶 NOWEX:Ｂ绝顶 NOWEX:Ａ绝顶 NOWEX:Ｍ绝顶
;各部位の绝顶强度。まず0~3が割り当てられ、多重绝顶により最大16倍される。つまり单部位なら最大3、5重绝顶で最大48
;NOWEX:TotalEX
;多重绝顶补正を接受前の各部位绝顶强度の合计值。1~15。このテンプレにおける分岐の基准。

;TEQUIP:50
;Ｖに谁の竿が入ってるか。TEQUIP:50 = MASTERで你が插入中となる。
;TEQUIP:51
;Ａに谁の竿が入ってるか。TEQUIP:51 = MASTERで你が插入中となる。
;NOWEX:射精
;射精量、最大3。たとえばIF NOWEX:MASTER:射精 > 1だと、你が大量射精以上した场合となる。
;SYNCED_ORGASM(TARGET)
;相手と你が同时に绝顶した场合真となる。绝顶の部位は问わない。

;-------------------------------------------------
;绝顶时の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K63
#DIM TotalEX


;-------------------------------------------------
;喷乳
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:喷乳
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精した
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;绝顶
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF NOWEX:MASTER:射精
	IF FIRSTTIME("射精前兆")
		PRINTFORMW （总感觉鸡鸡一跳一跳的……这、莫非是……）
	ELSEIF (TEQUIP:50 == MASTER || TEQUIP:51 == MASTER)
	;插入中
		IF TALENT:63:自己爱 >= 0 && NOWEX:TotalEX > 0 && ABL:欲望 >= 10 && ABL:亲密 >= 12
			IF !RAND:2 && NOWEX:TotalEX >= 5
				CALL HPH_PRINT,@"「来吧HPH 　%CALLNAME:MASTER%的种子、满满的给我吧HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「%TEXTR("嗯/啊/啊恩")%HPH　%CALLNAME:MASTER%也要去了吗？　一起去吧HPH」","W"
			ENDIF
		ELSEIF RAND:2
			CALL HPH_PRINT,@"「%TEXTR("嗯/啊")%…在里面%TEXTR("大大的/一跳一跳的/膨胀起来")%……HPH」","W"
		ELSE
			CALL HPH_PRINT,@"（%TEXTR("嗯……HPH　动的好快哟……HPH/像那样拼命地晃动着腰……　是想要我也疯狂起来吗……HPH")%）","W"
		ENDIF
	ELSEIF TALENT:63:自己爱 < 0
		PRINTFORMW （呜呜、还没出来吗……）
	ELSE
		CALL HPH_PRINT,@"（%TEXTR("明明就要出来了却还在忍耐着…好可爱呀……HPH/还差一点点吗？出来吧HPH　快出来吧HPH")%）","W"
	ENDIF
ENDIF
;合计绝顶数で分岐
;これだけではどの部位が绝顶したかは区别されないため、书き分けたければ任意で分岐を追加されたし。
IF LOCAL
	IF SHIRAHU(TARGET)
		TCVAR:361 = 1
	ENDIF
	IF NOWEX:TotalEX
	SELECTCASE NOWEX:TotalEX + TCVAR:发情
		;强
		;潜伏中と圧抑が取れるまでは堪える
		CASE IS >= 6
			IF GETBIT(FLAG:潜伏,0) || TALENT:自己爱 < 0
				CALL HPH_PRINT,@"「%TEXTR("嗯HPH　嗯！HPH　嗯HPH　嗯~HPH/呜HPH　呼啊HPH　咕呜HPH　嗯咕HPH")%」","L"
				CALL HPH_PRINT,@"「%TEXTR("嗯嗯哈嗯/嗯嗯HPH　哈哇呜/嗯咕……")%～～～～～～～～～～～～～～HPH」","W"
				CALL HPH_PRINT,@"「呼ーー……HPH　呼ーー……HPH　呼ーー……HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「%TEXTR("嗯哈HPH　嗯哦HPH　喔喔HPH/　嗯哦HPH　啊噢HPH　喔喔HPH")%HPH」","W"
				CALL HPH_PRINT,@"「%TEXTR("哦…HPH　喔喔～～～～～～～～HPH/啊哦呜～～～～～～～～～HPH")% 」","W"
			ENDIF
			TCVAR:360 += 5
		;中
		CASE 3 TO 5
			IF GETBIT(FLAG:潜伏,0) || TALENT:自己爱 < 0
				CALL HPH_PRINT,@"「%TEXTR("嗯～～～/呼～～～～")%HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「%TEXTR("啊HPH　啊HPH　咿呀HPH　啊啊HPH/咿嗯HPH　啊啊HPH　啊哈呜HPH")%」","W"
				CALL HPH_PRINT,@"「%TEXTR("啊啊～～～～HPH　啊～～～～～HPH/啊HPH　啊HPH　啊啊哦～～～～～～～～HPH　")%」","W"
			ENDIF
			TCVAR:360 += 1
		CASE 2
			IF GETBIT(FLAG:潜伏,0) || TALENT:自己爱 < 0
				CALL HPH_PRINT,@"「………%TEXTR("嗯嗯呐/哈咕/呼ー呜 呼ー")%HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「%TEXTR("嗯/啊/啊嗯")%HPH　%TEXTR("嗯/啊/啊嗯/啊嗯哦/呀呜")%HPH　%TEXTR("嗯/啊喔/呀呜/啊嗯/咕呜")%HPH」","L"
				CALL HPH_PRINT,@"「～～～～～～～～～HPH」","W"
			ENDIF
		;弱
		CASE 1
			IF GETBIT(FLAG:潜伏,0) || TALENT:自己爱 < 0
				CALL HPH_PRINT,@"「………%TEXTR("嗯/啊/啊嗯")%HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「%TEXTR("嗯嗯/啊啊/啊嗯/嗯哈啊")%HPH」","W"
				CALL HPH_PRINT,@"「好舒服……HPH」","W"
			ENDIF
	ENDSELECT
	ENDIF
ENDIF
IF TALENT:Ｂ感度 < 0 && NOWEX:Ｂ绝顶 >= 2
	CALL HPH_PRINT,@"「原来欧派也能像这么舒服的啊……HPH」","L"
	CALL COLORMESSAGE(@"【Ｂ钝感】消失",C_YELLOW,1)
	TALENT:Ｂ感度 = 0
ENDIF
