;==================================================
;一日の结束时(AFTERTRAニーの前)、认识がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K63_31(ARG,ARG:1)
;口上主はTARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:自慰暴露で分岐入れてもよさそう
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;今日会っていない
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG > 1 && !RAND:4
		IF ARG >= 7
			IF TALENT:[[影狼]]:恋慕
				CALL EVENTEND_TSUBUYAKI
				CALL PRINT_FACE, [[影狼]], "通常", "服", "", ""
				PRINTFORML 「总觉得已经很久没有和%CALLNAME:MASTER%见到了呢…」
				PRINTFORMW 「好想见见他啊ー…」
				CFLAG:[[影狼]]:1004 ++
			ELSEIF TALENT:[[影狼]]:思慕 && !RAND:2
				CALL EVENTEND_TSUBUYAKI
				CALL PRINT_FACE, [[影狼]], "通常", "服", "", ""
				PRINTFORMW 「虽说有段时间没看见了、不知%CALLNAME:MASTER%最近如何呢」
				CFLAG:[[影狼]]:1004 ++
			ELSEIF ABL:[[影狼]]:亲密 >= 3 && !RAND:3
				CALL EVENTEND_TSUBUYAKI
				CALL PRINT_FACE, [[影狼]], "通常", "服", "", ""
				PRINTFORMW 「说起来最近好想没怎么见到%CALLNAME:MASTER%那个家伙呢」
			ENDIF
		ELSEIF !RAND:2
			CALL EVENTEND_TSUBUYAKI
			CALL 草の根と杂谈
		ELSE
			CALL KAGEROU_LIFE
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:思慕 && !RAND:5
			PRINTFORML 「啊ー唉」
			PRINTFORMW 「要是%CALLNAME:MASTER%是狼男的话就好了呢」
		ENDIF
		RETURN 1
	ENDIF
	IF !RAND:20 && TALENT:恋慕 && !TALENT:妊娠 && !TALENT:育儿中
		PRINTFORML 「唔～～～～嗯……」
		PRINTFORMW 影狼在镜子前碎碎念着
		IF !RAND:3
			PRINTFORMW 「明明想跟%CALLNAME:MASTER%聊聊天的但被刻意避开了、感觉有点怪怪的呢……」
		ELSE
			PRINTFORML 「最近花在打扮上的时间明显变多了、感觉有点违反我的原则了诶……」
			PRINTFORMW "时尚的打扮只是为了让自己愉快"、这是她一直以来的自我坚持
			PRINTFORMW 「想要尽可能让%CALLNAME:MASTER%看见自己漂亮的样子也算是为了我自身的考量吗？但怎么想都有动机不纯的感觉……」
		ENDIF
		PRINTFORMW 「嗯嗯～～～～……」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

