;-------------------------------------------------

;==================================================
;赠り物イベント
;==================================================
@M_KOJO_EVENT_K63_GIFT(ARG, GIFT_ID, 评价点, GIFT_NAME, SENSE)
#DIM GIFT_ID
#DIM 评价点
#DIMS GIFT_NAME
#DIMS SENSE
;---------------------------------------------------------------------------------------------------
;评价点       赠り物の评价点（1～999）,口上主があげた场合でも口上主の评价点
;GIFT_NAME    赠り物の名称
;SENSE        赠り物に设定された属性。STRCOUNT(DISH_TASTE, "ほげほげ/")を使用と特定の属性を判定できます。
;GIFT_ID      これ单体では使いません。以下の式内函数を使用とデータを抽出できます（こだわりたい人用）
;　GET_GIFTDATA(GIFT_ID, "回数")    贳った（あげた）回数
;　GET_GIFTDATA(GIFT_ID, "得点")    得点（1～999）,口上テンプレ内では用意されてます。
;　GET_GIFTDATA(GIFT_ID, "日期")    贳った（あげた）日期（1～9999）
;　GET_GIFTDATA(GIFT_ID, "场所")    买った场所（约会の行き先）
;　GET_GIFTDATA(GIFT_ID, "形容词")  形容词番号（赠り物_形容词.erb参照）
;　GET_GIFTDATA(GIFT_ID, "本体")    本体番号（赠り物_本体.erb参照）
;--------------------------------------------------------------------------------------------------
;约会归りに口上主が你に赠り物を渡す（陷落や好感度に关わらず亲密5以上から发生）
;评价点は701～999（渡す角色本人の评价）
;--------------------------------------------------------------------------------------------------
IF ARG == 1
	;PRINTFORMW 「%GIFT_NAME%をあげるから大事に使て」
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物を大事にする（700点以上）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 2
	PRINTFORML 「唔～～～嗯……」
	PRINTFORMW 影狼前后摆弄着%GIFT_NAME%、从各个角度或近或远欣赏似的看着礼物
	PRINTFORML 「%TEXTR("果然很棒呢/真是漂亮呀/让人心中一紧呢/让人沉迷其中呢")%」
	IF STRCOUNT(GIFT_NAME, "(狼の|满月の)")
		PRINTFORMW 「简直是“就是这个！”…程度的好选择呀」
	ELSEIF STRCOUNT(GIFT_NAME, "(爪|牙)")
		PRINTFORMW 「要是我自己也有这样的就好了呢」
	ELSEIF STRCOUNT(GIFT_NAME, "(朴素|艺术的)") && RAND:3
		PRINTFORMW 「这个礼物、让人有种能明白送礼人心情的感觉呢」
	ELSEIF STRCOUNT(GIFT_NAME, "(别致|纤细)") && RAND:3
		PRINTFORMW 「真是不得了的好做工呢」
	ELSE
		PRINTFORMW 「%CALLNAME:MASTER%还真是%TEXTR("很懂行/很有眼光/有很棒的品味")%呢」
	ENDIF
	K63_WADAI:0 = 赠り物
	K63_WADAI:1 '= GIFT_NAME
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物をお气に入りにする（↑の大事にするから续き）（700点以上かつ最高得点）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 3
	;IF STRCOUNT(SENSE, "穿戴/")
	;	PRINTFORMW 「明日からこれを付けて行こうと」
	;ELSE
	;	PRINTFORMW 「明日からこれを持て行こうと」
	;ENDIF
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物をとりあえず保管する（400～699点）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 4
	PRINTFORML 「唔～～～嗯……」
	PRINTFORMW 影狼前后摆弄着%GIFT_NAME%、从各个角度或近或远欣赏似的看着礼物
	PRINTFORM 「虽说也很不错啦、但
	IF STRCOUNT(GIFT_NAME, "(威严|壮大)")
		PRINTFORMW 总觉得是%TEXTR("谜之庄严/令人敬畏")%还是该怎么说呢……」
	ELSEIF STRCOUNT(SENSE, "夸张/")
		PRINTFORMW 稍微有点%TEXTR("华而不实/太华丽/晃眼/亮丽过头")%了呢」
	ELSEIF STRCOUNT(GIFT_NAME, "(可爱的|洛丽塔)")
		PRINTFORMW 稍微有点孩子气了呢……」
	ELSEIF STRCOUNT(SENSE, "金属/")
		PRINTFORMW 这种质感该怎么说呢……」
	ELSE
		PRINTFORMW %TEXTR("不能让人眼前一亮呢/不太合我的兴趣呢")%」
	ENDIF
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物をこっそり质に入れる（400点未满）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 5
	PRINTFORML 「唔～～～嗯……该怎么处理呢、这东西」
	RETURN 1
ENDIF
RETURN 1

