;-------------------------------------------------
;绝顶
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合计绝顶数で分岐
;これだけではどの部位が绝顶したかは区别されないため、书き分けたければ任意で分岐を追加されたし。
IF LOCAL
	;-------------------------------------------------
	;五重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:五重绝顶 && LOCAL:1
		IF TCVAR:10:烂醉 == 1
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「呜呜呜……要飞起来了……飞起来了……」
				PRINTFORML 「飞的好高……要掉下来了……呜呜」
				PRINTFORML 「要死掉了……」
				PRINTFORML 灵梦含糊不清的说着醉话
			ELSEIF TALENT:炮友 || TALENT:爱欲
				PRINTFORML 「咕呜呜呜呜……」
				PRINTFORML 「咿呀……」
				PRINTFORML 「不要……再去了……」
			ELSE
				PRINTFORML 「咿呀……」
				PRINTFORML 「好痒……好麻……好难受……」
				PRINTFORML 「不要……好难受……」
				PRINTFORML 灵梦的脑袋有些空空
			ENDIF
		ELSE
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 
			ELSEIF TALENT:炮友 || TALENT:爱欲
				PRINTFORML 
			ELSE
				PRINTFORML 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:四重绝顶
		SELECTCASE NOWEX:四重绝顶
			;Ｃ＆Ｖ＆Ａ＆Ｂ绝顶
			CASE 1
			;Ｃ＆Ｖ＆Ａ＆Ｍ绝顶
			CASE 2
			;Ｃ＆Ｖ＆Ｂ＆Ｍ绝顶
			CASE 3
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ绝顶
			CASE 4
			;Ｖ＆Ａ＆Ｂ＆Ｍ绝顶
			CASE 5
		ENDSELECT
		PRINTW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;三重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:三重绝顶
		SELECTCASE NOWEX:三重绝顶
			;Ｃ＆Ｖ＆Ａ绝顶
			CASE 1
			;Ｂ＆Ｖ＆Ａ绝顶
			CASE 2
			;Ｃ＆Ｂ＆Ａ绝顶
			CASE 3
			;Ｃ＆Ｖ＆Ｂ绝顶
			CASE 4
			;Ｃ＆Ｖ＆Ｍ绝顶
			CASE 5
			;Ｂ＆Ｖ＆Ｍ绝顶
			CASE 6
			;Ａ＆Ｖ＆Ｍ绝顶
			CASE 7
			;Ｃ＆Ｂ＆Ｍ绝顶
			CASE 8
			;Ａ＆Ｂ＆Ｍ绝顶
			CASE 9
			;Ｃ＆Ａ＆Ｍ绝顶
			CASE 10
		ENDSELECT
		PRINTW
		RETURN 1
		ENDIF
	;-------------------------------------------------
	;二重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:二重绝顶
		SELECTCASE NOWEX:二重绝顶
			;Ｃ＆Ｖ绝顶
			CASE 1
			;Ｃ＆Ａ绝顶
			CASE 2
			;Ｖ＆Ａ绝顶
			CASE 3
			;Ｃ＆Ｂ绝顶
			CASE 4
			;Ｖ＆Ｂ绝顶
			CASE 5
			;Ａ＆Ｂ绝顶
			CASE 6
			;Ａ＆Ｍ绝顶
			CASE 7
			;Ｖ＆Ｍ绝顶
			CASE 8
			;Ｃ＆Ｍ绝顶
			CASE 9
			;Ｂ＆Ｍ绝顶
			CASE 10
		ENDSELECT
	ELSEIF NOWEX:Ｃ绝顶
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ｖ绝顶
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ａ绝顶
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ｂ绝顶
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ELSEIF NOWEX:Ｍ绝顶
		IF TCVAR:10:烂醉 == 1
		ELSEIF CFLAG:MASTER:恶作剧 == 2
		ELSE
		ENDIF
	ENDIF
ENDIF
RETURN 1
