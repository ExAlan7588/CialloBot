	ENDIF
	RETURN 1
;--------------------------------------------------------------------------------------------------
;一日の终わりに今日获得的礼物を大事にする（700点以上）
;--------------------------------------------------------------------------------------------------
ELSEIF ARG == 2
	IF STRCOUNT(SENSE, "(地狱の|狂气の|疯狂な|骨|妖术)")
		CALL M_KOJO_COLOR_K111_HECA
		PRINTFORML 「皮斯、这是哪儿来的，超好看」
