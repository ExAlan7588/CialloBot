;心情不快判定
@M_KOJO_K111_FUKIGEN(ARG)
#FUNCTION
SIF TALENT:ARG:心情 == -1
	RETURNF 1
SIF CFLAG:ARG:勃然大怒
	RETURNF 1
SIF MARK:ARG:反抗刻印 >= 3
	RETURNF 1
RETURNF 0

;おうち判定
;1=克劳恩皮丝的房间,2=纯狐邸,3=入海口
@M_KOJO_K111_HOUSE(ARG)
#FUNCTION
IF MAIN_MAP == 10
	IF ARG == CFLAG:[[克劳恩皮丝]]:初始位置
		RETURNF 1
	ELSEIF ARG == 1016
		RETURNF 3
	ELSEIF INRANGE(ARG, 1060, 1072)
		RETURNF 2
	ENDIF
ELSE
	SIF ARG == OMANEKIBEYA() || ARG == CFLAG:[[克劳恩皮丝]]:初始位置
		RETURNF 1
ENDIF

;纯狐邸に住んでる判定,参数无
@M_KOJO_K111_JUNKO_KURASHI
#FUNCTION
;月じ呀ないなら×
SIF MAIN_MAP != 10
	RETURNF 0
;一応二段阶で设定しておく
SIF INRANGE(CFLAG:MASTER:初始位置, 1060, 1072)
	RETURNF 1
SIF GROUPMATCH(CFLAG:MASTER:初始位置, CFLAG:[[克劳恩皮丝]]:初始位置, CFLAG:[[纯狐]]:初始位置, CFLAG:[[赫卡提亚]]:初始位置)
	RETURNF 1

;地上判定
@M_KOJO_K111_EARTH(ARG)
#FUNCTION
;神社～森
IF ARG < 600
	RETURNF 1
;冥界
ELSEIF INRANGE(ARG, 600, 700)
	IF MAIN_MAP == 6
		;中有之道～积石冢
		IF INRANGE(ARG, 601, 603)
			RETURNF 1
		;八云邸
		ELSEIF INRANGE(ARG, 630, 639)
			RETURNF 1
		ENDIF
	ELSE
		;中有之道、迷途之家
		SIF GROUPMATCH(ARG, 610, 630)
			RETURNF 1
	ENDIF
;山麓
ELSEIF INRANGE(ARG, 700, 800)
	RETURNF 1
;山顶
ELSEIF INRANGE(ARG, 800, 900)
	IF MAIN_MAP == 8
		;入り口～守矢神社
		SIF INRANGE(ARG, 801, 825)
			RETURNF 1
	ELSE
		;九天瀑布～守矢神社本殿
		SIF INRANGE(ARG, 810, 850)
			RETURNF 1
	ENDIF
;地底～月
ELSEIF INRANGE(ARG, 900, 1100)
	RETURNF 0
ENDIF

;你の印象
@M_KOJO_K111_MASTER_IMPRESSION
#FUNCTIONS
VARSET LOCALS
VARSET LOCAL
SIF CFLAG:[[克劳恩皮丝]]:既成事实
	LOCALS:1 = 萝莉控
SIF TALENT:MASTER:清扫中毒 || ABL:MASTER:清扫技能 >= 6
	LOCALS:2 = 清扫中毒者
SIF LEARNED_MENU:[[克劳恩皮丝]] >= 100
	LOCALS:3 = 料理人
SIF CFLAG:[[克劳恩皮丝]]:弹幕胜负胜利
	LOCALS:4 = 弹幕强者
SIF CFLAG:[[克劳恩皮丝]]:斗虫直接胜利
	LOCALS:5 = 昆虫王
SIF TALENT:MASTER:恋人 && !GROUPMATCH(TALENT:MASTER:恋人, [[克劳恩皮丝]], [[赫卡提亚]], [[纯狐]])
	LOCALS:6 = 万人迷
SIF ABL:MASTER:教养 >= 4
	LOCALS:7 = 聪明人
SIF ABL:MASTER:话术技能 >= 5
	LOCALS:8 = 饶舌
SIF MONEY >= 1000000
	LOCALS:9 = 富哥
FOR LOCAL, 1, 10
	IF LOCALS:LOCAL != ""
		LOCAL:1 = 1
		BREAK
	ENDIF
NEXT
;何も特征が无い
SIF !LOCAL:1
	LOCALS:10 = 平凡

LOCAL = 0
WHILE LOCALS == ""
	LOCAL = RAND:10 + 1
	SIF LOCALS:LOCAL != ""
		LOCALS = %LOCALS:LOCAL%
WEND
RETURNF LOCALS

