	IF ARG == 65
		PRINTFORMW 「呼、%CALLNAME:MASTER%的肉棒、就要这样插进来了哦……%UNICODE(0x2764)%」
	;それ以外
	ELSE
		PRINTFORMW 「唔、%CALLNAME:MASTER%的欧金金，来撑开我的下流小穴了%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ENDIF
ELSE
	SELECTCASE RAND:5
	CASE 0
		PRINTFORMW 「嗯%UNICODE(0x2764)% 嗯嗯%UNICODE(0x2764)% 啊嗯%UNICODE(0x2764)% 啊嗯%UNICODE(0x2764)% 」
	CASE 1
		PRINTFORMW 「%CALLNAME:MASTER%%UNICODE(0x2764)% %CALLNAME:MASTER%%UNICODE(0x2764)% 啊%UNICODE(0x2764)%」
	CASE 2
		PRINTFORMW 「嗯%UNICODE(0x2764)% 啊嗯%UNICODE(0x2764)% 好舒服%UNICODE(0x2764)%」
		PRINTFORMW 「再来%UNICODE(0x2764)% 狠狠干我吧%UNICODE(0x2764)%%UNICODE(0x2764)%」
	CASE 3
		PRINTFORMW 「啊%UNICODE(0x2764)% 啊嗯%UNICODE(0x2764)% 哈啊%UNICODE(0x2764)% 好激烈%UNICODE(0x2764)% 咦啊%UNICODE(0x2764)%」
	CASE 4
		PRINTFORMW 「啊啊%UNICODE(0x2764)% 咦咦%UNICODE(0x2764)% %CALLNAME:MASTER%的大肉棒%UNICODE(0x2764)%」
		PRINTFORMW 「好厉害%UNICODE(0x2764)% 好棒%UNICODE(0x2764)% 啊嗯%UNICODE(0x2764)%」
	ENDSELECT
ENDIF

