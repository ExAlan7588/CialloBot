;==================================================
;5,手淫口交
;==================================================
@M_KOJO_MESSAGE_SCOM_K111_5
CALL M_KOJO_MESSAGE_SCOM_K111_5_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K111_5_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;口交と同じ
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	IF FLAG:时间停止
		RETURN 1
	ELSEIF NOWEX:MASTER:11
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「嗯%UNICODE(0x2764)% 嗯%UNICODE(0x2764)% 咕嗯……是快要去了吧」
			PRINTFORMW 「可以哦、就这样射到我嘴巴里%UNICODE(0x2764)% 嗯嗯%UNICODE(0x2764) * 2% 唔啊～～～%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「嗯%UNICODE(0x2764)% 嗯%UNICODE(0x2764)% 啊、好多……先走汁灌到嘴巴里了……%UNICODE(0x2764)%」
			PRINTFORMW 「是不是快要去了？ 就这样……射进我的喉咙深处吧……咕、咕、啾、嗯～～%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「这样子、舒服吗？%UNICODE(0x2764)% 被我舔弄%UNICODE(0x2764)%」
			PRINTFORMW 「阿姆…舔舔……滑滑的%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「啊嗯%UNICODE(0x2764)%」
			PRINTFORMW 「prpr……滑溜溜滑溜溜%UNICODE(0x2764)% 这样做舒服吗？ prprprpr……%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「嗯姆、啾啾啾……%UNICODE(0x2764)%」
			PRINTFORMW 「%CALLNAME:MASTER%的肉棒，好好吃…%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
