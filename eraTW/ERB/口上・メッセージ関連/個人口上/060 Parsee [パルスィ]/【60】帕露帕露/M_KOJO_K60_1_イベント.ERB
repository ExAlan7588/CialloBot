@M_KOJO_Axl_MESSAGE_COM_KX_SUIKA(ARGS)
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL:1 = 0
;-------------------------------------------------
IF LOCAL:1
	SELECTCASE ARGS
	CASE "冲突"
