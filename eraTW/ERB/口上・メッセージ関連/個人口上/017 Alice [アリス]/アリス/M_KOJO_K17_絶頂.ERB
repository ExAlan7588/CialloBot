;奴隷の绝顶时の口上
;NOWEX:0～4 CVABM绝顶
;NOWEX:6 二重绝顶
;NOWEX:7 三重绝顶
;NOWEX:8 四重绝顶
;NOWEX:9 五重绝顶
;NOWEX:10 喷乳
;NOWEX:11 射精
;NOWEX:12 放尿
;随意组み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K17
#DIM DYNAMIC iType = 0
#DIM DYNAMIC iSub_TARGET = 0
#DIM DYNAMIC iABLnum,2 = -1,-1
#DIM DYNAMIC iCount = 0
VARSET LOCALS


SIF FLAG:时间停止
	RETURN 1

;喘ぎタイプの决定
iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ

;强制变换
;愤怒つく すでに强行
SIF K17_CHECK_ANGER_COM(TARGET) || TCVAR:强行
	iType = K17C_受け_强行系


;-------------------------------------------------
;喷乳
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:喷乳
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;绝顶
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;绝顶部位选出
	FOR i,4,-1,-1
		IF NOWEX:i
			iABLnum:iCount = i
			iCount++
		ENDIF
		SIF iCount >= 2
			BREAK
	NEXT
		
	;他TARGETを取得
	iSub_TARGET = K17_GET_TARGETS_ID()
	IF iSub_TARGET
		IF CFLAG:诶嘿嘿
			LOCALS += "被@%K17_CNAME(iSub_TARGET)%看见的同时、\n　"
		ELSEIF K17_GET_FRIENDS(iSub_TARGET)
			;友人
			LOCALS += "明明被@%K17_CNAME(iSub_TARGET)%看见了、\n　"
		ELSE
			LOCALS += @"明明被别的女孩子看见了\n　"
		ENDIF
	ENDIF
	;插入系
	IF (NOWEX:Ｖ绝顶 && TEQUIP:Ｖ性交 != -1) || (NOWEX:Ａ绝顶 && TEQUIP:Ａ性交 != -1)
		IF NOWEX:PLAYER:射精
			IF iType & K17C_喘ぎF_袭い && !(iType & K17C_喘ぎF_攻め)
				PRINTFORML 「%K17_RS("不行啊/住手啊")%%K17_喘ぎ_繋ぎ(iType,ABL:K17C_部位V)%%K17_RS("中出/射在里面")%に不行啊、%K17_RS("射到外面/快拔出来")%%K17_喘ぎ声(iType,ABL:Ｖ感觉,-1,1)%」
			ELSE
				PRINTFORML 「%K17_RS("射吧/射出来")%%K17_喘ぎ_繋ぎ(iType,ABL:K17C_部位V)%一起去……呐？　呐？　%K17_喘ぎ声(iType,ABL:Ｖ感觉,-1,1)%」
			ENDIF
		ENDIF
		
		;插入部位と绝顶の合致
		LOCALS += @"%K17_CNAME()%的%K17_CALL_G(TEQUIP:Ｖ性交!=-1 ? TEQUIP:Ｖ性交#TEQUIP:Ａ性交,0,1)%、"
	ENDIF
;	;舐められ系
;	ELSEIF (SELECTCOM == 1 && (NOWEX:Ｖ绝顶 || NOWEX:Ａ绝顶)) || (SELECTCOM == 11 || NOWEX:Ｂ绝顶)
;		LOCALS += @"%K17_CNAME()%にぺろぺろされて、\n　"
;
;	;其他
;	ELSEIF NOWEX:Ｖ绝顶  ||  TEQUIP:Ａ性交
;		;LOCALS += @"\n　%K17_CNAME()%の%K17_RS("手/指")%で、"
;	ENDIF
;	

	
	;-------------------------------------------------
	;五重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:五重绝顶
		
	ENDIF
	;-------------------------------------------------
	;四重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:四重绝顶
	
	ENDIF
	;-------------------------------------------------
	;三重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:三重绝顶
		
	ENDIF
	;-------------------------------------------------
	;二重绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:二重绝顶
		LOCALS += K17_CALL_P(iABLnum:1)+"和"
	ENDIF
	
	;どこも绝顶してなければ戻し
	SIF !iCount
		RETURN 1
	
	;どこか绝顶してたら文本
	IF NOWEX:Ｍ绝顶 || (!CFLAG:诶嘿嘿 && iSub_TARGET) || DATE_HITOGOMI(CFLAG:PLAYER:当前位置) || (FLAG:异常气象 == 6 && !CFLAG:诶嘿嘿)
		PRINTFORML （%K17_喘ぎ_绝顶(iType,iABLnum:0,LOCALS)%）
	ELSE
		PRINTFORML 「%K17_喘ぎ_绝顶(iType,iABLnum:0,LOCALS)%」
	ENDIF
	
	;诶嘿嘿以外など
	IF FLAG:异常气象 == 6 && !CFLAG:诶嘿嘿
		;掀裙子イき
		PRINTL
		PRINTFORML （什么！？怎么回事！？仅仅是被风吹着就觉得好不舒服啊…………）
		PRINTFORMDL %CALLNAME:TARGET%身体颤抖着、按着股间坐了下来
		
		;疑いあり　误解だけどｗ
		IF CFLAG:K17C_ID:K17C_CF_疑い
			CFLAG:K17C_ID:K17C_CF_疑い = MIN(CFLAG:K17C_ID:K17C_CF_疑い +5,100)
			PRINTFORMDL %CALLNAME:MASTER%被%CALLNAME:TARGET%瞪了一眼……怀疑加深了
		ENDIF
	ELSEIF !CFLAG:诶嘿嘿
		;日常
		PRINTL
		PRINTFORML 「做得太过分了……%K17_RS("笨蛋/变态/色鬼/下流")%」
		PRINTFORMDL 明明不是在%K17_RS("性交/性交")%中却糟糕地%K17_RS("绝顶了/丢了")%、%CALLNAME:TARGET%鼓起腮帮子抗议道
		PRINTFORMDL 背对着%CALLNAME:MASTER%、咿咻咿咻地整理着凌乱的衣服
	ENDIF
	PRINTW
ENDIF
