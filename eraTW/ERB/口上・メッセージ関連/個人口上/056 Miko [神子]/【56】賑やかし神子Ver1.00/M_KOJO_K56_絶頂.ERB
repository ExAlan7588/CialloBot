		;[SKIPSTART]
		SELECTCASE NOWEX:三重绝顶
			;Ｃ＆Ｖ＆Ａ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ａ绝顶
			CASE 2
