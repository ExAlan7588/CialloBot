	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;插入中
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V插入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			IF RAND:3 == 0 
				PRINTFORML 「噫、啊啊、嗯、呼…」
			ELSEIF RAND:2 == 0 
				PRINTFORML 「啊、唔啊………你的那个、在一跳一跳、的…」
			ELSE
				PRINTFORML 「噫啊、嗯嗯、啊、啊、射…出来了…」
			ENDIF 
			PRINTFORMW 一边沉浸在射精的快感中，一边抚摸着纤细的肉体…
			RETURN 1
		ELSE
			SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「哈呜、啊嗯%UNICODE(0x2764)% %CALLNAME:MASTER%」
			PRINTFORMW 「太舒服了…多做一点…%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「嗯%UNICODE(0x2764)% 啊%UNICODE(0x2764)% %CALLNAME:MASTER%、快点%UNICODE(0x2764)%」
			PRINTFORMW 「啊嗯%UNICODE(0x2764)% %CALLNAME:MASTER%%UNICODE(0x2764)% 系欢～%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「嗯%UNICODE(0x2764)% 啊%UNICODE(0x2764)% 那样做的话%UNICODE(0x2764)% 会区的～%UNICODE(0x2764)%」
			PRINTFORMW 「啊嗯%UNICODE(0x2764)% 啊嗯%UNICODE(0x2764)% 好腻害～%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
		ENDIF
	;A插入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 0
		ELSE
			PRINTFORMW 「啊呜…、%CALLNAME:MASTER%、那里……不行…%UNICODE(0x2764)%」
			PRINTFORMW 被%CALLNAME:MASTER%玩弄着身体、发痒的扭了扭身体
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;扶她
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 0
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 0
	;浴场
			ELSEIF BATHROOM(CFLAG:当前位置)
			IF RAND:4 == 0 
				PRINTFORML 为了不让被泡沫抹过的身体溜走，紧紧地抱着纤细的身体，温柔地揉着。
				PRINTFORMW 「噫嗯、啊！不行…、不要……嗯嗯%UNICODE(0x2665)%」
				PRINTFORMW 神子为了逃避快感而扭动着身体，却反而被刺激到了敏感的地方。
					PRINTFORMW 因为黏糊糊的触感而兴奋起来的%CALLNAME:MASTER%开始用手揉腋下和肚子。
				ELSEIF RAND:3 == 0 
					PRINTFORML 和神子面对面，拥抱着慢慢爱抚。
					PRINTFORMW 纤细的身体因为被淋湿，比平时更容易吸附，让人上瘾。
					PRINTFORMW 「嗯嗯%UNICODE(0x2665)%…呼呼、只是摸摸就满足了吗？%UNICODE(0x2665)%」
					PRINTFORMW 夺过神子恶作剧似的笑着的嘴唇，兴奋的脸贴近了%CALLNAME:MASTER%的胸板。
				ELSEIF RAND:2 == 0
					PRINTFORMW %CALLNAME:MASTER%在神子身上涂满泡沫，温柔地用手揉搓着。
					PRINTFORMW 「噫呜…、嗯嗯、啊…、哈、啊啊！」
					PRINTFORMW 努力压着声音而喘着粗气的神子太过可爱，使坏的伸手抓住神子敏感的地方，刺激着她。
				ELSE
					PRINTFORML 为了给神子洗背，%CALLNAME:MASTER%绕到她身后。
					PRINTFORMW 从脖子到肩膀，从腰到臀部，修长的流线让人看得入迷。
					PRINTFORMW 一边用肉棒顶着屁股的裂缝，一边慢慢地洗着。
					PRINTFORMW 也许是意识到了滚烫滚烫的肉棒，神子吐着滚烫的气息，抚摸着神子的肚子，手指一边往下腹部滑去…
				ENDIF
			RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「哈呜、那、那么着迷地探索着…」
			PRINTFORMW 「啊、等等……那里被突然攻击的话、会变得敏……呼啊啊」
		CASE 1
			PRINTFORMW 「哈啊…%UNICODE(0x2764)% 啊%UNICODE(0x2764)% %CALLNAME:MASTER%、嗯嗯%UNICODE(0x2764)%」
			PRINTFORMW 神子沉浸在%CALLNAME:MASTER%的爱抚中。
		CASE 2
			PRINTFORMW 「嗯%UNICODE(0x2764)% 啊%UNICODE(0x2764)% 那样做的话%UNICODE(0x2764)% 会区的～%UNICODE(0x2764)%」
			PRINTFORMW 仅仅只是轻轻碰了下秘部，神子就大幅度的反仰着身体
		ENDSELECT
	RETURN 1
	ENDIF
	;屈服3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
		RETURN 0
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 0
	ENDIF
RETURN 1
