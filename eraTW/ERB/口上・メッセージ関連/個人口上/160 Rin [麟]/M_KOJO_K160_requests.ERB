		RESULTS:1 = 波隆哥
		RESULTS:2 = 盖勒盖勒
		RESULTS:3 = 普克尔
		RESULT:1 = 5
	CASE 4
		;このお恼みは、无条件で发生し、どれを选んでも正解になります
		SIF 1
			RESULT = 1
		RESULTS:0 = 晚餐吃什么比较好呢？
		RESULTS:1 = 焖肉土豆
		RESULTS:2 = 可乐饼
		RESULTS:3 = 烤鱼
		RESULT:1 = 7
	ENDSELECT
[SKIPEND]

	SIF !RESULT
		RESTART
	RETURN 1
ENDSELECT

