;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな变量などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:Ｃ绝顶 NOWEX:Ｖ绝顶 NOWEX:Ｂ绝顶 NOWEX:Ａ绝顶 NOWEX:Ｍ绝顶
;各部位の绝顶强度。まず0~3が割り当てられ、多重绝顶により最大16倍される。つまり单部位なら最大3、5重绝顶で最大48
;NOWEX:TotalEX
;多重绝顶补正を接受前の各部位绝顶强度の合计值。1~15。このテンプレにおける分岐の基准。

;TEQUIP:50
;Ｖに谁の竿が入ってるか。TEQUIP:50 = MASTERで你が插入中となる。
;TEQUIP:51
;Ａに谁の竿が入ってるか。TEQUIP:51 = MASTERで你が插入中となる。
;NOWEX:射精
;射精量、最大3。たとえばIF NOWEX:MASTER:射精 > 1だと、你が大量射精以上した场合となる。
;SYNCED_ORGASM(TARGET)
;相手と你が同时に绝顶した场合真となる。绝顶の部位は问わない。

;-------------------------------------------------
;绝顶时の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K83
#DIM TotalEX
SIF FLAG:时间停止
	RETURN 1
;-------------------------------------------------
;喷乳
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:喷乳
	PRINTFORMW 「讨、讨厌……\/\/\/ 胸部要……%M_KOJO_K83_AUTO("单刺激…！")%」
ENDIF
;;-------------------------------------------------
;;射精した
;;记入チェック（=0, 非表示、1, 表示）
;LOCAL = 0
;;-------------------------------------------------
;IF LOCAL && NOWEX:射精
;	PRINTFORMW 
;ENDIF
;-------------------------------------------------
;放尿
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 「呜诶……要尿出来了……！」
ENDIF
;-------------------------------------------------
;绝顶
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合计绝顶数で分岐
;これだけではどの部位が绝顶したかは区别されないため、书き分けたければ任意で分岐を追加されたし。
IF LOCAL
	IF M_KOJO_K83_UFUFU([[琪斯美]])
		SELECTCASE NOWEX:TotalEX
		;强
		CASE 5 TO 15
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二连刺激")%、不行呀、%M_KOJO_K83_AUTO("单刺激")%！ 要去了要去了HPH 啊啊啊啊啊HPHHPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二连刺激")%、要来了、%M_KOJO_K83_AUTO("单刺激")%！ 唔啊HPH 要去了～～HPHHPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二连刺激")%、咿呀、%M_KOJO_K83_AUTO("单刺激")%！ 要去了～っHPH 对不起～～HPHHPH」", "W")
			ENDSELECT
		;中
		CASE 2 TO 4
			PRINTFORM 「%M_KOJO_K83_AUTO("二连刺激")%%M_KOJO_K83_AUTO("单刺激")% 
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"要去了！ 去了HPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"不、不行了！ 要去了HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"要去了！ 唔啊啊HPH」", "W")
			ENDSELECT
		;弱
		CASE 1
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「%M_KOJO_K83_AUTO("单刺激")% 要去了…！」
			CASE 2
				PRINTFORMW 「%M_KOJO_K83_AUTO("单刺激")% 不、不行…！」
			CASEELSE
				PRINTFORMW 「%M_KOJO_K83_AUTO("单刺激")% 去、去了…！」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE NOWEX:TotalEX
		;强
		CASE 5 TO 15
			PRINTFORMW 「%M_KOJO_K83_AUTO("二连刺激")%%M_KOJO_K83_AUTO("单刺激")% 去了呀！」
		;中
		CASE 2 TO 4
			PRINTFORMW 「%M_KOJO_K83_AUTO("单刺激")% 去了！」
		;弱
		CASE 1
			PRINTFORMW 「%M_KOJO_K83_AUTO("单刺激…！")% 去了…！」
		ENDSELECT
	ENDIF
ENDIF
