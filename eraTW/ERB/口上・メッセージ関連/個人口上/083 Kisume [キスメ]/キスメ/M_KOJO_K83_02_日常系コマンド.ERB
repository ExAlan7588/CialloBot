		RETURN 1
	;纸切れ
	CASE 131
		IF TFLAG:194 > 1000
			PRINTFORML 「啊っ！」
		ELSE
			PRINTFORMDL 钓上了纸条
		ENDIF
		PRINTDW 
		PRINTDL ┌─────────────┐
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　守矢之守札　　　　　　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　集齐五张　　　　　　　｜
		PRINTDL ｜　　　可以抽一次签　　　　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　不保证灵验　　　　　　　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　　　　　　　守矢神社　｜
		PRINTDL └─────────────┘
		PRINTW 
		PRINTFORMW 「这是、什么？」
		RETURN 1
	CASE 1 TO 20, 22 TO 60
		LOCALS = %FISH_NAME(TFLAG:193)%
	CASE 21
		LOCALS = 跳蛋
	CASEELSE
		LOCALS = %ITEMNAME:(TFLAG:193)%
	ENDSELECT
	;琪斯美が钓った
	IF TFLAG:194 > 1000
		PRINTFORMW 「啊！」
		PRINTL 
		PRINTFORMDL %CALLNAME:TARGET%钓上了%LOCALS%！
		RESETCOLOR
		;说明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		CALL M_KOJO_COLOR_K83
		SELECTCASE LOCALS
		CASE "跳蛋"
			PRINTFORMW 「这个是……\/\/\/」
		CASE "青蛙"
			PRINTFORMW 「原来是青蛙啊……」
		CASE "冰之鳞", "龙之爪"
			PRINTFORMW 「哇、好漂亮……！」
		CASE "龙涎香"
			PRINTFORMW 「这是石头吧……？」
		CASE "雀鳝", "雷鱼", "腔棘鱼", "鲟", "伊富鱼", "巨骨舌鱼", "泷太郎"
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鸣：强")% 啊、好重！ %CALLNAME:MASTER%、帮帮我……！」
		CASE "山女"
			PRINTFORMW 「山女酱……诶嘿嘿……」
		CASE "鳗鱼", "八目鳗", "鲶鱼"
			PRINTFORMW 「滑溜溜黏糊糊……」
		CASEELSE
			PRINTFORMW 「上、上钩了……！」
		ENDSELECT
	;MASTERが钓った
	ELSE
		RESETCOLOR
		PRINTFORMDL 钓上了%LOCALS%！
		;跳蛋以降を弹いて尺寸と渔获数の处理
		IF INRANGE(TFLAG:193, 1, 60) && TFLAG:193 != 21
			;尺寸の出力
			LOCAL:1 = FISHING_SIZE(TFLAG:193)
			PRINTFORML 　尺寸：{LOCAL:1}cm
			;ここまでの最高尺寸と比较して大きければ记录
			IF LOCAL:1 > FISHSIZE_RECORD:(TFLAG:193)
				SETCOLOR C_YELLOW
				SIF FISH_EXP:(TFLAG:193)
					PRINTFORML 　迄今为止钓上来的%LOCALS%中最大的一条！
				RESETCOLOR
				FISHSIZE_RECORD:(TFLAG:193) = LOCAL:1
			ENDIF
			;渔获数の加算
			FISH_EXP:(TFLAG:193) ++
		ENDIF
		;说明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		CALL M_KOJO_COLOR_K83
		SELECTCASE LOCALS
		CASE "跳蛋"
			IF TALENT:[[琪斯美]]:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%、在做什么……？」
			ELSE
				PRINTFORMW 「这、这个是……\/\/\/」
			ENDIF
		CASE "青蛙"
			PRINTFORMW 「青蛙好可爱……」
		CASE "冰之鳞", "龙之爪"
			PRINTFORMW 「很漂亮呢……！」
		CASE "龙涎香"
			PRINTFORMW 「那、那个是……什么？」
		CASE "山女"
			PRINTFORMW 「啊、山女酱……」
		CASE "鳗鱼", "八目鳗", "鲶鱼"
			PRINTFORMW 「滑溜溜黏糊糊……」
		CASEELSE
			IF LOCAL:1 >= 80
				PRINTFORMW 「好、好厉害……！」
			ELSEIF INRANGE(TFLAG:193, 11, 20) || INRANGE(TFLAG:193, 24, 60)
				PRINTFORMW 「看起来好好吃……！」
			ELSEIF TALENT:[[琪斯美]]:恋慕
				PRINTFORMW 「太好了……！」
			ELSEIF TALENT:[[琪斯美]]:思慕
				PRINTFORMW 「真好啊……」
			ELSE
				PRINTFORMW 「噢ー……」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
