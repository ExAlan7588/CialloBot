			CASE 13
				CALL M_KOJO_K83_TALKSET("思慕")
			CASEELSE
				CALL M_KOJO_K83_TALKSET("角色")
			ENDSELECT
		;思慕
		ELSEIF TALENT:[[琪斯美]]:思慕
			IF !RAND:6
				CALL M_KOJO_K83_TALKSET("泛用")
			ELSE
				CALL M_KOJO_K83_TALKSET("思慕")
			ENDIF
		;それ以外
		ELSE
			;地底住まい
			IF MAIN_MAP == 9
				SELECTCASE RAND:6
				CASE 1
					PRINTFORMW 「是从外面来的啊……嗯……」
				CASE 2
					PRINTFORMW 「不怎么说话是因为……不擅长……」
				CASE 3
					PRINTFORMW 「地、地底……怎么样？」
				CASE 4
					PRINTFORMW 「发、发生了各种各样的事情呢……」
				CASE 5
					PRINTFORMW 「我觉得人类在这里有点危险呢……」
				CASEELSE
					CALL M_KOJO_K83_TALKSET("泛用")
				ENDSELECT
			;それ以外
			ELSE
