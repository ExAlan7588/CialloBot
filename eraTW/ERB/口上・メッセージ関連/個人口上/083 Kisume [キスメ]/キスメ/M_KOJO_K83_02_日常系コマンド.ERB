;会话セット
;==================================================
@M_KOJO_K83_TALKSET(ARGS)
SELECTCASE ARGS
CASE "泛用"
	IF TALENT:[[琪斯美]]:恋慕
		SELECTCASE RAND:5
		CASE 1
			PRINTFORMW 「我以前……」
		CASE 2
			PRINTFORMW 「那个……可能……」
		CASE 3
			PRINTFORMW 「嗯……是吧」
		CASE 4
			PRINTFORMW 「昨天发生了……」
		CASEELSE
			PRINTFORMW 「那个，%CALLNAME:MASTER%……」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 1
			PRINTFORMW 「呀……那个……」
		CASE 2
			PRINTFORMW 「嗯、嗯……」
		CASE 3
			PRINTFORMW 「那个……所以说……」
		CASEELSE
			PRINTFORMW 「%CALLNAME:MASTER%的……」
		ENDSELECT
	ENDIF
CASE "角色"
	SELECTCASE RAND:7
	CASE 1
		PRINTFORMW 「山女酱……打从出生起就和她一起玩了……是重要的人」
	CASE 2
		PRINTFORMW 「帕露西是……心地善良的人。对我也很温柔……」
	CASE 3
		PRINTFORML 「勇仪桑……对待谁都是一样的态度……」
		PRINTFORMW 「不过，我太弱了……不是很应付得来……」
	CASE 4
		PRINTFORMW 「觉桑……因为不怎么到外面，所以也不经常见到……」
	CASE 5
		PRINTFORMW 「燐酱……有时候会照顾我……」
		PRINTFORMW 「不过，不许我到猫车那看看……」
	CASE 6
		PRINTFORMW 「空酱……有点搞不懂她在想什么……」
	CASEELSE
		PRINTFORMW 「恋、恋酱的话……好、好像没怎么见到过……」
	ENDSELECT
CASE "思慕"
	SELECTCASE RAND:5
	CASE 1
		PRINTFORMW 「外、外面的世界……想了解……」
	CASE 2
		PRINTFORMW 「这个桶……就、就像是我的一部分……」
	CASE 3
		IF MAIN_MAP == 9
			PRINTFORMW 「地、地底的生活怎么样……？」
		ELSE
			PRINTFORMW 「在、在地底很快乐……大概……」
		ENDIF
	CASE 4
		PRINTFORMW 「%CALLNAME:MASTER%明明是个人类……怪」
	CASEELSE
		PRINTFORMW 「钓瓶落……会放出鬼火哦……」(原作中ZUN缝合了钓瓶落和钓瓶火两种妖怪)
	ENDSELECT
CASE "醉酒"
	SELECTCASE RAND:4
	CASE 1
		PRINTFORML 「嘿嘿嘿……呼呼」
	CASE 2
		PRINTFORML 「诶嘿嘿嘿嘿……」
	CASE 3
		PRINTFORML 「噗嘿嘿……嗝！」
	CASEELSE
		PRINTFORML 「呼呼、啊哈哈！」
	ENDSELECT
	PRINTFORMDW 琪斯美喝醉了……。
ENDSELECT
