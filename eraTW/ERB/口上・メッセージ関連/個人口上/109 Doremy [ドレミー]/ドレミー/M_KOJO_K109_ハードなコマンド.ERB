;==================================================
;150,双乳间抽插
;==================================================
@M_KOJO_MESSAGE_COM_K109_150
CALL M_KOJO_MESSAGE_COM_K109_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_150_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕 || TALENT:爱欲 || TALENT:炮友 || TALENT:风骚
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「啊HPH这么、粗暴…HPH」","L"
			PRINTFORMDL %CALLNAME:MASTER%紧紧抓住哆来咪的双乳，砰砰地摆动着腰
			PRINTFORMDW 手指也不停地搓着乳头，哆来咪虽然被粗暴地侵犯着胸部，但还是微笑着，感受『被使用』的幸福而浑身颤抖
		CASE 1
			CALL HPH_PRINT,@"「真是的HPH不能老实点嘛HPH」","L"
			PRINTFORMDL 哆来咪用责备顽皮孩子的口吻说到，侍奉的动作并没有停下
			PRINTFORMDW 就像是训练一样，哆来咪用力推着胸部，整根茎竿被肉海压迫的快感，%CALLNAME:MASTER%不由得呻吟了起来
		CASE 2
			CALL HPH_PRINT,@"「嗯呜HPH不要那么用力抓胸部啊…HPH」","L"
			PRINTFORMDL 感受着肉棒被胸部摩擦的快感,不由得紧紧抓住了哆来咪的胸
			PRINTFORMDL 哆来咪虽然嘴上说着不行，但看起来乐在其中的样子…
			CALL HPH_PRINT,@"「啪嗒啪嗒地摆着腰在我胸部里磨蹭…真是没办法的人啊…HPH」","L"
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		CALL AUTO_AEGI(NO:TARGET)
		RETURN 1
	ENDIF
ENDIF
RETURN 1
