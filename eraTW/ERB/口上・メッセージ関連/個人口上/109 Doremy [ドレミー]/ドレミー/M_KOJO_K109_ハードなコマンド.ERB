;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K109_140
CALL M_KOJO_MESSAGE_COM_K109_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_140_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORML 「插到喉咙深处什么的…那种事…呕诶！？」
			PRINTFORMDW %CALLNAME:MASTER%不容分说地插进了喉咙里
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「呕…咕呕HPH」","L"
				PRINTFORMDL 射精感临近、在喉咙中抽插的速度越来越快，哆来咪的脸被眼泪和鼻涕弄的乱七八糟
				PRINTFORMDW 当%CALLNAME:MASTER%发出射精宣言时，哆来咪放松了身体任由摆布、准备接受精液
			CASE 1
				CALL HPH_PRINT,@"「呕吼HPH咕啊HPH嗯咕呜呜…HPH」","L"
				PRINTFORMDL 没有余裕去关心哆来咪、%CALLNAME:MASTER%只是一味在喉咙里抽插
				PRINTFORMDW 哆来咪努力忍耐着胃部翻滚带来的一阵阵呕吐感…
			CASE 2
				CALL HPH_PRINT,@"「咕、诶诶…嗯咕…咕呕HPH」","L"
				PRINTFORMDL 咕噗、咕噗、就像在用自慰套一样侵犯着哆来咪的喉咙
				PRINTFORMDW 射精感临近的%CALLNAME:MASTER%、为了排出欲望提高了动腰的速度
		ENDSELECT
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:爱欲 || TALENT:炮友 || TALENT:风骚
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「呼HPH…呼呜呜…诶咕HPH」","L"
				PRINTFORMDL 被当做物品对待的哆来咪，产生了奇妙的昂扬感
				PRINTFORMDW 哆来咪的痴态并没有被发现、%CALLNAME:MASTER%甚至没有看她，只是一个劲侵犯着喉咙
			CASE 1
				CALL HPH_PRINT,@"「哦噗！？…呋ーHPH…嘎…呋ーHPH」","L"
				PRINTFORMDL %CALLNAME:MASTER%推倒哆来咪跨在她脸上、就像抵着床自慰那样，把肉棒插进喉咙深处
				PRINTFORMDW 哆来咪一开始还抓住%CALLNAME:MASTER%的屁股反抗、过了一会儿就老实了…
			CASE 2
				CALL HPH_PRINT,@"「咕、噜嘶…呕、咕、咕呕HPH」","L"
				PRINTFORMDL %CALLNAME:MASTER%用力按住哆来咪的头、只是为了自己舒服侵犯着她的喉咙
				PRINTFORMDW 看着%CALLNAME:MASTER%舒服到流口水的样子、哆来咪的子宫隐隐作痛，大腿间已经濡湿了…
		ENDSELECT
	;屈服3
	ELSEIF MARK:屈服刻印 == 3
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「唔…嘎、咕噗、呕诶…」
				PRINTFORMDL 喉咙深处被侵犯着的哆来咪吐了出来、可是食导入口已经被肉棒紧紧堵住，连一滴胃液都无法通过
				PRINTFORMDW %CALLNAME:MASTER%摸了摸哆来咪的头，拔出了阴茎，然后又一口气插了进去
			CASE 1
				PRINTFORML 「嗯嗯诶…呴、诶…」
				PRINTFORMDL 哆来咪发出像被压扁的青蛙般的呻吟、忍受着对咽喉施加的暴力
				PRINTFORMDW 觉得收缩力度弱下来了的%CALLNAME:MASTER%用力掐住喉咙、哆来咪翻起了白眼，手耷拉了下来
			CASE 2
				PRINTFORML 「呃………」
				PRINTFORMDL 哆来咪受到插进喉咙的冲击，瞪大眼睛僵住了
				PRINTFORMDW 施虐心涌现的%CALLNAME:MASTER%捏住鼻子、无法呼吸的哆来咪啪嗒啪嗒挣扎着
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「咕、唔……噗、呋…」
				PRINTFORMDL 哆来咪被侵犯着喉咙的肉棒弄得翻起了白眼
				PRINTFORMDW %CALLNAME:MASTER%看到哆来咪的样子，稍微放慢了速度
			CASE 1
				PRINTFORML 「噗诶…嗯咕呕…嗯咕…」
				PRINTFORMDL 突然一下子插进喉咙里，哆来咪身体弹了起来发出猪一样的声音
				PRINTFORMDW %CALLNAME:MASTER%看到那副样子满足地笑了、又开始了活塞运动…
			CASE 2
				PRINTFORML 「呕诶…呀…咕呜…」
				PRINTFORMDL 用阴茎刺入打算对粗暴的抽插提出抗议的哆来咪的食管，对她的抗议进行封杀
				PRINTFORMDW 终于在食道壁里摩擦够了拔出龟头、哆来咪流着鼻水呕吐了出来
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
