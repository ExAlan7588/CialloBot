		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		GOTO YORIHIME_ENCOUNTER
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラと约会中に遭遇した
	;ARG:1に约会的对象
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;既に会っている场合は非表示
		IF !TCVAR:[[依姫]]:今天有否见面
			;灵梦とデート
			IF FLAG:约会的对象 == 1
				PRINTFORMDL 灵梦牵着你的手，准备离开这个地方时……
				PRINTFORML 「——%CALLNAME:MASTER%？嗯，那里的是……」
				PRINTFORMDW 灵梦露出一副像是在说『被发现很麻烦了』的表情……
			;永琳とデート
			ELSEIF FLAG:约会的对象 == 72
				PRINTFORML 「哎！？」
				PRINTFORML （八意大人为什么……）
				PRINTFORMDW %CALLNAME:TARGET%一边捡起掉落的文件，一边偷偷地瞄着你们两个……
			;丰姫とデート
			ELSEIF FLAG:约会的对象 == 99
				PRINTFORML 「姐姐，没问题。」
				PRINTFORML 「……不，确实有问题……」
				PRINTFORMDW 看着你和丰姬手挽手的样子，%CALLNAME:TARGET%叹了口气。
			;铃仙orレイセンとデート
			ELSEIF FLAG:约会的对象 == 52 || FLAG:约会的对象 == 100
				PRINTFORML 「唉……玩耍而没有履行义务啊」
				PRINTFORML 「看来我又需要给你们上一课了」
				PRINTFORMDW \@ FLAG:约会的对象 == 52 ? 铃仙 # 灵梦 \@颤抖着。既然是我邀请的，有点不好意思呢。
			;清兰or铃瑚とデート
			ELSEIF FLAG:约会的对象 == 107 || FLAG:约会的对象 == 108
				PRINTFORML 「……虽然我觉得最近兔子们的纪律松懈了」
				PRINTFORML 「唉……没想到连你们也……」
				PRINTFORMDW %CALLNAME:TARGET%一副疲惫的样子捂着眼角。\@ FLAG:约会的对象 == 107 ? 清兰 # 铃瑚 \@不知为何在吹口哨……
			;纯狐orヘカーティアとデート
			ELSEIF FLAG:约会的对象 == 112 || FLAG:约会的对象 == 113
				PRINTFORML 「什……」
				PRINTFORML （为什么……%CALLNAME:MASTER%会跟"那个"……）
				PRINTFORMDW %CALLNAME:TARGET%惊讶地睁大了眼睛……
			;それ以外
			ELSE
				;恋慕
				IF TALENT:恋慕
					CALL PRINT_FACE, [[依姫]], "通常", "服", "", ""
					PRINTFORML 「他们那么亲密……」
					CALL PRINT_FACE, [[丰姫]], "笑颜", "服", "", ""
					CALL M_KOJO_COLOR_K99
					PRINTFORML 「真羡慕啊……」
					CALL PRINT_FACE, [[依姫]], "愤怒", "服", "", ""
					CALL M_KOJO_COLOR_K98
					PRINTFORML 「不、没什么……」
					PRINTFORMDW 看着你与他人约会，%CALLNAME:TARGET%心中涌起一股复杂的情绪。
					RESETCOLOR
				;それ以外
				ELSE
					PRINTFORML 「…………」
					PRINTFORMDW %CALLNAME:TARGET%保持着无关痛痒的态度。
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接续します
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で一日最初の挨拶
	;ARG:1に约会的对象、ARG:1が0の场合は非デート
	;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接续します
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1と约会中
		IF ARG:1
		;一人で外出
		ELSE
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

