;1,シックスナイン
;==================================================
;==================================================
;（任意）:通常の地の文を表示するかスキップするか决定する。
;MESSAGECHECK函数は、以下のような返回值を返す
;RETURN 0;通常地の文と口上を表示
;RETURN 1;口上のみ表示
;RETURN 2;通常地の文のみ表示
;RETURN 3;どちらも表示しない
;以下原文
;Optional: Determines if the standard description should be skipped.
;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_SCOM_K98_1
RETURN 0

@M_KOJO_MESSAGE_SCOM_K98_1
CALL M_KOJO_MESSAGE_SCOM_K98_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K98_1_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		CALL HPH_PRINT,@"「为什么……要这样……在这种姿势下……啊HPH」","L"
		PRINTFORMDL 就在这时，你舔舐着%CALLNAME:TARGET%的秘密之处
		CALL HPH_PRINT,@"「——啊啊HPH 呼……原来我也要……舔吗……就是这个意思吗……啊HPH」","L"
		PRINTFORMDL 在%CALLNAME:TARGET%面前，一根坚硬而挺立的雄性器
		PRINTFORMDW 就像在催促一样跳动着，%CALLNAME:TARGET%小心翼翼地用舌头舔了上去
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;侍奉精神が１０以上または恋人
		IF ABL:侍奉精神 > 9 || TALENT:恋人
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「啪啦……滋滋……啊HPH 哎呀……嗯嗯……啊HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%用舌头、嘴唇，甚至喉咙，让你的肉棒感到愉悦
						PRINTFORMDW 当忍不住射精时，%CALLNAME:TARGET%眯起眼角，嘴里吞下积聚的精液
					CASE 1
						CALL HPH_PRINT,@"「嗯嗯……呼……HPH 嗯……嗯……啊HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%无言，只是陶醉地继续吮吸着肉棒
						PRINTFORMDW 尽管你射精了，%CALLNAME:TARGET%只是略微喘息，很快就开始咽喉发声
					CASE 2
						PRINTFORMDL 当你预告射精时，%CALLNAME:TARGET%紧紧地含住整根肉棒
						CALL HPH_PRINT,@"「——嗯嗯、嗯、唔……呼呼、嗯……啊HPH」","L"
						PRINTFORMDW %CALLNAME:TARGET%紧闭嘴唇，以免溢出的精液，紧紧地咬住……
				ENDSELECT
			;其他情况
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「嗯嗯……啊HPH」","L"
						PRINTFORMDL 当你为了呼吸而松开嘴时，%CALLNAME:TARGET%不满地将私处向你挤压
						PRINTFORMDW 你轻声说着『对不起对不起』，抚摸着屁股，然后再次用舌头舔舐%CALLNAME:TARGET%的私处
					CASE 1
						CALL HPH_PRINT,@"「嗯、嗯呜啊HPH 啊啊……舌头……啊HPH」","L"
						PRINTFORMDL 当舌头进入蜜穴时，%CALLNAME:TARGET%因愉悦而颤抖着腰部
						PRINTFORMDW 你抱紧%CALLNAME:TARGET%的腰，轻柔地用舌头的腹部在蜜穴壁上轻轻擦动……
					CASE 2
						CALL HPH_PRINT,@"「那么……这次，我在下面……HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%在下面，你在上面，重新开始互相舔舐
						PRINTFORMDW %CALLNAME:TARGET%如同吮吸奶水的小牛，紧紧吸住你的肉棒……
				ENDSELECT
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「咕噗……啾……啊HPH 呃、嗯嗯HPH」","L"
						PRINTFORMDL 将舌头深入%CALLNAME:TARGET%的蜜穴，用粗糙的舌面反复擦拭黏膜
						PRINTFORMDW %CALLNAME:TARGET%紧抓着你的腿，试图尽量不让快感溢出……
					CASE 1
						CALL HPH_PRINT,@"「哈啊……嗯……啊HPH 嗯嗯、嗯……啊HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%含住肉棒的中段，摇动着头部
						PRINTFORMDW 感受着下半身渐热的感觉，你努力用舌头探索%CALLNAME:TARGET%的隐秘之处……
					CASE 2
						CALL HPH_PRINT,@"「——再多……舔我，这样……啊HPH」","L"
						CALL HPH_PRINT,@"「如果你想要这样……你也要亲自舔……啊HPH」","L"
						PRINTFORMDL 或许是不够满足？于是，你将舌头深入%CALLNAME:TARGET%的蜜穴
						CALL HPH_PRINT,@"「不、不对HPH 嗯嗯HPH 咕啊HPH 呀啊HPH」","W"
				ENDSELECT
			;其他情况
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「嗯、嗯……啊HPH 呼……嗯噗……啊HPH」","L"
						PRINTFORMDL 互相展露性器，互相舔舐的行为
						PRINTFORMDW %CALLNAME:TARGET%似乎还有些害羞，腰部略微退缩着……
					CASE 1
						CALL HPH_PRINT,@"「啾……噗啊HPH 别……动得太厉害……啊HPH」","L"
						PRINTFORMDL 温暖的舌头在身体周围游走，让腰部不受控制地摆动
						PRINTFORMDW 你向%CALLNAME:TARGET%道歉，然后专注于舔舐她的私处
					CASE 2
						CALL HPH_PRINT,@"「呼……嗯呼……啊HPH 嗯噗……啾……啊HPH」","L"
						PRINTFORMDL %CALLNAME:TARGET%含住肉棒的顶端，用嘴唇抚摸龟头
						PRINTFORMDW 或许因为不愿意深咽，所以这样做，但意外地令人愉悦……
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
