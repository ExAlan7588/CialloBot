;==================================================
;300,会话
;TFLAG:193(1=大成功 0=成功 -1=失败 -2=大失败（TARGETがMASTERより教养が4以上高い)
;TCVAR:302(0=会话可能 1>=非恋慕时会话不能)
;MARK:反抗刻印(3以上=会话不能)(4以上はバグ
;TCVAR:烂醉(1=通常成功or大失败)
;==================================================
@M_KOJO_SUCCESS_COM_K85_300
;成否判定
;TFLAG:192(-2=指令结束、-1＝强制失败、0=COM依存、1=强制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K85_300
;指令共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～说着话。」とかが消えます。また、削除した场合は自动で口上色が付きません。
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K85_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K85_300_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL

	;大成功
	IF TFLAG:193 == 1
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「……噗、啊哈哈哈！」
				PRINTFORML 一轮一边抱着肚子笑一边擦眼泪、
				PRINTFORMW %CALLNAME:MASTER%被那种样子吸引，也不由得笑了起来。
			CASE 1
				PRINTFORML 「噗噗……！　说得真好啊、%CALLNAME:MASTER%」
				PRINTFORMW 很好地发起话题、一轮流露出真挚的笑容
		ENDSELECT
	;会话累积值が阈值を超えて失败
	ELSEIF TCVAR:302
		PRINTFORML 「啊ー…不知道说些什么…」
	;失败
	ELSEIF TFLAG:193 == -1
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「嗯、什么呀云山」
				PRINTFORMW 一轮和云山开始对话…。
			CASE 1
				PRINTFORML 「呼哇…」
				PRINTFORMW 「啊、对不起对不起、我没听清楚」
		ENDSELECT
	;仕事中
	ELSEIF CFLAG:行动 == 5
				PRINTFORML 「快点结束工作吧」
				PRINTFORMW 一轮没有停下扫除的手，和%CALLNAME:MASTER%聊着天
	ELSE
		;角色に关するトーク
		IF RAND:2 == 0
			SELECTCASE RAND:4
				;星莲船角色
				CASE 0
					SELECTCASE RAND:4
						;ひじりん
						CASE 0
							PRINTFORML 「圣大人是我憧憬的对象。又漂亮、又帅气、还很温柔」
							;ひじりんが恋人or恋慕
							IF 陷落状态(55) > 2
								PRINTFORMW 「嘛、去和圣大人打个招呼什么的，事到如今%CALLNAME:MASTER%不是更应该做这件事吗♪」
							ELSEIF 陷落状态(55) > 0
								PRINTFORMW 「%CALLNAME:MASTER%好像和圣大人的关系也很好、对我来说也很开心」
							ELSE
								PRINTFORMW 「%CALLNAME:MASTER%也是、一起给予圣大人帮助吧、请多关照呢♪」
							ENDIF
						;村纱
						CASE 1
							PRINTFORML 「和村纱交往了很久呢」
							PRINTFORMW 「那个孩子是容易感到寂寞的类型…」
							;村纱が恋人or恋慕
							IF 陷落状态(86) > 2
								PRINTFORMW 「%CALLNAME:MASTER%也要好好照顾村纱哦」
							ELSEIF 陷落状态(86) > 0
								PRINTFORMW 「%CALLNAME:MASTER%好像和村纱的关系也很好、对我来说也很开心」
							ELSE
								PRINTFORMW 「%CALLNAME:MASTER%也是、要多和村纱说说话哦。」
							ENDIF
						;星ちゃん（原作での星ちゃんの呼び方はわからないけど、鵺さん呼びから御本尊样呼びとかもありかなと。气に入らなければ各自で修正を）
						CASE 2
							PRINTFORML 「御本尊大人的话、啊啊你也看到了，她很喜欢喝酒」
							PRINTFORMW 「但是、酒品很差呢…」
							;星ちゃんが恋人or恋慕
							IF 陷落状态(87) > 2
								PRINTFORMW 「如果可以的话，希望%CALLNAME:MASTER%也能和我一起去…」
							ELSEIF 陷落状态(87) > 0
								PRINTFORMW 「%CALLNAME:MASTER%也是、试着和御本尊大人喝酒交流一下怎么样？」
							ELSE
								PRINTFORMW 「啊、这个应该是对%CALLNAME:MASTER%保密的…」
							ENDIF
						;纳兹琳（TWでは命莲寺に永住してるけど、一応原作を意识した解釈で）
						CASE 3
							PRINTFORML 「纳兹琳原本不是命莲寺的居民」
							PRINTFORMW 「因为住处被破坏了，所以现在暂时来这里避难」
							;ナズが恋人or恋慕
							IF 陷落状态(39) > 2
								PRINTFORMW 「嘛但是、只要%CALLNAME:MASTER%在这的话、那孩子现在也不会想回去的事情了吧♪」
							ELSEIF 陷落状态(39) > 0
								PRINTFORMW 「啊、这个应该是对%CALLNAME:MASTER%保密的…」
							ELSE
								PRINTFORMW 「如果原来的住处修复了的话、那孩子也会从这里离去吧」
							ENDIF
					ENDSELECT
				;神子
				CASE 1
					PRINTFORML 「对了对了、命莲寺正下方的灵庙？好像是有这种说法」
					;神子が恋人or恋慕
							IF 陷落状态(56) > 2 
								PRINTFORMW 「%CALLNAME:MASTER%代我向神子大人问个好吧？下次告诉我怎么去吧♪」
							ELSEIF 陷落状态(56) > 0
								PRINTFORMW 「%CALLNAME:MASTER%知道进庙的方法？」
							ELSE
								PRINTFORMW 「住在那里的人是我们的商业敌人，请小心」
							ENDIF
				;鵺さん
				CASE 2
					PRINTFORML 「鵺桑是我们回到地上后加入命莲寺的人」
					;神子が恋人or恋慕
							IF 陷落状态(41) > 2 
								PRINTFORMW 「鵺桑平时和%CALLNAME:MASTER%都会说些什么呢？」
							ELSEIF 陷落状态(41) > 0
								PRINTFORMW 「有和鵺桑关系变好的诀窍吗？」
							ELSE
								PRINTFORMW 「所以对鵺桑的事情不太了解呢…」
							ENDIF
				;猯藏亲分＆响子ちゃん
				CASE 3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「猯藏桑和鵺桑好像是老相识啊」
							PRINTFORMW 「现在在经楼住着」
						CASE 1
							PRINTFORML 「响子是最近进入命莲寺的孩子哦」
							PRINTFORMW 「是个很努力的孩子，所以请多多支持她」
					ENDSELECT
				;小伞
				CASE 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「多多良桑看起来很厉害。很擅长锻冶呢…」
							
						CASE 1
					ENDSELECT
				;布都
				CASE 5
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「看见和灵庙里的银发孩子在村里喝酒？」
							PRINTFORMW 「撒—…窝卜之刀这是增末回事…」
						CASE 1
					ENDSELECT
			ENDSELECT
		;ふりーとーく
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「已经和云山认识很久了」
					PRINTFORMW 「啊啊、想起苦难的日子就像在昨天一样呢」
				CASE 1
					PRINTFORML 「%TEXTR("哦哦…那可真是！/嘿诶—、这种事还真不知道ー！/真厉ー害！/呵呵、感觉不错呢！/呼嗯、是那样哟ー♪/")%」
					PRINTFORMW 一轮随声附和…
				CASE 2
					PRINTFORML 「想坐到云山身上看看？」
					PRINTFORMW 「诶呀…因为是个怕生的家伙，我觉得这种事是不可能的」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
