		CASE 2
			PRINTFORML 「好深%UNICODE(0x2764)%居然被那样子被绮福%UNICODE(0x2764)%%UNICODE(0x2764)%」
			PRINTFORML 「哦哦%UNICODE(0x2764)%腰区了%UNICODE(0x2764)%」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「啊嗯%UNICODE(0x2764)%那样子好深%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「哦咕—%UNICODE(0x2764)%更加%UNICODE(0x2764)%噫噫噫—%UNICODE(0x2764)%喜欢—%UNICODE(0x2764)%%UNICODE(0x2764)%」
