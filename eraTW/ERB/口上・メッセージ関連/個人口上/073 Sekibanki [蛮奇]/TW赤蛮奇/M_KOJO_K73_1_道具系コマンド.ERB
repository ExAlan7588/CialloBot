;==================================================
;40,跳蛋
;==================================================
@M_KOJO_不太孤高_MESSAGE_COM_K73_40
CALL TRAIN_MESSAGE
CALL M_KOJO_不太孤高_MESSAGE_COM_K73_40_1
RETURN RESULT

@M_KOJO_不太孤高_MESSAGE_COM_K73_40_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	SIF FLAG:70
		RETURN 1
	SIF CFLAG:MASTER:恶作剧 == 2
		RETURN 1
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人
		ELSEIF TALENT:恋慕 || TALENT:爱欲
		ELSEIF TALENT:思慕 || TALENT:炮友
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORML 把跳蛋完全埋入蛮奇的小穴中
		PRINTFORML %CALLNAME:MASTER%将开关慢慢调到最大。看着身体止不住颤抖的蛮奇
		PRINTFORMW %CALLNAME:MASTER%期待着结束时停时蛮奇的反应
		RETURN 1
	ELSEIF TCVAR:烂醉
		PRINTFORML 趁着蛮奇不省人事时将跳蛋塞入了小穴内
		IF TALENT:恋慕 || TALENT:爱欲
			PRINTFORML 醉酒的蛮奇轻声娇喘，放任着身体感受快乐
			PRINTFORML 「 %CALLNAME:MASTER%君的、呼啊、摸摸、好酥糊...呼唔~~zzz」
			PRINTFORMW 好像听见了自己的名字
		ELSE
			PRINTFORML 「啊、嗯嗯唔...好酥糊...呼啊...」
			PRINTFORML 蛮奇双眼涣散、因为快感而留着口水咕嘿的笑着
			PRINTFORMW 很难想象这是平时高冷孤僻的蛮奇
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORML 
		RETURN 1
	;屈服3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

