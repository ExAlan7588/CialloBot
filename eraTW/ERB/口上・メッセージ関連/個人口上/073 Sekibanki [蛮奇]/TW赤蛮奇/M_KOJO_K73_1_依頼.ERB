		SIF TALENT:恋慕
			RESULT = 1
		RESULTS:0 = 赤ちゃん作るなら何人がいいかな？
		RESULTS:1 = ５人でバスケチームを作ろう
		RESULTS:2 = ９人で野球チームを作ろう
		RESULTS:3 = １１人でサッカーチームを作ろう
		IF ABL:亲密 >= 100
			RESULT:1 = 3
		ELSEIF ABL:亲密 >= 20
			RESULT:1 = 2
		ELSE
			RESULT:1 = 1
		ENDIF
	CASE 1
		;このお恼みは、无条件で发生し、正解は绝对に2番で固定化されます
		SIF 1
			RESULT = 1
		RESULTS:0 = どのジョブがいいかな？
		RESULTS:1 = かっこいしナイト
		RESULTS:2 = シーフが便利だよ
		RESULTS:3 = 青魔道士最强！
		RESULT:1 = 2
		RESULT:2 = 1
	CASE 2
		;このお恼みは、夏の月に发生し、正解がランダムで变わります
		SIF DAY:2 == 2
			RESULT = 1
		RESULTS:0 = どの水着を买おうかな？
		RESULTS:1 = かわいい连衣裙
		RESULTS:2 = クールなセパレート
		RESULTS:3 = 色っぽいビキニ
		RESULT:1 = RAND:3 + 1
	CASE 3
		;このお恼みは、无条件で发生し、1番と3番が正解になります（5 - 3 = 2番がハズレになります）
		SIF 1
			RESULT = 1
		RESULTS:0 = ワンちゃんの名前は何にしよう？
		RESULTS:1 = ボロンゴ
		RESULTS:2 = ゲレゲレ
		RESULTS:3 = プックル
		RESULT:1 = 5
	CASE 4
		;このお恼みは、无条件で发生し、どれを选んでも正解になります
		SIF 1
			RESULT = 1
		RESULTS:0 = 夕饭は何がいい？
		RESULTS:1 = 肉じゃが
		RESULTS:2 = コロッケ
		RESULTS:3 = 烧き鱼
		RESULT:1 = 7
	ENDSELECT
[SKIPEND]

	SIF !RESULT
		RESTART
	RETURN 1
ENDSELECT

