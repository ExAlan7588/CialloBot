@M_KOJO_MESSAGE_COM_K79_83
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K79_83_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K79_83_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	IF FIRSTTIME(SELECTCOM)
		IF TALENT:恋慕
			PRINTFORML 
			PRINTFORMW 
		ELSE
			PRINTFORML 
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:时间停止
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:恋慕
		IF ABL:腰 >= 5
			SELECTCASE RAND:5
				CASE 0
					PRINTFORMW 
				CASE 1
					PRINTFORMW 
				CASE 2
					PRINTFORMW 
				CASE 3
					PRINTFORMW 
				CASE 4
					PRINTFORMW 
			ENDSELECT
		ELSEIF ABL:腰 >= 2
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;屈服3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF
RETURN 1

