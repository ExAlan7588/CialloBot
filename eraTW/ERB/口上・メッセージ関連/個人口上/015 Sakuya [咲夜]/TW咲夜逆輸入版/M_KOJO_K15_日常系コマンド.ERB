	;in bath
	ELSEIF BATHROOM()
		CALL M_KOJO_MESSAGE_COM_K15_300_BATH
	;other
	ELSE
		CALL M_KOJO_MESSAGE_COM_K15_300_OTHER
	ENDIF
	
ENDIF
RETURN 1

