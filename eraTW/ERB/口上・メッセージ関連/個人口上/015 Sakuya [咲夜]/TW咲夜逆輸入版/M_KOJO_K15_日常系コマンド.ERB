;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;first time
	;IF FIRSTTIME(SELECTCOM)
		;TODO
	;ENDIF
	
	IF TFLAG:193 == -3 ;hate mark 3
		;custom code
		IF K15_LIKES_PLAYER()
			;PRINTFORMW 「I don't think we should be speaking.」
			;PRINTFORMW 「I'm still very, very angry with you.」
			PRINTFORMW 「我不想跟你说话。」
			PRINTFORMW 「我仍然很生你气。」
