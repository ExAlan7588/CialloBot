;ナマでやらせろ时
;==================================================
@M_KOJO_POWERED_BY_OVERSEAS_EVENT_K112_18(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;避孕要求台词
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORMW 「对不起，要生孩子还有点……」
			PRINTFORMW 「…还没有做好决心」
			PRINTFORMW 「再多一些时间我们两个在一起吧，好吗？」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「啊，等一下…」
			PRINTFORMW 「那个…我觉得做到那一步还为时过早…」
			PRINTFORMW 「只要做好避孕措施应该没问题，拜托了…？」
		ELSEIF TALENT:思慕
			PRINTFORMW 「那个…」
			PRINTFORMW 「我觉得那还太早了…」
			PRINTFORMW 「至少要成为恋人才行…」
		ELSE
			PRINTFORMW 「不行」
			PRINTFORMW 被以比平时更强烈的口气拒绝了。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;断った
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「拜托了…我还害怕…」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「不行」
			PRINTFORMW 「要好好考虑未来才能做决定…」
		ELSEIF TALENT:思慕
			PRINTFORMW 「不行」
			PRINTFORMW 「先了解对方更多再说吧」
		ELSE
			PRINTFORMW 「不行」
			PRINTFORMW 以比刚才更强烈的口气拒绝了。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;断ったのに粘られて切れた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORMW 「为什么…？」
			PRINTFORMW 「你原来是这样的人吗…？」
		ELSEIF TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「我说了不行吧」
			PRINTFORMW 「别再这样了」
		ELSE
			PRINTFORMW 「别再这样了」
			PRINTFORMW 纯狐显然生气了。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;危险日以外许可
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF TALENT:恋人
			PRINTFORMW 「那么…除了危险的日以外的话…」
		ELSEIF TALENT:恋慕 || TALENT:思慕
			PRINTFORMW 「没办法呢…」
			PRINTFORMW 「因为危险的日不行吧…？」
		ELSE
			PRINTFORMW 「……只要不是危险的日」
			PRINTFORMW 以平常的口吻这样说道。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;危险日许可
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF TALENT:恋人
			PRINTFORMW 「…我知道了」
			PRINTFORMW 「孩子…我们要生一个吗」
			PRINTFORMW 「其实，我也很想要和你的孩子…%UNICODE(0x2665) *1%」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「…我知道了」
			PRINTFORMW 「请负起责任来」
			PRINTFORMW 「我相信你…」
		ELSEIF TALENT:思慕
			PRINTFORMW 「…我知道了」
			PRINTFORMW 「请负起责任来」
			PRINTFORMW 「如果你逃避的话…」
		ELSE
			PRINTFORMW 「……」
			PRINTFORMW 纯狐的脸红了起来，眼神躲开了。
			PRINTFORMW 看起来达成了共识。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
