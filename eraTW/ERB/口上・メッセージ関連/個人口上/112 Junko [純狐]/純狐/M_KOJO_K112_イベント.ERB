				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
						PRINTFORMW 「这个，你想要吗…？」
						PRINTFORMW 「可以的，如果你能用它，我会很高兴的%UNICODE(0x2665) *1%」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
						PRINTFORMW 「嗯…，你想要这个吗…？」
						PRINTFORMW 「嗯…」
						PRINTFORMW 「可以啦…但是不要拿它做奇怪的事情哦…？」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
						PRINTFORMW 「嗯…，不喜欢吗…」
						PRINTFORMW 「你打算用它做什么呢…」
					ENDIF
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
