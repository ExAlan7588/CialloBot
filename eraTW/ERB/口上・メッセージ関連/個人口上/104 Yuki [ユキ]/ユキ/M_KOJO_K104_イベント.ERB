;==================================================
@M_KOJO_EVENT_K104_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:烂醉 == 1
			PRINTFORML 
		ELSEIF DAY:2 == 4 && DAY:3 == 14 && CFLAG:104:好感度 >= 1000 && !TCVAR:104:今天有否见面
			CALL EVENTDAY_GREETING_K104
		ELSEIF CFLAG:104:自慰暴露 == 1
			PRINTFORML 「啊、%CALLNAME:MASTER%……那个啊、之前的那个……」
			PRINTFORML 「啊啊～……没什么事哦」
			PRINTFORMW 「哈啊……」
		ELSEIF CFLAG:104:自慰暴露
			IF (TALENT:恋慕 || TALENT:爱欲 || TALENT:炮友)
				PRINTFORML 「%CALLNAME:MASTER%……%UNICODE(0x2665) *1%」
				PRINTFORMW 「下一次、要是正好遇到了……也和我一起哦」
			ELSE
				PRINTFORML 「呃、%CALLNAME:MASTER%……」
				PRINTFORML 「之前的事就这么过去吧、但下一次可不会这样了啊！」
				PRINTFORMW 「要注意哦！」
			ENDIF
		ELSEIF TALENT:恋慕 || TALENT:爱欲
			PRINTFORML 「啊、%CALLNAME:MASTER%！」
			PRINTFORMW 「在等你哦%UNICODE(0x2665) *1%」
		ELSEIF TALENT:思慕
			PRINTFORML 「啊、%CALLNAME:MASTER%！」
			PRINTFORMW 「今天在做什么呢？」
		ELSE
			PRINTFORMW 「啊啦、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:烂醉 == 1
			PRINTFORML 
		ELSEIF DAY:2 == 4 && DAY:3 == 14 && CFLAG:104:好感度 >= 1000 && !TCVAR:104:今天有否见面
			CALL EVENTDAY_GREETING_K104
		ELSEIF CFLAG:104:自慰暴露 == 1
			PRINTFORML 「啊、%CALLNAME:MASTER%……那个啊、之前的那个……」
			PRINTFORML 「啊啊～……没什么事哦」
			PRINTFORMW 「哈啊……」
		ELSEIF CFLAG:104:自慰暴露
			IF (TALENT:恋慕 || TALENT:爱欲 || TALENT:炮友)
				PRINTFORML 「%CALLNAME:MASTER%……%UNICODE(0x2665) *1%」
				PRINTFORMW 「下一次、要是正好遇到了……也和我一起哦」
			ELSE
				PRINTFORML 「呃、%CALLNAME:MASTER%……」
				PRINTFORML 「之前的事就这么过去吧、但下一次可不会这样了啊！」
				PRINTFORMW 「要注意哦！」
			ENDIF
		ELSEIF TALENT:恋慕 || TALENT:爱欲
			PRINTFORML 「啊、有了有了！」
			PRINTFORMW 「呐～、来陪我一下好不好」
		ELSEIF TALENT:思慕
			PRINTFORML 「啊、%CALLNAME:MASTER%……」
			PRINTFORMW 「啊～……如果有空的话、要不要一起去哪里看看？」
		ELSE
			PRINTFORMW 「啊啦、%CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて一绪に入る
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:烂醉 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:爱欲
			PRINTFORML 「啊、正在洗吗？」
			PRINTFORML 「要我来帮你搓背吗～」
		ELSEIF TALENT:思慕
			PRINTFORML 「哦呀、%CALLNAME:MASTER%」
			PRINTFORMW 「那～个、可以一起来洗吗？」
		ELSE
			PRINTFORML 「这不是%CALLNAME:MASTER%嘛」
			PRINTFORMW 「啊哈哈、总感觉有点害羞呢」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて出て行った
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF TCVAR:烂醉 == 1
			PRINTFORML 
		ELSEIF TALENT:恋慕 || TALENT:爱欲
			PRINTFORML 「和%CALLNAME:MASTER%一起的话、没办法冷静的洗澡了呢」
