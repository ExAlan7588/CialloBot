@M_KOJO_SPEVENT_K76_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常回家
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:1100 == 1
			PRINTFORML 「很开心哦、%CALLNAME:MASTER%」
			PRINTFORML 「那么、在这之后、要做什么呢？」
			IF CFLAG:MASTER:初始位置 == CFLAG:76:初始位置
				PRINTFORML 「回到房间，一直到早上都在调情」
				IF YOUBI_MATCH(DAY, "月水金")
					PRINTFORML 「明天是休息日，一整天都在一起吧%UNICODE(0x2665) *1%」
					PRINTFORML 「在房间里、酸臭得不行的、相爱吧……%UNICODE(0x2665) *1%」
				ELSE
					PRINTFORML 「虽然明天要工作，如果迟到了的话……就到时候再说吧%UNICODE(0x2665) *1%」
					PRINTFORML 「虽然工作很重要，但是和你一起做开心的事更重要……%UNICODE(0x2665) *1%」
				ENDIF
				PRINTW 
			ELSE
				PRINTFORML 「要来咱家吗？　那样的话、就住下来吧」
				IF YOUBI_MATCH(DAY, "月水金")
					PRINTFORML 「明天是休息日、睡到中午也没关系哦……%UNICODE(0x2665) *1%」
				ELSE
					PRINTFORML 「虽然明天要工作，如果迟到了的话……就到时候再说吧%UNICODE(0x2665) *1%」
				ENDIF
				PRINTW 
			ENDIF
		ELSEIF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%、下次再一起去吧，非常开心哦」
			IF CFLAG:MASTER:初始位置 == CFLAG:76:初始位置
			ELSE
				PRINTFORML 「今天、怎么样？要住下来吗？」
			ENDIF
			PRINTW 
		ELSEIF TALENT:恋慕
			PRINTFORML 「真开心啊。下次、再两个人一起去吧」
			PRINTW 
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%、真是个有趣的家伙啊」
			PRINTFORML 「如果可以的话、下次再约吧」
			PRINTW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反抗刻印消去イベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「嘛，好像有在反省的样子呢。咱会原谅你的」
		PRINTFORML 「作为代价、下次要邀请咱去约会哦♪」
		PRINTW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反抗刻印2>1レベルダウンイベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML (嘛、其实已经可以原谅你了）
		PRINTFORML 「嗯，很开心、哟。但是、咱还没有原谅你」
		PRINTW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反抗刻印3>2レベルダウンイベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「嘛，咱承认这样是很开心」
		PRINTFORML 「但是，那个和这个是两码事。你还需要反省」
		PRINTW 
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

