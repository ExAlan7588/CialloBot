;-------------------------------------------------
;名前を教えるイベント（函数）
;纳兹琳口上からお借りしました
;-------------------------------------------------
@NAME_SET_K100
 PRINTL 请输入你的名字(如果想加上敬称，请一并输入)
 IF NAME:MASTER != "你"
	LOCALS = %CALLNAME:MASTER%
ELSE
	LOCALS = %NAME:MASTER%
ENDIF
PRINTBUTTON @"[就叫我%LOCALS%]", @"%LOCALS%"
PRINTL 　
;名前を入力するループ
$INPUT_LOOP_ONAMAE
INPUTS
; 行头行尾の空白文字削除
LOCALS = %RESULTS%
REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
;10文字以上は受け付けない
IF STRLENS(LOCALS) > 10
	CLEARLINE 1
	REUSELASTLINE 名前が长すぎます
	GOTO INPUT_LOOP_ONAMAE
;空白は受け付けない
ELSEIF RESULTS == ""
	PRINTL 请输入你的名字
	GOTO INPUT_LOOP_ONAMAE
ELSE
	PRINTFORML 就决定是%RESULTS%了吗？
	PRINTL [0] 是的
	PRINTL [1] 不是
	;入力した名前を确定するループ
	$INPUT_LOOP_KAKUTEI_1
	INPUT
	IF RESULT == 0
		;你の名前を%MASTERNAME:100%に格纳
		MASTERNAME:100 = %RESULTS%
		PRINTFORML 
	ELSEIF RESULT == 1
		PRINTL 请输入你的名字
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTL 请输入你的名字
		GOTO INPUT_LOOP_ONAMAE
	ENDIF
ENDIF

