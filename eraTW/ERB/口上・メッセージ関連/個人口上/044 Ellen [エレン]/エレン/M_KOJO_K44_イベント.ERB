;==================================================
;相手から约会のお诱い
;==================================================
@M_KOJO_EVENT_K44_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;诱い文句
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
			CASE 0
				LOCALS = 博丽神社
			CASE 1
				LOCALS = 命莲寺
			CASE 2
				LOCALS = 人里
			CASE 3
				LOCALS = 雾之湖
			CASE 4
				LOCALS = 迷途竹林
			;选ばれる事は无いんだけど一応
			CASE 5
				LOCALS = 魔法之森
			;约会で三途之川に诱われたら唖然とするよね、という事で
			CASE 6
				LOCALS = 迷途之家
			CASE 7
				LOCALS = 妖怪之山的山麓
			CASE 8
				LOCALS = 妖怪之山
			CASE 9
				LOCALS = 地底
			CASE 10
				LOCALS = 月
			CASEELSE
				LOCALS = "これが出たらスレで报告してね"
			ENDSELECT
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「今天两个人一起去%LOCALS%吧%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1
				PRINTFORMW 「嗯，两个人约会怎么样？%UNICODE(0x2665)%//」
			;恋慕缚りが解除されたときのために
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、如果有空的话，一起去%LOCALS%怎么样？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「太好了%UNICODE(0x2665)%那么我们走吧%UNICODE(0x2665)%%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1
				PRINTFORMW 「太好了%UNICODE(0x2665)%//」
			ELSE
				PRINTFORMW 「那咱们赶紧走吧%UNICODE(0x2665)%」
			ENDIF
			;约会に诱われたフラグを立てる
			TCVAR:356 = 1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「那么改天再约吧%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「啊呀呀，哎呀%CALLNAME:MASTER%如果不感兴趣就算了」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;房间に诱われた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「去我家悠闲地休息吧%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
				PRINTFORMW 「如果可以的话，要不要去我家？//」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;房间に诱われたのを受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「太好了%UNICODE(0x2665)%那么我们走吧%UNICODE(0x2665)%%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1
				PRINTFORMW 「太好了%UNICODE(0x2665)%//」
			ELSE
				PRINTFORMW 「那咱们赶紧走吧%UNICODE(0x2665)%」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;房间に诱われたのを断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「是吗，那下次再约你吧%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「啊呀呀，哎呀%CALLNAME:MASTER%如果不感兴趣就算了」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF

