;60,掏耳朵
;前ターン膝枕后身体接触指令
;TFLAG:193(1=大成功 0=成功 -1=失败)(失败するのはTARGET
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_60
CALL M_KOJO_MESSAGE_SCOM_K2_60_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K2_60_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K2_CONSCIOUS_AND_TOLERANT()
		IF ( TALENT:恋慕 || TALENT:爱欲 ) && TFLAG:193 >= 0
			SELECTCASE RAND:2
			CASE 1
				CALL K2_HPH_PRINT,@"「因为对自己的作用有实感、所以喜欢帮忙掏耳朵HPH」","L"
				CALL K2_HPH_PRINT,@"「是的、结束了。但是我还想继续。请让我再来吧HPH」","L"
				PRINTFORMDW 必须注意不能做得过火。
			CASEELSE
				PRINTFORML 「%CALLNAME:MASTER%大人的耳朵，让我弄干净吧~」
				PRINTFORMDL 即使没有拜托她，%CALLNAME:TARGET%主动掏起了耳朵。
				PRINTFORMW 」啊，掏出大的一块了。呵呵，好开心」
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
			CASE 1
				PRINTFORMDL 只是告诉她心情非常好，%CALLNAME:TARGET%就开心地笑了。
				PRINTFORMW 「能帮到你真是太好了。」
			CASE 0
				PRINTFORMW 「相当……困难。不，不疼吗？
			CASE -1
				PRINTFORMDL 听到%CALLNAME:MASTER%疼痛的声音后，%CALLNAME:TARGET%很对不起似的道歉了。
				PRINTFORMW 「呜……对不起……我作为女仆不合格……」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
