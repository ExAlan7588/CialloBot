LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TCVAR:烂醉
		PRINTFORMW 「哦？」
	ELSEIF SHIRAHU(TARGET) && TFLAG:194 == 4 && CFLAG:勃然大怒
		PRINTFORML 「就算想这样蒙混过去也不行。请好好反省」
		PRINTFORMDW 想重归于好，却失败了。
	ELSEIF K2_BASIC_CONDITION_IN_MATING()
		;失败
		IF TFLAG:194
			SELECTCASE TFLAG:194
			CASE 1,2
				;诶嘿嘿既成事实あり
				IF CFLAG:既成事实 & 2
					PRINTFORML 「这到底是……」
					PRINTFORMW 「让我们冷静一下吧。」
				ENDIF
			CASE 3
				;诶嘿嘿既成事实あり
				IF CFLAG:既成事实 & 2
					IF TALENT:恋慕 || TALENT:爱欲
						PRINTFORML 「哦，是和其他人一起吗？」
						PRINTFORML 「呜……那是……」
						PRINTFORMDW %CALLNAME:TARGET%好像希望到只有两个人的地方。
					ELSE
						PRINTFORMW 「怎么会……一起什么的不行……」
					ENDIF
				ENDIF
			CASE 4
				IF TALENT:恋慕 || TALENT:爱欲 || TALENT:思慕 || TALENT:炮友
					PRINTFORMW 「恩，我还想再多待一会儿。不行吗？
				ELSE
					PRINTFORML 「对不起。清扫的工作还没有做……」
					PRINTFORMDW %CALLNAME:TARGET%好像还没有那种心情。
				ENDIF
			ENDSELECT
		;初回
		ELSEIF TCVAR:初次拥抱
			SELECTCASE TCVAR:初次拥抱
			CASE 1
				IF TALENT:恋慕
					CALL K2_HPH_PRINT,@"「啊HPH ……我明白了」","W"
					PRINTFORMW 「我一直在等你……」
					PRINTFORMDW %CALLNAME:TARGET%脸颊像是染红了一样，一直看着%CALLNAME:MASTER%的眼睛。
				ELSEIF TALENT:风骚
					CALL K2_HPH_PRINT,@"「啊嗯HPH ……终于HPH」","W"
					CALL K2_HPH_PRINT,@"「什么时候都可以哦HPH」","W"
				ELSEIF TALENT:思慕
					PRINTFORMW 「啊……」
					PRINTFORMDW %CALLNAME:TARGET%似乎从%CALLNAME:MASTER%的样子中察觉到了欲望。
					PRINTFORMW 「……给你吧。就任由%CALLNAME:MASTER%大人了……」
					PRINTFORMDW %CALLNAME:TARGET%微微颤动着嘴唇，将身体交给了%CALLNAME:MASTER%。
				ELSE
					PRINTFORMW 「咦？啊、呀…」
					PRINTFORMDW %CALLNAME:TARGET%一副很吃惊的样子，凝视了一会%CALLNAME:MASTER%的眼睛，吐出一口气，放松了身体。
					PRINTFORMDW 好像是接受了。
					PRINTFORMW 「希望能温柔……请吧」
				ENDIF
			CASE 2
				IF TALENT:风骚
					CALL K2_HPH_PRINT,@"「你啊，不接吻就想干了HPH」","W"
					CALL K2_HPH_PRINT,@"「我喜欢这种拼命努力的感觉HPH」","W"
					IF !CFLAG:1001 && ( TALENT:处女 == 1 || TALENT:处女 == -1 )
						CALL K2_HPH_PRINT,@"「但其实我是第一次……所以，请温柔一点HPH」","W"
					ELSE
						PRINTFORMW 「但，请不要做过分的事。」
					ENDIF
				ELSE
					PRINTFORMW 「哇，我们还是……不，没什么」
					PRINTFORMDW %CALLNAME:TARGET%刚要说什么，就如放弃般闭上眼睛接受了%CALLNAME:MASTER%。
				ENDIF
			CASE 3
				PRINTFORMW 「哈……哈……身体好热」
				PRINTFORMDW 被火灼烧般的%CALLNAME:TARGET%的身体无法拒绝%CALLNAME:MASTER%了。
			CASE 4
				PRINTFORMW 「我好奇怪……想要……受不了」
				PRINTFORMDW 似乎在不知不觉中被开发出来的%CALLNAME:TARGET%的身体在追求%CALLNAME:MASTER%。
			CASE 5
			CASE 6
				PRINTFORMW 「很抱歉，我不能满足您的心意。」
			CASE 7
				PRINTFORMW 「不，请别这样。我是机器人哦」
				;灵梦と诶嘿嘿既成事实あり、かつ灵梦に扮演でない
				SIF CFLAG:1:既成事实 & 2 && FLAG:扮演 != 1
					PRINTFORMW 「而且，%CALLNAME:MASTER%大人是不是还有灵梦吗？」
				PRINTFORMDW %CALLNAME:TARGET%轻易地被拒绝了。
			CASE 8
				PRINTFORMW 「你觉得我是性处理工具吗？」
				PRINTFORMDW 被%CALLNAME:TARGET%断然拒绝了。
			ENDSELECT
		;成功二回目以降
		ELSE
			;被逆推中に推倒かえす
			IF TFLAG:COMABLE管理 == 3
				IF TALENT:恋慕 || TALENT:思慕
					CALL K2_HPH_PRINT,@"「啊呀HPH 从那边开始做吗？」","L"
					PRINTFORMW 「呵呵，请多关照」
				ELSEIF TALENT:爱欲 || TALENT:炮友 || TALENT:风骚
					PRINTFORML 「我也是这么想的……」
					CALL K2_HPH_PRINT,@"「……明白了。\@ K2_SEXUAL_FRANKNESS() ?请做好吃醋的准备。 #请以您喜欢的方式来做 \@HPH」","W"
				ELSE
					PRINTFORML 「从那边开始吗？」
					PRINTFORMW 「那么……交给你了」
				ENDIF
			;ふつうにMASTERから推倒
			ELSE
				;屋外
				IF OPENPLACE(CFLAG:MASTER:当前位置) >= 2
					IF ABL:露出癖 >= 6
						PRINTFORML 「在外面还想要吗？」
						PRINTFORMW 「呵呵，明白了。希望能让您心情舒畅。」
					ELSE
						PRINTFORML 「呜……在这种地方吗？
						PRINTFORMW 「哇，我明白了」
					ENDIF
				;屋内
				ELSE
					IF TALENT:恋慕 || TALENT:爱欲
						SELECTCASE RAND:6
						CASE 1
							PRINTFORML 「忽然想做了吗？呵呵，好啊」
							CALL K2_HPH_PRINT,@"「请随意使用我HPH」","W"
						CASE 2
							PRINTFORMDL 欲望支配下的%CALLNAME:MASTER%说着想要做的话，%CALLNAME:TARGET%露出了迷人的笑容
							PRINTFORMDW 沉默着将屁股朝向这边，表示接受了%CALLNAME:MASTER%的欲望。
						CASE 3
							PRINTFORMD %CALLNAME:TARGET%感受到了%CALLNAME:MASTER%的兴奋
							;掀裙子できて裤子没穿场合
							IF ( EQUIP:裙子 || EQUIP:连衣裙 || EQUIP:和服 ) && !EQUIP:裤子
								PRINTFORMDL 抬起裙子下摆露出大腿的同时，诱惑着%CALLNAME:MASTER%。
							ELSE
								PRINTFORMDL 拿起%CALLNAME:MASTER%的手，引到胸前。
							ENDIF
							CALL K2_HPH_PRINT,@"「请享用我吧HPH」","W"
						CASE 4
							PRINTFORMDL 被欲望支配的%CALLNAME:MASTER%从背后抱住了%CALLNAME:TARGET%。
							PRINTFORMDL %CALLNAME:TARGET%露出微笑，用屁股摩擦%CALLNAME:MASTER%的胯间。
							CALL K2_HPH_PRINT,@"「我也变得想要了HPH」","W"
						CASE 5
							PRINTFORMDL %CALLNAME:TARGET%笑着说自己并不高兴。
							CALL K2_HPH_PRINT,@"「好吧。那么我来帮您处理吧HPH」","W"
						CASEELSE
							PRINTFORMDL 抱住了%CALLNAME:TARGET%的腰。
							PRINTFORML 「啊，你真不像话。」
							CALL K2_HPH_PRINT,@"「好啊♪ 再加点料吧HPH」","W"
						ENDSELECT
					ELSEIF TALENT:风骚
						SELECTCASE RAND:2
						CASE 1
							CALL K2_HPH_PRINT,@"「您还想再享用我吗？ 真是没办法啊HPH」","W"
						CASEELSE
							IF TALENT:MASTER:性别 == 2 && RAND:2
								CALL K2_HPH_PRINT,@"「又来了吗？嗯，男人真的很喜欢这个呢HPH」","W"
							ELSE
								CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%大人的精力太旺盛了～HPH」","W"
							ENDIF
						ENDSELECT
					ELSEIF TALENT:思慕
						SELECTCASE RAND:2
						CASE 1
							PRINTFORML 「啊……又要做了……我明白了」
							PRINTFORMDW %CALLNAME:TARGET%一边红着脸颊一边把身体交给了%CALLNAME:MASTER%。
						CASEELSE
							PRINTFORML 「太粗暴了……请不要这样」
							PRINTFORMDW %CALLNAME:TARGET%闭上水润的眼睛，接受了%CALLNAME:MASTER%。
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
						CASE 1
							PRINTFORMW 「请，请使用」
						CASEELSE
							PRINTFORMW 「您又来了。我明白了」
						ENDSELECT
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
