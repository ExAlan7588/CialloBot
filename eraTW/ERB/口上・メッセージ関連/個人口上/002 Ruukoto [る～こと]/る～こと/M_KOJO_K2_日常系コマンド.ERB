					ENDIF
				CASEELSE
					IF TALENT:恋慕 || TALENT:爱欲
						;据点が博丽神社
						IF GET_MAPNAME(MAIN_MAP) == "博丽神社"
							;午后6时以前
							IF TIME < 1080
								PRINTFORMW 「今天的晚饭吃什么好？做%CALLNAME:MASTER%大人喜欢的东西哦」
							;午后6时以降
							ELSE
								PRINTFORMW 「明天的菜单怎么办呢？做%CALLNAME:MASTER%大人喜欢的东西吧」
							ENDIF
						ELSE
							;午后6时以前
							IF TIME < 1080
								PRINTFORMW 「今天的晚饭吃什么好呢~♪」
							;午后6时以降
							ELSE
								PRINTFORMW 「明天的菜单怎么办呢~♪」
							ENDIF
						ENDIF
					ELSE
						;午后6时以前
						IF TIME < 1080
