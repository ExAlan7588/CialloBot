;========================================================
; 推倒率取得(式中函数)
;-------------------------------------------------
; まあ口上侧だけで済まそうと思ったらLOCALを外部参照しに行くか、
; 新しく计算する函数を作るか、本体に手を入れるかの3街道で、
; LOCAL外部参照なんて恐ろしい事したくないし本体に手は入れたくないしで、
; 结局それがこのざまである。
; もちろん本体で变更されたらこっちも变更しなければならない('A`)
;-------------------------------------------------
; 参数
;  ARG   = 比较对象角色の登录番号(省略时内部でTARGETをセット)
;-------------------------------------------------
; 返回值
;  数值(0~100) 推倒率。单位は%
;========================================================
@K88_KYOKO_TRYENDURE(ARG = -1)
#FUNCTION
#LOCALSIZE 2
VARSET LOCAL, 0
SIF ARG == -1
	ARG = TARGET
SIF ARG < 0 || ARG > CHARANUM
	RETURNF 0
; まるっとコピペ
;素質により判定变化
LOCAL = 100
SIF TALENT:ARG:处女
	LOCAL = LOCAL / 2
SIF TALENT:ARG:恋慕
	LOCAL = LOCAL + 30
SIF TALENT:ARG:胆量 < 0 || TALENT:ARG:回应 < 0 || TALENT:ARG:冷漠 || TALENT:ARG:羞耻心 < 0
	LOCAL = LOCAL - 30
SIF TALENT:ARG:性的兴趣 > 0 || TALENT:ARG:羞耻心 > 0
	LOCAL = LOCAL + 20
SIF TALENT:ARG:自制心 || TALENT:ARG:性的兴趣 < 0
	LOCAL = LOCAL - ((LOCAL / 10) * 2)
SIF TALENT:ARG:贞操 < 0 || TALENT:ARG:小恶魔
	LOCAL = LOCAL + ((LOCAL / 10) * 2)
SIF TALENT:ARG:贞操 > 0 || TALENT:ARG:难以越过的底线
	LOCAL = LOCAL / 2

SIF TALENT:ARG:淫乱
	LOCAL = 300
SIF TALENT:ARG:性别嗜好 == 3 || MARK:ARG:反抗刻印 == 3
	LOCAL = 0

;体力に依存して推倒率减少
LOCAL:1 = MAXBASE:ARG:体力 - BASE:ARG:体力
IF LOCAL:1 < 500
	;Do Notting
ELSEIF LOCAL:1 < 1000
	LOCAL = LOCAL / 2
ELSEIF LOCAL:1 < 1500
	LOCAL = LOCAL / 3
ELSE
	LOCAL = 10
ENDIF

;理性に依存して推倒率减少
IF BASE:ARG:理性 > 800
	LOCAL = LOCAL / 3
ELSEIF BASE:ARG:理性 > 600
	LOCAL = LOCAL / 2
ELSE
	;Do Notting
ENDIF

;范围越えないように补正
SIF LOCAL < 0
	LOCAL = 0
SIF LOCAL > 300
	LOCAL = 300

; 概率を返す(元の式は SIF RAND:300 + 1 <= LOCAL)
RETURNF LOCAL / 300

;========================================================
; 体格差取得(式中函数)
;-------------------------------------------------
; 对象との体格差を见て对応する数值を返す
; 别に式中函数でやる必要はないのだけどよく使用处理だし
;-------------------------------------------------
; 参数
;  ARG   = 比较对象角色の登录番号(省略时内部でTARGETをセット)
;  ARG:1 = 比较元角色の登录番号(省略时内部でMASTERをセット)
;-------------------------------------------------
; 返回值
;  数值(-6~+6) 参考用: [小人(-3)][幼儿体型(-2)][矮小体型(-1)][(0)][长身(1)][巨躯(2)][巨人(3)]
;   最小は比较元が小人体型かつ比较对象が巨人。最大は最小の逆となる。
;========================================================
@K88_KYOKO_DIFF_BODYSIZE(ARG = -1, ARG:1 = -1)
#FUNCTION
SIF ARG == -1
	ARG = TARGET
SIF ARG:1 == -1
	ARG:1 = MASTER
SIF ARG < 0 || ARG > CHARANUM || ARG:1 < 0 || ARG:1 > CHARANUM
	RETURNF 0
RETURNF TALENT:(ARG:1):体型 - TALENT:ARG:体型

;========================================================
; 关系取得(式中函数)
;-------------------------------------------------
; 对象との关系を见て对応する数值を返す
;-------------------------------------------------
; 参数
;  ARG = 角色の登录番号
;-------------------------------------------------
; 返回值
;  数值(-1 = 敌视, 0 = 无关系, 1 = 友好的, 2 = 目上, 3 = 尊敬)
;========================================================
@K88_KYOKO_RELATION(ARG)
#FUNCTION
SIF ARG < 0 || ARG > CHARANUM
	RETURNF 0
SELECTCASE CALLNAME:ARG
	; 尊敬(星, 白莲)
	CASE "星" ,"白莲"
		RETURNF 3
	; 目上(鼠, 一轮, 村纱, 鵺, 狸)
	CASE "纳兹琳", "一轮", "村纱", "鵺", "猯藏"
		RETURNF 2
	; 友好(小伞)
	CASE "小伞"
		RETURNF 1
	CASEELSE
		IF FLAG:扮演 == ARG
			RETURNF -2
		ELSE
			RETURNF 0
		ENDIF
ENDSELECT

RETURNF 0

;========================================================
; 名前表示(式中函数)
;-------------------------------------------------
; 对象との关系を见て名前を表示
;-------------------------------------------------
; 参数
;  ARG = 角色の登录番号(省略时内部でMASTERをセット)
;-------------------------------------------------
; 返回值
;  文字列(对象角色への呼称)
;========================================================
@K88_KYOKO_CALLNAME(ARG = -1)
#FUNCTIONS
#LOCALSSIZE 1
VARSET LOCALS, ""
SIF ARG == -1
	ARG = MASTER
SIF ARG < 0 || ARG > CHARANUM
	RETURNF "范围外の参数"
LOCALS = \@ ARG == MASTER ? %CSTR:20% # %CALLNAME:ARG% \@
SELECTCASE K88_KYOKO_RELATION(ARG)
	; 尊敬(样づけ)
	CASE 3
		LOCALS += "大人"
	; 目上(さんづけ)
	CASE 2
		LOCALS += "桑"
	; 友好(ちゃんづけ)
	CASE 1
		LOCALS += "酱"
	; 敌视(呼び舍て)
	CASE -1
	; 主人(变更を加えない)
	CASE -2
	; 其他
	CASEELSE
		LOCALS += "酱"
ENDSELECT
; ここから主人専用处理
IF ARG == MASTER
	; CSTR:20とMASTERNAME:88の同期
	MASTERNAME:88 = %LOCALS%
	; 特定プレイ时のみ呼び名を变えたければここ以下に条件式を追加
ENDIF
RETURNF LOCALS

;========================================================
; 口上互换处理
;-------------------------------------------------
; バージョンUPでフラグの移动などを行う际に使用する函数
; 一日で最初に出会った时に呼ばれる
; とりあえず函数の雏形だけ作っておくが使用する必要がないことを祈る
;========================================================
@K88_KYOKO_UPDATE
WHILE CFLAG:1100 < K88_KYOKO_GET_VARSION()
	SELECTCASE CFLAG:1100
		; 初期バージョン
		CASE IS < 100
			; 呼び名が空ならセット
			IF !STRLENS(CSTR:20)
				CSTR:20 = %CALLNAME:MASTER%
				CALLF K88_KYOKO_CALLNAME(MASTER)
			ENDIF
			CFLAG:1100 = 100
		; ここに互换处理を书く
;		CASE 条件
;			处理
;		;CASE 条件
;			处理
		; 无限ループ予防
		CASEELSE
			CFLAG:1100 = K88_KYOKO_GET_VARSION()
	ENDSELECT
WEND
RETURN 1

