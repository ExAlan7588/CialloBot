@M_KOJO_MESSAGE_COM_K139_DANMAKU(ARGS, 相手残机)
#DIM 相手残机
;全体の流れ
;①【战斗前】→②ダイスロールとプレイヤー选择肢、ハンデをつけた场合は【ハンデ口上】→③对応スキルが发动した场合【スキル口上】→④被弹判定、キャラが被弹したなら【被弹口上】→
;どちらかの残机が0になっていなければ②に戻る→⑤【战斗后口上】

;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
SIF FLAG:口上色
	TRYCALLFORM M_KOJO_COLOR_K{NO:TARGET}
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;战斗前
	;HANDICAP_FIXED = 0
	;HANDICAP_RAND = 0
	;ハンデをつけることができ、数值が大きいほどあなたが不利になる　数值が负ならあなたが有利に
	;キャラの能力に关わらずクッソ强くしたり弱くしたりできる
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "战斗前"
		IF (TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行)
			PRINTFORML 「%CALLNAME:MASTER%大人、姐姐我、有个请求」
			PRINTFORML 「你看、%CALLNAME:MASTER%大人不是相当厉害吗」
			PRINTFORML 「只有一方压倒性地赢、这样不是很无聊吗、%CALLNAME:MASTER%大人不会欺负弱者的对吧？」
			PRINTFORML 「所以呢、能不能对姐姐稍加手下留情呢…♪」
			CALL ASK_YN("好的","不要")
			IF !RESULT
				HANDICAP_FIXED += ABL:MASTER:战斗能力 * 50
			ELSE
				PRINTFORMW 「哼唔…%CALLNAME:MASTER%真是个坏心眼…」
			ENDIF
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%大人。我只不过是只无聊的管狐而已哦」
			PRINTFORML 「真的打算对这么弱的对手全力以赴吗？」
			PRINTFORML 「欺负比自己弱的对手，会让%CALLNAME:MASTER%大人的战绩留下污点的哦」
			PRINTFORML 「如何呢？ 这样能否稍微手下留情一下呢？」
			PRINTW 
			CALL ASK_YN("好的","不行。我要彻底击溃你")
			IF !RESULT
				HANDICAP_FIXED += ABL:MASTER:战斗能力 * 50
			ELSE
				PRINTFORMW 「噫、噫呀…」
			ENDIF
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ハンデ
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "ハンデ"
		;ハンデつけなかったら表示されないので无记入でよい
		IF (TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行)
			PRINTFORML 「好开心♪　%CALLNAME:MASTER%大人对姐姐真是温柔呢♪」
			IF ABL:MASTER:战斗能力 >= 3
				PRINTFORML (这位比\@ ABL:MASTER:战斗能力 > 4 ? 饭纲丸大人 # 我 \@更强的孩子如果全力以赴的话我可是拿他一点办法都没有呢…）
			ELSE
				PRINTFORML （太好了…♪）
				;生意气か反抗的持ち
				IF (TALENT:MASTER:回应 == 1 || TALENT:MASTER:态度 == 1)
					PRINTFORML （如果给予一些限制、或许能够让这个有些倔强的孩子明白个一二呢…♪）
					PRINTFORML （反弹也是一种可爱、但有时候也要听姐姐的话才行呢…♪）
				ELSE
					PRINTFORML （在击败他后、再加言语攻击粉碎他的自尊、再加宠爱他个乱七八糟的…♪）
					PRINTFORML （然后、就会完全依赖我了…完美的计划♪）
				ENDIF
			ENDIF
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%大人果然厉害。真正强大的人是不需要炫耀的」
			PRINTFORML （这真是太容易了呢…这么好操作、真是帮大忙了♪）
			PRINTW 
		ENDIF
	ENDIF
	;-------------------------------------------------
	;战斗中TARGET被弹时口上
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "被弹" 
		;别に一律で「やられたー」だけでもいいよ
		SELECTCASE 相手残机
			;残り残机3が表示されるのは【洽览深识】持ちのみ
			CASE 3
				PRINTFORML 
			CASE 2
				PRINTFORML 「姆姆姆、果然是%CALLNAME:MASTER%大人呢」
				PRINTW 
			CASE 1
				PRINTFORML 「这真是太强了。典这边已经是风中残燭了呢」
				IF HANDICAP_FIXED > 0
					PRINTFORML （好奇怪、明明%CALLNAME:MASTER%确实是在故意放水的）
					PRINTFORML （我居然仍被压着打呢）
				ELSE
					IF ABL:战斗能力 > ABL:MASTER:战斗能力
						PRINTFORML （不可能！？　明明我应该是更强的、是我太低估能力了吗！？）
					ELSE
						PRINTFORML （啊啊、真是的。虽如预期、但被打成这样还真是有些不甘心呢）
					ENDIF
				ENDIF
			CASE 0
				IF HANDICAP_FIXED > 0
					PRINTFORML 「居然输了！？　怎么可能…」
					PRINTFORML （这、这不可能吧！？）
					PRINTFORML （在被放水的状况下、力量差距居然还如此的明显！？）
					PRINTW 
				ELSE
					IF ABL:MASTER:战斗能力 > 2
						PRINTFORML 「完败了。真令人惊艳」
						IF (TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行)
							IF TALENT:恋慕 || TALENT:恋人
								PRINTFORML 「可是、%CALLNAME:MASTER%大人的力量…」
								IF FLAG:周回数
									PRINTFORML （怎么说呢…这孩子的灵魂…灵性与普通孩子相比完全不同呢）
									PRINTFORML （难道、是轮回不断循环吗？）
									PRINTFORML （这也太不可思议了吧。能力保持不变地轮回转生、这可不是普通人能做到的事情）
									PRINTFORML 「啊啊、没什么。只是姐姐的自言自语而已啦」
								ELSE
									IF TALENT:MASTER:动物耳
										IF (TALENT:MASTER:妖狐 || TALENT:MASTER:狐)
											PRINTFORML 「其实…跟八云家的九尾大人有血缘关系吗…」
										ELSE
											PRINTFORML 「跟我一样长着兽耳朵…但其实流著名门妖怪的血液呢…」
											PRINTFORML 「嘛、可能不是跟我一样的管狐啦」
											PRINTW 
											PRINTFORML （啊啊不过…）
											PRINTFORML （在各地都有着七十五只的随从、在附身方面更是没有人能比得上的「同族」存在呢…确实、名字是…）
										ENDIF
									ELSE
										IF (TALENT:MASTER:妖狐 || TALENT:MASTER:狐)
											PRINTFORML 「显然地混有妖怪的血液呢…而且还是相当强大的」
											PRINTFORML 典看着%CALLNAME:MASTER%的眼睛说道
											PRINTFORML 抗议脸贴太靠近时、典说着「又害羞了呀～♪」地抱了过来…
										ELSE
											PRINTFORM 「无论从哪个角度看
											IF (TALENT:MASTER:回应 == 1 || TALENT:MASTER:态度 == 1)
												PRINTFORML 都是一个相当嚣张的人类小孩呢」
												PRINTFORML 对趁乱抱了上来而表示抗议
												PRINTFORML 「这样不是很好吗ー♪　\@ TALENT:恋人 ? 毕竟我们是恋人呢～♪ # 毕竟我只是拥抱喜欢的人而已呀～ \@」
												PRINTW 
											ELSE
												PRINTFORML 都是一个相当可爱的小孩呢」
												PRINTFORML 对被抱住会很害羞而表示抗议
												PRINTFORML 「这样不是很好吗ー♪　\@ TALENT:恋人 ? 毕竟我们是恋人呢～♪ # 毕竟我只是拥抱喜欢的人而已呀～ \@」
												PRINTW 
											ENDIF
										ENDIF
									ENDIF
								ENDIF
							;ショタ恋慕・恋人なし
							ELSE
								PRINTFORML 「「这小小的身体究竟是从哪里来这么大的力量呢…」
								IF TALENT:MASTER:动物耳
									IF (TALENT:MASTER:妖狐 || TALENT:MASTER:狐)
										PRINTFORML 「其实…跟八云家的九尾大人有血缘关系吗…」
									ELSE
										PRINTFORML 「跟我一样长着兽耳朵…但其实流著名门妖怪的血液呢…」
									ENDIF
								ELSE
									IF (TALENT:MASTER:妖狐 || TALENT:MASTER:狐)
										PRINTFORML 「显然地混有妖怪的血液呢…而且还是相当强大的」
										PRINTFORML 典看着%CALLNAME:MASTER%的眼睛说道
										PRINTFORML 抗议脸贴太靠近时、典说着「又害羞了呀～♪」地抱了过来…
									ELSE
										PRINTFORM 「无论从哪个角度看
										IF (TALENT:MASTER:回应 == 1 || TALENT:MASTER:态度 == 1)
											PRINTFORML 都是一个相当嚣张的人类小孩呢」
											PRINTFORML 对趁乱抱了上来而表示抗议
											PRINTFORML 「这样不是很好吗ー♪　累了的话可以靠在姐姐上休息的哦～」
											PRINTW 
										ELSE
											PRINTFORML 都是一个相当可爱的小孩呢」
											PRINTFORML 对被抱住会很害羞而表示抗议
											PRINTFORML 「这样不是很好吗ー♪　累了的话可以靠在姐姐上休息的哦～」
											PRINTW 
										ENDIF
									ENDIF
								ENDIF
							ENDIF
						;大人・战斗能力3以上
						ELSE
							IF TALENT:恋慕 || TALENT:恋人
								PRINTFORML （强大的人…好喜欢%UNICODE(0x2665) *1%）
								PRINTW 
								PRINTFORML 「已经不是我能对付的了、如果想追求强大的力量、最好找个同等强度的对手」
								PRINTFORML 「以%CALLNAME:MASTER%大人的情况来说的话…嗯…」
								SELECTCASE ABL:MASTER:战斗能力
								CASE 6
									PRINTFORML 「地狱女神赫卡提亚大人、以及拥有能够降临神灵之身能力的月人绵月依姬大人…就这两位吧」
									PRINTFORML 「如果能够战胜这两位的话、%CALLNAME:MASTER%就会成为名副其实的幻想乡最强者了呢」
									PRINTFORML 「…实际上也不仅限于幻想乡内了呢」
								CASE 5
									PRINTFORML 「幻想乡的阎魔大人四季映姬大人、栖息于地狱中的鬼的四天王之一、星熊勇仪大人等等…」
									PRINTFORML 「可以直接说是与『神』争斗的程度呢。天上人的战斗、典也完全不清楚是什么样的呢」
									PRINTFORML 「饭纲丸大人的好友百百世大人也是这个范围内的一员吧。那一位、可是有名的大妖呢」
									PRINTFORML 「听说还有更高次元之上的战斗呢…唉、真是不可思议呀」
								CASE 4
									PRINTFORML 「我主饭纲丸大人、曾在某次异变中与我们合作的千亦大人、守矢神社的神明等等…」
									PRINTFORML 「对手的强度逐渐超越人类的极限了呢。不过也是理所当然的呢」
									PRINTFORML 「如果%CALLNAME:MASTER%大人希望的话、和饭纲丸大人的对练就由我来求情、您意下如何呢？」
								CASE 3
									PRINTFORML 「射命丸大人、姬海棠大人、犬走大人、守矢的青椒…咳咳、巫女大人都是适合的对手呢」
									PRINTFORML 「不过、天狗们个个都有一两个怪癖呢…而巫女大人的底细还没有被完全知晓呢」
									PRINTFORML 「就目前来说、何不先和一本正经的犬走大人进行一场对战试试看呢？」
								ENDSELECT
								PRINTW 
							ELSE
								PRINTFORML （咕呜、战力差距果然如此明显呢）
								PRINTW 
								PRINTFORML 「%CALLNAME:MASTER%大人与我这种低级妖怪相比、更应该找一位更有实力的对手才对吧？」
								PRINTFORML 「比如我主或百百世大人那样的…」
								SIF CFLAG:140:弹幕胜负胜利 == 1
									PRINTFORML 「……说起来、似乎已经从主人那里取得过胜利了呢」
								SIF CFLAG:142:弹幕胜负胜利 == 1
									PRINTFORML 「……啊啊、百百世大人也是一样呢」
								IF CFLAG:98:弹幕胜负胜利 == 1 && CFLAG:113:弹幕胜负胜利 == 1
									PRINTFORML 「唔唔唔、那二位、被公认为幻想乡最强者的丝卡提亚大人与绵月依姬大人…也被战胜了吗ー！　好烦呀ー！」
									PRINTFORML 「总感觉太过荒谬呢…」
									PRINTFORML 「要不要停一下呢？　指的是像疯狗一样到处找人弹幕对决」
								ENDIF
								PRINTFORML 「这么强大的您、为什么要找典这样的来做对手呢、真是让人不解啊…」
							ENDIF
							PRINTW 
						ENDIF
						PRINTW 
					;あなたの战斗能力2以下
					ELSE
						PRINTFORML 「不、表现得非常出色呢」
						IF (TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行)
							IF TALENT:恋慕 || TALENT:恋人
								PRINTFORML 「果然是被姐姐看中的%CALLNAME:MASTER%大人、真是了不起呢♪」
								PRINTFORML 「你变得这么强、姐姐也很开心哦♪　嗯ー啾～♪」
									PRINTFORM 在这混乱中典亲了一下、
									IF (TALENT:MASTER:回应 == 1 || TALENT:MASTER:态度 == 1)
										PRINTFORML 让%CALLNAME:MASTER%吓得紧张到发僵
									ELSE
										PRINTFORML 让%CALLNAME:MASTER%脸颊泛红
									ENDIF
									PRINTW 
							ELSE
								IF TALENT:MASTER:性别 == 2
									PRINTFORML 「虽然有句谚语说『士别三日、刮目相看』但…」
									PRINTFORML 「变得更强了呢。姐姐已经无法与之匹敌了呢」
									PRINTFORM 典轻轻摸了摸头
									IF (TALENT:MASTER:回应 == 1 || TALENT:MASTER:态度 == 1)
										PRINTFORML 、但%CALLNAME:MASTER%没有拍开那只手
									ELSE
										PRINTFORML 、%CALLNAME:MASTER%露出了害羞的笑容
									ENDIF
								ELSE
									PRINTFORML 「像我这种程度的妖怪、只要不大意应该就不会输了呢」
									PRINTFORML 「不过不过、也有那种既强大也很狡猾的妖怪呢、所以姐姐的指导也还是有必要的呢」
								ENDIF
								PRINTW 
							ENDIF
						ELSE
							IF TALENT:恋慕 || TALENT:恋人
								PRINTFORML （不愧是我所迷恋的人…%UNICODE(0x2665) *1%）
								PRINTW 
								PRINTFORML 「不过、还请不要大意呢」
								PRINTFORML 「典的实力、在妖怪中堪称下下等。从我往下数能轻轻松松的数过来」
								PRINTFORML 「先不说有更强大的妖怪存在、就连比%CALLNAME:MASTER%大人弱的妖怪之中、也不乏狡猾的家伙」
								PRINTFORML 「所以请不要掉以轻心、请努力精进。典、也会尽微薄之力协助你的」
							ELSE
								PRINTFORML （这家伙、原本我以为会比我还要弱呢）
								PRINTFORML （我果然、不太适合做这种肉体劳动呢）
							ENDIF
							PRINTW 
						ENDIF
					ENDIF
				ENDIF
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;战斗中
	;TARGETの【残忍酷薄】发动时: 相手の被弹からの复归时のボム数が-1される
	;ARGはTARGETの残机
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "残忍酷薄"
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「被『那个』击中的话可是麻烦事呢…还是给你封印起来吧」
			PRINTW 
		CASE 1
			PRINTFORML 「封锁对手的后路。这是战斗的基本吧」
			PRINTW 
		CASE 2
			PRINTFORML 「呵呵呵…我可不会放水的哦」
			PRINTW 
		ENDSELECT
		PRINTW 
	ENDIF
	;-------------------------------------------------
	;战斗中
	;TARGETの【乾坤一掷】发动时
	;ARGはTARGETの残机
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "乾坤一掷"
		
	ENDIF
	;-------------------------------------------------
	;战斗中
	;TARGETの【怪力乱神】发动时
	;ARGはTARGETの残机
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "怪力乱神"
		
	ENDIF
	
	;-------------------------------------------------
	;战斗后
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "战斗后" 
		PRINTFORML 
		;残り残机4が表示されうるのは【洽览深识】持ちのみ
		;残机が1以上ならあなたの负け、0の场合はあなたの胜利
		SELECTCASE 相手残机
		CASE 4
			PRINTFORML 
		CASE 3
			;ハンデ付き负け。ここぞとばかりに煽られる
			;あなたの素質で煽り内容が变化（增加）
			IF HANDICAP_FIXED > 0
				CALL HPH_PRINT,@"「杂鱼HPH　鱼HPH」","L"
				CALL HPH_PRINT,@"「得意忘形地加上限制、然后完败给管狐了HPH」","L"
				SIF TALENT:MASTER:非童贞 == 0
					CALL HPH_PRINT,@"「处男HPH」","L"
				SIF (TALENT:MASTER:形状 == 1 || TALENT:MASTER:形状 == 2)
					CALL HPH_PRINT,@"「短小HPH　包茎HPH」","L"
				SIF TALENT:MASTER:回应 == -1
					CALL HPH_PRINT,@"「死社恐HPH」","L"
				SIF TALENT:MASTER:胆量 == -1
					CALL HPH_PRINT,@"「没出息HPH　只会欺软怕硬HPH」","L"
				SIF ABL:MASTER:话术技能 < 1
					CALL HPH_PRINT,@"「只会对自己感兴趣的东西讲个不停HPH　一点都不会找话题HPH」","L"
				SIF ABL:MASTER:料理技能 < 1
					CALL HPH_PRINT,@"「明明就不擅长做菜还一直不按食谱做出难吃死了的料理HPH」","L"
				SIF ABL:MASTER:音乐技能 < 1
					CALL HPH_PRINT,@"「玩乐器只学了个样子真是浪费HPH」","L"
				SIF (TALENT:MASTER:酒耐性 == -2 || TALENT:MASTER:形状 == -1)
					CALL HPH_PRINT,@"「一喝点酒就满脸通红的HPH」","L"
				CALL HPH_PRINT,@"「真是、无可救药的废柴呢HPH」","L"
				SIF TALENT:恋慕
					CALL HPH_PRINT,@"「不过我就是最喜欢这样的你了呢HPH」","L"
				
				;自虐用
				;CALL HPH_PRINT,@"「ざぁこHPH　ざこ口上作者HPH」","L"
				;CALL HPH_PRINT,@"「仕样を理解してないのにすぐ新しい机能を使いたがるHPH」","L"
				;CALL HPH_PRINT,@"「そのくせDEBUGはユーザー任せHPH」","L"
				;CALL HPH_PRINT,@"「空欄埋めなきゃ病に罹ってるくせに好きな部分と温度差が激しすぎるHPH」","L"
				;CALL HPH_PRINT,@"「スレで感想がないと『自分の口上って、实は需要ない…？』ってすぐ落ち込むHPH」","L"
				
				IF CFLAG:1045 == 1
