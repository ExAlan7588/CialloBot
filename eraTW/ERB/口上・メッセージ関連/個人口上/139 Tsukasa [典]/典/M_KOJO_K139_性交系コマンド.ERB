ELSEIF TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行 && (!TALENT:139:心情 == -1 || !CFLAG:139:勃然大怒)
	IF TALENT:处女 == 1
	ELSE
		SOURCE:反感 = 0
		SOURCE:欢乐 = 200
		SIF SOURCE:反感 < 0
			SOURCE:反感 = 0
