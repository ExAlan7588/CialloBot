;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K139_100
CALL M_KOJO_MESSAGE_COM_K139_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_100_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:发情
			IF (TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行)
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「哦嗯HPH」","L"
				CALL HPH_PRINT,@"「不要～HPH　不要打姐姐的屁股啦～HPH」","L"
				RESETCOLOR
				PRINTL 
				PRINTFORML 啪！　啪！　啪！
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「啊呜HPH　噫嘶HPH　哎咿HPHHPH」","L"
				CALL HPH_PRINT,@"「好过分…HPH　不要欺负姐姐嘛…HPH」","L"
				PRINTL 
				RESETCOLOR
				PRINTFORML 典因发情而性感度高到极致的情况下、身体的感觉甚至强制性地将疼痛变为快乐
				PRINTFORML 屁股受到拍打形成的冲击被感觉化为一股快感的波浪、一直传达到子宫
				PRINTW 
			ELSE
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「唔啊～HPH　咿呀～HPH　噫～HPHHPH」","L"
				RESETCOLOR
				PRINTFORML 每当啪～啪～的声音响起时、都会听到典的叫声
				PRINTW 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「明明很痛的HPH　可是感觉却好舒服啊～HPH　为什么呀HPH」","L"
				RESETCOLOR
				PRINTFORML 典因发情而性感度高到极致的情况下、身体的感觉甚至强制性地将疼痛变为快乐
				PRINTFORML 屁股受到拍打形成的冲击被感觉化为一股快感的波浪、一直传达到子宫
				PRINTW 
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「不要～HPH　不要打了～HPH　好疼～HPH　啊咕～HPH　啊啊ーーHPH　啊啊ーーHPH」","L"
				RESETCOLOR
				PRINTW 
			ENDIF
		ELSE
			IF (TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行)
				IF ABL:受虐属性 >= 2
					CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","W"
					PRINTFORML 「啊、刚刚那个…不是那样的… 姐姐、才没有很舒服哦…」
					PRINTW 
					PRINTFORML %CALLNAME:MASTER%无言地将手挥下
					CALL HPH_PRINT,@"「咿噫咿咿HPH」","W"
					CALL HPH_PRINT,@"「不、不要HPH　很痛啦、停下、停下来、嘻咿嗯HPH」","L"
					PRINTW 
				ELSE
					PRINTFORML 「Kon！！？」
					PRINTFORML 「等、等一下…打屁股是怎…ＫＯＯＯＮ！！」
					PRINTFORML 「姐姐、会生气、的喔！」
					PRINTFORML 「好痛、很痛啊、不要再打了啦、啊！」
					PRINTW 
				ENDIF
			ELSE
				IF ABL:受虐属性 >= 2
					CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","W"
					PRINTFORML 「啊、感觉不一样了、变得舒服了、怎么会ＫＯＯＯＮ！！？」
					PRINTW 
					PRINTFORML 两次、三次的拍在典的屁股上、红晕一次次浮现在那圆滚滚的臀部上
					CALL HPH_PRINT,@"「不行～HPH　不可以在打屁股了～HPH　好痛的不行呀～HPH　ＫＯＯＯＮ！！」","L"
					PRINTW 
				ELSE
					PRINTFORML 「Kon！！？」
					IF GROUPMATCH(TALENT:MASTER:体型,2,3)
						PRINTFORML （好痛好痛好痛！！？　认真的好痛啊！！！）
						PRINTW 
						PRINTFORML 啪啪啪啪啪啪！
						PRINTFORML 「噫噫！！？」
						PRINTW 
						PRINTFORML 「啊、啊…%CALLNAME:MASTER%大人、拜托停下来吧…」
						PRINTFORML 典的眼角溢出了泪水、全身都在颤抖着真的很害怕
						PRINTW 
					ELSE
						PRINTFORML 「等、等一下…打屁股是怎…ＫＯＯＯＮ！！」
						PRINTFORML 「好痛、拜托停下来吧、不要、ＫＯＯＯＮ！」
						PRINTW 
					ENDIF
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		IF (TALENT:MASTER:年龄 == -1 || TALENT:MASTER:幼儿／幼儿退行)
			IF ABL:受虐属性 >= 2
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「ＫＯＮHPH　再来HPH」","L"
					CALL HPH_PRINT,@"「姐姐…被打太多次、好像觉醒什么了呢…HPH」","L"
					PRINTW 
				CASE 1
					CALL HPH_PRINT,@"「ＫＯＯＮHPH」","L"
					PRINTFORML 「喂、喂、不应该打姐姐的屁股…才对吧…」
					PRINTL 
					PRINTFORML 啪！
					CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","L"
					PRINTL 
					PRINTFORML 「哦、姐姐的…」
					PRINTL 
					PRINTFORML 啪！
					CALL HPH_PRINT,@"「屁股HPH　呀HPH」","L"
					PRINTL 
					PRINTFORML 啪！
					CALL HPH_PRINT,@"「别带这样玩的啊HPH　呀啊HPH」","L"
					PRINTW 
				ENDSELECT
			ELSE
				PRINTFORML 啪！
				SELECTCASE RAND:6
				CASE 0
					PRINTFORML 「姐姐、很痛啊！　真的、很痛啊」
				CASE 1
					PRINTFORML 「现在我会原谅你的、所以停下来吧、ＫＯＮ！」
				CASE 2
					PRINTFORML 「喂、喂、不要把姐姐的屁股当太鼓打啊！」
				CASE 3
					PRINTFORML 「啊、之后你就晓得了、ＫＯＮ！」
				CASE 4
					PRINTFORML 「再、再这样下去我真的会生气哦！」
				CASE 5
					IF (TALENT:MASTER:回应 == 1 || TALENT:MASTER:态度 == 1)
						PRINTFORML 「你、你这可恶的小鬼！　之后就给我记住了！　ＫＯＮ！」
					ELSE
						PRINTFORML 「这、这到底是怎么了啊！？　你不是会做这种事的孩子啊！？　ＫＯＮ！」
					ENDIF
				ENDSELECT
				PRINTW 
			ENDIF
		ELSE
			IF ABL:受虐属性 >= 2
				CALL HPH_PRINT,@"「ＫＯＯＮHPH」","L"
				PRINTFORML 「啊、刚刚那个、不是的…并不是因为很舒服…」
				PRINTL 
				PRINTFORML 啪！
				CALL HPH_PRINT,@"「ＫＯＯＯＮHPH」","L"
				PRINTFORML 明显不同于痛苦的音色在悲鸣中交织
				PRINTFORML 而且更重要的是、从秘处流出的爱液量正在增加这一点、已经无言地说明了一切
				PRINTW 
			ELSE
				PRINTFORML 啪！
				PRINTFORML 「ＫＯＯＯＮ！？」
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「对我做这种事、噫！？　好、好痛！」
					PRINTW 
				CASE 1
					PRINTFORML 「啊咕呜！　就、就说好痛了啊…」
					PRINTW 
				CASE 2
					PRINTFORML 「好、好痛！　好痛！！　求求你停下来吧！」
					PRINTW 
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF
RETURN 1

