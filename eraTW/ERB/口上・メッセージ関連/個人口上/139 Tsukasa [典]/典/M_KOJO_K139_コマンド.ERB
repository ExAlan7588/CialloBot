RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_189_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;服を着てない（无罩罩・パンツありはブラを自由に变更できないので多分できない）
		IF TEQUIP:5 && TEQUIP:0 & 0
			PRINTFORML 「这就是传闻中的『裸体围裙』吗…」
			;下着あり
			IF EQUIP:下半身内衣１ || EQUIP:下半身内衣２
				PRINTFORML 「不过、我还穿着内裤呢」
				PRINTFORML 「还是说、希望我将内裤脱下来呢？　亲・爱・的%UNICODE(0x2665) *1%」
				PRINTW 
			ELSE
				PRINTFORML 「感觉凉飕飕的、真是不让人安心呢」
				PRINTFORML 「等一下、难道你想让我穿成这样做家务？」
				CALL HPH_PRINT,@"「不能这样做、如果以这样的姿势来清洁的话、重要的地方会全部暴露出来的…HPH」","L"
				CALL HPH_PRINT,@"「而且这样的话、还有可能会被性欲高涨的%CALLNAME:MASTER%大人给袭击的…HPH」","L"
				CALL HPH_PRINT,@"「如果那样的话…典就可能会无法抵抗被压制住的命运呢…HPH」","L"
				PRINTW 
			ENDIF
		ELSE
			PRINTFORML 「哦哦、这真是新奇的装扮呢」
			PRINTFORML 「说实话、我更倾向于穿厨师服」
			PRINTFORML 「不过按照这样穿确实更像新娘一样呢%UNICODE(0x2665) *1%」
			PRINTW 
			PRINTFORML 「那么、要按照标准的模式来吗？」
			CALL ASK_YN("拜托请做到最好","没关系、不用了")
			IF !RESULT
				PRINTFORMW 「我明白了♪」
				PRINTFORML 「亲爱的、今天也辛苦了呢%UNICODE(0x2665) *1%」
				PRINTFORML 「你是要先吃饭呢？　还是要先洗澡呢？　还是要……先・吃・我・呢？」
				PRINTW 
			ELSE
				PRINTFORML 「啊～嗯、坏心眼」
				PRINTFORML 典用双手交叉于胸前、上下晃动着身体
				PRINTW 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;开始
	IF TEQUIP:新妻PLAY
		PRINTFORML 「呼呼、又来了吗？」
		IF TEQUIP:5 && TEQUIP:0 & 0
			IF DAY:2 == 4
				PRINTFORML 「好冷！　好冷啊！」
				PRINTFORML 典抱着自己的身体颤抖着
				PRINTW 
			;下着あり
			ELSEIF EQUIP:下半身内衣１ || EQUIP:下半身内衣２
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「保留着内衣不脱下来、会感觉有些怪怪的呢」
				CASE 1
					PRINTFORML 「不让人把内衣脱下来、是有什么特殊喜好吗？」
					PRINTFORML 「对典来说、只要是能让%CALLNAME:MASTER%满意的事、典就什么都可以做」
				ENDSELECT
				PRINTW 
			ELSE
				SELECTCASE RAND:3
				CASE 0
					IF TALENT:TARGET:胸围 >= 0
						SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「%CALLNAME:MASTER%大人、典的裸体围裙、你觉得怎么样呢？」
							PRINTFORML 典像是强调胸部一样将身体往前倾
						CASE 1
							PRINTFORML 「我的胸部都快要从围裙中溢出来了呢…」
							PRINTFORML 「好像、不太能遮掩住呢」
						CASE 2
							SELECTCASE RAND:2
							CASE 0
								PRINTFORML 虽然没有什么胸部、但那樱花色的凸起隐约可见…
								PRINTFORML 「因为胸部被摩擦着…感觉有点怪怪的呢…%UNICODE(0x2665) *1%」
							CASE 1
								PRINTFORML 「呼呼、从侧边能看见的胸部也是一种魅力呢♪」
							ENDSELECT
						ENDSELECT
					ELSE
						PRINTFORML 「如果我有更大的胸部、我就能够诱惑到%CALLNAME:MASTER%大人了…」
						PRINTFORML 从小缝隙间看见微小胸部跟凸起的樱花色奶头才是最高境界、如此说道
						CALL HPH_PRINT,@"「真是的…%CALLNAME:MASTER%大人、Ｈ…HPH」","L"
					ENDIF
					PRINTW 
				CASE 1
					PRINTFORML 「嘿嘿、我可不会让你看到屁股的%UNICODE(0x2665) *1%」
					PRINTFORML 典故意用尾巴遮住了屁股、让人有些焦躁
				CASE 2
					PRINTFORML 「在我打扫的时候、不可以从后面偷窥喔？」
				ENDSELECT
				PRINTW 
			ENDIF
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「特地换上这个也是有点…」
				PRINTFORML 「这就是所谓的『情趣play』是吗？」
			CASE 1
				PRINTFORML 「亲爱的、今天就请多多关照了呢…%UNICODE(0x2665) *1%」
			CASE 2
				PRINTFORML 「穿上这个后会不会让我看起来更像%CALLNAME:MASTER%大人的新妻呢…%UNICODE(0x2665) *1%」
			ENDSELECT
			PRINTW 
			RETURN 1
		ENDIF
	;结束
	ELSE
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「不ー要、我不要脱下来」
		PRINTFORML 说出脱下来吧后、典意外地有所抵抗
	CASE 1
		PRINTFORML 「诶～、要脱下来啊」
	CASE 2
		PRINTFORML 「还想在多穿一下啊」
	ENDSELECT
	PRINTW 
		RETURN 1
	ENDIF
