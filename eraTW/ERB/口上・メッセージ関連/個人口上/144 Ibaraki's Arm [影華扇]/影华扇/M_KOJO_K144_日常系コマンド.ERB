;Return 0 if description and KOJO should be shown, 1 if description should be hidden and KOJO shown, 2 if description should be shown and KOJO hidden, and 3 if both description and KOJO should be hidden.
;==================================================
@M_KOJO_MESSAGE_MESSAGECHECK_COM_K144_300
RETURN 0

@M_KOJO_MESSAGE_COM_K144_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～话をした。」とかが消えます。また、削除した场合は自动で口上色が付きません。
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K144_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K144_300_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TFLAG:193 >= 0
		IF TALENT:恋慕 || TALENT:爱欲
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
					PRINTFORML 「为什么你要离那么远呢、%CALLNAME:MASTER%」
					PRINTFORML 「嘛、再往这边靠近一点」
					PRINTFORML 「现在的话、已经没有什么好顾虑了的吧」
					PRINTFORMDW %CALLNAME:TARGET%用右手%CALLNAME:MASTER%轻轻地拉了过来。
					PRINTFORML 
				CASE 1
					PRINTFORML 
					PRINTFORML 「竟然能让身为外道之鬼的我产生这种情感」
					PRINTFORML 「你真是一个不可思议的人类啊、%CALLNAME:MASTER%」
					PRINTFORMW 「不过――出乎意料的、还蛮不错呢」
					PRINTFORML 
				CASE 2
					PRINTFORML 
					PRINTFORML 「喂、在什么有意思的事吗」
					PRINTFORML 「对对、就是有没有有趣的事、之类的？」
					PRINTFORML 「你在说什么啊。如果是%CALLNAME:MASTER%的话总该会有些有趣的事吧」
					PRINTFORMW 「总之、是很开心的事」
					PRINTFORML 
			ENDSELECT
		ELSEIF TALENT:思慕 || TALENT:炮友
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
					PRINTFORML 「能和%CALLNAME:MASTER%在一起，而不会感到无聊真是太好了」
					PRINTFORML 「因为没有为了打发时间而进行胡闹的必要、也不会被巫女盯上」
					PRINTFORMW 「不过和她对着干也是一种乐趣」
					PRINTFORML 
				CASE 1
					PRINTFORML 
					PRINTFORML 「对了、要不要来喝酒？」
					PRINTFORML 「什么？你问我能不能喝？」
					PRINTFORMW 「我可是鬼、好酒可是理所当然的」
					PRINTFORML 
				CASE 2
					PRINTFORML 
					PRINTFORML 「啊啊、是%CALLNAME:MASTER%啊、刚刚好」
					PRINTFORML 「把你吃掉、成为我身体的一部分吧――开玩笑的啦」
					PRINTFORMDW %CALLNAME:TARGET%这么说着戏弄了%CALLNAME:MASTER%一番。
					PRINTFORML 
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 
					PRINTFORML 「奇怪的人类、明明已经知道我是鬼了却还和我对话什么的……」
					PRINTFORMW 「普通的人类可是不会和我有来往的哦？」
					PRINTFORML 
				CASE 1
					PRINTFORML 
					PRINTFORML 「不会对我感到害怕吗」
					PRINTFORMW 「不管怎么和我交流、我的本质都不会变哦」
					PRINTFORML 
				CASE 2
					PRINTFORML 
					PRINTFORML 「人类、你也是个好事的家伙呢」
					PRINTFORMW 「我可是、不知何时心血来潮就会把你吃掉、知道吗？」
					PRINTFORML 
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 
				PRINTFORMW 「这句话刚刚就听过了」
				PRINTFORML 
			CASE 1
				PRINTFORML 
				PRINTFORMW 「嗯？你在说啥？」
				PRINTFORML 
			CASE 2
				PRINTFORML 
				PRINTFORMW 「真无聊、说点别的吧」
				PRINTFORML 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
