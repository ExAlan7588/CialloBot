		PRINTFORMDW %CALLNAME:MASTER%还是决定尊重觉的想法
		RETURN 1
	ELSEIF RESULT == 1
		PRINTFORMDL 显然被绳束缚住的觉没有拒绝%CALLNAME:MASTER%的余力
		TCVAR:49:强行 = 1
			SETCOLOR 250,100,100
			PRINTFORMW %CALLNAME:49%彻底被惹火了…
			RESETCOLOR
			CALL GET_ANGRY(49)
			MARK:49:反抗刻印 = 2
			CFLAG:49:1502 = 1
		RETURN 0
	ENDIF
ENDIF

