;==================================================
;0,爱抚
;==================================================
@M_KOJO_MESSAGE_COM_K49_0
SIF FLAG:70
	SETBIT CFLAG:1491,0
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K49_0_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_0_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「「...这副模样真是H」？这不正是你所想要的样子嘛、变态先生——」
		PRINTFORMDW  尽管娇小的身体正在被享用着，觉的语气还是平静如往常.
		PRINTFORMDW  不过，正享受着觉身体的柔软的%CALLNAME:MASTER%、还是能感受得到她的身体在微微颤抖着...
		PRINTFORMW 「啊呀、在奇怪的地方反而集中注意力了吗？%CALLNAME:MASTER%的这副样子也真是可爱呢—」
		PRINTFORMW 「「才不想被这么说」？但是、的确如此啊～」
		PRINTFORMDW  这样自顾自地讲着的觉，轻轻地抚摩起%CALLNAME:MASTER%的头来
		PRINTFORMW 「「觉才称得上是可爱吧」？呼呼——能这么想的话、还真是多谢夸奖～」
		PRINTFORMDW  声音忽地的颤抖了一下、觉好像不知为何有些开心的样子...
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;插入中
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V插入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ELSE
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A插入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ELSE
			IF !FLAG:70 && (TALENT:2 & 3) == 3
				CALL 渧泣()
			ELSEIF !FLAG:70
				CALL 渧泣()
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:屈服刻印 == 3
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		IF !FLAG:70
			CALL 渧泣()
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1
