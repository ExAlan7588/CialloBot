@M_KOJO_MESSAGE_COM_K49_180
SIF FLAG:70
	SETBIT CFLAG:1491,0
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K49_180_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K49_180_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

