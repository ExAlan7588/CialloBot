	IF NAME:MASTER != "你"
		LOCALS = %NAME:MASTER%
	ELSE
		LOCALS = %CALLNAME:MASTER%
	ENDIF
	PRINTBUTTON @"[名字就叫(%LOCALS%)]", @"%LOCALS%"
	PRINTL 　
	;名前を入力するループ
	$INPUT_LOOP_ONAMAE
	INPUTS
	;/见事な响子口上の丸パクだと感心するがどこもおかしくはない
	; 行头行尾の空白文字削除
	LOCALS = %RESULTS%
	REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
	;10文字以上は受け付けない
	IF STRLENS(LOCALS) > 10
		CLEARLINE 1
		REUSELASTLINE 「…有点太长了呢。您有统称或者昵称吗？」
		GOTO INPUT_LOOP_ONAMAE
	;空白は受け付けない
	ELSEIF RESULTS == ""
		PRINTL 请输入名称
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		; 同姓同名
		IF FLAG:扮演 != 72 && STRCOUNT(LOCALS, "\(八意\)\*\\s\*永琳")
			CLEARLINE 1
			REUSELASTLINE 「…别看着我的名签念字好么，你的名字告诉我」W
			GOTO INPUT_LOOP_ONAMAE
