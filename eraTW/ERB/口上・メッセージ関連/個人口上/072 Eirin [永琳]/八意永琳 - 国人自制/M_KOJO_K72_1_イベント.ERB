		ELSEIF FIRSTTIME("U72-1") &&  (ABL:顺从 < 7 || ABL:侍奉精神 < 7) && STRCOUNT(LOCALS, "\(\(御\|ご\)\(主人\|Master\)\(样\|大人\)\)$")
