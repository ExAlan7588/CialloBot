@STONELEVEL(difficulty)
#DIM C_ID
#DIM difficulty
#DIMS ENEMY
#DIMS 相手候补, 9

相手候补:0 = 久侘歌/桑尼/斯塔
相手候补:1 = 心/小町/妖梦
相手候补:2 = 灵梦/成美/白莲
相手候补:3 = 荷取/小恶魔/菫子
相手候补:4 = 布都/青娥/艾伦
相手候补:5 = 袿姫/隐岐奈/神子/映姫
相手候补:6 = 久侘歌/妖梦/成美/斯塔/心/小町/灵梦/白莲/小恶魔/菫子/荷取/桑尼/心/映姫/艾伦/袿姫/隐岐奈/布都/青娥/神子/赫卡提亚
相手候补:7 = 璎花
相手候补:8 = 璎花

LOCALS = %TEXTR(@"%相手候补:difficulty%")%
C_ID = NameToID(LOCALS)
IF C_ID == -1
	THROW %LOCALS%不存在
ENDIF
CALL PRINT_FACE(C_ID)
IF difficulty == 8
	;ラスボス特殊分岐
	TRYCALLFORM StoneMessage_{C_ID}("lastbattle_b")
ELSE
	TRYCALLFORM StoneMessage_{C_ID}("beginning")
ENDIF
CALL STONEBATTLE(difficulty, C_ID)
RETURN RESULT

