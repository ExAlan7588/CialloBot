@NASIKUZUSI(ARG, 本番はどっちか)
#DIM 前戏１
;1=C 2=B 3=M
#DIM 前戏２
#DIM 本番はどっちか
#DIM 注入量
#DIM 基本注入量
#DIM 好感度上升量
#DIM 信赖低下量
#DIM 回数
#DIM 经验增加数,58
#DIMS 日记记述
;０ならＶ、１ならＡ
VARSET 经验增加数
CALL KOJO_MESSAGE_SEND("EVENT",26,ARG,4,0)
回数 = MAX(MIN(ABL:ARG:欲望 / 2, 5) + CFLAG:ARG:积攒度 / 200,1)
注入量 = 0
IF TALENT:ARG:无接吻经验
	前戏１ = 3
	经验增加数:4 += 3 + ABL:ARG:Ｍ感觉 * 2
	经验增加数:25 = 经验增加数:4
	经验增加数:27 = 经验增加数:4
	EXP:MASTER:Ｍ经验 += 经验增加数:4
	EXP:MASTER:接吻经验 += 经验增加数:4
	EXP:MASTER:口淫经验 += 经验增加数:4
ELSEIF !RAND:3
	前戏１ = 3
	经验增加数:4 += 3 + ABL:ARG:Ｍ感觉 * 2
	经验增加数:27 = 经验增加数:4
ELSEIF ABL:ARG:Ｃ感觉 >= ABL:ARG:Ｂ感觉
	前戏１ = 1
	经验增加数:0 += RAND:(回数 * 2) 
	经验增加数:5 += 经验增加数:0 / 2
	经验增加数:10 += 经验增加数:0 / 2
ELSE
	前戏１ = 2
	经验增加数:3 += RAND:(回数 * 2) 
	经验增加数:8 += 经验增加数:3 / 2
	经验增加数:10 += 经验增加数:3 / 2
ENDIF
前戏２ = RAND:3

SIF 本番はどっちか == 1
	前戏２ += 10
IF 本番はどっちか
	IF HAS_PENIS(MASTER)
		基本注入量 = (ABL:ARG:肛门 + 1) * 20 + BASE:MASTER:精力 /25
		经验增加数:21 += 回数
		经验增加数:17 += 回数
		经验增加数:10 += 回数
		经验增加数:7 += 2 * 回数
		经验增加数:2 += 回数 * 2
	ELSE
		经验增加数:10 += 回数
		经验增加数:7 += 2 * 回数
		经验增加数:2 += 回数 * 2
	ENDIF
ELSE
	IF HAS_PENIS(MASTER)
		基本注入量 = (ABL:ARG:膣 + 1) * 20 + BASE:MASTER:精力 / 25
		SIF TALENT:ARG:处女 == 1
			基本注入量 += 100
		经验增加数:20 += 回数
		经验增加数:16 += 回数
		经验增加数:10 += 2 * 回数
		经验增加数:6 += 2 * 回数
		经验增加数:1 += 2 * 回数
	ELSE
		经验增加数:10 += 2 * 回数
		经验增加数:6 += 2 * 回数
		经验增加数:1 += 2 * 回数
	ENDIF
ENDIF

IF HAS_PENIS(MASTER)
	EXP:MASTER:插入经验 += 回数
	EXP:MASTER:射精经验 += 回数
	EXP:MASTER:绝顶经验 += 回数
ENDIF

IF HAS_PENIS(MASTER)
	FOR LOCAL,1,回数
		注入量 += 基本注入量 + RAND:(基本注入量)
	NEXT
ENDIF

注入量 = MIN(注入量, BASE:MASTER:精力)

SIF !HAS_PENIS(MASTER)
	注入量 = 0
IF !本番はどっちか
	CALL ADD_SAMEN_V(ARG, 注入量)
ELSE
	CALL ADD_SAMEN_A(ARG, 注入量)
ENDIF

;口上チェック,存在しないならRESULT = 2
CALL KOJO_MESSAGE_SEND("ONABARE", 3, ARG, 前戏１, 本番はどっちか, "", "", 回数, 注入量)
IF RESULT == 2
	;以下默认地の文
	PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%看着动摇的%NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%冒失的闯了进来还闭上了门、
	PRINTFORMW 抱住%NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%打听她现在想要什么
	PRINTFORM 然后按照她的期望、

	SELECTCASE 前戏１
		CASE 1
			PRINTDATA
				DATAFORM 捏着阴蒂
				DATAFORM 用手指弹阴蒂
				DATAFORM 用手揉捏着阴蒂
				DATAFORM 舔着阴蒂
			ENDDATA
		CASE 2
			PRINTDATA
