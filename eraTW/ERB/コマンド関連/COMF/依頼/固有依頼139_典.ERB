@固有依赖139(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE ARG % 1000
;ID101：分一些精液
CASE 101
	SELECTCASE O_DATA
	CASE "依赖名"
		CALLF MAKE_STR(V_NAME, "分一些精液")
	CASE "依赖内容表示"
		CALLF MAKE_STR(V_NAME, "提供浓厚的精液")
	CASE "依赖报酬表示"
		CALLF MAKE_STR(V_NAME, @"%金额表示(50000)%")
	CASE "依赖实行指令名"
		CALLF MAKE_STR(V_NAME, "精液采取")
		
	CASE "依赖内容"
		CALLF MAKE_INT(V_NAME, MAKE_IRAI_DATA(0, "作业"))
	CASE "依赖サブ"
	CASE "依赖期限"
	;发生时ではなく受托时に确定します
		CALLF MAKE_INT(V_NAME, 30)
	CASE "依赖发生率"
	;これは他の固有依赖との相对概率なんで10％ではありません
		IF TCVAR:发情 || CFLAG:TARGET:积攒度 >= 750
			CALLF MAKE_INT(V_NAME, 100)
		ELSE
		CALLF MAKE_INT(V_NAME, 20)
		ENDIF
	CASE "依赖发生条件"
	SIF EXP:139:精液经验 > 0 && (TALENT:MASTER:性别 == 2 || TALENT:MASTER:性别 == 3)
		CALLF MAKE_INT(V_NAME, 1)
	CASE "依赖受托可能条件"
	;受托可能は@COM_ABLE366も确认のこと
	;ここを参照するのは依赖人をTARGETにしてる时なので素通しでよし
		CALLF MAKE_INT(V_NAME, 1)
	CASE "依赖实行可能条件"
	;实行可能は@COM_ABLE367も确认のこと
		IF FLAG:70
		ELSE
			;SIF TALENT:MASTER:浓厚精液
			CALLF MAKE_INT(V_NAME, 1)
		ENDIF
	;成功/失败处理はCARRYOUTで直接书く
	;SOURCEやITEM入手など、内容が复杂なので
	CASE "依赖消灭条件"
	;30％で消失
		SIF RAND:100 < 30
			CALLF MAKE_INT(V_NAME, 1)
	ENDSELECT
ENDSELECT



;固有依赖の设定用
;この函数は实行/成功/失败时の
;报酬とペナルティと地の文のみを扱います
;各角色のメッセージは别所にて
@IRAI_固有139(CHARA, IRAI_ID, SCENE)
#DIM  CHARA
#DIM  IRAI_ID
#DIMS SCENE
#DIM  CLIENT_ID
CLIENT_ID = IRAI_ID_TO_CLIENT(IRAI_ID)
SELECTCASE STR_DATA_IRAI(IRAI_ID, "依赖名", CLIENT_ID)
	
CASE "分一些精液"
	SELECTCASE SCENE
	CASE "依赖条件表示"
	;何か専用の表示情报を设けたい时用
	;受托済みの依赖表示で使用されます
	;
	;PRINT系だけでSOURCEとかは入れちゃダメ、WAITも无しで
	;ここ以外の表示项目は
	;PRINTFORML 　　　　　：
	;となっているのでスペースを揃えたほうが见栄えが良いです
	
	CASE "依赖实行时"
	;このCASEに限り、RESULTが意味を持ちます
	;依赖实行时のRESULTにはIRAI_CONST_RESULT()を使用してください
	;可能な参数は"中止", "实行して成功", "实行して失败", "实行して未解决"です
	;独自处理を仕込みたい时は"实行して未解决"で先延ばしするなりしてください
		DOWNBASE:MASTER:气力 += 50
		DOWNBASE:MASTER:精力 += 250
		IF TALENT:139:处女 == 1
			IF EXP:口淫经验 > 0
				PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%一边想象着典用嘴拔蘑菇的情景、一边抚摸着阴茎…
			ELSE
				PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%一边想象着典的裸体、一边抚摸着阴茎…
			ENDIF
		ELSE
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%一边回想着与典的性交、一边抚摸着阴茎…
		ENDIF
		TIME += 10
			IF TALENT:MASTER:浓厚精液
				PRINTFORML 在典给予的试管里、注込了满满的浓如糨糊的精液…
				PRINTFORML 无论是量还是粘度都无可挑剔吧
				PRINTW 
				RETURN IRAI_CONST_RESULT("实行して成功")
			ELSE
				PRINTFORML 在典给予的试管里、注满了精液…
				PRINTFORML 但是、典的委托索要的精液需要是浓厚的
				PRINTFORML 这种的话应该不会被接受吧
				PRINTL 
				PRINTFORML ※还是改日等在[浓厚精液]状态下再进行吧
				PRINTW 
				RETURN IRAI_CONST_RESULT("实行して未解决")
			ENDIF
			SOURCE:MASTER:达成 = 500
	CASE "依赖成功时"
	;成功报告时の报酬など
		CALL KIGEN_CHANGE(TARGET, 100, 1)
		SOURCE:TARGET:欢乐 = 1000
		SOURCE:TARGET:征服 = 500
		SOURCE:TARGET:达成 = 500
		SOURCE:TARGET:情欲 = 3000
		SOURCE:MASTER:情欲 = 1000
		;システムメッセージつき好感度变化
		;最后の参数だけ好感度が变化
		CALL CHANGE_CFLAG(2, TARGET, 40)
		;システムメッセージつき信赖度变化
		;最后の参数だけ信赖度が变化
		CALL CHANGE_CFLAG(4, TARGET, 25)
		CALL GET_MONEY_YEN(50000, 2)
	CASE "依赖失败时"
	;失败报告时の处理
		;システムメッセージつき好感度变化
		;最后の参数だけ好感度が变化
		CALL CHANGE_CFLAG(2, TARGET, -20)
		;システムメッセージつき信赖度变化
		;最后の参数だけ信赖度が变化
		CALL CHANGE_CFLAG(4, TARGET, -10)
	ENDSELECT
ENDSELECT
