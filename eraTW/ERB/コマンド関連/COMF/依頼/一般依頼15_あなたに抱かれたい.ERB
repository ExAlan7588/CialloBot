;存否函数
;F函数はTRYCALL系が使えないので通常函数で存否配列を作っちゃう
@EXIST_一般依赖15
#LOCALSIZE 1
#LOCALSSIZE 1

;オブジェクト本体
;ここでARGで渡されるのはCLIENT_IDです
@一般依赖15(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "依赖名"
	IF TALENT:ARG:无知
		CALLF MAKE_STR(V_NAME, @"%TEXTR("心脏砰砰直跳的/晃晃悠悠站不稳的")%")
	ELSEIF TALENT:ARG:受虐狂
		CALLF MAKE_STR(V_NAME, @"想被%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%%TEXTR("侵犯/弄得乱七八糟的")%")
	ELSE
		CALLF MAKE_STR(V_NAME, @"想和%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%做爱")
	ENDIF
CASE "依赖内容表示"
	IF TALENT:ARG:无知
		CALLF MAKE_STR(V_NAME, "无论如何")
	ELSEIF TALENT:ARG:受虐狂
		CALLF MAKE_STR(V_NAME, "想要被欺负")
	ELSE
		CALLF MAKE_STR(V_NAME, @"消解%NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%的欲求不满")
	ENDIF
CASE "依赖报酬表示"
	IF TALENT:ARG:无知
		CALLF MAKE_STR(V_NAME, "报酬")
	ELSE
	;性技术を卖ると考えたら技巧*5000円とかのがいいのかも？
		CALLF MAKE_STR(V_NAME, @"%金额表示(20000)%(援金)")
	ENDIF
CASE "依赖实行指令名"
	IF TALENT:ARG:无知
		CALLF MAKE_STR(V_NAME, "无论如何都想做的事情")
	ELSEIF TALENT:ARG:受虐狂
		CALLF MAKE_STR(V_NAME, @"侵犯%NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%")
	ELSE
		CALLF MAKE_STR(V_NAME, @"和%NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%做")
	ENDIF
CASE "依赖内容"
	CALLF MAKE_INT(V_NAME, MAKE_IRAI_DATA(0, "作业"))
CASE "依赖サブ"
CASE "依赖期限"

;发生时ではなく受托时に确定します
CASE "依赖发生率"

	;初期设定で风骚幻想乡にした场合は依赖发生频度高すぎると阿鼻叫唤になりそうだから风骚の场合は概率低下
	;かわりに积攒度に关わらず发生するようにして蔷薇ンスが取れるといいなぁ
	IF TALENT:ARG:风骚
		CALLF MAKE_INT(V_NAME, 4)
	ELSEIF TALENT:ARG:自尊心 == 1 || TALENT:ARG:羞耻心 == 1
		;自尊心が高かったり害羞を持ってる角色は言い出しづらいだろうから概率低下
		;CALLF MAKE_INT(V_NAME, 9)
		CALLF MAKE_INT(V_NAME, 50)
	ELSE
		;それ以外は发生条件が严しめなので基本の发生率はとりあえず高めに
		;CALLF MAKE_INT(V_NAME, 15)
		CALLF MAKE_INT(V_NAME, 70)
	ENDIF

CASE "依赖发生条件"
;炮友or爱欲かつ积攒度500以上 or 风骚
	;debug
	;SIF TALENT:ARG:处女
	SIF (TALENT:ARG:炮友 && CFLAG:ARG:积攒度 > 500) || (TALENT:ARG:爱欲 && CFLAG:ARG:积攒度 > 500) || TALENT:ARG:风骚
		CALLF MAKE_INT(V_NAME, 1)
CASE "依赖受托可能条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "依赖实行可能条件"
	CALLF MAKE_INT(V_NAME, 1)
;成功/失败处理はCARRYOUTで直接书く
;SOURCEやITEM入手、消费が烦杂になるので
CASE "依赖消灭条件"
;依赖以外の要因で积攒度が解消された场合消えるべきなのかもしれないけど文法とかよくわかんないので杂にスルー
	SIF RAND:100 < 30
		CALLF MAKE_INT(V_NAME, 1)
CASE "依赖人连れ出し可能"
	CALLF MAKE_INT(V_NAME, 1)
ENDSELECT



