@COM401
#DIM 种类
FLAG:700 = 0
FOR LOCAL,1,CHARANUM
	FOR 种类,0,MAXPANTS
		CFLAG:LOCAL:(种类 + 胖次管理) += PANTS_TEMP:LOCAL:种类
		SIF PANTS_TEMP:LOCAL:种类
		PRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL)%の%PANTSNAME(种类, LOCAL)%在收藏中追加了
		PANTS_TEMP:LOCAL:种类 = 0
		CFLAG:LOCAL:293 = 0
		SIF CFLAG:LOCAL:(种类 + 胖次管理)
			FLAG:700 ++
	NEXT
NEXT
FLAG:胖次输送 = 0

PRINTFORMW 现在收藏有{FLAG:700}种
RETURN 1

