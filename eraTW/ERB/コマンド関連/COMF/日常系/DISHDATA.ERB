@EHOU_DIRECTION()
#FUNCTIONS
GETTIME
LOCAL = RESULT / 10000000000000
LOCAL:1 = DAY_TO_DATE(DAY)
LOCAL:1 = LOCAL:1 / 1000
LOCAL = LOCAL + LOCAL:1
SELECTCASE LOCAL % 10
CASE 4, 9
	RETURNF "东北东"
CASE 0, 5
	RETURNF "西南西"
CASE 1, 6, 3, 8
	RETURNF "南南东"
CASE 2, 7
	RETURNF "北北西"
ENDSELECT
