RESULT:1 = SKILL_LV
RESULTS:1 = %TYPECHECK%
RETURN RESULT
;-------------------------------------------------
;料理の材料を设定する函数
;材料は１つで作れるので多いほど作る手段が多い≒作りやすい
;"泛用"を入れると泛用食材がセットされる
;"鱼"を入れると鱼食材がセットされる
;"作成不能"を入れるとどうやっても作れないようになる（表示はされる）
;-------------------------------------------------
@材料设定(ARGS:1, ARGS:2, ARGS:3, ARGS:4, ARGS:5, ARGS:6, ARGS:7, ARGS:8, ARGS:9, ARGS:10)
VARSET DISH_ING, -1
IF ARGS:1 == "泛用"
	DISH_ING:1 = GETNUM(ITEM, "高级食材")
	DISH_ING:2 = GETNUM(ITEM, "优质食材")
	DISH_ING:3 = GETNUM(ITEM, "平价食材")
	RETURN
ELSEIF ARGS:1 == "鱼"
	DISH_ING:1 = GETNUM(ITEM, "好吃的鱼")
	DISH_ING:2 = GETNUM(ITEM, "鱼")
	RETURN
ELSEIF ARGS:1 == "虫"
	DISH_ING:1 = GETNUM(ITEM, "稀有昆虫")
	DISH_ING:2 = GETNUM(ITEM, "虫")
	RETURN
ELSEIF ARGS:1 == "作成不能"
	FOR LOCAL, 1, 11
		DISH_ING:LOCAL = -1
	NEXT
	RETURN
ENDIF
FOR LOCAL, 1, 11
	IF ARGS:LOCAL == ""
		;跳蛋が0番なので无いものは-1を入れておく
		DISH_ING:LOCAL = -1
	ELSE
		DISH_ING:LOCAL = GETNUM(ITEM, ARGS:LOCAL)
		IF GETNUM(ITEM, ARGS:LOCAL) == -1
			PRINTFORML 道具名出错了哦！＞%ARGS:LOCAL%
			THROW
		ENDIF
	ENDIF
NEXT

;-------------------------------------------------
;DISH_TASTEにあとから付け加える函数
;ARG    食材のID
;ARGS   加える文字列,使ってないけど一应用意した
;-------------------------------------------------
@DISH_TASTE_ADD(ARG, ARGS)
VARSET RESULTS
LOCALS = 
IF ARGS != ""
	DISH_TASTE += ARGS + "/"
	;角色依存のものは@DISH_TASTE_FROM_CHARAでやる
ENDIF

IF ARG >= 0 && ITEMNAME:ARG != ""
	SELECTCASE ITEMNAME:ARG
	;肉は野鸡肉以外追加しない
	CASE "野味", "禽肉"
	;冰淇淋は追加しない
	CASE "冰淇淋"
	;蘑菇素材は[蘑菇]を追加する
	CASE "普通的菌菇", "毒菌菇", "粘性菌菇", "冬虫夏草", "可疑菌菇", "魔法菌菇"
		DISH_TASTE += "蘑菇/"
	;草莓と西瓜と仙桃は[果物]と两方追加する
	CASE "草莓", "西瓜", "仙桃", "木莓", "山葡萄"
		DISH_TASTE += ITEMNAME:ARG + "/"
		DISH_TASTE += "果物/"
	;キイチゴ,ヤマブドウはさらに[野趣]を追加する
	CASE "草莓", "西瓜", "仙桃", "木莓", "山葡萄"
		DISH_TASTE += ITEMNAME:ARG + "/"
		DISH_TASTE += "果物/"
		DISH_TASTE += "野趣/"
	;(酒造パッチ)红酒は[家庭的]を陈酿红酒は[上品]を优质陈酿红酒は[上品]と[豪华]を追加する
	CASE "红酒"
		DISH_TASTE += "家庭的/"
	CASE "陈酿红酒"
		DISH_TASTE += "上品/"
	CASE "优质陈酿红酒"
		DISH_TASTE += "上品/"
		DISH_TASTE += "豪华/"
	CASEELSE
		DISH_TASTE += ITEMNAME:ARG + "/"
	ENDSELECT
ENDIF

