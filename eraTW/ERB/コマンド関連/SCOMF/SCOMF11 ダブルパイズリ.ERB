;-------------------------------------------------
;双重乳交
;助手による指令、及びレズプレイ、レベル3
;-------------------------------------------------
@SCOM11
;-------------------------------------------------
;实行决定
;-------------------------------------------------
STR:0 = 双重乳交
;-------------------------------------------------
;COM别FLAGの设定
;-------------------------------------------------
;行为者
TCVAR:116 = TARGET
;TCVAR:0 射精对象フラグ（ビット0=TARGET 1=PLAYER 2=MASTER 3=ASSI）
;TCVAR:1 予备射精对象フラグ（ビットを使いたくない人用、CFLAG:100に翻译されます）	（1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI）
;TCVAR:2 射精部位フラグ（-1=避孕套 1=膣内 2=肛门 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛门侍奉 10=道具 11=触手）

;二人目
LOCAL = MASTER_POSE(SET_B,SET_P,1)
LOCALS = {TARGET}/{LOCAL}/
CSTR:PLAYER:10 += LOCALS
TCVAR:MASTER:2 = 5
;强度
TCVAR:MASTER:4 = 2000
;侍奉快乐
TCVAR:108 = 1
TCVAR:LOCAL:108 = 1

;-------------------------------------------------
;ソース追加处理
;-------------------------------------------------
;消费体力、气力
DOWNBASE:体力 += 10
DOWNBASE:气力 += 100
SOURCE:MASTER:快Ｃ = 150

SOURCE:快Ｂ = 150
SOURCE:情爱 = 200
SOURCE:性行动 = 500
SOURCE:达成 = 250
SOURCE:屈从 = 900
SOURCE:偏离正轨 = 400
SOURCE:反感 = 400

;上のほうで计算した污れデータ
SOURCE:不洁 = Y*40 + 100

;消费体力、气力
DOWNBASE:LOCAL:体力 += 10
DOWNBASE:LOCAL:气力 += 100

SOURCE:LOCAL:快Ｂ = 150
SOURCE:LOCAL:情爱 = 100
SOURCE:LOCAL:性行动 = 250
SOURCE:LOCAL:达成 = 120
SOURCE:LOCAL:屈从 = 450
SOURCE:LOCAL:偏离正轨 = 400
SOURCE:LOCAL:反感 = 400

;上のほうで计算した污れデータ
SOURCE:LOCAL:不洁 = Y*40 + 100
;-------------------------------------------------
;特殊ソース处理
;-------------------------------------------------
;圧抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:郁闷 += 600
SIF TALENT:LOCAL:自己爱 < 0 || TALENT:LOCAL:抵抗
	SOURCE:LOCAL:郁闷 += 600

;-------------------------------------------------
;污れの处理
;-------------------------------------------------
;奴隷のＢ⇔调教者のＰ的污垢が移动
STAIN:5 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:5

;助手のＢ⇔调教者のＰ的污垢が移动
STAIN:LOCAL:5 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:LOCAL:5

;-------------------------------------------------
;経験の处理
;-------------------------------------------------
EXP:LOCAL:乳交经验 ++
EXP:乳交经验 ++
;同性爱　レズなら返回值1ゲイなら返回值2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 3
ENDIF
;同性爱　レズなら返回值1ゲイなら返回值2
IF HETEROSEX(TARGET,LOCAL)
	EXP:LOCAL:(50 + HETEROSEX(TARGET,LOCAL)) += 3
	EXP:TARGET:(50 + HETEROSEX(TARGET,LOCAL)) += 3
ENDIF
;同性爱　レズなら返回值1ゲイなら返回值2
IF HETEROSEX(LOCAL,PLAYER)
	EXP:LOCAL:(50 + HETEROSEX(LOCAL,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(LOCAL,PLAYER)) += 3
ENDIF

RETURN 1

@CAN_SCOM11(ARG)
SKIPDISP ARG
PRINTL 双重乳交
;-------------------------------------------------
;实行判定处理
;-------------------------------------------------
A = 0
S = 0

;全部の命令に共通の要素を考虑
;(顺从が高いと命令に从いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	S = 1
ENDIF
;ABL:侍奉精神
IF ABL:侍奉精神
	SIF S
		PRINT  + 
	A += ABL:侍奉精神 * 4
	PRINT 侍奉精神
	PRINTV 'LV,ABL:侍奉精神,'(,ABL:侍奉精神 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 3
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
	S = 1
ENDIF

IF MARK:快乐刻印
	SIF S
		PRINT  + 
	A += MARK:快乐刻印 * 1
	PRINT 快乐刻印
	PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 1,')
	S = 1
ENDIF
;PALAM:情欲
IF GETPALAMLV(PALAM:情欲,5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:情欲,5) * 1
	PRINT 情欲
	PRINTV 'LV,GETPALAMLV(PALAM:情欲,5),'(,GETPALAMLV(PALAM:情欲,5) * 1,')
	S = 1
ENDIF
;害羞
IF TALENT:羞耻心 > 0
	PRINT  - 
	A -= 1
	PRINT 害羞
	PRINTV '(,1,')
	S = 1
ENDIF
;污臭钝感
IF TALENT:污臭耐性 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 污臭钝感
	PRINTV '(,1,')
	S = 1
ENDIF
;污臭敏感
IF TALENT:污臭耐性 < 0
	PRINT  - 
	A -= 3
	PRINT 污臭敏感
	PRINTV '(,3,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINT 献身的
	PRINTV '(,6,')
	S = 1
ENDIF
;否定快感（淫乱、娼妇で无效）
IF TALENT:快感应答 < 0 && !TALENT:淫乱
	PRINT  - 
	A -= 1
	PRINT 否定快感
	PRINTV '(,1,')
	S = 1
ENDIF
;讨厌男人（恋慕、亲爱で无效）
IF OTOKOGIRAI(TARGET) == 1
	PRINT  - 
	A -= 12
	PRINT 讨厌男人
	PRINTV '(,12,')
	S = 1
ENDIF
;淫乱
IF TALENT:淫乱 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 淫乱
	PRINTV '(,5,')
	S = 5
ENDIF
;娼妇
IF TALENT:淫乱 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 娼妇
	PRINTV '(,5,')
	S = 5
ENDIF

;调教者が扶她
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT 扶她
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;爱液的污垢
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液的污垢
SIF STAIN:PLAYER:2 & 4
	Y += 3
;肛门的污垢
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15
Y = Y * (2 - TALENT:污臭耐性) / 2

;有污垢
IF Y
	PRINT  - 
	A -= Y
	PRINT 有污垢
	;污臭钝感
	IF TALENT:污臭耐性 > 0
		PRINT 、污臭钝感
	ELSEIF TALENT:污臭耐性 < 0
		PRINT 、污臭敏感
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合计を表示(38以上で实行)
PRINT  = 
PRINTV A

V = 38
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 实行值
PRINTV V

WAIT
SKIPDISP 0
;实行できない
SIF A < V
	RETURN 0
RETURN 1

;-------------------------------------------------
;选择可能判定
;-------------------------------------------------
@COM_ABLE511
;双重乳交
SIF !TFLAG:100
	RETURN 0
;批量管理
SIF GLOBAL_COMABLE(511)
	RETURN RESULT
LOCAL = MASTER_POSE(SET_B,SET_P,1)
;主人が男人か半人半妖か扶她じゃないとダメ
SIF !(TALENT:PLAYER:2 & 2)
	RETURN 0
;猫舌の奴隷の场合は技巧Lv2以上必要
SIF TALENT:猫舌 && ABL:技巧 < 2
	RETURN 0
;猫舌の助手の场合は技巧Lv2以上必要
SIF TALENT:LOCAL:猫舌 && ABL:LOCAL:技巧 < 2
	RETURN 0

;助手が侍奉精神ＬＶ３以上、技巧ＬＶ４以上じゃないとダメ
SIF ABL:LOCAL:技巧 < 4 || ABL:LOCAL:侍奉精神 < 3
	RETURN 0
;助手とTARGETの两方に胸がないとダメ
SIF !(TALENT:2 & 1) || !(TALENT:LOCAL:2 & 1)
	RETURN 0
;上半身着衣
SIF TEQUIP:1 > 1
	RETURN 0
;绳使用中は不可
SIF TEQUIP:绳 || TEQUIP:LOCAL:绳
	RETURN 0
;停止中は不可
SIF FLAG:70 == 1
	RETURN 0
RETURN 1

@ASSISTANT_KOJO_SCOM11
