		CALL COLORMESSAGE(@"自慰经验＋{ABL:LOCAL:自慰中毒 * 2 + 1}",C_PINK,1)
		SELECTCASE ABL:LOCAL:自慰中毒
			CASE 0 TO 2
				珠入手倍率 = 5
			CASE 3 TO 5
				珠入手倍率 = 3
			CASEELSE
				珠入手倍率 = 1
		ENDSELECT
		;快C
		CALL JUEL_UP_SPECIAL(LOCAL,"快Ｃ","Ｃ感觉",珠入手倍率,1)
		SIF 追加Ｖ自慰
			CALL JUEL_UP_SPECIAL(LOCAL,"快Ｖ","Ｖ感觉",珠入手倍率,1)
		SIF 追加Ａ自慰
			CALL JUEL_UP_SPECIAL(LOCAL,"快Ａ","Ａ感觉",珠入手倍率,1)
		SIF 追加Ｂ自慰
			CALL JUEL_UP_SPECIAL(LOCAL,"快Ｂ","Ｂ感觉",珠入手倍率,1)
		;羞耻
		CALL JUEL_UP_SPECIAL(LOCAL,"羞耻","自慰中毒",珠入手倍率 * 2,1)
		;情欲
		CALL JUEL_UP_SPECIAL(LOCAL,"情欲","欲望",珠入手倍率,1)
		CFLAG:LOCAL:积攒度 /= 4
		PRINTW
		IF CFLAG:LOCAL:340 >= 2000
			SETCOLOR C_CREAM
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL)%的身体不断渴求受孕...
			RESETCOLOR
			CFLAG:LOCAL:生子补正 ++
			PRINTFORMW
		ENDIF
	ENDIF
	;まず欲望で基礎值を决める
	SELECTCASE ABL:LOCAL:欲望
		CASE 0 TO 2
			欲求不满度 = 10
		CASE 3 TO 5
			欲求不满度 = 20
		CASE 6 TO 10
			欲求不满度 = 30
		CASE 10 TO 15
			欲求不满度 = 50
		CASEELSE
			欲求不满度 = 80
	ENDSELECT

	欲求不满度 += ABL:LOCAL:自慰中毒 * 7 + (ABL:LOCAL:膣射中毒 + ABL:LOCAL:肛射中毒) * 3
	欲求不满度 = 欲求不满素質补正(LOCAL,欲求不满度)
	SELECTCASE 欲求不满度
		CASE IS > 400
			欲求不满度 = (欲求不满度 - 400) / 10 + 400
		CASE IS > 200
			欲求不满度 = (欲求不满度 - 200) / 4 + 200
		CASE IS > 100
			欲求不满度 = (欲求不满度 - 100) / 2 + 100
	ENDSELECT
	SIF 自慰タイプ
		欲求不满度 = 0
	SIF TCVAR:LOCAL:今天有否见面
		欲求不满度 /= 2
	SIF TALENT:LOCAL:妊娠
		欲求不满度 /= 5
	SIF TALENT:LOCAL:育儿中
		欲求不满度 /= 2
	;积攒度は0~1000の间に
	CFLAG:LOCAL:积攒度 = LIMIT(CFLAG:LOCAL:积攒度 + 欲求不满度, 0, 1000)
	DEBUGPRINTFORML %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL)%的满足度：{满足度}　欲求不满上升值：{欲求不满度}　积攒度：{CFLAG:LOCAL:积攒度}
NEXT

