@GOD_FUTANARI
CALL COLORMESSAGE("嚯、没想到人类察觉到了吾的正体",C_D_RED,2,1)
PRINTFORML 
PRINTFORMW 声音主人的气息变强大了……
CALL COLORMESSAGE("你对扶她肉棒的爱真是了不起",C_D_RED,1,1)
CALL COLORMESSAGE("给你奖励、说出你的愿望吧",C_D_RED,1,1)
$LOOP
CALL ASK_M("没什么",1,"长出扶她肉棒",1,"强化扶她肉棒",1,"想吃扶她肉棒",1,"请给我内裤",1)
SELECTCASE RESULT
	CASE 0
		CALL COLORMESSAGE("不懂风趣……你应该展示出光荣接受恩惠的样子",C_D_RED,1,1)
		CALL COLORMESSAGE("就这样吧",C_D_RED,1,1)
	CASE 1
		PRINTFORML 选谁？
		DRAWLINE
		FOR LOCAL,0,CHARANUM
			SIF TALENT:LOCAL:性别 == 1
				PRINTFORMLC [{LOCAL}] %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL),15,LEFT%
		NEXT
		DRAWLINE
		PRINTFORML [999] 算了
		INPUT
		IF INRANGE(RESULT,0,CHARANUM) && TALENT:RESULT:性别 == 1
			TALENT:RESULT:性别 = 3
			MAXBASE:RESULT:精力 = 1000
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", RESULT)%变成了扶她
			CALL COLORMESSAGE("这很容易",C_D_RED,1,1)
		ELSE
			GOTO LOOP
		ENDIF
	CASE 2
		PRINTFORML 谁的？
		DRAWLINE
		FOR LOCAL,1,CHARANUM
			SIF TALENT:LOCAL:性别 == 3 && MAXBASE:LOCAL:精力 < 1500
				PRINTFORMLC [{LOCAL}] %NAME_OUTPUT_TRANSLATION("CALLNAME", LOCAL),15,LEFT%
		NEXT
		PRINTL 
		DRAWLINE
		PRINTFORML [999] 算了
		INPUT
		IF INRANGE(RESULT,1,CHARANUM) && TALENT:RESULT:性别 == 3 && MAXBASE:RESULT:精力 < 1500
			PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", RESULT)%的精力最大值增加
			MAXBASE:RESULT:精力 += 100
			CALL COLORMESSAGE("这很容易",C_D_RED,1,1)
		ELSE
			GOTO LOOP
		ENDIF
	CASE 3
		CALL COLORMESSAGE("呵呵、对扶她肉棒的爱让吾非常愉快",C_D_RED,1,1)
		CALL COLORMESSAGE("就在这收下它、尽情的品尝吧",C_D_RED,1,1)
		PRINTFORML 突然%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%的口中出现了巨大的质量
		PRINTFORML 从那既坚挺又柔软的肉棒上、能感到圧溃般的力道和温暖的安心感
		PRINTFORML 片刻之后被圧倒的%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%反应过来、拼命的侍奉着超越人知的东西
		PRINTFORML 在无限的苦闷和至福的时间的尽头、那超常的阳物终于迎来了绝顶
		PRINTFORML 品味着那无法言喻的芳醇气味和喉咙深处被射入的膨大神气、%NAME_OUTPUT_TRANSLATION("CALLNAME", MASTER)%失去了意识……
		CALL BUFF_BASE(MASTER,BASE_气力,2000)
		CALL BUFF_BASE(MASTER,BASE_体力,2000)
	CASE 4
		CALL COLORMESSAGE("不穿那种东西",C_D_RED,2,1)
		GOTO LOOP
ENDSELECT
CALL COLORMESSAGE("不要忘记、扶她与您同在",C_D_RED,1,1)
PRINTFORML 强大的气息离开了……


