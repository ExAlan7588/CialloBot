;一日の终わりに呼び出される函数
@GET_CHILD(ARG)
#DIM C_ID
#DIM 妊娠必要值
#DIM 中毒补正
#DIM 妊娠精液判定

#DIM 目前怀孕值
#DIM 怀孕必要值
#DIM 怀孕率

SELECTCASE ABL:ARG:膣射中毒
	CASE 0 TO 2
		中毒补正 = 5
	CASE 3 TO 5
		中毒补正 = 4
	CASEELSE
		中毒补正 = 2
ENDSELECT

EX:ARG:子宫内精液 += EX:ARG:膣内精液 / 中毒补正

SIF EX:ARG:子宫内精液 > EX:ARG:膣内精液
	EX:ARG:子宫内精液 = EX:ARG:膣内精液

SIF !HAS_VAGINA(ARG) || (EX:ARG:子宫内精液 <= 0)
	RETURN

FOR LOCAL,0,CHARANUM
	IF !CFLAG:ARG:(LOCAL + 2000)
		CONTINUE
	ELSE
		CFLAG:ARG:(LOCAL + 2000) += CFLAG:ARG:(LOCAL + 3000) / 中毒补正
		SIF CFLAG:ARG:(LOCAL + 2000) > CFLAG:ARG:(LOCAL + 3000)
			CFLAG:ARG:(LOCAL + 2000) = CFLAG:ARG:(LOCAL + 3000)
	ENDIF	
NEXT


IF !TALENT:ARG:初潮 && (TALENT:ARG:幼稚 || TALENT:ARG:幼儿／幼儿退行) && EX:ARG:子宫内精液
	PRINTFORMW %NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%的身体还很幼小，还不能怀孕呢……
	RETURN 0
ELSEIF INRANGE(CFLAG:ARG:分娩经过日, 1, 59) && EX:ARG:子宫内精液 > 0
	PRINTFORMW 刚出产不久的%NAME_OUTPUT_TRANSLATION("CALLNAME", ARG)%的身体似乎暂时不能怀孕……
	RETURN 0
ENDIF

