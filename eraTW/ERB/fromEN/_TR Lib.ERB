@GET_NAME_WEATHER_TIMEZOME_TR(ARG = -1)
#FUNCTIONS

SIF ARG == -1
	ARG = TIME

SELECTCASE ARG
	;AM0~3
	CASE 0 TO 179
		LOCALS = early dawn
	;AM3~6
	CASE 180 TO 359
		LOCALS = dawn
	;AM6~9
	CASE 360 TO 539
		LOCALS = morning
	;AM9~12
	CASE 540 TO 719
		LOCALS = noon
	;PM0~3
	CASE 720 TO 899
		LOCALS = afternoon
	;PM3~6
	CASE 900 TO 1079
		LOCALS = evening
	;PM6~9
	CASE 1080 TO 1259
		LOCALS = night
	;PM9~12
	CASE IS >= 1260
		LOCALS = late night
	CASEELSE
		LOCALS = TIME ZONE ERROR!
ENDSELECT
SIF ARG:1
	RETURNF CAPITALIZE(LOCALS)
RETURNF LOCALS
[SKIPEND]