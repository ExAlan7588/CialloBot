@CHARAMOVE_DATA_71(ARG, ARG:2)
LOCAL = 71
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;主に起居室に居る
			CASE P9起居室
				RETURN 5
			;私室にはあまり入らない
			CASE P15灵梦私室, P52别馆
				RETURN -5
			;他所
			CASE P27三妖精之家・大厅 TO P35三妖精之家・地下室, P37梦幻遗迹・一层廊道 TO P51梦幻遗迹・仓库, P56阿吽的小屋
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 神灵庙广场
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ71
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ71(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "针妙丸")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "小碗"))
CASE "便装：和服"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("和服", "和服"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "肌襦袢"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "带"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "大和小都合适的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "小人の胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "小人の吊带衫")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[针妙丸]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/卵/和食/叶物/垃圾/豪华/上品/新鲜/软绵绵/甜/甜/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "朴素/辣/辣/谜料理/谜料理/谜料理/" + "青蛙/虫肉/鬼的魔力/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "得意料理＊上品＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/秩序/帅气的/时尚/活泼/纤细/巫术/" + "装饰/" * 3 + "小人の/小小の/针/茶碗/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "充满力量/知的/" + "烟袋/" * 3 + "鬼の/鬼力の/" * 10)

;农民关联
;ころぽっくる
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "蕗农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "蜂斗叶")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "蕗畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博丽神社")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 350)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 3)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_71
DISH_NAME = 小人面包
RESULTS = 轻食
CALL 材料设定("泛用")
DISH_TASTE = 洋食/面包/甜/简易/机能的/松脆/魔力/
DISH_COMMENT = 为吃了就能掌握周围地形的小人所制作的魔法面包
RETURN 1

;IMAGE_I71_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I71_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ENDIF

