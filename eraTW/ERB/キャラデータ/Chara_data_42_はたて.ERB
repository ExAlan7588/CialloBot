@CHARAMOVE_DATA_42(ARG, ARG:2)
LOCAL = 42
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;天界には行かない
			CASE P830玄云海 TO P842比那名居邸库房
				RETURN -100
			;私室やお澡堂は入らない
			CASE P817神奈子私室, P818诹访子私室, P820早苗私室
				RETURN -100
			CASE P851久侘歌的小屋, P852驹草的小屋, P853魅须丸的小屋, P824厕所, P825澡堂
				RETURN -100
			;自宅に引きこもりがち
			CASE P809果宅
				RETURN 5
			;虹龙洞は无理
			CASE P855虹龙洞, P856百百世的住所
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 正门, 大厅 , 食堂, 大图书馆
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ42
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ42(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "果")

;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "兜巾"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "木屐"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "领带"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "像是时今少女的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "褶边胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "褶边胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "鱼料理/野味/虫肉/叶物/丰盛/浓厚/豪华/机能的/新鲜/软绵绵/天狗流/外界风/甜/甜/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "简易/辣/辣/卵/卵/卵/地底风/地底风/地底风/禽肉/禽肉/禽肉/禽肉/禽肉/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "浓郁/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "紫/可爱的/时尚/活泼/不思议/" + "风/鸟/文具/风の/硬币/" * 3 + "鸦の/天狗の/疾风の/吊饰/钥匙扣/领带/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "朴素/怀旧/别致/" + "刃物/弹/护臂/" * 3 + "龟の/河童の/鬼の/" * 10)
;ENDSELECT
;ここは依赖IDが关联しない情报
;依赖发生率スカ2/一般3/固有5
CASE "没有依赖发生率"
	CALLF MAKE_INT(V_NAME, 0)
CASE "一般依赖发生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "固有依赖发生率"
	CALLF MAKE_INT(V_NAME, 8)
CASE "固有依赖最终要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依赖关联のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依赖")
		CALLF 固有依赖42(ARG, O_DATA, V_NAME)
ENDSELECT

