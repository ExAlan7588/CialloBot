@CHARAMOVE_DATA_106(ARG, ARG:2)
LOCAL = 106
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;未设定
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 墓地, 神灵庙广场
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
			CASE 槐安通路
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT
;清兰
@EXIST_キャラデータ106
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ106(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "菫子")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：饰品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("饰品", "眼镜"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "长筒袜"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "水手服"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "对女高中生来说非常土的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "标准裤衩")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "素色的文胸")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[菫子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "面包/乳酪/洋食/汁物/派/垃圾/豪华/上品/家庭的/软绵绵/灵障/梦幻风/甜/甜/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "酸/酸/" + "青蛙/虫肉/怪诞/谜料理/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "秘术/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "高级/混沌/帅气的/奇妙/活泼/不思议/狂热/" + "紫/眼镜/遗物/" * 3 + "超自然な/堇の/梦の/斗篷/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/怀旧/" + "刃物/弹/生体品/护臂/烟袋/" * 3 + "外界の/恶梦の/" * 10)
ENDSELECT

