@CHARAMOVE_DATA_85(ARG, ARG:2)
LOCAL = 85
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命莲寺メンバーの私室はあんまり入らない
			CASE P113纳兹琳私室, P114村纱私室, P115星私室, P117鵺私室, P118白莲私室, P109女苑私室, P136响子私室
				RETURN -5
			;别馆、猯藏的小屋には入らない,神灵庙内部は行かない
			CASE P119别馆, P137猯藏的小屋, P125屠自古私室 TO P134藏, P138钟楼, P139邪仙楼
				RETURN -100
			;起居室に行きやすい
			CASE P108起居室
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 命莲寺方丈, 墓地
				RETURN 1
			CASE 广场, 商业街
				RETURN 1
			CASE 天界
				RETURN 1
		ENDSELECT
	;引力设定
	CASE 8
		;白莲が本堂にいる时はやってくる
		SIF CFLAG:[[白莲]]:当前位置 == P103本堂 && !CFLAG:[[白莲]]:诶嘿嘿 && SHIRAHU([[白莲]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ85
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ85(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "一轮")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "头巾"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "法衣"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "闻着香香的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "半旧的棉布胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "半旧的棉布胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[一轮]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/香料/和食/锅物/甜/简易/健康/清淡/朴素/家庭的/豪快/软绵绵/" + "咸/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "洋食/上品/外界风/酸/酸/青蛙/青蛙/青蛙/虫肉/虫肉/虫肉/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "薄味/得意料理＊软绵绵＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/白/秩序/美的/充满力量/" + "风/法具/" * 3 + "云の/天轮の/法轮/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/纤细/洛丽塔/狂热/" + "土/" * 3 + "蜘蛛の/" * 5)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
