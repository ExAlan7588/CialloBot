@CHARAMOVE_DATA_7(ARG, ARG:2)
LOCAL = 7
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
		RESULT:1 = [[桑尼]]
		RESULT:2 = [[露娜]]
		RESULT:3 = [[斯塔]]
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P27三妖精之家・大厅
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊, 妖精的大树
				RETURN 1
			CASE 雾之湖, 冰造的小雪屋, 正门
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大树, 雾雨魔法店, 人偶的洋馆, 再思之道, 无缘冢
				RETURN 1
		ENDSELECT
	;引力设定
	CASE 8
		;桑尼に依存
		TCVAR:LOCAL:引力点 = TCVAR:[[桑尼]]:引力点
ENDSELECT

@EXIST_キャラデータ7
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ7(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "斯塔")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（睡袍）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "蝴蝶结"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "连衣裙"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "有星星的样子的刺绣")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "斑点三角胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "半首位胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[斯塔]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "探访迹象")
CASE "固有指令实行可能"
	SIF CFLAG:7:好感度 >= 200 && GET_MAPID(CFLAG:7:当前位置) == MAIN_MAP
		CALLF MAKE_INT(V_NAME, 1)

;农民关联
;むりやりな斯塔フルーツから秋葵（星形断面）に变更
;秋葵って英语だったのか・・・
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "秋葵农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "秋葵")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "秋葵畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博丽神社")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 25)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 200)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 3)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "鱼料理/和食/锅物/健康/艺术的/上品/家庭的/扑鼻/闪耀/甜/甜/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "辣/野趣/怪诞/怪诞/怪诞/谜料理/谜料理/谜料理/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "隐し味/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/果实/可爱的/洋风/别致/洛丽塔/" + "星/书物/" * 3 + "星の/星光の/三位一体の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "暗/日/月/生体品/护臂/烟袋/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_7
DISH_NAME = 京丸牡丹锅
RESULTS = 主食
CALL 材料设定("高级食材")
DISH_TASTE = 和食/锅物/野菜/咸/简易/浓厚/温暖/温暖/豪华/艺术的/谜料理/灵障/
DISH_COMMENT = 将传说中的京丸牡丹按字面意思做成牡丹锅的神秘料理。
RETURN 1

;IMAGE_I7_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I7_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
			IF GROUPMATCH(CFLAG:CHARA:应用差分替换, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _变T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF CHECK_CLO("bra_visible")
					RESULTS = _内衣
				ELSEIF DAY:2 == 2
					RESULTS = _夏服
				ENDIF
			ENDIF
		ELSE
			;立绘
			IF GROUPMATCH(CFLAG:CHARA:应用差分替换, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _变T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF DAY:2 == 2
					RESULTS = _夏服
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

