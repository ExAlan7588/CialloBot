@CHARAMOVE_DATA_77(ARG, ARG:2)
LOCAL = 77
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
		RESULT:1 = [[静叶]]
		RESULT:2 = [[穣子]]
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;各私室
			CASE P704河童的宣传站, P706厄神的家, P729仙人的宅邸・玄关 TO P730仙人的宅邸・庭
				RETURN -100
			CASE P733正邪的藏身处, P736山姥之家, P737招财猫的家, P740山童的宣传站, P741美天的栖息处
				RETURN -100
				;畑によく居る
			CASE P715秋姊妹的家・旱田
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道
				;秋の间だけ
				SIF DAY:2 == 3
					RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 森林深处
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ77
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ77(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "静叶")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "发饰"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "长裙"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "体现秋天的终结")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "红叶の胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "红叶の胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/米/香料/叶物/简易/健康/清淡/艺术的/野趣/新鲜/松脆/甜/甜/辣/辣/秋物/秋物/秋物/秋物/秋物/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "垃圾/浓厚/咸/咸/" + "青蛙/虫肉/冬物/" * 3 + "谜料理/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "绚烂/得意料理＊秋物＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "美的/艺术的/和风/夸张/纤细/" + "朱/风/植物/美术品/" * 3 + "红叶の/秋の/秋风の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "威严/" + "冰/刃物/弹/护臂/机械/" * 3 + "夏の/樱花の/向日葵の/西瓜の/" * 5  + "春の/冬の/春风の/" * 10)

;农民关联
;葡萄がついてるのは妹のほうだが
;葡萄ならオサレ感あるし红叶もするし果实も色づくし
;变えるなら穣子を葡萄にして静叶は漆（非食用の有用植物、かつ红叶する）とか？？？
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "山葡萄农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "山葡萄")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "山葡萄畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "妖怪之山・山麓")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 715)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 4)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_77
DISH_NAME = 红叶天妇罗
RESULTS = 轻食
CALL 材料设定("山菜")
DISH_TASTE = 和食/油炸食品/天妇罗/野菜/咸/简易/健康/清淡/温暖/秋物/朴素/家庭的/野趣/野趣/松脆/
DISH_COMMENT = 这是一种把山上的红叶直接制成天妇罗的料理。味道很淡。
RETURN 1

;IMAGE_I77_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I77_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:衣物暂时更换 == 55 || (CFLAG:CHARA:基本服装SET == 55 && CFLAG:CHARA:衣物暂时更换==0)
			IF FLAG:立绘种类
				;颜绘
				RESULTS = _バニー
			ELSE
				;立绘
				RESULTS = _バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF

