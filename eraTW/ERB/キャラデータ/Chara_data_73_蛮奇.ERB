@CHARAMOVE_DATA_73(ARG, ARG:2)
LOCAL = 73
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
		;水曜日は连动
		SIF YOUBI_MATCH(DAY, "水")
			RETURN [[若鹭姫]]
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;酒屋,自室,广场によく居る
			CASE P211酒屋, P231蛮奇的房间, P202中央广场
				RETURN 5
			;私室には行かない
			CASE P221稗田邸, P224慧音宅, P222寺子屋, P226小铃私室, P229阿求私室
				RETURN -100
			CASE P208雷鼓的房间, P209八桥的房间, P210弁弁的房间, P226小铃私室, P229阿求私室
				RETURN -100
			CASE P232莲子的房间, P233梅莉的房间, P234雪的房间, P235​舞​的房间, P239美宵的房间
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 墓地
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

