	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "卵/香料/中华/汁物/简易/垃圾/清淡/机能的/上品/新鲜/软糯的/辣/辣/咸/咸/")
