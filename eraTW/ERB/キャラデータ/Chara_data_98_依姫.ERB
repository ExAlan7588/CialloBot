@CHARAMOVE_DATA_98(ARG, ARG:2)
LOCAL = 98
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;梦幻馆には行かない
			CASE P450玄关 TO P466庭
				RETURN -100
			;他人的家には入らない
			CASE P432影狼的家, P437梅蒂欣的家, P439拉尔瓦的家, P443米斯蒂亚的家, P445妹红的家
				RETURN -100
			;走廊,起居室によく居る
			CASE P409走廊, P416起居室
				RETURN 5

			;宁静海の方へは行かない
			CASE P1016入海口, P1060纯狐邸・玄关 TO P1072赫卡提亚私室
				RETURN -100
			;哆来咪宅には行かない
			CASE P1050哆来咪宅・起居室 TO P1055哆来咪宅・厕所
				RETURN -100
			;值班室、食料库は行かない
			CASE P1023门卫值班室 TO P1025玉兔值班室, P1028食料库
				RETURN -100
			;中庭、凉亭によく居る
			CASE P1035中庭, P1036凉亭
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 月之都, 绵月亭, 玉兔之街, 桃林～沙滩
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ98
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ98(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "依姫")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "蝴蝶结"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "连体裙"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "短袖女式衬衫"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "手链"))
CASE "便装：其他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他２", "网格吊带"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "毫无污秽的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "无邪の下衣")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "清浄の肌衣")
CASE "固有内衣着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "鱼料理/面类/健康/朴素/上品/艺术的/软糯的/月都风/赏月/美味的水/中华/中华/甜/甜/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "洋食/垃圾/野趣/酸/酸/" + "青蛙/虫肉/怪诞/" * 3 + "谜料理/生命力/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "月都风/切れ味/神力/神力/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/秩序/美的/朴素/大陆风/别致/威严/" + "水/月/神灵/巫术/刃物/护符/" * 3 + "海神の/海の/无秽の/可爱的/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/奇妙/洋风/夸张/洛丽塔/狂热/" + "土/暗/死生/生体品/烟袋/" * 3 + "地上の/万魔殿な/" * 10)
ENDSELECT

