@CHARAMOVE_DATA_161(ARG,ARG:2)
LOCAL = 161
SELECTCASE ARG
	;移动对象メンバーチェック
	CASE 1
	;移动概率
	CASE 2
	;起床时间带
	CASE 3
		RETURN 睡眠时间(LOCAL)
	;移动倾向
	CASE 4
		SELECTCASE ARG:2
			;とくに无
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事时间带
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASEELSE
				RETURN 0
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

