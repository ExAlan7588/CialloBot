@CHARAMOVE_DATA_61(ARG, ARG:2)
LOCAL = 61
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;梦幻馆には行かない
			CASE P450玄关 TO P466庭
				RETURN -100
			;永远亭の奥には行かない
			CASE P405配药室, P407走廊 TO P430地下室
				RETURN -100
			;他人的家には入らない
			CASE P432影狼的家, P437梅蒂欣的家, P439拉尔瓦的家, P443米斯蒂亚的家
				RETURN -100
			;斜角的竹林,迷失的小道,夜雀的小摊,自宅,竹林入口によく居る
			CASE P433斜角的竹林, P434迷失的小道, P444夜雀的小摊, P445妹红的家, P447竹林入口
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道, 寺子屋, 长屋街
				RETURN 1
			CASE 竹林入口, 迷失的小道, 斜角的竹林, 兔的洞穴, 永远亭, 无名之丘, 太阳花田
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

