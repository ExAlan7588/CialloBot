@CHARAMOVE_DATA_89(ARG, ARG:2)
LOCAL = 89
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命莲寺メンバーの私室は入らない
			CASE P113纳兹琳私室, P114村纱私室, P115星私室, P116一轮私室, P117鵺私室, P118白莲私室, P109女苑私室, P136响子私室
				RETURN -100
			;神灵庙メンバーの私室は入らない
			CASE P125屠自古私室, P127布都私室, P128神子私室
				RETURN -100
			;墓地によく居る
			CASE P121墓地
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 墓地, 神灵庙广场
				RETURN 1
			CASE 再思之道, 无缘冢
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
		;よく墓地に行く
		SIF CFLAG:LOCAL:今日的运势 % 4 == 0 && INRANGE(TIME, 0, 360)
			TCVAR:LOCAL:引力点 = P121墓地
ENDSELECT

@EXIST_キャラデータ89
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ89(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "芳香")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：饰品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("饰品", "御札"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "外套"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "和青娥穿的类似")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "三层褶边短裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "褶边胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)


;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/中华/和食/油炸食品/垃圾/浓厚/冷/朴素/野趣/粘稠/妖力/灵障/甜/甜/酸/酸/丰盛/丰盛/" + "怪诞/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "温暖/神力/" + "咸/简易/清淡/" * 2 + "健康/" * 3)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "未知の味/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黑/混沌/可怕的/奇妙/" + "死生/妖术/生体品/" * 3 + "死灵の/不灭の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/知的/" + "光/神灵/书物/护符/机械/" * 3)
ENDSELECT

