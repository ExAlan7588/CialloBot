@CHARAMOVE_DATA_32(ARG, ARG:2)
LOCAL = 32
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
		;月曜日は早苗と一绪
		SIF YOUBI_MATCH(DAY, "月")
			RETURN [[早苗]]
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;分社引かれる
			CASE P21守矢神社分社
				RETURN 5
			;主に本殿
			CASE P812本殿
				RETURN 5
			;天界には行かない
			CASE P830玄云海 TO P842比那名居邸库房
				RETURN -100
			;他所的家には行かない
			CASE P807椛宅, P808文宅, P809果宅, P851久侘歌的小屋, P852驹草的小屋, P853魅须丸的小屋, P854龙宅, P856百百世的住所
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 间歇泉地下中枢, 森林深处
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内, 守矢神社本殿, 虹龙洞
				RETURN 1
			CASE 旧地狱街道, 灼热地狱遗迹
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ32
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ32(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "神奈子")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "发饰"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "长裙"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "外套"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "注连绳"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "贡献给神的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "奇妙内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "裹胸")
CASE "固有内衣着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "青蛙/好吃的鱼/禽肉/和食/面类/辣/丰盛/豪华/野趣/豪快/松脆/" + "咸/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "简易/朴素/甜/甜/妖力/妖力/虫肉/虫肉/虫肉/谜料理/谜料理/谜料理/谜料理/谜料理/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "加护/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/美的/怀旧/充满力量/威严/" + "风/巫术/" * 3 + "神圣庄严の/注连绳の/蛇の/风神の/山の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "活泼/洋风/纤细/洛丽塔/" + "土/" * 3 + "蛙の/博灵の/" * 10)

;农民关联
;神奈子さま高付加价值农业とか好きそう
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "香瓜农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "甜瓜")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "温室")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "守谷神社")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 8)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 814)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 700)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 18)
ENDSELECT

