@CHARAMOVE_DATA_86(ARG, ARG:2)
LOCAL = 86
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命莲寺メンバーの私室はあんまり入らない
			CASE P113纳兹琳私室, P115星私室, P116一轮私室, P117鵺私室, P118白莲私室, P109女苑私室, P136响子私室
				RETURN -5
			;别馆、猯藏的小屋には入らない,神灵庙内部は行かない
			CASE P119别馆, P137猯藏的小屋, P125屠自古私室 TO P134藏, P138钟楼, P139邪仙楼
				RETURN -100
			;起居室に行きやすい
			CASE P108起居室
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 命莲寺方丈, 墓地
				RETURN 1
			CASE 中有之道, 三途之川
				RETURN 1
			CASE 玄武之泽, 间歇泉地下中枢
				RETURN 1
			CASE 九天瀑布, 山之湖
				RETURN 1
		ENDSELECT
	;引力设定
	CASE 8
		;白莲が本堂にいる时はやってくる
		SIF CFLAG:[[白莲]]:当前位置 == P103本堂 && !CFLAG:[[白莲]]:诶嘿嘿 && SHIRAHU([[白莲]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ86
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ86(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "水蜜")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裤子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裤子", "短裤"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "水手服"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "代替名字写了锚的标志")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "男孩子气内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "吊带背心胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[村纱]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "鱼料理/香料/汁物/咖喱/垃圾/浓厚/冷/机能的/豪快/怪诞/扑鼻/辣/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "油炸食品/甜/甜/青蛙/青蛙/青蛙/虫肉/虫肉/虫肉/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "刺激的/美味的水/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/混沌/可怕的/" + "水/幽灵/妖术/" * 3 + "海の/深海の/锚の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "知的/威严/" + "光/火/护符/护臂/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_86
DISH_NAME = 深海料理
RESULTS = 主食
CALL 材料设定("好吃的鱼")
DISH_TASTE = 鱼料理/咸/丰盛/清淡/野趣/豪快/怪诞/灵障/
DISH_COMMENT = 使用深海鱼制作的略显怪异的料理。
RETURN 1

;IMAGE_I86_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I86_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ENDIF

