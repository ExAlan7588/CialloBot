@CHARAMOVE_DATA_52(ARG, ARG:2)
LOCAL = 52
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;梦幻馆には行かない
			CASE P450玄关 TO P466庭
				RETURN -100
			;他人的家には入らない
			CASE P432影狼的家, P437梅蒂欣的家, P439拉尔瓦的家, P443米斯蒂亚的家, P445妹红的家
				RETURN -100
			;来客用厕所,永琳私室,辉夜私室,后院はあんまり行かない
			CASE P406来客用厕所, P413永琳私室, P414辉夜私室, P422后院
				RETURN -5
			;永远亭入口,等候室,诊察室,配药室,帝私室,铃仙私室,庭が稍微好き
			CASE P401永远亭入口, P403等候室, P404诊察室, P405配药室, P410帝私室, P411铃仙私室, P421庭
				RETURN 3
			;走廊,起居室によく居る
			CASE P409走廊, P416起居室
				RETURN 5
			
			;哆来咪宅には行かない
			CASE P1050哆来咪宅・起居室 TO P1055哆来咪宅・厕所
				RETURN -100
			;门卫值班室は行かない
			CASE P1023门卫值班室
				RETURN -100
			;二阶はあまり行かない
			CASE P1040二层回廊 TO P1044会客厅
				RETURN -5
			;玉兔街方面,玉兔食堂,玉兔值班室によく居る
			CASE P1003玉兔街, P1004广场, P1010饭店 TO P1013蔬菜店, P1024玉兔食堂, P1025玉兔值班室
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
			CASE 竹林入口, 迷失的小道, 斜角的竹林, 兔的洞穴, 永远亭
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
		;たまーに帝と连动
		SIF CFLAG:LOCAL:今日的运势 % 10 == 0
			TCVAR:LOCAL:引力点 = TCVAR:[[帝]]:引力点
ENDSELECT

@EXIST_キャラデータ52
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ52(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "铃仙")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "白衬衫"))
CASE "便装：上身衣服１"
;夏は西装上衣を脱掉
	SIF DAY:2 != 2
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "西装上衣"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "领带"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "用丝带绑起了兔尾的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "条纹内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "条纹胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/果物/叶物/健康/上品/机能的/家庭的/软糯的/玉兔流/月都风/赏月/甜/甜/酸/酸/胡萝卜/胡萝卜/胡萝卜/胡萝卜/胡萝卜/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "野趣/辣/辣/" + "青蛙/野味/香料/虫肉/怪诞/" * 3 + "谜料理/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "玉兔流/得意料理＊健康＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/可爱的/纤细/知的/" + "兔の/地上の/狂气の/药瓶/领带/可爱黑暗的/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/可怕的/奇妙/夸张/洛丽塔/" + "刃物/生体品/护臂/" * 3)

;特殊衣装
CASE "おうち约会衣装"
	SIF MAIN_MAP == 2
		CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "卖药套"))
CASE "人里约会衣装"
	CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "衣装セット", "卖药套"))

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "买药")
CASE "固有指令实行可能"
	CALLF MAKE_INT(V_NAME, 1)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_52
DISH_NAME = 狂气之赤红煎鸡蛋
RESULTS = 轻食
CALL 材料设定("泛用")
DISH_TASTE = 卵/咸/简易/健康/温暖/家庭的/新鲜/怪诞/妖力/玉兔流/
DISH_COMMENT = 如同狂气之瞳一样的煎鸡蛋。看起来很怪异、但营养价值很高。
RETURN 1

;IMAGE_I52_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I52_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:衣物暂时更换 == 71 || (CFLAG:CHARA:基本服装SET == 71 && CFLAG:CHARA:衣物暂时更换 == 0)
			RESULTS = _Medseller
		ELSEIF EQUIP:CHARA:上身衣服１ != 4
			RESULTS = _女式衬衫
		ENDIF
	ENDIF
ENDIF
SIF RESULTS == "_MedSeller" && CFLAG:CHARA:基本服装SET != OBJNAME_TO_ID(CHARA, "GET", "衣装セット", "卖药套")
	CALL EX_COSTUME(CHARA, "人里约会")

