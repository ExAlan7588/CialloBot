@CHARAMOVE_DATA_124(ARG, ARG:2)
LOCAL = 124
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;未设定
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大树, 雾雨魔法店 , 再思之道, 无缘冢
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家
				RETURN 1
			CASE 地下室
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ124
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ124(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "隐岐奈")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "乌帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "长靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "道士服"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "投影了夜空")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "星辰の三角胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "星辰の胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/煮物/健康/豪华/艺术的/上品/扑鼻/辣/辣/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "朴素/咸/咸/" + "青蛙/虫肉/谜料理/" * 3 + "天狗流/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "魔性/加护/绚烂/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/高级/混沌/美的/艺术的/怀旧/夸张/知的/威严/" + "巫术/美术品/四季の/" * 3 + "七星の/后户の/隐匿の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋风/别致/洛丽塔/" + "安物/" * 3 + "天狗の/沉眠の/" * 10)
ENDSELECT

