@CHARAMOVE_DATA_113(ARG, ARG:2)
LOCAL = 113
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;绵月亭には行かない
			CASE P1020绵月亭・门 TO P1044会客厅
				RETURN -100
			;宁静海は渡らない
			CASE P1001槐安通路 TO P1015沙滩
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 中有之道, 三途之川, 彼岸, 地狱
				RETURN 1
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 宁静海
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ113
