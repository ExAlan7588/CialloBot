;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/青/黄/高级/混沌/可怕的/奇妙/夸张/充满力量/威严/壮大/狂热/" + "暗/宝石/妖术/冥王の/月の/地上の/异界の/" * 3 + "万魔殿な/绀珠の/女神の/地狱の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "朴素/怀旧/别致/纤细/" + "安物/" * 3)
