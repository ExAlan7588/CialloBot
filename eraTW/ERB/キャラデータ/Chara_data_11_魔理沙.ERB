@CHARAMOVE_DATA_11(ARG, ARG:2)
LOCAL = 11
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;神社のよくいるところ
			CASE P2境内, P10走廊
				RETURN 5
			;よく居る
			CASE P502香霖堂, P507菌菇群生地
				RETURN 5
			;引きこもらない
			CASE P512魔理沙宅・寝室
				RETURN -10
			;爱丽丝以外の私室には入らない
			CASE P522人偶洋馆・神绮的房间, P523人偶洋馆・梦子的房间, P526人偶洋馆・澡堂, P531人偶洋馆・空的房间
				RETURN -100
			CASE P535艾伦宅・寝室, P537艾伦宅・澡堂, P541朱鹭子的栖息处, P542莉格露的栖息处, P543露米亚的栖息处, P544成美的小屋, P545慧之子的栖息处
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊, 梦幻遗迹
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
			CASE 雾之湖, 正门, 大厅, 食堂, 大图书馆, 地下室
				RETURN 1
			CASE 竹林入口, 迷失的小道, 斜角的竹林, 永远亭, 梦幻馆
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大树, 雾雨魔法店, 人偶的洋馆, 再思之道, 无缘冢
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
		;水木金15～19时は看店,たまにサボる
		IF YOUBI_MATCH(DAY, "水木金") && INRANGE(TIME, 900, 1140) && CFLAG:LOCAL:今日的运势 > 200
			TCVAR:LOCAL:引力点 = P510雾雨魔法店
		;19～22时はまぁまぁな频度で研究房间にこもる
		ELSEIF INRANGE(TIME, 1140, 1320) && CFLAG:LOCAL:今日的运势 % 3
			TCVAR:LOCAL:引力点 = P513魔理沙宅・研究房间
		ENDIF
ENDSELECT

@EXIST_キャラデータ11
