@CHARAMOVE_DATA_10(ARG, ARG:2)
LOCAL = 10
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P2境内, P10走廊, P11庭
				RETURN 5
			;私室にはあまり入らない
			CASE P15灵梦私室, P52别馆
				RETURN -5
			;他所
			CASE P27三妖精之家・大厅 TO P35三妖精之家・地下室, P37梦幻遗迹・一层廊道 TO P51梦幻遗迹・仓库, P56阿吽的小屋
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内, 天界
				RETURN 1
			CASE 幻想风穴, 地狱的深道, 旧地狱街道
				RETURN 1
		ENDSELECT
ENDSELECT

@EXIST_キャラデータ10
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ10(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "萃香")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "蝴蝶结"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "无袖女式衬衫"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "拘束具"))

;どう见ても原作が无罩罩
CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "非常结实")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "鬼の胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[萃香]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)

;农民关联
;この娘の场合はそこら边の林をえいやっと畑にしてくれたんだろな
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "西瓜农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "西瓜")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "西瓜畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博丽神社")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 300)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 4)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/香料/和食/煮物/开胃/辣/丰盛/浓厚/豪华/野趣/豪快/软糯的/天狗流/地狱风/咸/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "甜/清淡/" + "谜料理/小人族秘传的味道/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "浓郁/刺激的/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "金/混沌/帅气的/夸张/充满力量/" + "锁の/食器/酒/" * 3 + "西瓜の/鬼の/鬼力の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/纤细/" + "书物/机械/" * 3 + "河童の/天狗の/" * 5)

;ここは依赖IDが关联しない情报
;依赖发生率スカ2/一般3/固有5
CASE "没有依赖发生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依赖发生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依赖发生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依赖最终要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依赖关联のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依赖")
		CALLF 固有依赖10(ARG, O_DATA, V_NAME)
ENDSELECT

;IMAGE_I10_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I10_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
			IF GROUPMATCH(CFLAG:CHARA:应用差分替换, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _变T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 90
					RESULTS = _茨歌仙
				ENDIF
			ELSE

			ENDIF
		ELSE
			;立绘
			IF GROUPMATCH(CFLAG:CHARA:应用差分替换, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _变T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 90
					RESULTS = _茨歌仙
				ENDIF
			ELSE

			ENDIF
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ENDIF

