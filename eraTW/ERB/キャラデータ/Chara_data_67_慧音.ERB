	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：上身衣服２"
;	IF DAY:2 == 2
;		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "短袖女式衬衫"))
;	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
;	ENDIF
;CASE "便装：裤子"
;	SIF DAY:2 == 2
;		CALLF MAKE_INT(V_NAME, CLOTHES_ID("裤子", "裤子"))
;CASE "便装：其他１"
;	SIF DAY:2 == 2
