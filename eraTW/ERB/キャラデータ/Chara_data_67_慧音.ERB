;CASE "便装：其他２"
;	SIF DAY:2 == 2
;		CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他２", "吊带"))
CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "连衣裙"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "预先打开了尾巴")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "蓝染的内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "蓝染的胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[慧音]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	IF DAY:3 == 15 && INRANGE(TIME:2, 5, 7)
		CALLF MAKE_STR(V_NAME, "野菜/和食/叶物/简易/清淡/朴素/上品/家庭的/松脆/妖力/妖力/妖力/赏月/甜/甜/酸/酸/")
	ELSE
		CALLF MAKE_STR(V_NAME, "野菜/和食/叶物/简易/清淡/朴素/上品/家庭的/松脆/神力/赏月/甜/甜/酸/酸/")
	ENDIF
CASE "料理：讨厌的味道"
	IF DAY:3 == 15 && INRANGE(TIME:2, 5, 7)
		CALLF MAKE_STR(V_NAME, "神力/辣/辣/" + "青蛙/虫肉/怪诞/" * 3 + "谜料理/" * 5)
	ELSE
		CALLF MAKE_STR(V_NAME, "妖力/辣/辣/" + "青蛙/虫肉/怪诞/" * 3 + "谜料理/" * 5)
	ENDIF
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "得意料理＊家庭的＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/秩序/可爱的/朴素/怀旧/纤细/知的/" + "文具/书物/" * 3 + "令人怀念の/过去の/牛の/知识の/知惠の/记录/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/夸张/洛丽塔/" + "刃物/弹/生体品/护臂/鼠の/" * 3)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_67
DISH_NAME = 满月乌冬面
RESULTS = 主食
CALL 材料设定("泛用")
DISH_TASTE = 和食/面类/卵/咸/简易/清淡/温暖/温暖/艺术的/家庭的/粘稠/
SIF FESTIVAL() == "十五夜"
	DISH_TASTE += "缘起物/"
DISH_COMMENT = 加入了月亮形状的生鸡蛋的乌冬面。十五夜（秋之月15日）的时候吃也不失为一种乐趣。
RETURN 1

;IMAGE_I67_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I67_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ELSE
	;通常画像
	SIF DAY:3 == 15 && INRANGE(TIME:2, 5, 7)
		RESULTS = _白泽
ENDIF

