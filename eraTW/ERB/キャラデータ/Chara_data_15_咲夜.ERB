@CHARAMOVE_DATA_15(ARG, ARG:2)
LOCAL = 15
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;仓库には入らない、屋敷外には出ない
			CASE P305仓库, P334废洋馆・入口 TO P348雾之湖・湖底
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
			CASE 雾之湖, 正门, 大厅, 食堂, 大图书馆, 地下室
				RETURN 1
			CASE 森林的入口, 香霖堂, 雾雨魔法店, 人偶的洋馆
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ15
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ15(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "咲夜")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "白色女仆发箍"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "吊带袜"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "长筒袜"))
	ENDIF
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "围裙洋装"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "蝴蝶结"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "吊袜带内衣")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "高级丝绸胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "高级丝绸胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 胜负内衣判定([[咲夜]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "红魔馆出差服务")
CASE "固有指令实行可能"
	IF TFLAG:上门清扫 && (FLAG:浇水代行 || !ITEM:150)
	ELSEIF GET_MAPNAME(MAIN_MAP) == "红魔馆"
	ELSE
		CALLF MAKE_INT(V_NAME, 1)
	ENDIF

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "鱼料理/香料/中华/煮物/甜/清淡/豪华/上品/家庭的/松脆/绚烂/" + "酸/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "朴素/野趣/咸/咸/虫肉/虫肉/虫肉/谜料理/谜料理/谜料理/谜料理/谜料理/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "隐し味/秘术/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/美的/朴素/洋风/别致/" + "钟表/食器/" * 3 + "潇洒な/女仆的/时の/时空の/怀表/小刀/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "充满力量/洛丽塔/" + "烟袋/" * 3)

;ここは依赖IDが关联しない情报
;依赖发生率スカ2/一般3/固有5
CASE "没有依赖发生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依赖发生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依赖发生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依赖最终要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依赖关联のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依赖")
		CALLF 固有依赖15(ARG, O_DATA, V_NAME)
ENDSELECT

