@CHARAMOVE_DATA_129(ARG, ARG:2)
LOCAL = 129
SELECTCASE ARG
	;连动设定（参照する侧＞RETURN=参照先, 参照される侧＞RESULT:1～3=连动对象）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向
	CASE 4
		SELECTCASE ARG:2
			;八云邸、畜生界は行かない
			CASE P630八云邸玄关 TO P655尤魔宅的澡堂
				RETURN -100
			;彼岸によくいる
			CASE P604彼岸
				RETURN 5

			;天界には行かない
			CASE P830玄云海 TO P842比那名居邸库房
				RETURN -100
			;私室やお澡堂は入らない
			CASE P817神奈子私室, P818诹访子私室, P820早苗私室
				RETURN -100
			CASE P807椛宅, P808文宅, P809果宅, P854龙宅
				RETURN -100
			CASE P852驹草的小屋, P853魅须丸的小屋, P824厕所, P825澡堂
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 中有之道, 三途之川, 彼岸, 白玉楼庭院, 白玉楼, 地狱
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 间歇泉地下中枢
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ129
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ129(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "久侘歌")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "连衣裙"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "长靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "丝带项链"))
CASE "便装：其他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他２", "雏鸡"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "ピヨピヨ音の出る")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "雏鸡胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "雏鸡ブラ")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/米/叶物/简易/清淡/朴素/上品/新鲜/软绵绵/冥界风/甜/甜/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "油炸食品/丰盛/浓厚/咸/咸/青蛙/青蛙/青蛙/" + "谜料理/" * 5 + "禽肉/卵/" * 10)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "冥界风/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黄/秩序/美的/活泼/纤细/" + "鸟/" * 3 + "鸡の/勇气の/蛋の/卵/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/奇妙/狂热/" + "弹/护臂/生体品/机械/" * 3 + "鸦の/" * 10)
ENDSELECT

