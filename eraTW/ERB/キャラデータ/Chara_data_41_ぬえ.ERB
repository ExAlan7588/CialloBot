	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ENDIF

