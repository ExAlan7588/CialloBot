@CHARAMOVE_DATA_41(ARG, ARG:2)
LOCAL = 41
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命莲寺メンバーの私室はあんまり入らない
			CASE P113纳兹琳私室, P114村纱私室, P115星私室, P116一轮私室, P118白莲私室, P109女苑私室, P136响子私室
				RETURN -5
			;别馆には入らない,神灵庙内部は行かない
			CASE P119别馆, P125屠自古私室 TO P134藏, P138钟楼, P139邪仙楼
				RETURN -100
			;起居室、猯藏的小屋に行きやすい
			CASE P108起居室, P137猯藏的小屋
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 命莲寺方丈, 墓地
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 森林深处
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
		;白莲が本堂にいる时はやってくる
		SIF CFLAG:[[白莲]]:当前位置 == P103本堂 && !CFLAG:[[白莲]]:诶嘿嘿 && SHIRAHU([[白莲]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ41
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ41(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "鵺")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "长筒袜"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "连衣裙"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "马赛克模糊的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "正体不明胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "正体不明胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/香料/面类/丰盛/垃圾/浓厚/新鲜/怪诞/扑鼻/辣/辣/咸/咸/" + "谜料理/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "简易/健康/朴素/甜/甜/野菜/野菜/野菜/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "未知の味/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黑/混沌/可怕的/奇妙/不思议/" + "暗/妖术/生体品/酒/" * 3 + "谜の/正体不明の/夜空の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/朴素/知的/" + "光/书物/照明/领带/" * 3 + "真实の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_41
DISH_NAME = 未确认飞行料理
RESULTS = 主食
CALL 材料设定("暗黑物质")
DISH_TASTE = 香料/未知の味/未知の味/未知の味/中立温度/谜料理/妖力/
DISH_COMMENT = 正体不明的宇宙舞蹈料理。非常难以下咽。
RETURN 1


;IMAGE_I41_特殊服装(CHARA)-------------------------------------------------------------------
