@CHARAMOVE_DATA_153(ARG, ARG:2)
LOCAL = 153
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;私室には入らない
			CASE P704河童的宣传站, P706厄神的家, P709莉莉们的家, P716秋姊妹的家・土间 TO P730仙人的宅邸・庭
				RETURN -100
			CASE P733正邪的藏身处, P737招财猫的家, P740山童的宣传站, P736山姥之家
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 中有之道, 三途之川, 彼岸, 地狱, 畜生界
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 森林深处
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ153
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ153(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "美天")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "连衣裙"))
CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "金轮"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "西游记风の")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "おサルパンツ")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "おサルブラ")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "面类/丰盛/浓厚/豪华/豪快/新鲜/松脆/神力/畜生流/" + "中华/辣/咸/" * 2 + "炸薯条/肉料理/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "野菜/酸/酸/简易/清淡/朴素/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "畜生流/得意料理＊中华＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/帅气的/大陆风/充满力量/壮大/" + "神灵/水獭の/" * 3 + "猴子の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/洋风/纤细/知的/" + "狼の/鹫の/犬の/" * 10)
ENDSELECT

