@CHARAMOVE_DATA_59(ARG, ARG:2)
LOCAL = 59
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;仓库と屋敷外には出ない
			CASE P305仓库, P334废洋馆・入口 TO P348雾之湖・湖底
				RETURN -100
			;大体图书馆
			CASE P310图书馆
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 正门, 大厅, 食堂, 大图书馆, 地下室
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ59
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ59(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "小恶魔")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "长筒袜"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "长裙"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：外衣"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "背心"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "领带"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "挑衅的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "蕾丝T字")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "透明胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 胜负内衣判定([[小恶魔]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/蘑菇/洋食/面类/垃圾/豪华/新鲜/怪诞/扑鼻/魔界风/甜/甜/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "鱼料理/和食/丰盛/酸/酸/谜料理/谜料理/谜料理/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "魔界风/刺激的/")
;赠り物关联
;长短发口上用に好みを变化

CASE "感性：好き"
	IF CFLAG:[[小恶魔]]:口上セレクタ == 1
		CALLF MAKE_STR(V_NAME, "黑/混沌/可怕的/洋风/知的/" + "魔法/书物/" * 3 + "恶魔の/领带/" * 5 + "天使の/" * 10)
	ELSE
		CALLF MAKE_STR(V_NAME, "黑/混沌/可怕的/洋风/知的/" + "魔法/书物/" * 3 + "恶魔の/领带/" * 5)
	ENDIF
CASE "感性：嫌い"
	IF CFLAG:[[小恶魔]]:口上セレクタ == 1
		CALLF MAKE_STR(V_NAME, "秩序/和风/" + "光/神灵/护符/护臂/" * 3)
	ELSE
		CALLF MAKE_STR(V_NAME, "秩序/和风/" + "光/神灵/护符/护臂/" * 3 + "天使の/" * 10)
	ENDIF

;农民关联
;まさかの室内栽培
;主人をからかうのが目的だが气づかれていない样子
;ぜんぜん储からない
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "豆芽农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "豆芽")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "豆芽房间")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "红魔馆")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 3)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 310)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 150)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 10)
ENDSELECT

