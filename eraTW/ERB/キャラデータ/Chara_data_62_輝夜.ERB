@IMAGE_I62_通常颜绘特殊表情
IF !FLAG:巨乳差分表示 || TALENT:62:胸围 == CSVTALENT(62, GETNUM(TALENT, "胸围")) ;no big breast support
	IF !CHECK_NUDE(62)
		;服
		LOCALS '= RESULTS:0
		IF RESULTS:0 == "笑颜"
			IF (TALENT:62:恋慕 || TALENT:62:爱欲) && RAND:2 ;non-smug
				RESULTS:0 = Smile
			ELSEIF TALENT:62:思慕 || TALENT:62:炮友
				RESULTS:1 = Wink
			ENDIF
		ENDIF
		IF RESULTS:0 == "发情" || TCVAR:62:媚药 || CFLAG:62:积攒度 > 850
			;return back in case smile overrode it
			RESULTS:0 '= LOCALS
			IF !(TALENT:62:恋慕 || TALENT:62:爱欲) && RAND:2
				RESULTS:1 = Hmph
			ELSE
				RESULTS:1 = Uehh
			ENDIF
		ENDIF
	ELSE
		;裸
		SIF RESULTS:0 == "发情" || TCVAR:62:媚药 || CFLAG:62:积攒度 > 850
			RESULTS:1 = Ufufu
	ENDIF
ENDIF

