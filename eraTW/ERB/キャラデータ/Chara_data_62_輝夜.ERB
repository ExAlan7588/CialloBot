@CHARAMOVE_DATA_62(ARG, ARG:2)
LOCAL = 62
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;梦幻馆には行かない
			CASE P450玄关 TO P466庭
				RETURN -100
			;他人的家には入らない
			CASE P432影狼的家, P437梅蒂欣的家, P439拉尔瓦的家, P443米斯蒂亚的家, P445妹红的家
				RETURN -100
			;来客用厕所は行かない
			CASE P406来客用厕所
				RETURN -100
			;诊察室,配药室,帝私室,铃仙私室,后院,二阶はあんまり行かない
			CASE P404诊察室, P405配药室, P410帝私室, P411铃仙私室, P422后院, P426二层走廊, P427仓库, P428二层空的房间
				RETURN -5
			;走廊,辉夜私室,起居室,庭によく居る
			CASE P409走廊, P414辉夜私室, P416起居室, P421庭
				RETURN 5

			;绵月亭には行かない
			CASE P1020绵月亭・门 TO P1044会客厅
				RETURN -100
			;他人的家には行かない
			CASE P1005探女宅・起居室, P1006探女宅・寝室, P1008铃瑚宅, P1009清兰宅
				RETURN -100
			;月之都には行かない
			CASE P1002月之都
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 竹林入口, 迷失的小道, 斜角的竹林, 永远亭, 永远亭的内院
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

