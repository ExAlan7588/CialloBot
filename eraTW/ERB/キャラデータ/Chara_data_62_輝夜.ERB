		RESULTS:0 = 眯笑
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 睡眠
		RESULTS:1 = 睡眠
	ENDIF
	RETURN
ENDIF

IF CFLAG:62:勃然大怒
	RESULTS:0 = 怒{RAND:4+1}
	RESULTS:1 = 怒{RAND:4+1}
	RETURN
ELSEIF BASE:62:愤怒 > 600 || TALENT:62:心情 < 0 || CFLAG:62:心情不快 == 1
	RESULTS:0 = 不悦
	RETURN
ENDIF

IF TALENT:62:恋人
	IF BASE:62:情绪 >= 100
		RESULTS:0 = 笑颜
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
