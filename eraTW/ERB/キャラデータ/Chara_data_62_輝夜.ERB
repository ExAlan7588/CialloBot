	IF BASE:62:情绪 >= 300
		RESULTS:0 = 笑颜
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSEIF TALENT:62:思慕 || TALENT:62:炮友
	IF BASE:62:情绪 >= 600
		RESULTS:0 = 笑颜
		RESULTS:1 = 眯笑
	ELSE
		RESULTS:0 = 通常
	ENDIF
ELSE
	IF BASE:62:情绪 >= 1200
		RESULTS:0 = 笑颜
	ELSE
		RESULTS:0 = 通常
	ENDIF
ENDIF

IF RESULTS:0 == "通常"
	IF MARK:62:反抗刻印 <= 0
		RESULTS:1 = 微开口
	ENDIF
ELSEIF RESULTS:0 == "笑颜"
	IF PALAM:62:情欲 >= PALAMLV:10 || TCVAR:62:发情 || CFLAG:62:积攒度 >= 900
		RESULTS:1 = 开口
	ELSEIF MARK:62:反抗刻印 <= 0 && (TFLAG:103==109 || TARGET==109)
