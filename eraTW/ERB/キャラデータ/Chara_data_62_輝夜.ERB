	IF LOCAL > 3
		RESULTS:エフェクト数 = 精渍外衣
	ELSEIF LOCAL==3 && IMAGE_I62_别立ち判定_futa()
		RESULTS:エフェクト数 = 精渍扶她内衣
	ELSEIF LOCAL==3
		RESULTS:エフェクト数 = 精渍内衣
	ELSE
		RESULTS:エフェクト数 = 精渍裸
	ENDIF
	エフェクト数++
ENDIF

; only show 断面图 effect and 气泡 effect when current target or last target is 62
SIF !(TFLAG:103==62 || TARGET==62)
	RETURN エフェクト数

;断面图
IF (SELECTCOM == 306 && TFLAG:103==62) || (NOWEX:MASTER:射精 && TEQUIP:62:Ｖ性交 == MASTER && RAND:3==0)
	IF CFLAG:62:妊娠经过日数 >= 40 && IMAGE_I62_别立ち判定_腹精液() >= 800 && 充填率(62,3) >= 100
			RESULTS:エフェクト数 = 断面图安定期储精罐
			エフェクト数++
	ELSEIF CFLAG:62:妊娠经过日数 >= 40
			RESULTS:エフェクト数 = 断面图安定期
			エフェクト数++
	ELSEIF IMAGE_I62_别立ち判定_腹精液() >= 800 && 充填率(62,3) >= 100
			RESULTS:エフェクト数 = 断面图储精罐
			エフェクト数++
	ELSEIF CFLAG:62:妊娠经过日数 && IMAGE_I62_别立ち判定_腹精液() >= 500
			RESULTS:エフェクト数 = 断面图怀孕内射
			エフェクト数++
	ELSEIF CFLAG:62:妊娠经过日数
			RESULTS:エフェクト数 = 断面图怀孕
			エフェクト数++
	ELSEIF IMAGE_I62_别立ち判定_腹精液() > 500
			RESULTS:エフェクト数 = 断面图内射
			エフェクト数++
	ENDIF
ENDIF

;气泡
气泡概率:0 = MAX(0, GETPALAMLV(PALAM:62:好意,10) -5 + BASE:62:情绪/200)
气泡概率:1 = 5 - MAX(0, GETPALAMLV(PALAM:62:好意,10) -5)
气泡概率:2 = MAX(0, GETPALAMLV(PALAM:62:羞耻,5))
气泡概率:3 = MAX(0, GETPALAMLV(PALAM:62:不快,7))
气泡概率:4 = MAX(0, GETPALAMLV(PALAM:62:抑郁,5)+ GETPALAMLV(PALAM:62:反感,5))
气泡概率:5 = BASE:62:愤怒/100
气泡概率:6 = TCVAR:62:发情*5
SIF (CFLAG:62:既成事实 & 合意_诶嘿嘿)
	气泡概率:6 += MAX(0, CFLAG:62:积攒度/100 - 5)
气泡概率:7 = 0

IF CFLAG:62:勃然大怒
	VARSET 气泡概率
	气泡概率:1 = 1
	气泡概率:5 = 8
	气泡概率:4 = 2
ENDIF

IF TALENT:62:心情 < 0 || CFLAG:62:心情不快 == 1
	气泡概率:3 += 2
	气泡概率:4 += 4
ENDIF

SIF CFLAG:睡眠
	气泡概率:7 += 5 + TCVAR:62:睡眠深度/500

LOCAL:1 = 气泡概率:0 + 气泡概率:1 + 气泡概率:2 + 气泡概率:3 + 气泡概率:4 + 气泡概率:5 + 气泡概率:6 + 气泡概率:7 + 5
SIF LOCAL:1 == 气泡概率:1 + 5
	气泡概率:1 = 0
IF LOCAL:1 > 0
	LOCAL:1 = RAND:(LOCAL:1)
	FOR LOCAL, 0, 8
		LOCAL:1 -= 气泡概率:LOCAL
		IF LOCAL:1 < 0
			RESULTS:エフェクト数 = 气泡%气泡名:LOCAL%
			エフェクト数++
			BREAK
		ENDIF
	NEXT
ENDIF

RETURN エフェクト数
