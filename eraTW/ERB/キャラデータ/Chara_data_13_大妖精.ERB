@CHARAMOVE_DATA_13(ARG, ARG:2)
LOCAL = 13
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;三妖精宅内に行きやすい
			CASE P27三妖精之家・大厅 TO P34斯塔私室
				RETURN 5
			;よく居る
			CASE P343湖畔, P345琪露诺的冰屋, P346大妖精的小屋, P347雾之湖
				RETURN 5
			;仓库と地下
			CASE P303守卫小屋, P304空的小屋, P305仓库, P328地下走廊 TO P333大浴场
				RETURN -100
			;琪露诺以外の私室には入らない
			CASE P311帕秋莉私室, P312小恶魔私室, P320咲夜私室, P321空的房间, P326蕾米莉亚私室, P327小露台
				RETURN -100
			CASE P338废洋馆・澡堂, P340露娜萨私室, P341梅露兰私室, P342莉莉卡私室, P344蕾蒂的冰屋
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 妖精的大树
				RETURN 1
			CASE 雾之湖, 冰造的小雪屋, 正门
				RETURN 1
			CASE 森林的入口, 古木的大树, 再思之道
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ13
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ13(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "大妖精")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "蝴蝶结"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "连衣裙"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "自己做的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "毛织胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "毛织胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[大妖精]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/果物/叶物/派/酸/简易/健康/清淡/朴素/新鲜/家庭的/软绵绵/闪耀/甜/甜/甜/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "丰盛/辣/辣/" + "虫肉/香料/浓厚/怪诞/" * 3 + "谜料理/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "薄味/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "绿/植物/可爱的/纤细/洛丽塔/" + "风/" * 3 + "风の/花の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "可怕的/夸张/充满力量/威严/" + "刃物/弹/护臂/烟袋/" * 3)

;ここは依赖IDが关联しない情报
;依赖发生率スカ2/一般3/固有5
CASE "没有依赖发生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依赖发生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依赖发生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依赖最终要素"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;固有依赖关联のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依赖")
		CALLF 固有依赖13(ARG, O_DATA, V_NAME)
ENDSELECT

