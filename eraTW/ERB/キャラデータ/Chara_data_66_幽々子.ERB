@CHARAMOVE_DATA_66(ARG, ARG:2)
LOCAL = 66
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;是非曲直厅、畜生界、地狱は行かない
			CASE P610是非曲直厅大堂 TO P618空的房间, P640异界的关卡 TO P655尤魔宅的澡堂, P605地狱
				RETURN -100
			;他人的家には入らない
			CASE P608润美的家, P609璎花的家, P606日狭美的家, P607残无的家
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 倒垂的柳树下, 墓地
				RETURN 1
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家, 彼岸, 白玉楼庭院, 白玉楼
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ66
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ66(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "幽幽子")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：和服"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("和服", "和服"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "肌襦袢"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "带"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "鲜艳的樱花花色")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "和装三角胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "和装胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 胜负内衣判定([[幽幽子]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/肉料理/油炸食品/开胃/丰盛/浓厚/艺术的/上品/家庭的/软糯的/冥界风/赏月/和食/和食/甜/甜/酸/酸/" + "春物/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "咸/简易/清淡/天界风/天界风/天界风/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "冥界风/得意料理＊春物＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "粉色/美的/艺术的/和风/夸张/纤细/" + "死生/幽灵/扇/食器/樱桃の/睡眠の/" * 3 + "黑死蝶の/樱花の/亡灵の/春の/冥界の/带来死亡の/死の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "洋风/充满力量/洛丽塔/" + "神灵/刃物/生体品/护臂/" * 3 + "不灭の/" * 10)

;农民关联
;石榴に设定したのは独断
;冥界（死）＆女性に缘のある果物としてチョイス
;最初は米农家にしたけど彼岸で米栽培してるのはとても违和感が
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "柘榴农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "石榴")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "柘榴畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "冥界")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 6)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 630)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 500)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 13)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_66
DISH_NAME = 樱饼
RESULTS = 甜点
CALL 材料设定("甜味料")
DISH_TASTE = 和食/甜/简易/春物/艺术的/上品/新鲜/软糯的/冥界风/
DISH_COMMENT = 用樱叶包裹着糕点的和果子。糕点的颜色也是樱花色。
RETURN 1

;IMAGE_I66_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I66_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		SIF DAY:0 % 2
			RESULTS = _日替わり
	ENDIF
ENDIF

