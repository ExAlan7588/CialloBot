;-------------------------------------------------------------------------------------------
@IMAGE_I43_特殊表情(CHARA)
#DIM CHARA

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF FLAG:立绘种类
		;颜绘
	ELSE
		;立绘
	ENDIF
ELSE
	;通常画像
	IF FLAG:立绘种类
		;颜绘
		IF (CFLAG:CHARA:衣物暂时更换 == 101 || (CFLAG:CHARA:基本服装SET == 101 && CFLAG:CHARA:衣物暂时更换==0)) && !CHECK_NUDE(CHARA)
		;灵梦巫女set
			IF SELECTCOM == 364
				RESULTS = BEANED
			ELSEIF GROUPMATCH(SELECTCOM, 415, 610, 615, 617) || (SELECTCOM == 414 && INRANGE(TFLAG:192, -1, 1)) || (Activity_Duration:CHARA && (Activity_Type:CHARA == 2 || Activity_Type:CHARA == 3))
				RESULTS = MGMG
			ENDIF
		ENDIF
	ELSE
		;立绘
	ENDIF
ENDIF

