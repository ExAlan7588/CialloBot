;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/米/中华/油炸食品/开胃/丰盛/浓厚/上品/新鲜/豪快/软糯的/仙术/地底风/地狱风/甜/甜/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "清淡/酸/酸/冥界风/冥界风/冥界风/" + "谜料理/小人族秘传的味道/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "浓郁/得意料理＊中华＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/美的/大陆风/知的/" + "动物/食器/生体品/锁の/幻兽の/西方龙の/酒/" * 3 + "鬼の/龙の/仙人の/鬼力の/茨の/华の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/活泼/洛丽塔/" + "机械/" * 3 + "河童の/天狗の/" * 10)

;特殊衣装设定、おうち约会で稀にみこかせん
CASE "おうち约会衣装"
	SIF MAIN_MAP == 0 && !RAND:15
		CALLF MAKE_INT(V_NAME, 101)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
;DISH_MAXLIMIT      （任意）料理の持续时间
;-------------------------------------------------
@SPECIAL_MENU_43
DISH_NAME = 中式包子
RESULTS = 主食
CALL 材料设定("野味", "竹笋", "洋葱", "卷心菜", "白菜", "鱼")
DISH_TASTE = 中华/野味/野菜/咸/丰盛/豪快/温暖/软糯的/鬼的魔力/
DISH_COMMENT = 这种中式包子除了肉以外,还会加入了各种不同的馅料。所有的馅料都塞得满满当当的在面皮里。
;めっちゃ量出来て料理に时间かかるし腹持ちもすごいっていうのを表现したいけど数值どのくらいがいいかわかんないから保留
RETURN 1

;IMAGE_I43_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用时注意点:
;	RESULTSに特殊服装を格纳すること
;	书式は %服装%%特殊服装% 适宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I43_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立绘种类
			;颜绘
		ELSE
			;立绘
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:衣物暂时更换 == 55 || (CFLAG:CHARA:基本服装SET == 55 && CFLAG:CHARA:衣物暂时更换==0)
			IF FLAG:立绘种类
				;颜绘
				RESULTS = _バニー
			ELSE
				;立绘
				RESULTS = _バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF


