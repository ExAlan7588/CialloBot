@CHARAMOVE_DATA_43(ARG, ARG:2)
LOCAL = 43
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;あまり入らない房间
			CASE P725仙人的宅邸・大鹫的房间, P728仙人的宅邸・空的房间
				RETURN -5
			;各私室
			CASE P704河童的宣传站, P706厄神的家, P716秋姊妹的家・土间 TO P719秋姊妹的家・会客室
				RETURN -100
			CASE P733正邪的藏身处, P736山姥之家, P737招财猫的家, P740山童的宣传站, P741美天的栖息处
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 仙人的宅邸, 森林深处
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定
	CASE 8
		;平日は3分の2で出步行の
		IF YOUBI_MATCH(DAY, "月火水木金") && INRANGE(TIME, 600, 960)
			SELECTCASE CFLAG:LOCAL:今日的运势 % 3
			CASE 1
				TCVAR:LOCAL:引力点 = P703玄武之泽
			CASE 2
				TCVAR:LOCAL:引力点 = P711休憩所
			CASEELSE
				TCVAR:LOCAL:引力点 = 0
			ENDSELECT
		ENDIF
ENDSELECT

