@CHARAMOVE_DATA_65(ARG, ARG:2)
LOCAL = 65
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;天界には行かない
			CASE P830玄云海 TO P842比那名居邸库房
				RETURN -100
			;私室やお澡堂は入らない
			CASE P817神奈子私室, P818诹访子私室, P820早苗私室
				RETURN -100
			CASE P851久侘歌的小屋, P852驹草的小屋, P853魅须丸的小屋, P824厕所, P825澡堂
				RETURN -100
			;虹龙洞は无理
			CASE P855虹龙洞, P856百百世的住所
				RETURN -100
			;桥、瀑布背面によく居る
			CASE P802桥, P806瀑布背面
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 森林深处
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ65
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ65(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "椛")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "兜巾"))
CASE "便装：饰品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("饰品", "袖"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "木屐"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "无袖外套"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "实用性第一の")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "零碎的兜裆布")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "裹胸")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[椛]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "千里眼")
CASE "固有指令实行可能"
	CALLF MAKE_INT(V_NAME, 1)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "鱼料理/野味/虫肉/和食/油炸食品/丰盛/机能的/野趣/豪快/软糯的/天狗流/甜/甜/咸/咸/" + "秋物/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "野菜/酸/酸/地底风/地底风/地底风/谜料理/谜料理/谜料理/谜料理/谜料理/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "切れ味/得意料理＊秋物＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/秩序/可爱的/朴素/和风/" + "刃物/" * 3 + "犬の/狼の/红叶の/天狗の/千里の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/洛丽塔/威严/狂热/" + "弹/" + "河童の/鬼の/" * 10)

;农民关联
;山っぽい作物を
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "蕎麦农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "蕎麦")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "蕎麦畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "妖怪之山・山顶")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 8)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 801)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 7)
ENDSELECT

