@CHARAMOVE_DATA_39(ARG, ARG:2)
LOCAL = 39
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
		;たまに连动
		SIF CFLAG:LOCAL:今日的运势 % 10 == 0
			RETURN [[星]]
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命莲寺メンバーの私室はあんまり入らない
			CASE P114村纱私室, P115星私室, P116一轮私室, P117鵺私室, P118白莲私室, P109女苑私室, P136响子私室
				RETURN -5
			;别馆、猯藏的小屋には入らない,神灵庙内部は行かない
			CASE P119别馆, P137猯藏的小屋, P125屠自古私室 TO P134藏, P138钟楼, P139邪仙楼
				RETURN -100
			;起居室に行きやすい
			CASE P108起居室
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 命莲寺方丈, 墓地
				RETURN 1
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大树, 再思之道, 无缘冢
				RETURN 1
			CASE 中有之道, 三途之川
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
		;白莲が本堂にいる时はやってくる
		SIF CFLAG:[[白莲]]:当前位置 == P103本堂 && !CFLAG:[[白莲]]:诶嘿嘿 && SHIRAHU([[白莲]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_キャラデータ39
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ39(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "纳兹琳")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子+兜帽）套")

CASE "便装：外衣"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "披肩"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "无袖外套"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "尾部有洞的灰色")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "编织物短裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "编织物胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "灵摆查找")
CASE "固有指令实行可能"
	CALLF MAKE_INT(V_NAME, 1)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "香料/中华/油炸食品/辣/丰盛/垃圾/浓厚/野趣/新鲜/怪诞/粘稠/甜/甜/甜/乳酪/乳酪/乳酪/乳酪/乳酪/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "简易/健康/新鲜/咸/咸/清淡/清淡/清淡/谜料理/谜料理/谜料理/谜料理/谜料理/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "得意料理＊乳酪＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "灰/可爱的/怀旧/纤细/知的/" + "宝石/遗物/" * 3 + "鼠の/小小の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "充满力量/夸张/" + "护臂/" * 3 + "猫の/" * 10)

;农民关联
;泣きついたら小言を言いながら何とかしてくれそう
;特に寺の连中が见てたら断りきれない
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "茄子农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "茄子")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "茄子畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "命莲寺")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 1)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 106)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 400)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 8)
ENDSELECT

