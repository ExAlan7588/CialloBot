@CHARAMOVE_DATA_133(ARG, ARG:2)
LOCAL = 133
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;是非曲直厅、冥界、八云邸には行かない（入り口部分は出る时用）
			CASE P612法庭 TO P639八云邸起居室
				RETURN -100
			;他人的家には入らない
			CASE P608润美的家, P609璎花的家, P606日狭美的家, P607残无的家
				RETURN -100
			;灵长园には行かない
			CASE P642灵长园 TO P645磨弓的房间
				RETURN -100
			;他所的澡堂には行かない
			CASE P646八千慧宅的澡堂, P655尤魔宅的澡堂
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 中有之道, 三途之川, 彼岸, 地狱, 畜生界
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ133
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ133(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "早鬼")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "牛仔帽"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "长靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "结丝巾"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "蹄鉄の印章が施された")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "ブラックシェイプ三角胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "黑翼ブラ")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 胜负内衣判定([[早鬼]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/香料/中华/锅物/丰盛/垃圾/浓厚/温暖/豪华/野趣/豪快/松脆/畜生流/" + "辣/胡萝卜/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "朴素/" + "甜/酸/简易/清淡/" * 2 + "天界风/" * 3)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "畜生流/得意料理＊豪快＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黑/赤/混沌/帅气的/朴素/充满力量/壮大/" + "狼の/" * 3 + "马の/天马の/结丝巾/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/纤细/知的/" + "神灵/书物/机械/" * 3 + "埴轮の/水獭の/鹫の/龟の/龙の/埴轮/" * 10)
ENDSELECT

