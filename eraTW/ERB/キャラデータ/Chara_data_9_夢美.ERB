        ;    エフェクト数 += 1
        ;ELSE
        ;    RESULTS:エフェクト数 = 半脱自慰
        ;    エフェクト数 += 1
        ;ENDIF
    ELSE
        ;立绘
    ENDIF
ENDIF

RETURN エフェクト数
