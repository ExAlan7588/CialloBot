@CHARAMOVE_DATA_9(ARG, ARG:2)
LOCAL = 9
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P49梦幻遗迹・研究室
				RETURN 5
			;他所
			CASE P27三妖精之家・大厅 TO P35三妖精之家・地下室, P15灵梦私室, P56阿吽的小屋
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 梦幻遗迹
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
		ENDSELECT
	;引力设定
	CASE 8
		;日曜日は千百合と一绪に境内まで出てくる
		SIF YOUBI_MATCH(DAY, "日") && INRANGE(TIME, 600, 960)
			TCVAR:LOCAL:引力点 = P2境内
ENDSELECT

