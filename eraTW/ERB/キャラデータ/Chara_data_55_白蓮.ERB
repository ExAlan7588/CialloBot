@CHARAMOVE_DATA_55(ARG, ARG:2)
LOCAL = 55
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命莲寺メンバーの私室はあんまり入らない
			CASE P113纳兹琳私室, P114村纱私室, P115星私室, P116一轮私室, P117鵺私室, P109女苑私室, P136响子私室
				RETURN -5
			;别馆、猯藏的小屋には入らない,神灵庙内部は行かない
			CASE P119别馆, P137猯藏的小屋, P125屠自古私室 TO P134藏, P138钟楼, P139邪仙楼
				RETURN -100
			;起居室に行きやすい
			CASE P108起居室
				RETURN 5
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 命莲寺境内, 命莲寺方丈, 墓地
				RETURN 1
			CASE 广场, 商业街, 长屋街
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
		;日曜の朝9时～13时までは本堂,命莲寺メンバーもやってくる
		SIF YOUBI_MATCH(DAY, "日") && INRANGE(TIME, 540, 780)
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

