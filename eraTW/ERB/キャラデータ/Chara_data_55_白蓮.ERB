;********************************************************************************************
@IMAGE_I55_通常颜绘特殊エフェクト
#DIM DYNAMIC エフェクト数 = 0

IF TALENT:55:母乳体质 && NOWEX:55:喷乳 && CHECK_NUDE(55)
	IF FLAG:巨乳差分表示 && TALENT:55:胸围>=2
		RESULTS:エフェクト数 = 爆乳母乳
	ELSE
		RESULTS:エフェクト数 = 母乳
	ENDIF
	エフェクト数 += 1
ENDIF
IF CFLAG:55:脸上精液
	;胸ぶっかけ
	RESULTS:エフェクト数 = ぶっかけ
	エフェクト数 += 1
ENDIF
IF CFLAG:55:口内精液
	;口交
	RESULTS:エフェクト数 = お口
	エフェクト数 += 1
ENDIF

RETURN エフェクト数

;********************************************************************************************
@IMAGE_I55_别立特殊エフェクト
#DIM DYNAMIC エフェクト数 = 0

SIF TCVAR:55:350
	RETURN エフェクト数

;お腹抚で指令で受精时
IF SELECTCOM == 306 && CFLAG:55:妊娠经过日数 && TFLAG:103==55
	SELECTCASE CFLAG:55:妊娠经过日数
		CASE 1 TO 9
			RESULTS:エフェクト数 = 胎内0
		CASE 10 TO 19
			RESULTS:エフェクト数 = 胎内1
		CASE 20 TO 29
			RESULTS:エフェクト数 = 胎内2
		CASE 30 TO 39
			RESULTS:エフェクト数 = 胎内3
		CASE 40 TO 59
			RESULTS:エフェクト数 = 胎内4
		CASE 60 TO 70
			RESULTS:エフェクト数 = 胎内5
		CASE 71 TO 100
			RESULTS:エフェクト数 = 胎内6
	ENDSELECT
	エフェクト数 ++
;开帐指令
ELSEIF GROUPMATCH(SELECTCOM, 8, 13)
	RESULTS:エフェクト数 = テレテレ
	エフェクト数 ++
ENDIF

RETURN エフェクト数

