;IMAGE_I55_特殊エフェクトチェック(CHAR, エフェクト)--------------------------------------------
;角色特有のエフェクト判定  特殊エフェクトを直接指定したい场合に参照
;参数 "エフェクト" に固有のエフェクトが含まれるかを判定する
;例の街道、STRFINDを使用してください
;-------------------------------------------------------------------------------------------
@IMAGE_I55_特殊エフェクトチェック(CHARA, エフェクト)
#DIM CHARA
#DIMS エフェクト

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	SIF STRFIND(エフェクト, "胎内") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "テレテレ") != -1
		RETURN 1
ELSE
	;通常画像
	SIF STRFIND(エフェクト, "母乳") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "ぶっかけ") != -1
		RETURN 1
	SIF STRFIND(エフェクト, "お口") != -1
		RETURN 1
ENDIF

RETURN 0

;IMAGE_I55_特殊エフェクト(CHARA)--------------------------------------------------------------------
;角色特有のエフェクト表示条件
;使用时注意点:
;	RESULTS配列に特殊エフェクトを格纳
;	RETURNで追加するエフェクト数を返还すること
;-------------------------------------------------------------------------------------------
@IMAGE_I55_特殊エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF FLAG:立绘种类
		;颜绘, 别立ち1没有颜绘
		CALL IMAGE_I55_通常颜绘特殊エフェクト
		エフェクト数 = RESULT
	ELSE
		;立绘
		CALL IMAGE_I55_别立特殊エフェクト
		エフェクト数 = RESULT
	ENDIF
ELSE
	;通常画像
	IF FLAG:立绘种类
		;颜绘
		CALL IMAGE_I55_通常颜绘特殊エフェクト
		エフェクト数 = RESULT
	ELSE
		;立绘
	ENDIF
ENDIF

RETURN エフェクト数

