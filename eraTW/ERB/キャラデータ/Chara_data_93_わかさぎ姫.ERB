@CHARAMOVE_DATA_93(ARG, ARG:2)
LOCAL = 93
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		;泳池を改造していないと动けない
		IF !GETBIT(FLAG:泳池, 3)
			SELECTCASE ARG:2
				CASE P347雾之湖, P348雾之湖・湖底
					RETURN 0
				CASEELSE
					RETURN -100
			ENDSELECT
		;动ける状态の时
		ELSE
			SELECTCASE ARG:2
				;湖畔と雾之湖によく居る
				CASE P343湖畔, P347雾之湖
					RETURN 5
				;かまくら、大ちゃん家には行かない
				CASE P344蕾蒂的冰屋, P345琪露诺的冰屋, P346大妖精的小屋
					RETURN -100
				;红魔馆内部、废洋馆には行かない
				CASE P303守卫小屋 TO P342莉莉卡私室
					RETURN -100
			ENDSELECT
		ENDIF
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 雾之湖
				RETURN 1
			;改造していないと动けない
			CASE 正门
				SIF GETBIT(FLAG:泳池, 3)
					RETURN 1
			CASE 竹林入口, 迷失的小道, 斜角的竹林
				SIF GETBIT(FLAG:泳池, 3)
					RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ93
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ93(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "若鹭姫")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：和服"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("和服", "和服"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "带"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "人鱼の王道")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "贝壳比基尼")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "贝壳胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 胜负内衣判定([[若鹭姫]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 99)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/叶物/酸/简易/健康/清淡/冷/朴素/野趣/家庭的/扑鼻/美味的水/甜/甜/甜/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "丰盛/垃圾/辣/辣/" + "怪诞/谜料理/" * 5 + "鱼料理/" * 10)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "美味的水/得意料理＊清淡＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/矿物/可爱的/艺术的/活泼/别致/" + "水/宝石/鱼/" * 3 + "鱼の/人鱼の/深海鱼の/古代鱼の/鳞/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/可怕的/夸张/威严/" + "冰/雷/刃物/弹/护臂/烟袋/" * 3 + "火/" * 5)
ENDSELECT

;-------------------------------------------------
;固有料理设定
;DISH_NAME          料理名
;RESULTS            料理の种类（轻食/主食/甜点）
;CALL 材料设定("")  材料の指定。"泛用"を入れると自动で泛用食材になる
;DISH_TASTE         味の设定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）调理にかかる时间
