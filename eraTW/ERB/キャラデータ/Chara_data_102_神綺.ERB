@CHARAMOVE_DATA_102(ARG, ARG:2)
LOCAL = 102
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;あんまり入らない
			CASE P512魔理沙宅・寝室, P521人偶洋馆・爱丽丝的房间, P523人偶洋馆・梦子的房间
				RETURN -5
			;引きこもらない
			CASE P522人偶洋馆・神绮的房间
				RETURN -10
			;客厅, 研究室に行きやすい
			CASE P524人偶洋馆・研究室, P525人偶洋馆・客厅
				RETURN 5
			;他所の私室には入らない
			CASE P511魔理沙宅・起居室, P515魔理沙宅・仓库, P535艾伦宅・寝室, P536艾伦宅・仓库, P537艾伦宅・澡堂
				RETURN -100
			CASE P541朱鹭子的栖息处 TO P544成美的小屋
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道
				RETURN 1
			CASE 森林的入口, 古木的大树, 雾雨魔法店, 人偶的洋馆, 再思之道, 无缘冢
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ102
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ102(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "神绮")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "晚礼服"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "无法创造出来的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "魔界神样の胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "魔界神大人の胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "面包/香料/洋食/锅物/派/简易/健康/豪华/上品/家庭的/软糯的/魔力/女仆流/魔界风/甜/甜/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "中华/丰盛/朴素/酸/酸/" + "青蛙/虫肉/谜料理/" * 3)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "魔界风/母亲的味道/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "白/高级/混沌/美的/威严/壮大/" + "魔法/妖术/" * 3 + "魔界蝶の/魔神の/魔界の/神话の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/洛丽塔/狂热/" + "安物/巫术/" * 3)
ENDSELECT

