@CHARAMOVE_DATA_34(ARG, ARG:2)
LOCAL = 34
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;他所的家には行かない
			CASE P807椛宅, P808文宅, P809果宅, P851久侘歌的小屋, P852驹草的小屋, P853魅须丸的小屋, P854龙宅, P855虹龙洞, P856百百世的住所
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社走廊
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内, 天界
				RETURN 1
		ENDSELECT
	;引力设定
	CASE 8
		;平日は3分の2で出步行の,紫苑と连动
		IF YOUBI_MATCH(DAY, "月火水木金") && INRANGE(TIME, 600, 960)
			SELECTCASE CFLAG:LOCAL:今日的运势 % 3
			CASE 1
				TCVAR:LOCAL:引力点 = P811境内
			CASE 2
				TCVAR:LOCAL:引力点 = P802桥
			CASEELSE
				TCVAR:LOCAL:引力点 = 0
			ENDSELECT
		ENDIF
ENDSELECT

@EXIST_キャラデータ34
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ34(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "天子")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "连衣裙"))
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "桃"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "没有任何针线")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "桃色内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "乳罩")
CASE "固有内衣着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "天候操作")
CASE "固有指令实行可能"
	CALLF MAKE_INT(V_NAME, 1)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "果物/香料/中华/面类/派/丰盛/垃圾/浓厚/豪华/上品/新鲜/软绵绵/天界风/甜/甜/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "野菜/简易/健康/清淡/朴素/野趣/酸/酸/青蛙/青蛙/青蛙/虫肉/虫肉/虫肉/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "天界风/绚烂/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/虹/混沌/美的/奇妙/活泼/夸张/壮大/威严/" + "土/刃物/" * 3 + "要石の/桃の/天使の/天人の/极光の/天界の/绯想の/非想非非想の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/朴素/别致/纤细/知的/" + "雷/死生/书物/生体品/领带/" * 3 + "恶魔の/" * 5)

;农民关联
;もも
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "桃农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "桃")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "桃农园")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "天界")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 8)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 833)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 600)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 7)
ENDSELECT

