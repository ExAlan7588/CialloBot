@CHARAMOVE_DATA_146(ARG, ARG:2)
LOCAL = 146
SELECTCASE ARG
	;连动设定（RETURN=依存するキャラ, RESULT:1～3=对等に连动するキャラ）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4 ;lives in 852
		SELECTCASE ARG:2
			;天界には行かない
			CASE P830玄云海 TO P842比那名居邸库房
				RETURN -100
			;私室やお澡堂は入らない
			CASE P817神奈子私室, P818诹访子私室, P820早苗私室
				RETURN -100
			;他所的家には行かない
			CASE P807椛宅, P808文宅, P809果宅, P851久侘歌的小屋, P853魅须丸的小屋, P854龙宅, P855虹龙洞, P856百百世的住所
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道, 长屋街
				RETURN 1
			CASE 正门, 食堂
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖
				RETURN 1
		ENDSELECT
	;引力设定
	CASE 8
ENDSELECT

@EXIST_キャラデータ146
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ146(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "伊丽丝")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（连衣裙）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "蝴蝶结"))
CASE "便装：和服"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "女式衬衫"))
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "长裙"))	
CASE "便装：其他１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他１", "丝带领巾"))
CASE "便装：其他２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他２", "花饰り"))
CASE "便装：其他３"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("其他３", "魔法のステッキ"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))	
CASE "便装：外衣"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("外衣", "夹克衫"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "mischievously devilish")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "red star-shaped panties")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "red star-shaped bra")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 胜负内衣判定([[伊丽丝]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/香料/辣/辣/辣/辣/洋食/新鲜/垃圾/豪华/怪诞/魔界风/未知の味/谜料理/妖气/" + "甜/" * 10)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "野菜/朴素/祝福/天界风/神力/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "魔界风/刺激的/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黑/紫/混沌/可怕的/活泼/充满力量/洛丽塔/" + "暗/血/妖术/赤/欲望の/" * 3 + "蝠の/" * 5 + "恶魔の/领带/暗の/" * 5 + "星/" * 10)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/朴素/怀旧/纤细/" + "水/光/日/护符/神灵/" * 3 + "天使の/" * 10)
ENDSELECT

