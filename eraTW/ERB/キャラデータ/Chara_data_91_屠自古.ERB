@CHARAMOVE_DATA_91(ARG, ARG:2)
LOCAL = 91
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;神灵庙メンバーの私室はあんまり入らない
			CASE P127布都私室, P128神子私室, P139邪仙楼
				RETURN -5
			;命莲寺内部
			CASE P103本堂 TO P120库房
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 地藏小路, 倒垂的柳树下, 墓地, 神灵庙广场, 神灵庙道场
				RETURN 1
			CASE 中有之道, 三途之川, 白玉楼庭院, 白玉楼, 地狱
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ91
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ91(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
