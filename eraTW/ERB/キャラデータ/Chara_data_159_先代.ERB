@CHARAMOVE_DATA_159(ARG, ARG:2)
LOCAL = 1
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P2境内, P10走廊
				RETURN 5
			;他所
			CASE P30三妖精之家・澡堂, P39梦幻遗迹・浴室, P56阿吽的小屋
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社本堂, 神社走廊
				RETURN 1
			CASE 广场, 商业街, 吞屋小道, 长屋街
			    RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大树, 雾雨魔法店, 人偶的洋馆, 再思之道, 无缘冢
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家, 白玉楼庭院, 白玉楼
			RETURN 1
			CASE 幻想风穴, 地狱的深道, 旧地狱街道
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ159
#LOCALSIZE 1
#LOCALSSIZE 1

;オブジェクト本体
@キャラデータ159(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "先代")

;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：饰品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("饰品", "袖"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "长筒袜"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
	ENDIF
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "博丽的巫女服"))
	
CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "特殊训练")
CASE "固有指令实行可能"
	CALLF MAKE_INT(V_NAME, !FLAG:特殊训练)
	

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/辣/洋食/叶物/简易/清淡/朴素/上品/家庭的/软绵绵/" + "甜/酸/" * 2 + "春物/煎饼/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "/妖力/" * 2 + "怪诞/谜料理/" * 5)
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "薄味/得意料理＊和食＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/可爱的/和风/" + "神灵/巫术/护符/狐の/春の/永远の/" * 3 + "莲华蝶の/樱花の/巫女の/乐园の/封魔の/充满力量/帅气的/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/洋风/洛丽塔/狂热/" + "妖术/" * 3 + "烟袋/" * 5 + "守矢の/妖怪の/" * 10)

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "放出奇怪的光泽")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "丝绸内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "丝绸内衣")
CASE "固有内衣着用条件"
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;农民关联
;神社は土地持ってそう、神田
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "米农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "大米")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博丽神社")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 800)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 5)

;ここは依赖IDが关联しない情报
;依赖发生率スカ2/一般3/固有5
CASE "没有依赖发生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依赖发生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依赖发生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依赖最终要素"
	CALLF MAKE_INT(V_NAME, 102)
CASEELSE
	;固有依赖关联のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依赖")
		CALLF 固有依赖159(ARG, O_DATA, V_NAME)

;固有Counterテスト


ENDSELECT

