;IMAGE_I1_特殊エフェクトチェック(CHAR, エフェクト)--------------------------------------------
;角色特有のエフェクト判定  特殊エフェクトを直接指定したい场合に参照
;参数 "エフェクト" に固有のエフェクトが含まれるかを判定する
;例の街道、STRFINDを使用してください
;-------------------------------------------------------------------------------------------
@IMAGE_I1_特殊エフェクトチェック(CHARA, エフェクト)
#DIM CHARA
#DIMS エフェクト

IF CFLAG:CHARA:应用差分替换
	;替换用画像
ELSE
	;通常画像
	SIF STRFIND(エフェクト, "すやすや照れ") != -1
		RETURN 1
ENDIF

RETURN 0

;IMAGE_I1_特殊エフェクト(CHARA)--------------------------------------------------------------------
;角色特有のエフェクト表示条件
;使用时注意点:
;	RESULTS配列に特殊エフェクトを格纳
;	RETURNで追加するエフェクト数を返还すること
;-------------------------------------------------------------------------------------------
@IMAGE_I1_特殊エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF FLAG:立绘种类
		;颜绘
	ELSE
		;立绘
	ENDIF
ELSE
	;通常画像
	IF FLAG:立绘种类 && CHECK_BLUSH(CHARA) && CFLAG:CHARA:睡眠
		;颜绘
		RESULTS:エフェクト数 = すやすや照れ
		エフェクト数 ++
	ELSE
		;立绘
	ENDIF
ENDIF

RETURN エフェクト数

;IMAGE_I1_自慰エフェクト(CHARA)--------------------------------------------------------------------
;角色特有の自慰エフェクト表示条件
@IMAGE_I1_自慰エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:应用差分替换
	;替换用画像
	IF FLAG:立绘种类
		;颜绘
	ELSE
		;立绘
	ENDIF
ELSE
	;活动时间中は通常画像
	SIF BETWEENTIME(CFLAG:CHARA:起床时间, CFLAG:CHARA:就寝时间)
		RETURN
	;通常画像
	IF FLAG:立绘种类
		;颜绘
		;AFTERTRAニーは寝间着
		RESULTS:エフェクト数 = 颜绘_AFTERTRAニー_1
		SIF TALENT:CHARA:胸围 > 0 && FLAG:巨乳差分表示
			RESULTS = 颜绘_AFTERTRAニー巨乳_1
		エフェクト数 += 1
	ELSE
		;立绘
	ENDIF
ENDIF

RETURN エフェクト数
