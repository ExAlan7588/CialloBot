@CHARAMOVE_DATA_149(ARG, ARG:2)
LOCAL = 149
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2 ;lives at 740
			;森林深处、秘天崖によく居る
			CASE P738森林深处, P739秘天崖
				RETURN 5
			;私室には入らない
			CASE P704河童的宣传站, P706厄神的家, P709莉莉们的家, P716秋姊妹的家・土间 TO P730仙人的宅邸・庭
				RETURN -100
			CASE P733正邪的藏身处, P736山姥之家, P737招财猫的家, P741美天的栖息处
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 广场, 商业街, 吞屋小道, 正门
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大树, 再思之道, 无缘冢
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 森林深处
				RETURN 1
			CASE 九天瀑布, 绝景之丘, 山之湖, 守矢神社境内
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT
@EXIST_キャラデータ149
