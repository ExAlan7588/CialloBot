@CHARAMOVE_DATA_25(ARG, ARG:2)
LOCAL = 25
SELECTCASE ARG
	;连动设定（RETURN=依存する角色, RESULT:1～3=对等に连动する角色）
	CASE 1
	;特殊生活时间设定（CFLAG:LOCAL:生活时间补正を设定する）
	CASE 3
	;移动倾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;紫样の私室はあまり入らない
			CASE P633紫私室
				RETURN -5
			;是非曲直厅、畜生界は行かない
			CASE P610是非曲直厅大堂 TO P618空的房间, P640异界的关卡 TO P655尤魔宅的澡堂
				RETURN -100
			;他人的家には入らない
			CASE P608润美的家, P609璎花的家
				RETURN -100
			;私室等には入らない
			CASE P623幽幽子私室, P624妖梦私室, P627澡堂, P606日狭美的家, P607残无的家
				RETURN -100
		ENDSELECT
	;出现场所
	CASE 7
		SELECTCASE ARG:2
			CASE 樱花道的鸟居, 神社境内, 神社本堂, 神社走廊
				RETURN 1
			CASE 吞屋小道
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家, 白玉楼庭院, 白玉楼
				RETURN 1
			CASE 山麓树海, 玄武之泽, 大蛤蟆之池, 森林深处
				RETURN 1
		ENDSELECT
	;引力设定（CFLAG:LOCAL:引力点を设定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ25
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ25(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "称呼"
	CALLF MAKE_STR(V_NAME, "蓝")
;こっから衣装设定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：袜子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "长筒袜"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("袜子", "袜子"))
	ENDIF
CASE "便装：连衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("连衣裙", "道士服"))

CASE "固有内衣形容词"
	CALLF MAKE_STR(V_NAME, "尾部开了个大洞的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "Ｏ洞内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "全剪子")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 胜负内衣判定([[蓝]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(后)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理关联
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/卵/和食/中华/油炸食品/丰盛/上品/家庭的/机能的/软糯的/辣/辣/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "咸/咸/谜料理/谜料理/谜料理/谜料理/谜料理/")
CASE "料理：助手效果"
	CALLF MAKE_STR(V_NAME, "妖力/妖力/得意料理＊和食＊/")
;赠り物关联
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/秩序/美的/和风/别致/知的/" + "妖术/书物/" * 3 + "狐の/式神の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "活泼/洛丽塔/狂热/" + "水/护臂/" * 3 + "狸の/" * 10)

;农民关联
;おあげ
CASE "农民：说明"
	CALLF MAKE_STR(V_NAME, "大豆农家")
CASE "农民：作物"
	CALLF MAKE_STR(V_NAME, "大豆")
CASE "农民：田的种类"
	CALLF MAKE_STR(V_NAME, "大豆畑")
CASE "农民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "人里")

CASE "农民：MapID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "农民：场所ID"
	CALLF MAKE_INT(V_NAME, 205)
CASE "农民：报酬"
	CALLF MAKE_INT(V_NAME, 600)
CASE "农民：金利"
	CALLF MAKE_INT(V_NAME, 5)
ENDSELECT

