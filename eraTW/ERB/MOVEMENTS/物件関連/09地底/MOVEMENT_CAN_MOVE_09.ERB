;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_9(ARG, ARG:1)
#FUNCTION
#DIM 参照先
SELECTCASE ARG
CASE P901地灵殿前
	SIF GROUPMATCH(ARG:1, P905一层走廊, P931大街)
		RETURNF 1
CASE P904大会堂, P906一层厕所, P911阿空私室, P912阿燐私室
	SIF GROUPMATCH(ARG:1, P905一层走廊)
		RETURNF 1
CASE P905一层走廊
	SIF GROUPMATCH(ARG:1, P901地灵殿前, P904大会堂, P906一层厕所, P909厨房, P911阿空私室, P912阿燐私室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P913食堂, P914二层走廊, P923后院, P926中庭)
		RETURNF 1
CASE P909厨房
	SIF GROUPMATCH(ARG:1, P905一层走廊, P910食料库, P913食堂, P925薔薇园)
		RETURNF 1
CASE P910食料库
	SIF GROUPMATCH(ARG:1, P909厨房)
		RETURNF 1
CASE P913食堂
	SIF GROUPMATCH(ARG:1, P905一层走廊, P909厨房, P923后院, P926中庭)
		RETURNF 1
CASE P914二层走廊
	SIF GROUPMATCH(ARG:1, P905一层走廊, P915露台, P916库房, P917二层厕所)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P918恋私室, P919觉私室, P920书斋, P922更衣间)
		RETURNF 1
CASE P915露台, P916库房, P917二层厕所, P918恋私室
	SIF GROUPMATCH(ARG:1, P914二层走廊)
		RETURNF 1
CASE P919觉私室
	SIF GROUPMATCH(ARG:1, P914二层走廊, P920书斋)
		RETURNF 1
CASE P920书斋
	SIF GROUPMATCH(ARG:1, P914二层走廊, P919觉私室)
		RETURNF 1
CASE P921澡堂
	SIF GROUPMATCH(ARG:1, P922更衣间)
		RETURNF 1
CASE P922更衣间
	SIF GROUPMATCH(ARG:1, P914二层走廊, P921澡堂)
		RETURNF 1
CASE P923后院
	SIF GROUPMATCH(ARG:1, P905一层走廊, P913食堂, P925薔薇园)
		RETURNF 1
CASE P925薔薇园
	SIF GROUPMATCH(ARG:1, P909厨房, P923后院)
		RETURNF 1
CASE P926中庭
	SIF GROUPMATCH(ARG:1, P905一层走廊, P913食堂, P928灼热地狱遗迹)
		RETURNF 1
CASE P928灼热地狱遗迹
	SIF GROUPMATCH(ARG:1, P926中庭, P929地狱的人工太阳)
		RETURNF 1
CASE P929地狱的人工太阳
	SIF GROUPMATCH(ARG:1, P928灼热地狱遗迹, P930旧血池地狱, P949托卡马克装置核心)
		RETURNF 1
CASE P930旧血池地狱
	SIF GROUPMATCH(ARG:1, P929地狱的人工太阳, P947血枪的栖息处)
		RETURNF 1
CASE P947血枪的栖息处
	SIF GROUPMATCH(ARG:1, P930旧血池地狱)
		RETURNF 1
CASE P931大街
	SIF GROUPMATCH(ARG:1, P901地灵殿前, P932后街, P933酒场, P937空屋, P944街道)
		RETURNF 1
CASE P932后街
	SIF GROUPMATCH(ARG:1, P931大街, P933酒场, P934温泉, P935情人旅馆, P936勇仪的家, P938鬼的聚集场所, P939小胡同)
		RETURNF 1
CASE P933酒场
	SIF GROUPMATCH(ARG:1, P931大街, P932后街)
		RETURNF 1
CASE P934温泉, P935情人旅馆, P936勇仪的家, P938鬼的聚集场所, P939小胡同
	SIF GROUPMATCH(ARG:1, P932后街)
		RETURNF 1
CASE P937空屋
	SIF GROUPMATCH(ARG:1, P931大街)
		RETURNF 1
CASE P940帕露西的家, P941山女的家, P942琪斯美的家
	SIF GROUPMATCH(ARG:1, P945河岸)
		RETURNF 1
CASE P943茅屋
	SIF GROUPMATCH(ARG:1, P944街道)
		RETURNF 1
CASE P944街道
	SIF GROUPMATCH(ARG:1, P946桥)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P931大街, P943茅屋, P945河岸)
		RETURNF 1
CASE P945河岸
	SIF GROUPMATCH(ARG:1, P940帕露西的家, P941山女的家, P942琪斯美的家, P944街道, P946桥)
		RETURNF 1
CASE P946桥
	SIF GROUPMATCH(ARG:1, P944街道, P948竖井底部)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P945河岸)
		RETURNF 1
CASE P948竖井底部
	SIF GROUPMATCH(ARG:1, P946桥)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, MAXROOM)
		RETURNF 2
CASE P949托卡马克装置核心
	SIF GROUPMATCH(ARG:1, P929地狱的人工太阳)
		RETURNF 1
CASE MAXROOM
	SIF ARG:1 == P948竖井底部
		RETURNF 1
ENDSELECT

RETURNF 0
