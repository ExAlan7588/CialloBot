;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_8(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P801山顶入口
	SIF GROUPMATCH(ARG:1, P802桥)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P807椛宅)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
CASE P802桥
	SIF GROUPMATCH(ARG:1, P801山顶入口, P803索道站)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P806瀑布背面, P807椛宅, P808文宅, P809果宅)
		RETURNF 1
CASE P803索道站
	SIF GROUPMATCH(ARG:1, P802桥)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P804守矢神社鸟居, P808文宅, P809果宅, P810山小屋)
		RETURNF 1
CASE P804守矢神社鸟居
	SIF GROUPMATCH(ARG:1, P811境内)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P803索道站, P805顶上, P810山小屋)
		RETURNF 1
CASE P805顶上
	SIF GROUPMATCH(ARG:1, P804守矢神社鸟居, P830玄云海, P851久侘歌的小屋, P852驹草的小屋, P853魅须丸的小屋, P854龙宅, P855虹龙洞)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P801山顶入口, P802桥, P803索道站)
		RETURNF 2
CASE P806瀑布背面
	SIF GROUPMATCH(ARG:1, P802桥)
		RETURNF 1
CASE P807椛宅
	SIF GROUPMATCH(ARG:1, P801山顶入口, P802桥)
		RETURNF 1
CASE P808文宅, P809果宅
	SIF GROUPMATCH(ARG:1, P802桥, P803索道站)
		RETURNF 1
CASE P810山小屋
	SIF GROUPMATCH(ARG:1, P803索道站, P804守矢神社鸟居)
		RETURNF 1

CASE P811境内
	SIF GROUPMATCH(ARG:1, P804守矢神社鸟居, P813社务所, P814湖畔)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P812本殿, P821土间)
		RETURNF 1
CASE P812本殿
	SIF GROUPMATCH(ARG:1, P811境内, P826走廊)
		RETURNF 1
CASE P813社务所
	SIF GROUPMATCH(ARG:1, P811境内)
		RETURNF 3
CASE P814湖畔
	SIF GROUPMATCH(ARG:1, P811境内, P815庭)
		RETURNF 3
CASE P815庭
	SIF GROUPMATCH(ARG:1, P814湖畔, P816走廊)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P821土间)
		RETURNF 1
CASE P816走廊
	SIF GROUPMATCH(ARG:1, P817神奈子私室, P818诹访子私室, P819起居室, P821土间, P826走廊)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P815庭)
		RETURNF 3
CASE P817神奈子私室
	SIF GROUPMATCH(ARG:1, P816走廊, P818诹访子私室, P819起居室)
		RETURNF 1
CASE P818诹访子私室
	SIF GROUPMATCH(ARG:1, P816走廊, P817神奈子私室, P820早苗私室, P821土间)
		RETURNF 1
CASE P819起居室
	SIF GROUPMATCH(ARG:1, P816走廊, P817神奈子私室, P820早苗私室, P826走廊)
		RETURNF 1
CASE P820早苗私室
	SIF GROUPMATCH(ARG:1, P818诹访子私室, P819起居室, P821土间, P826走廊)
		RETURNF 1
CASE P821土间
	SIF GROUPMATCH(ARG:1, P811境内, P815庭, P826走廊, P816走廊, P818诹访子私室, P820早苗私室, P823空室, P824厕所, P825澡堂)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P822厨房)
		RETURNF 3
CASE P822厨房
	SIF GROUPMATCH(ARG:1, P821土间)
		RETURNF 3
CASE P823空室, P824厕所, P825澡堂
	SIF GROUPMATCH(ARG:1, P821土间)
		RETURNF 1
CASE P826走廊
	SIF GROUPMATCH(ARG:1, P812本殿, P816走廊, P819起居室, P820早苗私室, P821土间)
		RETURNF 1

CASE P830玄云海
	SIF GROUPMATCH(ARG:1, P805顶上)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P832有顶天街)
		RETURNF 3
CASE P831比那名居邸前
	SIF GROUPMATCH(ARG:1, P832有顶天街, P834比那名居邸土间)
		RETURNF 1
CASE P832有顶天街
	SIF GROUPMATCH(ARG:1, P830玄云海)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P831比那名居邸前, P833桃园)
		RETURNF 1
CASE P833桃园
	SIF GROUPMATCH(ARG:1, P832有顶天街)
		RETURNF 1
CASE P834比那名居邸土间
	SIF GROUPMATCH(ARG:1, P831比那名居邸前, P835比那名居邸会客厅, P836比那名居邸厨房, P837比那名居邸澡堂, P838比那名居邸起居室)
		RETURNF 1
CASE P835比那名居邸会客厅
	SIF GROUPMATCH(ARG:1, P834比那名居邸土间, P836比那名居邸厨房)
		RETURNF 1
CASE P836比那名居邸厨房
	SIF GROUPMATCH(ARG:1, P834比那名居邸土间, P835比那名居邸会客厅)
		RETURNF 1
CASE P837比那名居邸澡堂
	SIF GROUPMATCH(ARG:1, P834比那名居邸土间)
		RETURNF 1
CASE P838比那名居邸起居室
	SIF GROUPMATCH(ARG:1, P834比那名居邸土间, P839比那名居邸和室, P840天子私室, P841衣玖私室, P842比那名居邸库房)
		RETURNF 1
CASE P839比那名居邸和室
	SIF GROUPMATCH(ARG:1, P838比那名居邸起居室, P840天子私室)
		RETURNF 1
CASE P840天子私室
	SIF GROUPMATCH(ARG:1, P838比那名居邸起居室, P839比那名居邸和室)
		RETURNF 1
CASE P841衣玖私室, P842比那名居邸库房
	SIF GROUPMATCH(ARG:1, P838比那名居邸起居室)
		RETURNF 1

CASE P851久侘歌的小屋, P852驹草的小屋, P853魅须丸的小屋, P854龙宅
	SIF GROUPMATCH(ARG:1, P805顶上)
		RETURNF 1
CASE P855虹龙洞
	SIF GROUPMATCH(ARG:1, P805顶上, P856百百世的住所)
		RETURNF 1
CASE P856百百世的住所
	SIF GROUPMATCH(ARG:1, P855虹龙洞)
		RETURNF 1

CASE MAXROOM
	SIF ARG:1 == P801山顶入口
		RETURNF 1
ENDSELECT

RETURNF 0