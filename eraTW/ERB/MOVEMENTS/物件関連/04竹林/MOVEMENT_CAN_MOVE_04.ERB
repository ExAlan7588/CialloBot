;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_4(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P401永远亭入口
		SIF GROUPMATCH(ARG:1, P403等候室, P405配药室, P433斜角的竹林, P435水井)
			RETURNF 1
	CASE P403等候室
		SIF GROUPMATCH(ARG:1, P401永远亭入口, P404诊察室, P406来客用厕所, P407走廊)
			RETURNF 1
	CASE P404诊察室
		SIF GROUPMATCH(ARG:1, P403等候室, P405配药室)
			RETURNF 1
	CASE P405配药室
		SIF GROUPMATCH(ARG:1, P401永远亭入口, P404诊察室, P417厨房, P422后院)
			RETURNF 1
	CASE P406来客用厕所
		SIF GROUPMATCH(ARG:1, P403等候室)
			RETURNF 1
	CASE P407走廊
		SIF GROUPMATCH(ARG:1, P403等候室, P408中走廊, P409走廊, P418厕, P419更衣间, P426二层走廊, P430地下室)
			RETURNF 1
	CASE P408中走廊
		SIF GROUPMATCH(ARG:1, P407走廊, P410帝私室, P411铃仙私室, P412空的房间, P413永琳私室, P414辉夜私室, P415大厅, P416起居室)
			RETURNF 1
	CASE P409走廊
		SIF GROUPMATCH(ARG:1, P407走廊, P412空的房间, P413永琳私室, P414辉夜私室, P416起居室, P424库房)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P421庭)
			RETURNF 3
	CASE P410帝私室
		SIF GROUPMATCH(ARG:1, P408中走廊, P411铃仙私室)
			RETURNF 1
	CASE P411铃仙私室
		SIF GROUPMATCH(ARG:1, P408中走廊, P410帝私室)
			RETURNF 1
	CASE P412空的房间
		SIF GROUPMATCH(ARG:1, P408中走廊, P409走廊, P413永琳私室)
			RETURNF 1
	CASE P413永琳私室
		SIF GROUPMATCH(ARG:1, P408中走廊, P409走廊, P412空的房间, P414辉夜私室)
			RETURNF 1
	CASE P414辉夜私室
		SIF GROUPMATCH(ARG:1, P408中走廊, P409走廊, P413永琳私室)
			RETURNF 1
	CASE P415大厅
		SIF GROUPMATCH(ARG:1, P408中走廊, P416起居室, P417厨房)
			RETURNF 1
	CASE P416起居室
		SIF GROUPMATCH(ARG:1, P408中走廊, P409走廊, P415大厅, P417厨房)
			RETURNF 1
	CASE P417厨房
		SIF GROUPMATCH(ARG:1, P405配药室, P415大厅, P416起居室, P422后院)
			RETURNF 1
	CASE P418厕
		SIF GROUPMATCH(ARG:1, P407走廊)
			RETURNF 1
	CASE P419更衣间
		SIF GROUPMATCH(ARG:1, P407走廊, P420澡堂)
			RETURNF 1
	CASE P420澡堂
		SIF GROUPMATCH(ARG:1, P419更衣间)
			RETURNF 1
	CASE P421庭
		SIF GROUPMATCH(ARG:1, P409走廊, P422后院)
			RETURNF 1
	CASE P422后院
		SIF GROUPMATCH(ARG:1, P405配药室, P417厨房, P421庭)
			RETURNF 1
	CASE P424库房
		SIF GROUPMATCH(ARG:1, P409走廊)
			RETURNF 1
	CASE P426二层走廊
		SIF GROUPMATCH(ARG:1, P407走廊, P427仓库, P428二层空的房间)
			RETURNF 1
	CASE P427仓库
		SIF GROUPMATCH(ARG:1, P426二层走廊)
			RETURNF 1
	CASE P428二层空的房间
		SIF GROUPMATCH(ARG:1, P426二层走廊)
			RETURNF 1
	CASE P430地下室
		SIF GROUPMATCH(ARG:1, P407走廊)
			RETURNF 1

	CASE P431兔的洞穴
		SIF GROUPMATCH(ARG:1, P433斜角的竹林)
			RETURNF 1
	CASE P432影狼的家
		SIF GROUPMATCH(ARG:1, P433斜角的竹林)
			RETURNF 1
	CASE P433斜角的竹林
		SIF GROUPMATCH(ARG:1, P401永远亭入口, P431兔的洞穴, P432影狼的家, P434迷失的小道, P442空的小屋, P443米斯蒂亚的家, P444夜雀的小摊)
			RETURNF 1
	CASE P434迷失的小道
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P435水井, P440废屋, P441温泉, P445妹红的家, P446祠, P447竹林入口)
			RETURNF 1
	CASE P435水井
		SIF GROUPMATCH(ARG:1, P401永远亭入口, P434迷失的小道, P436无名之丘)
			RETURNF 1
	CASE P436无名之丘
		SIF GROUPMATCH(ARG:1, P438太阳花田)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P435水井, P437梅蒂欣的家)
			RETURNF 1
	CASE P437梅蒂欣的家
		SIF GROUPMATCH(ARG:1, P436无名之丘)
			RETURNF 1
	CASE P438太阳花田
		SIF GROUPMATCH(ARG:1, P439拉尔瓦的家, P450玄关)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P436无名之丘, P466庭)
			RETURNF 3
	CASE P439拉尔瓦的家
		SIF GROUPMATCH(ARG:1, P438太阳花田)
			RETURNF 1
	CASE P440废屋
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P447竹林入口)
			RETURNF 1
	CASE P441温泉
		SIF GROUPMATCH(ARG:1, P434迷失的小道)
			RETURNF 1
	CASE P442空的小屋
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P444夜雀的小摊)
			RETURNF 1
	CASE P443米斯蒂亚的家
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P444夜雀的小摊)
			RETURNF 1
	CASE P444夜雀的小摊
		SIF GROUPMATCH(ARG:1, P433斜角的竹林, P442空的小屋, P443米斯蒂亚的家, P446祠, P447竹林入口)
			RETURNF 1
	CASE P445妹红的家
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P447竹林入口)
			RETURNF 1
	CASE P446祠
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P444夜雀的小摊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P447竹林入口)
			RETURNF 3
	CASE P447竹林入口
		SIF GROUPMATCH(ARG:1, P434迷失的小道, P440废屋, P444夜雀的小摊, P445妹红的家)
			RETURNF 1
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P446祠)
			RETURNF 3

	CASE P450玄关
		SIF GROUPMATCH(ARG:1, P438太阳花田, P451胡桃私室, P452艾丽私室, P465大堂, P466庭)
			RETURNF 1
	CASE P451胡桃私室
		SIF GROUPMATCH(ARG:1, P450玄关)
			RETURNF 1
	CASE P452艾丽私室
		SIF GROUPMATCH(ARG:1, P450玄关)
			RETURNF 1
	CASE P453餐厅
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P454浴室
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P455厕所
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P456仓库
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P457梦月私室
		SIF GROUPMATCH(ARG:1, P458走廊)
			RETURNF 1
	CASE P458走廊
		SIF GROUPMATCH(ARG:1, P457梦月私室, P459幻月私室, P461客厅, P463幽香私室, P465大堂)
			RETURNF 1
	CASE P459幻月私室
		SIF GROUPMATCH(ARG:1, P458走廊)
			RETURNF 1
	CASE P460空的房间
		SIF GROUPMATCH(ARG:1, P465大堂)
			RETURNF 1
	CASE P461客厅
		SIF GROUPMATCH(ARG:1, P458走廊, P462阳台, P465大堂)
			RETURNF 1
	CASE P462阳台
		SIF GROUPMATCH(ARG:1, P461客厅, P466庭)
			RETURNF 3
	CASE P463幽香私室
		SIF GROUPMATCH(ARG:1, P458走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P464露台)
			RETURNF 3
	CASE P464露台
		SIF GROUPMATCH(ARG:1, P463幽香私室)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P466庭)
			RETURNF 2
	CASE P465大堂
		SIF GROUPMATCH(ARG:1, P450玄关, P453餐厅, P454浴室, P455厕所, P456仓库, P458走廊, P460空的房间, P461客厅)
			RETURNF 1
	CASE P466庭
		SIF GROUPMATCH(ARG:1, P450玄关)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P438太阳花田, P462阳台)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P464露台)
			RETURNF 2

	CASE MAXROOM
		SIF ARG:1 == P447竹林入口
			RETURNF 1
ENDSELECT
RETURNF 0
