;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_3(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P301正门
		SIF GROUPMATCH(ARG:1, P303守卫小屋, P304空的小屋, P343湖畔)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P302庭
		SIF GROUPMATCH(ARG:1, P303守卫小屋, P306后院, P309大厅)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P301正门, P307东阳台, P308西阳台)
			RETURNF 3
	CASE P303守卫小屋, P304空的小屋
		SIF GROUPMATCH(ARG:1, P301正门, P302庭, P306后院)
			RETURNF 1
	CASE P305仓库
		SIF GROUPMATCH(ARG:1, P306后院)
			RETURNF 1
	CASE P306后院
		SIF GROUPMATCH(ARG:1, P302庭, P303守卫小屋, P304空的小屋, P305仓库, P313厨房)
			RETURNF 1
	CASE P307东阳台
		SIF GROUPMATCH(ARG:1, P310图书馆)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P308西阳台
		SIF GROUPMATCH(ARG:1, P309大厅)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 3
	CASE P309大厅
		SIF GROUPMATCH(ARG:1, P302庭, P310图书馆, P314食堂, P315会客厅, P316一层厕所, P319二层走廊, P328地下走廊)
			RETURNF 1
	CASE P310图书馆
		SIF GROUPMATCH(ARG:1, P307东阳台, P309大厅, P311帕秋莉私室, P312小恶魔私室)
			RETURNF 1
	CASE P311帕秋莉私室, P312小恶魔私室
		SIF GROUPMATCH(ARG:1, P310图书馆)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306后院)
			RETURNF 2
	CASE P313厨房
		SIF GROUPMATCH(ARG:1, P306后院, P314食堂)
			RETURNF 1
	CASE P314食堂
		SIF GROUPMATCH(ARG:1, P309大厅, P313厨房)
			RETURNF 1
	CASE P315会客厅
		SIF GROUPMATCH(ARG:1, P308西阳台, P309大厅)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭)
			RETURNF 2
	CASE P316一层厕所
		SIF GROUPMATCH(ARG:1, P309大厅)
			RETURNF 1
	CASE P319二层走廊
		SIF GROUPMATCH(ARG:1, P309大厅, P320咲夜私室, P321空的房间, P322二层厕所, P323妖精女仆的值班室, P326蕾米莉亚私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P324大露台)
			RETURNF 3
	CASE P320咲夜私室, P321空的房间
		SIF GROUPMATCH(ARG:1, P319二层走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306后院)
			RETURNF 2
	CASE P322二层厕所, P323妖精女仆的值班室
		SIF GROUPMATCH(ARG:1, P319二层走廊)
			RETURNF 1
	CASE P324大露台
		SIF GROUPMATCH(ARG:1, P319二层走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302庭, P301正门)
			RETURNF 2
	CASE P326蕾米莉亚私室
		SIF GROUPMATCH(ARG:1, P319二层走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P327小露台)
			RETURNF 3
	CASE P327小露台
		SIF GROUPMATCH(ARG:1, P326蕾米莉亚私室)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P302庭, P301正门, P324大露台)
			RETURNF 2
	CASE P328地下走廊
		SIF GROUPMATCH(ARG:1, P309大厅, P329仓库, P330佣人间, P331地下室, P332更衣间)
			RETURNF 1
	CASE P329仓库, P330佣人间, P331地下室
		SIF GROUPMATCH(ARG:1, P328地下走廊)
			RETURNF 1
	CASE P332更衣间
		SIF GROUPMATCH(ARG:1, P328地下走廊, P333大浴场)
			RETURNF 1
	CASE P333大浴场
		SIF GROUPMATCH(ARG:1, P332更衣间)
			RETURNF 1

	CASE P334废洋馆・入口
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P335废洋馆・大厅)
			RETURNF 3
	CASE P335废洋馆・大厅
		SIF GROUPMATCH(ARG:1, P338废洋馆・澡堂, P339废洋馆・二层走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P334废洋馆・入口, P337废洋馆・厨房)
			RETURNF 3
	CASE P337废洋馆・厨房
		SIF GROUPMATCH(ARG:1, P335废洋馆・大厅)
			RETURNF 3
	CASE P338废洋馆・澡堂
		SIF GROUPMATCH(ARG:1, P335废洋馆・大厅)
			RETURNF 1
	CASE P339废洋馆・二层走廊
		SIF GROUPMATCH(ARG:1, P335废洋馆・大厅, P340露娜萨私室, P341梅露兰私室, P342莉莉卡私室)
			RETURNF 1
	CASE P340露娜萨私室, P341梅露兰私室, P342莉莉卡私室
		SIF GROUPMATCH(ARG:1, P339废洋馆・二层走廊)
			RETURNF 1

	CASE P343湖畔
		SIF GROUPMATCH(ARG:1, P347雾之湖)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P301正门, P334废洋馆・入口, P344蕾蒂的冰屋, P345琪露诺的冰屋, P346大妖精的小屋)
			RETURNF 1
	CASE P344蕾蒂的冰屋, P345琪露诺的冰屋, P346大妖精的小屋
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 1
	CASE P347雾之湖
		SIF GROUPMATCH(ARG:1, P343湖畔)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P348雾之湖・湖底)
			RETURNF 1
	CASE P348雾之湖・湖底
		SIF GROUPMATCH(ARG:1, P347雾之湖)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P343湖畔
			RETURNF 1
ENDSELECT
RETURNF 0