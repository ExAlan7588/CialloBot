;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_1(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P101山门
		SIF GROUPMATCH(ARG:1, P102境内, P140地藏小路)
			RETURNF 3
	CASE P102境内
		SIF GROUPMATCH(ARG:1, P104讲堂, P105经藏, P107土间, P119别馆, P121墓地, P137猯藏的小屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P101山门, P122梵钟楼)
			RETURNF 3
	CASE P103本堂
		SIF GROUPMATCH(ARG:1, P104讲堂)
			RETURNF 3
	CASE P104讲堂
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P103本堂)
			RETURNF 3
	CASE P105经藏
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
	CASE P106后院
		SIF GROUPMATCH(ARG:1, P120库房, P121墓地)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P110走廊)
			RETURNF 3
	CASE P107土间
		SIF GROUPMATCH(ARG:1, P102境内, P108起居室, P112澡堂)
			RETURNF 1
	CASE P108起居室
		SIF GROUPMATCH(ARG:1, P107土间, P109女苑私室, P135走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P111厨房)
			RETURNF 3
	CASE P109女苑私室
		SIF GROUPMATCH(ARG:1, P108起居室, P118白莲私室, P135走廊)
			RETURNF 1
	CASE P110走廊
		SIF GROUPMATCH(ARG:1, P113纳兹琳私室, P115星私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P106后院, P135走廊)
			RETURNF 3
	CASE P111厨房
		SIF GROUPMATCH(ARG:1, P108起居室)
			RETURNF 3
	CASE P112澡堂
		SIF GROUPMATCH(ARG:1, P107土间)
			RETURNF 1
	CASE P113纳兹琳私室
		SIF GROUPMATCH(ARG:1, P110走廊, P116一轮私室, P135走廊)
			RETURNF 1
	CASE P114村纱私室
		SIF GROUPMATCH(ARG:1, P116一轮私室, P117鵺私室, P135走廊)
			RETURNF 1
	CASE P115星私室
		SIF GROUPMATCH(ARG:1, P110走廊, P118白莲私室, P135走廊)
			RETURNF 1
	CASE P116一轮私室
		SIF GROUPMATCH(ARG:1, P113纳兹琳私室, P114村纱私室, P135走廊)
			RETURNF 1
	CASE P117鵺私室
		SIF GROUPMATCH(ARG:1, P114村纱私室, P136响子私室, P135走廊)
			RETURNF 1
	CASE P118白莲私室
		SIF GROUPMATCH(ARG:1, P109女苑私室, P115星私室, P135走廊)
			RETURNF 1
	CASE P119别馆
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P112澡堂)
			RETURNF 2
	CASE P120库房
		SIF GROUPMATCH(ARG:1, P106后院)
			RETURNF 1
	CASE P121墓地
		SIF GROUPMATCH(ARG:1, P102境内, P106后院)
			RETURNF 1
	CASE P122梵钟楼
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 3

	CASE P123灵庙大门
		SIF GROUPMATCH(ARG:1, P124灵庙广场, P140地藏小路)
			RETURNF 3
	CASE P124灵庙广场
		SIF GROUPMATCH(ARG:1, P126道场, P133浴室, P134藏, P138钟楼, P139邪仙楼)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P123灵庙大门)
			RETURNF 3
	CASE P125屠自古私室
		SIF GROUPMATCH(ARG:1, P130走廊)
			RETURNF 1
	CASE P126道场
		SIF GROUPMATCH(ARG:1, P124灵庙广场, P130走廊)
			RETURNF 1
	CASE P127布都私室
		SIF GROUPMATCH(ARG:1, P130走廊)
			RETURNF 1
	CASE P128神子私室
		SIF GROUPMATCH(ARG:1, P130走廊)
			RETURNF 1
	CASE P129客房
		SIF GROUPMATCH(ARG:1, P130走廊)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P131枯山水)
			RETURNF 3
	CASE P130走廊
		SIF GROUPMATCH(ARG:1, P126道场, P125屠自古私室, P127布都私室, P128神子私室, P129客房, P132调药室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P131枯山水)
			RETURNF 3
	CASE P131枯山水
		SIF GROUPMATCH(ARG:1, P129客房, P130走廊)
			RETURNF 3
	CASE P132调药室
		SIF GROUPMATCH(ARG:1, P130走廊)
			RETURNF 1
	CASE P133浴室
		SIF GROUPMATCH(ARG:1, P124灵庙广场)
			RETURNF 1
	CASE P134藏
		SIF GROUPMATCH(ARG:1, P124灵庙广场)
			RETURNF 1

	CASE P135走廊
		SIF GROUPMATCH(ARG:1, P108起居室, P109女苑私室, P113纳兹琳私室, P114村纱私室, P115星私室, P116一轮私室, P117鵺私室, P118白莲私室, P136响子私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P110走廊)
			RETURNF 3
	CASE P136响子私室
		SIF GROUPMATCH(ARG:1, P117鵺私室, P135走廊)
			RETURNF 1
	CASE P137猯藏的小屋
		SIF GROUPMATCH(ARG:1, P102境内)
			RETURNF 1
	CASE P138钟楼
		SIF GROUPMATCH(ARG:1, P124灵庙广场)
			RETURNF 1
	CASE P139邪仙楼
		SIF GROUPMATCH(ARG:1, P124灵庙广场)
			RETURNF 1
	CASE P140地藏小路
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P101山门, P123灵庙大门)
			RETURNF 3

	CASE MAXROOM
		SIF ARG:1 == P140地藏小路
			RETURNF 1
ENDSELECT
RETURNF 0