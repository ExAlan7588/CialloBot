;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_2(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P201人里的门
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P203南大街)
			RETURNF 3
	CASE P202中央广场
		SIF GROUPMATCH(ARG:1, P215花屋, P216食料品店, P220瞭望楼, P227蔬菜店, P280警察署)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P203南大街, P204东大街, P205北大街, P206西大街, P207龙神像)
			RETURNF 3
	CASE P203南大街
		SIF GROUPMATCH(ARG:1, P204东大街, P211酒屋, P212咖啡馆, P214长屋前, P216食料品店, P218小料理屋)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P201人里的门, P202中央广场)
			RETURNF 3
	CASE P204东大街
		SIF GROUPMATCH(ARG:1, P203南大街, P205北大街, P213铃奈庵, P217甜品店, P219集会所, P222寺子屋, P223钱汤, P224慧音宅, P227蔬菜店)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202中央广场)
			RETURNF 3
	CASE P205北大街
		SIF GROUPMATCH(ARG:1, P204东大街, P206西大街, P219集会所, P221稗田邸, P222寺子屋, P224慧音宅, P280警察署, P281小兔姫私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202中央广场, P207龙神像)
			RETURNF 3
	CASE P206西大街
		SIF GROUPMATCH(ARG:1, P205北大街, P214长屋前, P215花屋, P220瞭望楼, P221稗田邸, P225情侣酒店, P238鲵吞亭, P281小兔姫私室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202中央广场)
			RETURNF 3
	CASE P207龙神像
		SIF GROUPMATCH(ARG:1, P202中央广场, P205北大街)
			RETURNF 3
	CASE P211酒屋, P212咖啡馆
		SIF GROUPMATCH(ARG:1, P203南大街)
			RETURNF 1
	CASE P213铃奈庵
		SIF GROUPMATCH(ARG:1, P204东大街, P226小铃私室)
			RETURNF 1
	CASE P214长屋前
		SIF GROUPMATCH(ARG:1, P203南大街, P206西大街)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P230空的房间, P231蛮奇的房间, P232莲子的房间, P233梅莉的房间, P234雪的房间, P235​舞​的房间)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P208雷鼓的房间, P209八桥的房间, P210弁弁的房间, P241空的房间, P242麟的房间, P243空的房间, P236公用厕所, P237公用水井)
			RETURNF 1
	CASE P215花屋
		SIF GROUPMATCH(ARG:1, P202中央广场, P206西大街)
			RETURNF 1
	CASE P216食料品店
		SIF GROUPMATCH(ARG:1, P202中央广场, P203南大街)
			RETURNF 1
	CASE P217甜品店
		SIF GROUPMATCH(ARG:1, P204东大街)
			RETURNF 1
	CASE P218小料理屋
		SIF GROUPMATCH(ARG:1, P203南大街)
			RETURNF 1
	CASE P219集会所
		SIF GROUPMATCH(ARG:1, P204东大街, P205北大街)
			RETURNF 1
	CASE P220瞭望楼
		SIF GROUPMATCH(ARG:1, P202中央广场, P206西大街)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P201人里的门, P203南大街, P204东大街, P205北大街, P207龙神像)
			RETURNF 2
	CASE P221稗田邸
		SIF GROUPMATCH(ARG:1, P205北大街, P206西大街, P229阿求私室)
			RETURNF 1
	CASE P222寺子屋
		SIF GROUPMATCH(ARG:1, P204东大街, P205北大街, P224慧音宅)
			RETURNF 1
	CASE P223钱汤
		SIF GROUPMATCH(ARG:1, P204东大街, P228包场浴场)
			RETURNF 1
	CASE P224慧音宅
		SIF GROUPMATCH(ARG:1, P204东大街, P205北大街, P222寺子屋)
			RETURNF 1
	CASE P225情侣酒店
		SIF GROUPMATCH(ARG:1, P206西大街)
			RETURNF 1
	CASE P226小铃私室
		SIF GROUPMATCH(ARG:1, P213铃奈庵)
			RETURNF 1
	CASE P227蔬菜店
		SIF GROUPMATCH(ARG:1, P202中央广场, P204东大街)
			RETURNF 1
	CASE P228包场浴场
		SIF GROUPMATCH(ARG:1, P223钱汤)
			RETURNF 1
	CASE P229阿求私室
		SIF GROUPMATCH(ARG:1, P221稗田邸)
			RETURNF 1
	CASE P230空的房间, P231蛮奇的房间, P241空的房间, P242麟的房间, P243空的房间, P236公用厕所, P237公用水井
		SIF GROUPMATCH(ARG:1, P214长屋前)
			RETURNF 1
	CASE P232莲子的房间
		SIF GROUPMATCH(ARG:1, P214长屋前, P233梅莉的房间)
			RETURNF 1
	CASE P233梅莉的房间
		SIF GROUPMATCH(ARG:1, P214长屋前, P232莲子的房间)
			RETURNF 1
	CASE P234雪的房间
		SIF GROUPMATCH(ARG:1, P214长屋前, P235​舞​的房间)
			RETURNF 1
	CASE P235​舞​的房间
		SIF GROUPMATCH(ARG:1, P214长屋前, P234雪的房间)
			RETURNF 1
	CASE P208雷鼓的房间
		SIF GROUPMATCH(ARG:1, P214长屋前, P209八桥的房间, P210弁弁的房间)
			RETURNF 1
	CASE P209八桥的房间
		SIF GROUPMATCH(ARG:1, P214长屋前, P208雷鼓的房间, P210弁弁的房间)
			RETURNF 1
	CASE P210弁弁的房间
		SIF GROUPMATCH(ARG:1, P214长屋前, P209八桥的房间, P208雷鼓的房间)
			RETURNF 1
	CASE P238鲵吞亭
		SIF GROUPMATCH(ARG:1, P206西大街, P239美宵的房间)
			RETURNF 1
	CASE P239美宵的房间
		SIF GROUPMATCH(ARG:1, P238鲵吞亭)
			RETURNF 1
	CASE P280警察署
		SIF GROUPMATCH(ARG:1, P202中央广场, P205北大街, P281小兔姫私室)
			RETURNF 1
	CASE P281小兔姫私室
		SIF GROUPMATCH(ARG:1, P205北大街, P206西大街, P280警察署)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P201人里的门
			RETURNF 1
ENDSELECT
RETURNF 0
