;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_10(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P1001槐安通路
	SIF GROUPMATCH(ARG:1, P1002月之都, P1050哆来咪宅・起居室)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
CASE P1002月之都
	SIF GROUPMATCH(ARG:1, P1001槐安通路, P1003玉兔街, P1005探女宅・起居室, P1007闲置的房子)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1020绵月亭・门)
		RETURNF 3
CASE P1003玉兔街
	SIF GROUPMATCH(ARG:1, P1002月之都, P1008铃瑚宅, P1009清兰宅, P1010饭店, P1011甜品店, P1014桃林)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1004广场)
		RETURNF 3
CASE P1004广场
	SIF GROUPMATCH(ARG:1, P1010饭店, P1011甜品店, P1012酒屋, P1013蔬菜店)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1003玉兔街)
		RETURNF 3
CASE P1005探女宅・起居室
	SIF GROUPMATCH(ARG:1, P1002月之都, P1006探女宅・寝室)
		RETURNF 1
CASE P1006探女宅・寝室
	SIF GROUPMATCH(ARG:1, P1005探女宅・起居室)
		RETURNF 1
CASE P1007闲置的房子
	SIF GROUPMATCH(ARG:1, P1002月之都)
		RETURNF 1
CASE P1008铃瑚宅
	SIF GROUPMATCH(ARG:1, P1003玉兔街)
		RETURNF 1
CASE P1009清兰宅
	SIF GROUPMATCH(ARG:1, P1003玉兔街)
		RETURNF 1
CASE P1010饭店
	SIF GROUPMATCH(ARG:1, P1003玉兔街, P1004广场)
		RETURNF 1
CASE P1011甜品店
	SIF GROUPMATCH(ARG:1, P1003玉兔街, P1004广场)
		RETURNF 1
CASE P1012酒屋
	SIF GROUPMATCH(ARG:1, P1004广场)
		RETURNF 1
CASE P1013蔬菜店
	SIF GROUPMATCH(ARG:1, P1004广场)
		RETURNF 1
CASE P1014桃林
	SIF GROUPMATCH(ARG:1, P1003玉兔街)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1015沙滩)
		RETURNF 3
CASE P1015沙滩
	SIF GROUPMATCH(ARG:1, P1014桃林)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P1016入海口)
		RETURNF 1
CASE P1016入海口
	SIF GROUPMATCH(ARG:1, P1015沙滩, P1060纯狐邸・玄关)
		RETURNF 1

CASE P1020绵月亭・门
	SIF GROUPMATCH(ARG:1, P1021绵月亭・玄关, P1023门卫值班室, P1024玉兔食堂)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1002月之都)
		RETURNF 3
CASE P1021绵月亭・玄关
	SIF GROUPMATCH(ARG:1, P1020绵月亭・门, P1024玉兔食堂, P1028食料库)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1022一层回廊)
		RETURNF 3
CASE P1022一层回廊
	SIF GROUPMATCH(ARG:1, P1026食堂, P1029绵月亭・更衣间, P1031宝物库, P1032铃仙二号私室, P1033绵月亭・空的房间, P1040二层回廊)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1021绵月亭・玄关, P1035中庭)
		RETURNF 3
CASE P1023门卫值班室
	SIF GROUPMATCH(ARG:1, P1020绵月亭・门)
		RETURNF 1
CASE P1024玉兔食堂
	SIF GROUPMATCH(ARG:1, P1020绵月亭・门, P1021绵月亭・玄关, P1025玉兔值班室)
		RETURNF 1
CASE P1025玉兔值班室
	SIF GROUPMATCH(ARG:1, P1024玉兔食堂)
		RETURNF 1
CASE P1026食堂
	SIF GROUPMATCH(ARG:1, P1022一层回廊, P1027厨房)
		RETURNF 1
CASE P1027厨房
	SIF GROUPMATCH(ARG:1, P1026食堂, P1028食料库)
		RETURNF 1
CASE P1028食料库
	SIF GROUPMATCH(ARG:1, P1021绵月亭・玄关, P1027厨房)
		RETURNF 1
CASE P1029绵月亭・更衣间
	SIF GROUPMATCH(ARG:1, P1022一层回廊, P1030绵月亭・澡堂)
		RETURNF 1
CASE P1030绵月亭・澡堂
	SIF GROUPMATCH(ARG:1, P1029绵月亭・更衣间)
		RETURNF 1
CASE P1031宝物库
	SIF GROUPMATCH(ARG:1, P1022一层回廊)
		RETURNF 1
CASE P1032铃仙二号私室
	SIF GROUPMATCH(ARG:1, P1022一层回廊)
		RETURNF 1
CASE P1033绵月亭・空的房间
	SIF GROUPMATCH(ARG:1, P1022一层回廊)
		RETURNF 1
CASE P1035中庭
	SIF GROUPMATCH(ARG:1, P1022一层回廊, P1036凉亭)
		RETURNF 3
CASE P1036凉亭
	SIF GROUPMATCH(ARG:1, P1035中庭)
		RETURNF 3

CASE P1040二层回廊
	SIF GROUPMATCH(ARG:1, P1022一层回廊, P1041丰姫私室, P1042依姫私室, P1043书斋, P1044会客厅)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1035中庭)
		RETURNF 2
CASE P1041丰姫私室
	SIF GROUPMATCH(ARG:1, P1040二层回廊)
		RETURNF 1
CASE P1042依姫私室
	SIF GROUPMATCH(ARG:1, P1040二层回廊)
		RETURNF 1
CASE P1043书斋
	SIF GROUPMATCH(ARG:1, P1040二层回廊)
		RETURNF 1
CASE P1044会客厅
	SIF GROUPMATCH(ARG:1, P1040二层回廊)
		RETURNF 1

CASE P1050哆来咪宅・起居室
	SIF GROUPMATCH(ARG:1, P1001槐安通路, P1051哆来咪宅・寝室, P1052哆来咪宅・仓库, P1053哆来咪宅・盥洗间)
		RETURNF 1
CASE P1051哆来咪宅・寝室
	SIF GROUPMATCH(ARG:1, P1050哆来咪宅・起居室)
		RETURNF 1
CASE P1052哆来咪宅・仓库
	SIF GROUPMATCH(ARG:1, P1050哆来咪宅・起居室)
		RETURNF 1
CASE P1053哆来咪宅・盥洗间
	SIF GROUPMATCH(ARG:1, P1050哆来咪宅・起居室, P1054哆来咪宅・澡堂, P1055哆来咪宅・厕所)
		RETURNF 1
CASE P1054哆来咪宅・澡堂
	SIF GROUPMATCH(ARG:1, P1053哆来咪宅・盥洗间)
		RETURNF 1
CASE P1055哆来咪宅・厕所
	SIF GROUPMATCH(ARG:1, P1053哆来咪宅・盥洗间)
		RETURNF 1

CASE P1060纯狐邸・玄关
	SIF GROUPMATCH(ARG:1, P1016入海口)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1061起居室)
		RETURNF 3
CASE P1061起居室
	SIF GROUPMATCH(ARG:1, P1063纯狐邸・空的房间, P1064克劳恩皮丝私室, P1065库房, P1066纯狐邸・更衣间, P1070屋上)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1060纯狐邸・玄关, P1062厨房, P1068纯狐邸・中庭)
		RETURNF 3
CASE P1062厨房
	SIF GROUPMATCH(ARG:1, P1061起居室)
		RETURNF 3
CASE P1063纯狐邸・空的房间
	SIF GROUPMATCH(ARG:1, P1061起居室)
		RETURNF 1
CASE P1064克劳恩皮丝私室
	SIF GROUPMATCH(ARG:1, P1061起居室)
		RETURNF 1
CASE P1065库房
	SIF GROUPMATCH(ARG:1, P1061起居室)
		RETURNF 1
CASE P1066纯狐邸・更衣间
	SIF GROUPMATCH(ARG:1, P1061起居室, P1067纯狐邸・澡堂)
		RETURNF 1
CASE P1067纯狐邸・澡堂
	SIF GROUPMATCH(ARG:1, P1066纯狐邸・更衣间)
		RETURNF 1
CASE P1068纯狐邸・中庭
	SIF GROUPMATCH(ARG:1, P1061起居室)
		RETURNF 3
CASE P1070屋上
	SIF GROUPMATCH(ARG:1, P1061起居室, P1071纯狐私室, P1072赫卡提亚私室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1068纯狐邸・中庭)
		RETURNF 2
CASE P1071纯狐私室
	SIF GROUPMATCH(ARG:1, P1070屋上)
		RETURNF 1
CASE P1072赫卡提亚私室
	SIF GROUPMATCH(ARG:1, P1070屋上)
		RETURNF 1

CASE MAXROOM
	SIF GROUPMATCH(ARG:1, P1001槐安通路)
		RETURNF 1
ENDSELECT

RETURNF 0