;-----------------------------------------------------------
;地点の接续设定
;ARG    基准の地点
;ARG:1  对象の地点
;RETURNF 0=移动できない,见えない
;        1=移动できる,见えない
;        2=移动できない,见える
;        3=移动できる,见える（直通）
;-----------------------------------------------------------
@CAN_MOVE_6(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P601中有之道
	SIF GROUPMATCH(ARG:1, P602赛河原, P630八云邸玄关)
		RETURNF 1
CASE P602赛河原
	SIF GROUPMATCH(ARG:1, P603积石冢)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P601中有之道, P603积石冢, P604彼岸, P608润美的家, P630八云邸玄关)
		RETURNF 1
CASE P603积石冢
	SIF GROUPMATCH(ARG:1, P602赛河原)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P602赛河原, P604彼岸, P609璎花的家)
		RETURNF 1
CASE P604彼岸
	SIF GROUPMATCH(ARG:1, P602赛河原, P603积石冢, P610是非曲直厅大堂, P620冥界之门, P640异界的关卡, P605地狱)
		RETURNF 1
CASE P605地狱
	SIF GROUPMATCH(ARG:1, P604彼岸, P606日狭美的家, P607残无的家)
		RETURNF 1
CASE P606日狭美的家
	SIF GROUPMATCH(ARG:1, P605地狱)
		RETURNF 1
CASE P607残无的家
	SIF GROUPMATCH(ARG:1, P605地狱)
		RETURNF 1
CASE P608润美的家
	SIF GROUPMATCH(ARG:1, P602赛河原)
		RETURNF 1
CASE P609璎花的家
	SIF GROUPMATCH(ARG:1, P603积石冢)
		RETURNF 1
CASE P610是非曲直厅大堂
	SIF GROUPMATCH(ARG:1, P604彼岸, P612法庭, P613是非曲直厅走廊)
		RETURNF 1
CASE P612法庭
	SIF GROUPMATCH(ARG:1, P610是非曲直厅大堂, P613是非曲直厅走廊)
		RETURNF 1
CASE P613是非曲直厅走廊
	SIF GROUPMATCH(ARG:1, P610是非曲直厅大堂, P612法庭, P614是非曲直厅等候室, P616小町的房间, P617澡堂, P618空的房间)
		RETURNF 1
CASE P614是非曲直厅等候室
	SIF GROUPMATCH(ARG:1, P613是非曲直厅走廊, P615映姫的房间)
		RETURNF 1
CASE P615映姫的房间
	SIF GROUPMATCH(ARG:1, P614是非曲直厅等候室)
		RETURNF 1
CASE P616小町的房间
	SIF GROUPMATCH(ARG:1, P613是非曲直厅走廊)
		RETURNF 1
CASE P617澡堂
	SIF GROUPMATCH(ARG:1, P613是非曲直厅走廊)
		RETURNF 1
CASE P618空的房间
	SIF GROUPMATCH(ARG:1, P613是非曲直厅走廊)
		RETURNF 1
CASE P620冥界之门
	SIF GROUPMATCH(ARG:1, P621庭)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P604彼岸)
		RETURNF 1
CASE P621庭
	SIF GROUPMATCH(ARG:1, P620冥界之门, P628起居室)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622走廊, P629西行妖)
		RETURNF 1
CASE P622走廊
	SIF GROUPMATCH(ARG:1, P621庭, P623幽幽子私室, P624妖梦私室, P625空室, P626厨房, P627澡堂, P628起居室)
		RETURNF 1
CASE P623幽幽子私室
	SIF GROUPMATCH(ARG:1, P629西行妖)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622走廊, P628起居室)
		RETURNF 1
CASE P624妖梦私室
	SIF GROUPMATCH(ARG:1, P622走廊)
		RETURNF 1
CASE P625空室
	SIF GROUPMATCH(ARG:1, P622走廊)
		RETURNF 1
CASE P626厨房
	SIF GROUPMATCH(ARG:1, P628起居室)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622走廊)
		RETURNF 1
CASE P627澡堂
	SIF GROUPMATCH(ARG:1, P622走廊)
		RETURNF 1
CASE P628起居室
	SIF GROUPMATCH(ARG:1, P621庭, P626厨房)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P622走廊, P623幽幽子私室)
		RETURNF 1
CASE P629西行妖
	SIF GROUPMATCH(ARG:1, P623幽幽子私室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P621庭)
		RETURNF 1
CASE P630八云邸玄关
	SIF GROUPMATCH(ARG:1, P601中有之道, P602赛河原, P631八云邸走廊, P632八云邸庭, P634空房间, P635橙私室)
		RETURNF 1
CASE P631八云邸走廊
	SIF GROUPMATCH(ARG:1, P630八云邸玄关, P633紫私室, P634空房间, P635橙私室, P636八云邸澡堂, P637八云邸厨房, P638蓝私室, P639八云邸起居室)
		RETURNF 1
CASE P632八云邸庭
	SIF GROUPMATCH(ARG:1, P630八云邸玄关, P633紫私室, P638蓝私室, P639八云邸起居室)
		RETURNF 1
CASE P633紫私室
	SIF GROUPMATCH(ARG:1, P631八云邸走廊, P632八云邸庭, P639八云邸起居室)
		RETURNF 1
CASE P634空房间
	SIF GROUPMATCH(ARG:1, P630八云邸玄关, P631八云邸走廊, P635橙私室)
		RETURNF 1
CASE P635橙私室
	SIF GROUPMATCH(ARG:1, P630八云邸玄关, P631八云邸走廊, P634空房间)
		RETURNF 1
CASE P636八云邸澡堂
	SIF GROUPMATCH(ARG:1, P631八云邸走廊)
		RETURNF 1
CASE P637八云邸厨房
	SIF GROUPMATCH(ARG:1, P631八云邸走廊)
		RETURNF 1
CASE P638蓝私室
	SIF GROUPMATCH(ARG:1, P631八云邸走廊, P632八云邸庭, P639八云邸起居室)
		RETURNF 1
CASE P639八云邸起居室
	SIF GROUPMATCH(ARG:1, P631八云邸走廊, P632八云邸庭, P633紫私室, P638蓝私室)
		RETURNF 1
CASE P640异界的关卡
	SIF GROUPMATCH(ARG:1, P641畜生界大都市)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P604彼岸)
		RETURNF 1
CASE P641畜生界大都市
	SIF GROUPMATCH(ARG:1, P640异界的关卡)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P642灵长园, P647八千慧宅的客厅, P650早鬼宅的客厅, P653尤魔宅的客厅)
		RETURNF 1
CASE P642灵长园
	SIF GROUPMATCH(ARG:1, P641畜生界大都市, P643灵长园的澡堂, P644袿姫的房间, P645磨弓的房间)
		RETURNF 1
CASE P643灵长园的澡堂 TO P645磨弓的房间
	SIF GROUPMATCH(ARG:1, P642灵长园)
		RETURNF 1
CASE P646八千慧宅的澡堂
	SIF GROUPMATCH(ARG:1, P647八千慧宅的客厅)
		RETURNF 1
CASE P647八千慧宅的客厅
	SIF GROUPMATCH(ARG:1, P641畜生界大都市, P646八千慧宅的澡堂, P648八千慧宅的寝室)
		RETURNF 1
CASE P648八千慧宅的寝室
	SIF GROUPMATCH(ARG:1, P647八千慧宅的客厅)
		RETURNF 1
CASE P650早鬼宅的客厅
	SIF GROUPMATCH(ARG:1, P641畜生界大都市, P651早鬼宅的寝室, P652早鬼宅的澡堂)
		RETURNF 1
CASE P651早鬼宅的寝室, P652早鬼宅的澡堂
	SIF GROUPMATCH(ARG:1, P650早鬼宅的客厅)
		RETURNF 1
CASE P653尤魔宅的客厅
	SIF GROUPMATCH(ARG:1, P641畜生界大都市, P654尤魔宅的寝室, P655尤魔宅的澡堂)
		RETURNF 1
CASE P654尤魔宅的寝室, P655尤魔宅的澡堂
	SIF GROUPMATCH(ARG:1, P653尤魔宅的客厅)
		RETURNF 1
CASE MAXROOM
	SIF ARG:1 == P601中有之道
		RETURNF 1
ENDSELECT

RETURNF 0
