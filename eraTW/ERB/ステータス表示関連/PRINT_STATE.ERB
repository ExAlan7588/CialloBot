@PRINT_STATE(选择中角色ID, 选择中页番号, 调教中フラグ)
#DIM 选择中角色ID
#DIM 调教中フラグ
#DIM REF 选择中页番号
#DIM CONST 前一位角色を表示 = 4
#DIM CONST 下一位角色を表示 = 6
#DIM CONST 返回よ = 5

$PRINT_LOOP
	DRAWLINEFORM ━
	PRINT ■
	PRINTS NAME:选择中角色ID
	PRINT (好感度:
	CALL PRINT_RANK(CFLAG:选择中角色ID:2, "MAX_50000")
	PRINTFORM  {CFLAG:选择中角色ID:2} 信赖度:
	CALL PRINT_RANK(CFLAG:选择中角色ID:信赖度, "MAX_20000")
	PRINTFORM  {CFLAG:选择中角色ID:信赖度}) 
	;MASTERならば所持金を记载
	SIF 选择中角色ID == MASTER
		PRINTFORM (所持金: %金额表示(MONEY)%)
	IF NOTICE_CYCLE(选择中角色ID)
		IF 调教中フラグ
			CALL CYCLE_PRINT(选择中角色ID)
		ELSE
			CALL CYCLE_PRINT翌日(选择中角色ID)
		ENDIF
		SETCOLOR C_AQUA
	ENDIF
	IF CFLAG:选择中角色ID:允许无套 == 2
		SETCOLOR C_PINK
		PRINT 危险日无套Ｈ❤也OK哦
		RESETCOLOR
	ELSEIF CFLAG:选择中角色ID:允许无套 == 1
		SETCOLOR C_AQUA
		PRINT 无套Ｈ❤也不是不行呢
		RESETCOLOR
	ELSEIF GETBIT(CFLAG:选择中角色ID:既成事实,1)
		SETCOLOR C_YELLOW
		PRINT Ｈ也是可以的❤
		RESETCOLOR
	ENDIF
	RESETCOLOR
	PRINTL 　
;*******************************************
;1页目
;*******************************************
IF 选择中页番号 == 0
	PRINT 体力 
				CALL PRINT_COLORBAR, BASE:选择中角色ID:0, MAXBASE:选择中角色ID:0, 28, UNICODE(0x2585), UNICODE(0x2585), BARCOLORSET("赤"), RESULT:1
				PRINTFORML  ({BASE:选择中角色ID:体力,5}/{MAXBASE:选择中角色ID:体力,5}) 
	PRINT 气力 
				CALL PRINT_COLORBAR, BASE:选择中角色ID:1, MAXBASE:选择中角色ID:1, 28, UNICODE(0x2585), UNICODE(0x2585), BARCOLORSET("绿"), RESULT:1
				PRINTFORML  ({BASE:选择中角色ID:气力,5}/{MAXBASE:选择中角色ID:气力,5}) 
	IF BASE:选择中角色ID:15
	PRINT 酒气 
				CALL PRINT_COLORBAR, BASE:选择中角色ID:15, MAXBASE:选择中角色ID:15, 28, UNICODE(0x2585), UNICODE(0x2585), BARCOLORSET("青"), RESULT:1
				PRINTFORML  ({BASE:选择中角色ID:酒气,5}/{MAXBASE:选择中角色ID:酒气,5}) 
	ENDIF
	SELECTCASE CFLAG:选择中角色ID:积攒度
		CASE 201 TO 499
			SETCOLOR C_AQUA
		CASE 500 TO 799
			SETCOLOR C_YELLOW
		CASE 800 TO 999
			SETCOLOR C_PINK
		CASE IS >= 1000
			SETCOLOR C_RED
	ENDSELECT
	SIF NOTICE_LUST(选择中角色ID)
		IF CFLAG:选择中角色ID:积攒度 >=1000
		PRINTFORML 欲求：{CFLAG:选择中角色ID:积攒度 / 10}％ ❤饥渴❤难耐❤
		ELSEIF CFLAG:选择中角色ID:积攒度 >=800
		PRINTFORML 欲求：{CFLAG:选择中角色ID:积攒度 / 10}％ ❤焦躁难忍❤
		ELSEIF CFLAG:选择中角色ID:积攒度 >=500
		PRINTFORML 欲求：{CFLAG:选择中角色ID:积攒度 / 10}％ 欲求不满❤
		ELSEIF CFLAG:选择中角色ID:积攒度 >=200
		PRINTFORML 欲求：{CFLAG:选择中角色ID:积攒度 / 10}％ 想发泄
		ELSE
		PRINTFORML 欲求：{CFLAG:选择中角色ID:积攒度 / 10}％
		ENDIF
	RESETCOLOR

	CALL SHOW_SIZE(选择中角色ID)
	PRINTL [99] 画像切换
	IF FLAG:画像表示
		CALL PRINT_STATE_IMAGE(选择中角色ID)
	ENDIF

	CALL PRINT_STATE_CLOTH(选择中角色ID)
	;道具というか道具だけど他が2文字だしねぇ
	IF 选择中角色ID == MASTER
		PRINT □ 
		CALL PRINTBUTTON_EX("道具",167,TFLAG:167)
		PRINTL  □---------------------------------------------------------------------------
		SIF !TFLAG:道具表示
			CALL LIST_ITEM("全て")
	ENDIF
	CALL PRINT_STATE_ABL(选择中角色ID)
	PRINTL
	CALL PRINT_STATE_TALENT(选择中角色ID)
	PRINTL
	IF 选择中角色ID != MASTER
		PRINTL
		PRINT □ 
		CALL PRINTBUTTON_EX("刻印",166,TFLAG:166)
		PRINTL  □---------------------------------------------------------------------------
		;PRINT_MARK 选择中角色ID
		CALL PRINT_刻印(选择中角色ID)
	ENDIF
;*******************************************
;2页目
;*******************************************
ELSEIF 选择中页番号 == 1
	PRINTL
	CALL PRINT_STATE_EXP(选择中角色ID)
	PRINTL
	SIF TALENT:选择中角色ID:妊娠 || CFLAG:选择中角色ID:儿童人数 > 0
		CALL 妊娠关联(选择中角色ID)
	SIF 选择中角色ID != MASTER && !调教中フラグ
		CALL PRINT_STATE_JUEL(选择中角色ID)
	SIF 调教中フラグ
		CALL PRINT_STATE_EX(选择中角色ID)
	PRINTL
	SIF 选择中角色ID
		CALL 胖次收集状况(选择中角色ID)
;*******************************************
;3页目
;*******************************************
ELSEIF 选择中页番号 == 2
	CALL PRINT_STATE_PERSONAL(选择中角色ID)
	CALL 肉体情报(选择中角色ID)
	CALL SHOW_RELATION(选择中角色ID)
;*******************************************
;4页目
;*******************************************
ELSEIF 选择中页番号 == 3
	CALL PRINT_STATE_PREFERENCE(选择中角色ID)
;*******************************************
;5页目
;*******************************************
ELSEIF 选择中页番号 == 4
	IF !选择中角色ID
		CALL 予定
	ELSE
		CALL 陷落条件表示(选择中角色ID)
		CALL 浮气把握状况(选择中角色ID)
	ENDIF
ENDIF
IF 调教中フラグ
	DRAWLINE
	IF 选择中页番号 > 0
		PRINTC [0] 察看前一页
	ELSE
		PRINTC 　
	ENDIF
	PRINTC 　
	IF 选择中页番号 < 详细能力表示最大页数的番号
		PRINTC [1] 察看下一页
	ELSE
		PRINTC 　
	ENDIF
	PRINTL
		PRINTC  
		PRINTFORMC [{返回よ}] 返回
	INPUT
	SELECTCASE RESULT
		CASE 0
		SIF 选择中页番号 > 0
			选择中页番号 --
			GOTO PRINT_LOOP
		CASE 1
			SIF 选择中页番号 < 详细能力表示最大页数的番号
				选择中页番号 ++
				GOTO PRINT_LOOP
		CASE 160 TO 167
			TFLAG:RESULT = !TFLAG:RESULT
			GOTO PRINT_LOOP
		;この边消しちゃってもいいか
		CASE 前一位角色を表示
			IF !调教中フラグ
				选择中角色ID--
			ENDIF
			IF 选择中角色ID < 0
				选择中角色ID = CHARANUM - 1
			ENDIF
			GOTO PRINT_LOOP
		CASE 下一位角色を表示
			IF !调教中フラグ
				选择中角色ID++
			ENDIF
			IF 选择中角色ID >= CHARANUM
				选择中角色ID = 0
			ENDIF
			GOTO PRINT_LOOP
		CASE 返回よ
			RETURN 0
		CASE 画像切换
			FLAG:画像表示 = !FLAG:画像表示
			GOTO PRINT_LOOP
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF
