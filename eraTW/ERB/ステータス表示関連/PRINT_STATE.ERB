@PRINT_STATE_TALENT(选择中角色ID)
#DIM 选择中角色ID
#DIM TLNT_CNT, 6
#DIM TLNT_NO, 6, 100
#DIM CST_CNT, 6
#DIM CST_NO, 6, 100
#DIM CAT
#DIM ENTRY
#DIMS 种族文本

VARSET TLNT_CNT
VARSET TLNT_NO
VARSET CST_CNT
VARSET CST_NO

PRINT □ 
CALL PRINTBUTTON_EX("素質",165,TFLAG:165)
PRINTL  □---------------------------------------------------------------------------
IF !TFLAG:165
	;TALENT:200(泛用角色)は表示させない
	FOR LOCAL, 0, 300
		SIF !STRLENS(GET_TALENTNAME(LOCAL, TALENT:选择中角色ID:LOCAL))
			CONTINUE
		LOCALS = {LOCAL}/
		IF GROUPMATCH(LOCAL,90,91,164,165,166,189,190,191,192,193,194,195,196,197,198)
			;种族,132=吸血鬼は被るので身体特征に移动
			TLNT_NO:0:(TLNT_CNT:0) = LOCAL
			TLNT_CNT:0 ++
		ELSEIF GROUPMATCH(LOCAL,0,1,3,4,5,6,7,8,9,60,70,72,73,74,75,76,77,78,82,83,122,142,152,183,184,185,250,254,255,256,257,258,259,260,261,262,263,264)
			;性的特征
			TLNT_NO:1:(TLNT_CNT:1) = LOCAL
			TLNT_CNT:1 ++
		ELSEIF GROUPMATCH(LOCAL,2,16,17,40,41,54,56,57,92,100,101,102,103,104,105,106,121,130,132,133,134,137,138,143,146,149)
			;身体的特征
			TLNT_NO:2:(TLNT_CNT:2) = LOCAL
			TLNT_CNT:2 ++
		ELSEIF GROUPMATCH(LOCAL,9,10,11,12,13,14,18,19,20,21,22,23,24,25,26,30,31,32,33,61,62,71,80,81,84,85,86,94,151,155)
			;精神的特征
			TLNT_NO:3:(TLNT_CNT:3) = LOCAL
			TLNT_CNT:3 ++
		ELSEIF GROUPMATCH(LOCAL,46, 47, 48, 49, 50,51,52,53,55,93,135,136,144,161,162,199)
			;技术的特征
			TLNT_NO:4:(TLNT_CNT:4) = LOCAL
			TLNT_CNT:4 ++
		ELSE
			;其他
			TLNT_NO:5:(TLNT_CNT:5) = LOCAL
			TLNT_CNT:5 ++
		ENDIF
	NEXT
	;カスタム素質读み込み
	CALL KOJO_MESSAGE_SEND("CUSTOM_TALENT", 0, 选择中角色ID)
	;カスタム素質特征振り分け
	FOR CAT, 0, MAX_CUSTOM_TALENT
		SIF !CUSTOM_TALENT:选择中角色ID:CAT
			CONTINUE
		LOCAL = CUSTOM_TALENT_TYPE:选择中角色ID:CAT
		IF INRANGE(LOCAL, 1, 5)
			LOCAL --
			CST_NO:LOCAL:(CST_CNT:LOCAL) = CAT
			CST_CNT:LOCAL ++
		ELSE
			CST_NO:5:(CST_CNT:5) = CAT
			CST_CNT:5 ++
		ENDIF
	NEXT
	
	FOR CAT, 0, 6
		SELECTCASE CAT
			CASE 0
				PRINT 　 种族 　：
			CASE 1
				PRINT 　性的特征：
			CASE 2
				PRINT 身体的特征：
			CASE 3
				PRINT 精神的特征：
			CASE 4
				PRINT 技术的特征：
			CASE 5
				PRINT 　 其他 　：
		ENDSELECT
		LOCALS = ""
		FOR ENTRY, 0, TLNT_CNT:CAT
			LOCAL = TLNT_NO:CAT:ENTRY
			;童贞
			SIF LOCAL == 1 && !(TALENT:选择中角色ID:性别 & 2)
				CONTINUE
			;男绝壁
			IF LOCAL == 105 && (TALENT:选择中角色ID:性别 == 2)
				SIF (TALENT:选择中角色ID:105 == -2) && (TALENT:选择中角色ID:性别 == 2)
					CONTINUE
				SIF (TALENT:选择中角色ID:105 == 0) && (TALENT:选择中角色ID:性别 == 2)
					PRINTFORM [普乳]
			ENDIF
			;扫除系は住んでる时だけ表示
			SIF LOCAL == GETNUM(TALENT, "扫除系") && !CFLAG:选择中角色ID:神社在住
				CONTINUE
			LOCALS += GET_TALENTNAME(LOCAL, TALENT:选择中角色ID:LOCAL)
			IF STRLENS(LOCALS) > 60
				LOCALS = " "
				PRINTL
				PRINT 　　　　　　
			ENDIF
			;素質によっては色变え
			IF GROUPMATCH(LOCAL,0,3,4,6,7,73,74,75,76,77,78,122,142) || (LOCAL == 199 && TALENT:选择中角色ID:LOCAL == 2)
				SETCOLOR 255, 120, 200
				FONTBOLD
			ELSEIF GROUPMATCH(LOCAL, 8, 199)
				SETCOLOR C_PINK
			ENDIF
			PRINTFORM [%GET_TALENTNAME(LOCAL,TALENT:选择中角色ID:LOCAL)%]
			RESETCOLOR
			FONTREGULAR
			IF CAT == 0
				种族文本 += @"%GET_TALENTNAME(LOCAL,TALENT:选择中角色ID:LOCAL)%"
			ELSE
				种族文本 = 
			ENDIF
		NEXT
		;カスタム素質を表示
		IF CST_CNT:CAT
			FOR ENTRY, 0, CST_CNT:CAT
				LOCAL = CST_NO:CAT:ENTRY
				LOCALS = %LOCALS%%CUSTOM_TALENT_NAME:选择中角色ID:LOCAL%
				IF STRLENS(LOCALS) > 60
					LOCALS = " "
					PRINTL
					PRINT 　　　　　　
				ENDIF
				SIF CUSTOM_TALENT_COLOR:选择中角色ID:LOCAL
					SETCOLOR CUSTOM_TALENT_COLOR:选择中角色ID:LOCAL
				PRINTFORM [%CUSTOM_TALENT_NAME:选择中角色ID:LOCAL%]
				RESETCOLOR
				FONTREGULAR
			NEXT
		ENDIF
		SIF CAT == 0
			CALL PRINT_种族抽出(选择中角色ID, 种族文本)
		SIF LOCALS != " "
			PRINTL 
	NEXT
	;口上自定义
	CALL showKojoCustomTalent(选择中角色ID)
ENDIF

@PRINT_种族抽出(选择中角色ID, 种族文本)
#DIM 选择中角色ID
#DIM TMP_RESULT
#DIMS STR_LIST, 10
#DIMS 种族文本

VARSET STR_LIST
TMP_RESULT:0 = RESULT
LOCALS =  %CSVCSTR(选择中角色ID,10)%
IF STRFIND(LOCALS, "●") > -1
	SPLIT LOCALS, "●", STR_LIST
	FOR LOCAL, 0, RESULT
		IF STRFIND(STR_LIST:LOCAL, "种族：") > -1
			LOCALS:1 = %REPLACE(STR_LIST:LOCAL, "种族：", "")%
			LOCALS:1 = %REPLACE(LOCALS:1, "　", "")%
			SIF STRFIND(种族文本, LOCALS:1) < 0
				PRINTFORM 〈%LOCALS:1%〉
			BREAK
		ENDIF
	NEXT
;ELSEIF TALENT:选择中角色ID:路人 とどっちがいいんでしょうね？
ELSEIF 选择中角色ID == RANDOM_CHARANUM
	PRINTFORM [%CSTR:选择中角色ID:路人子种族%]
ENDIF
RESULT = TMP_RESULT:0


