		ELSEIF ARG:1 == 1
			RETURNF "学习快"
		ENDIF
	;采伐钓鱼采集调合Lv
	CASE 46, 47, 48, 49
		IF ARG:1 > 0
			LOCALS = %TALENTNAME:ARG%
			LOCALS += TOSTR(ARG:1)
			RETURNF LOCALS
		ENDIF
	;污臭耐性
	CASE 61
		IF ARG:1 == -2
