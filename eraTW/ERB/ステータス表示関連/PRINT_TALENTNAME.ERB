		IF ARG:1 == -1
			RETURNF "恢复慢"
		ELSEIF ARG:1 == 1
			RETURNF "恢复快"
		ENDIF
	CASE 143
		SELECTCASE ARG:1
			CASE 1
				RETURNF "动物耳"
			CASE 2
				RETURNF "兔耳"
			CASE 3
				RETURNF "犬耳"
			CASE 4
				RETURNF "猫耳"
		ENDSELECT
	CASE 130
		IF ARG:1 == 1
			RETURNF "授乳可能"
		ELSEIF ARG:1 == 2
			RETURNF "母乳体质"
		ENDIF
	;音乐知识
	CASE 161
		SELECTCASE ARG:1
			CASE 1
				RETURNF "乐器知识"
			CASE 2
				RETURNF "歌唱知识"
			CASE 3
				RETURNF "舞蹈知识"
		ENDSELECT
	;音感
	CASE 162
		IF ARG:1 == -1
			RETURNF "音痴"
		ELSEIF ARG:1 == 1
			RETURNF "音感"
		ELSEIF ARG:1 == 2
			RETURNF "绝对音感"
		ENDIF
	;冰精
	CASE 164
		IF ARG:1 == 1
			RETURNF "冰精"
		ELSEIF ARG:1 == 2
			RETURNF "雪女"
		ENDIF
	;炮友
	CASE 185
		IF ARG:1 == 1
			RETURNF "炮友"
		ELSEIF ARG:1 == 2
			RETURNF "爱人"
		ENDIF
	;人类
	CASE 190
		IF ARG:1 == 1
