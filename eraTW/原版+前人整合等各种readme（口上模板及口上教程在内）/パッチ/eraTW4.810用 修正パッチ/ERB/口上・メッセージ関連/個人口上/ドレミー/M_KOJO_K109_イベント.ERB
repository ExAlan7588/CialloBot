;==================================================
@M_KOJO_EVENT_K109_22(ARG,ARG:1)
;口上主はARG
;ARG:0に自慰状况
;ARG:1に自慰种类
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;通常自慰
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋慕
			;十分の一の確率で夜袭
			SELECTCASE RAND:10
				CASE 9
					PRINTFORMW 「さすがに職权濫用かしら…まあ構いませんよね」
					PRINTFORML ………
					PRINTFORML ……
					PRINTFORMW …
					CALL DOREMY_YOBAI
				CASEELSE
					SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「ふっ…HPH…くっ…HPH」","L"
						PRINTFORMDL ドレミーは%CALLNAME:MASTER%の匂いの付いた衣服を自らの颜に押し付けながら自慰に耽っている
						PRINTFORMDW 次はいつ会えるだろうか。ドレミーの头にはそのことしかなかった
					CASE 1
						CALL HPH_PRINT,@"「…ぁっHPH…はぁ…HPH」","L"
						PRINTFORMDW ドレミーは%CALLNAME:MASTER%をおかずに自慰に耽った
					ENDSELECT
			ENDSELECT
		ELSE
			CALL HPH_PRINT,@"「…んっ…HPH」","L"
			PRINTFORMDW 昂ぶりを抑えられなくなったドレミーは淫らな梦を見ながら自慰に耽っている…
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同室时自慰
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕
			;十分の一の確率で夜袭
			SELECTCASE RAND:10
				CASE 9
					CALL HPH_PRINT,@"「んっHPH…こんなになるまで放っておいて…お仕置きが必要ですね…HPH」","L"
					PRINTFORML ………
					PRINTFORML ……
					PRINTFORMW …
					CALL DOREMY_YOBAI
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「ふっ…HPH…くっ…HPH」","L"
							PRINTFORMDL ドレミーは%CALLNAME:MASTER%の体に鼻を押し付けながら自慰に耽っている
							PRINTFORMDW 今起きたらどういった反応をするのだろうか。興味はありつつも眠气には勝てなかった
						CASE 1
							CALL HPH_PRINT,@"「…っ…っHPH」","L"
							PRINTFORMDW %CALLNAME:MASTER%が深い眠りに落ちていることをしつこく确认した后、ドレミーは自慰に耽った
					ENDSELECT
			ENDSELECT
		ELSE
			CALL HPH_PRINT,@"「………んっHPH」","L"
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;中だし精液自慰
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;无知持ち（自慰经验(调教自慰经验は差し引き)による分岐あり
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		LOCAL:2 = EXP:自慰经验 - EXP:调教自慰经验
		;无知消去イベントあり（表示は一度だけ
		IF LOCAL:1 >= 25
		;地の文分岐。书きたい场合は[SKIPSTART][SKIPEND]を削除してください
		[SKIPSTART]
		ELSEIF LOCAL:1 >= 20
		ELSEIF LOCAL:1 >= 15
		ELSEIF LOCAL:1 >= 10
		ELSEIF LOCAL:1 >= 5
		[SKIPEND]
		ELSE
		ENDIF
		PRINTFORML  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;陷落素質あり自慰中毒高め
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		;-------------------------------------------------
		;Ｃ敏感orＣ感觉10以上
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML  
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;Ｖ敏感orＶ感觉10以上
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML  
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;Ａ敏感orＡ感觉10以上
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 3
			PRINTFORML  
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;Ｂ敏感orＢ感觉10以上
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 4
			PRINTFORML  
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部位指定无し
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 5
			PRINTFORML  
			RETURN 1
		ENDIF
		;その他
		;上の分岐を一切书かずに、ここに书くことで陷落素質あり自慰中毒高め自慰の共通口上を表示することが出来ます
		PRINTFORML  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;自慰中毒高め
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;-------------------------------------------------
		;尻穴膣腔同时
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML  
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;膣腔のみ
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML  
			RETURN 1
		ENDIF
		;その他
		;上の分岐を一切书かずに、ここに书くことで自慰中毒高め自慰の共通口上を表示することが出来ます
		PRINTFORML  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;我慢
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW 「我慢…我慢よ…耐えるのよドレミースイート」
		RETURN 1
	ENDIF
	;その他
	;上の分岐を一切书かずに、ここに书くことで自慰の共通口上を表示することが出来ます
	PRINTFORML 
	RETURN 1
ENDIF
RETURN 1
;==================================================
;浴场连れだし
;==================================================
@M_KOJO_EVENT_K109_23(ARG,ARG:1)
;口上主はLOCAL
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;断られた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORML 「嫌ではないですが…今日はダメです」
			PRINTFORMW 「察してくださいね」
		ELSEIF TALENT:恋慕
			PRINTFORMW 「それはちょっと…恥ずかしいというか」
		ELSEIF TALENT:思慕
			PRINTFORMW 「さすがにそこまで气安い仲ではないと思いますが」
		ELSE
			PRINTFORMW 「それはちょっと生理的にきついですねぇ」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;一绪に入ってくれた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			PRINTFORMW 「いいですよ。洗いっこでもしますか？」
		ELSEIF TALENT:恋慕
			PRINTFORML 「混浴ですか？」
			PRINTFORMW 「少し恥ずかしいですが…良いですよ」
		ELSEIF TALENT:思慕
			PRINTFORMW 「本当に混浴だけが目的なんですかねぇ」
		ELSE
			PRINTFORMW 「气の迷いというか…今回だけですよ」
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1
