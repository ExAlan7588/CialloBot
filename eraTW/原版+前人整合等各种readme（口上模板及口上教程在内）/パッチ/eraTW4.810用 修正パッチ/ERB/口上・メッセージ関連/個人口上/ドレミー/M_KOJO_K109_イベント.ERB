		CASE 5
		;事后
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML
			PRINTFORML 「まったく…酷い目にあいましたよ…」
			PRINTFORMDW そう言うドレミーは満更でもなさそうな颜をしている
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 6
		;事后お泊り
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORML
			PRINTFORMW 「ふふっ、さすが話が分かりますね」
			PRINTFORMW 「ここまでしたのですから、今日は一日期き合ってもらいますよ」
			PRINTFORMW 「まずは梦の管理のお手伝いですね。最近は梦に干渉してくる者が多くて…」
			PRINTFORML 「それが終わったら……終わったら…」
			CALL HPH_PRINT,@"「………HPH」","W"
			PRINTFORMDW ……………
			PRINTFORMDW …………
			PRINTFORMDW ………
		RETURN 1
		ENDIF
	ENDSELECT
ENDIF

