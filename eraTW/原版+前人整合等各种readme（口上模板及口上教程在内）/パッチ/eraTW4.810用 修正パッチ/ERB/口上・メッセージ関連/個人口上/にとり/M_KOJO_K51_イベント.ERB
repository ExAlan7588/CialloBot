;デート帰り
;==================================================
@M_KOJO_SPEVENT_K51_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTW
			PRINTFORML 「%CALLNAME:MASTER%、今日も乐しかったよ」
			PRINTFORML 「また、行こうね？」
			PRINTFORML もちろん、と%CALLNAME:MASTER%は返す
			PRINTFORML 「ふふ……ありがと。大好きだよ」
			PRINTFORML そう言うとにとりはそっと%CALLNAME:MASTER%の頬に口づけた
			PRINTFORML 「%CALLNAME:MASTER%は、いつも私を幸せにしてくれるね」
			PRINTFORMW と、にとりが幸せそうな颜で言った
			RETURN 1
		ELSEIF TALENT:恋慕
			PRINTW
			PRINTFORML 「今日は乐しかったよ」
			PRINTFORML 「また诱ってくれると嬉しいな」
			PRINTFORML %CALLNAME:MASTER%がそうだなと返すと
			PRINTFORML 「本当？じゃあ、乐しみにしておくね！」
			PRINTFORMW と、にとりは嬉しそうな笑颜で言った
		ELSEIF TALENT:思慕
			PRINTW
			PRINTFORML 「盟友。今日は、パーツ购入に手伝ってくれてありがと」
			PRINTFORML 「良いモノができたらすぐに持ってきてあげるよ」
			PRINTFORML にとりの良いモノは当てにならないな、と%CALLNAME:MASTER%が言うと、
			PRINTFORML 「そんなことない、なんか良いモノができる气がするんだ」
			PRINTFORMW と、悪戯っぽく笑った。
			PRINTW
		ELSE
			PRINTW
			PRINTFORML 「いやーありがとう助かったよ。流石に一人でこの量は大変だからね」
			PRINTFORML にとりと出かけたもののほとんど荷物持ちだった。
			PRINTFORML 「盟友は赖りになるなぁ」
			PRINTFORML 河童の言うところの盟友とは使いっぱしりなのか？と%CALLNAME:MASTER%が聞くと
			PRINTFORML 「そんなことないよ？これでも感謝してるんだよ？」
			PRINTFORMW と、にとりは言った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反抗刻印消去イベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORML 「……うん、ごめん。私も愤怒すぎた」
			PRINTFORML 「でも本当に嫌だったんだからね？」
			PRINTFORML 「まあ……%CALLNAME:MASTER%と離れるのはもっと嫌だからね」
			PRINTFORMW にとりは困ったような笑颜で許してくれた。
			RETURN 1
		ELSE
			PRINTFORML 「……まあいつまでも怒ってても仕方ないしね」
			PRINTFORML 「河童と人间の友情に免じて今回は許してあげる」
			PRINTFORML 「でも次はないと思ってよ」
			PRINTFORMW にとりはようやく許してくれるようだ。
			PRINTW
		RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;反抗刻印2>1レベルダウンイベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%。もうしないって約束してくれるんなら」
			PRINTFORML 「この间のこと、少しだけ許したげる」
			PRINTFORML 「%CALLNAME:MASTER%は好きだけど、なんでもしていいって事じゃないんだからね」
			PRINTFORMW にとりの表情は少し柔らかくなった
			RETURN 1
		ELSE
			PRINTFORML 「あんなふざけたことする人间もいるんだって思ったけど」
			PRINTFORML 「少しだけ許してあげる」
			PRINTFORML 「河童をあんまり舐めると痛い目を見るよ」
			PRINTFORMW にとりの愤怒は大分収まってきたようだ。
			PRINTW
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反抗刻印3>2レベルダウンイベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORML 「%CALLNAME:MASTER%は私のこと好きって言ってくれたよね……？」
			PRINTFORML 「じゃあ、もう二度とあんなことはしないで」
			PRINTFORML 「私、%CALLNAME:MASTER%の气持ちが分かんなくなっちゃうよ……」
			PRINTFORMW にとりは沈痛な表情でそう呟いた
			RETURN 1
		ELSE
			PRINTFORML 「その昔、河童と人间の友情を引き裂いたのは愚かな一人の人间だったんだって」
			PRINTFORML 「二人目になりたくないのなら、もっと誠意を見せるべきだよ」
			PRINTFORML 「じゃないと河童全员が%CALLNAME:MASTER%を許さないよ」
			PRINTFORMW にとりは%CALLNAME:MASTER%を鋭い眼光で睨んでいる
			PRINTW
		ENDIF
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

