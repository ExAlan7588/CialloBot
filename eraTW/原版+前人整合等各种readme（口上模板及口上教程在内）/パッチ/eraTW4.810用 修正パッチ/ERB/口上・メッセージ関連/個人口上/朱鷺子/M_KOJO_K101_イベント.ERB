;帰宅时
;==================================================
@M_KOJO_EVENT_K101_20(ARG,ARG:1)
;口上主はLOCALS or RESULT
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			SELECTCASE RAND:4
				CASE 3
					PRINTFORMW 「そろそろ帰るわ！」
					PRINTFORMW 「じゃあまた明日！」
				CASE 2
					PRINTFORMW 「もうこんな时间ね……もっと居たいんだけど……」
					PRINTFORMW 「……また、明日ね！」
				CASE 1
					PRINTFORML 「そろそろ时间だし帰るわね………んっ」
					PRINTFORMDW 朱鷺子は%CALLNAME:MASTER%の頬に軽くキスをした
					PRINTFORMW 「じゃあまた明日！」
				CASE 0
					PRINTFORMW 「そろそろ帰るわね～」
					PRINTFORMW 「本当はずっと居たいんだけどねぇ」
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「もうこんな时间ね……また、今度ね！」
					PRINTFORMW そう言うと朱鷺子は飛び去っていった。
				CASE 1
					PRINTFORML 「一日ってなんでこんなに短いのかしら………」
					PRINTFORMDW %CALLNAME:MASTER%は少し落ち込んでいる朱鷺子の头を抚でた
					PRINTFORMW 「あっ……ありがとう………じゃあ帰るわね」
				CASE 0
					PRINTFORMW 「もう帰らないと……」
					PRINTFORMW 「また今度ね！」
			ENDSELECT	
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「夜は目が效かないから帰るわね」
					PRINTFORMW 朱鷺子はそう言って飛び去っていった。
				CASE 1
					PRINTFORML 「これ以上いると危ないし帰るわね」
					PRINTFORMW 「また来るわね～」
				CASE 0
					PRINTFORMW 「あっ、もうこんな时间！」
					PRINTFORMW 「それじゃあ帰るわね」
			ENDSELECT			
		ELSE
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「もうこんな时间ね」
					PRINTFORMW 朱鷺子はいつの间にか飛び去っていってしまっていた。
				CASE 1
					PRINTFORML 「じゃあまたね」
				CASE 0
					PRINTFORMW 「あっ、もうこんな时间！」
					PRINTFORMW 「それじゃあ帰るわね」
			ENDSELECT			
			
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;约会中帰宅
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋人
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「乐しい时间は早く過ぎてしまうものね…」
					PRINTFORMW 「また、デートしましょう」
					PRINTFORMW 朱鷺子はそう言うと飛び去っていった。
				CASE 1
					PRINTFORML 「今日のデートももうすぐ終わりねぇ……」
					PRINTFORMDW %CALLNAME:MASTER%はしんみりとしている朱鷺子の头を抚でた
					PRINTFORMW 「ふふ、ありがとう。こういう所好きよ♪」
				CASE 0
					PRINTFORMW 「もうこんな时间かぁ……そろそろ終わりねぇ……」
					PRINTFORMW 「またデート行きましょ♪」
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「また……また、デート行きましょ！」
					PRINTFORMW %CALLNAME:MASTER%は返事の代わりに头を抚でた
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%、今日はありがとう。乐しかったわ」
					PRINTFORMW 「その……また时间があればデートして欲しいなって……」
					PRINTFORMDW 朱鷺子は少し恥ずかしそうに尋ねてきた
				CASE 0
					PRINTFORMW 「今日は乐しかったわ、またデートしましょう」
					PRINTFORMW そう言うと朱鷺子は飛び去っていった。
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「もうこんな时间ね」
					PRINTFORMW 「今日はありがとう」
				CASE 1
					PRINTFORML 「今日は乐しかったわ」
					PRINTFORMW 「また机会があったら诱ってね」
				CASE 0
					PRINTFORMW 「それじゃ、そろそろ帰るわね」
					PRINTFORMW そう言うと朱鷺子は飛び去っていった。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「乐しかったわ♪」
				CASE 1
					PRINTFORMW 「もうこんな时间ね。じゃあね」
					PRINTFORMW 朱鷺子はいつの间にか飛び去っていってしまっていた。
				CASE 0
					PRINTFORMW 「それじゃ、そろそろ帰るわね」
					PRINTFORMW そう言うと朱鷺子は飛び去っていった。
			ENDSELECT		
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;玄関先でお見送り
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「ここでいいわ。じゃあまたね」
					PRINTFORMW 朱鷺子はそう言うと飛び去っていった。
				CASE 1
					PRINTFORML 「ここまで来たんだし别に泊まって行っても良いんだけどね」
					PRINTFORMW 「あはは、まぁ半分冗談よ」
				CASE 0
					PRINTFORMW 「送ってくれてありがとう」
					PRINTFORMW 「またデート行きましょ♪」
			ENDSELECT	
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「ここでいいわ。ありがとう」
					PRINTFORMW そう言うと朱鷺子は飛び去っていった。
				CASE 1
					PRINTFORML 「今日は乐しかったわ♪」
					PRINTFORMW 「その……またデートしましょ？」
				CASE 0
					PRINTFORMW 「ここまで送ってくれてありがとう」
					PRINTFORMW 「气を付けて帰るのよ」
			ENDSELECT	
		ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「今日はありがとう！」
				CASE 1
					PRINTFORML 「今日は乐しかったわ♪」
				CASE 0
					PRINTFORMW 「ここでいいわ」
					PRINTFORMW 朱鷺子はそう言って飛び去っていった。
			ENDSELECT	
		ELSE
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「じゃあね」
					PRINTFORMW 「また今度ね」
				CASE 1
					PRINTFORMW 「また时间があれば行きましょ」
				CASE 0
					PRINTFORMW 「ここでいいわ」
					PRINTFORMW 朱鷺子はそう言って飛び去っていった。
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
