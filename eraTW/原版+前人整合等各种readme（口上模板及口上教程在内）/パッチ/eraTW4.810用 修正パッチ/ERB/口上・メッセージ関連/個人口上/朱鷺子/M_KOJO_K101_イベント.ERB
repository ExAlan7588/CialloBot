;==================================================
;脱衣口上
;==================================================
@M_KOJO_EVENT_K101_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方选择
;ARG:1（ARG=3の场合のみあり）地の文の反応
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;时间停止时
			IF FLAG:70
			PRINTFORMW %CALLNAME:MASTER%は朱鷺子の服を脱がしていく……
			PRINTFORMW 时间が止まった彼女は全裸にされてもその小振りの胸も秘所も隠すことは出来ない
			;通常
			ELSE
			IF RAND:3 == 0
					PRINTFORMW 「……これでいい？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「ううう…やっぱり恥ずかしいわ…」
				ELSE
					PRINTFORMW 「ちょっと……あんまりジロジロ見ないでくれる……！」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;下着姿にする
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;时间停止时
			IF FLAG:70
			PRINTFORMW %CALLNAME:MASTER%は朱鷺子を下着姿にした
			PRINTFORMW 抵抗の出来ない彼女の下着姿をじっくりと视奸した
			;通常
			ELSE
			IF RAND:3 == 0
					PRINTFORMW 「ど、どう！？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「やっぱり恥ずかしい……」
				ELSE
					PRINTFORMW 「こういうのは…恥ずかしいわ……」
			    ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱ぎ
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;时间停止时
			IF FLAG:70
			;通常
			ELSE
			IF RAND:3 == 0
					PRINTFORMW 「不思議な感觉ね……」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「なんだろう……逆に全部脱ぐよりも恥ずかしい气が……」
				ELSE
					PRINTFORMW 「半分だけ……？」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交换です
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「いいわよ。でもその代わり交换してくれる？」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「……交换なら……え？ナニに使うのか？ば、馬鹿！ななな、何でもいいでしょ！」
					ELSE
						PRINTFORMW 「替えの下着に%CALLNAME:MASTER%のパンツをくれない？」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「いいわよ。はい、どうぞ」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「これでいいかしら？」
					ELSE
						PRINTFORMW 「ナニに使うか分からないけど、いいわよ」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「何に使うの……？」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「ええっと……うーん……まぁ、いいか」
					ELSE
						PRINTFORMW 「……変なことには使わないでよ」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「へ、変态！変态！変态！何考えてるのよ！」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「駄目に決まってるでしょ！？」
					ELSE
						PRINTFORMW 「うわぁ……」
					ENDIF
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

;==================================================
;愤怒口上
;==================================================
@M_KOJO_EVENT_K101_29(ARG,ARG:1)
;口上主はTARGET
;ARGはTARGETの度胸で分岐
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;怒らせてしまった
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
