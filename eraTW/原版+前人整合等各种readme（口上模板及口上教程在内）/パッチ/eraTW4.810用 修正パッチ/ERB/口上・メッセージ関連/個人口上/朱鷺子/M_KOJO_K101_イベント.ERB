RETURN 1

;==================================================
;相手が推倒を解除
;==================================================
@M_KOJO_EVENT_K101_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORMW 「ちょっと疲れたわ…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「ふぅ…スッキリしたわ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORMW 「もう…仕方ないわね」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;坏れるまでしてあげる
		PRINTFORMW 「坏れるまでしてあげるわ…」
		PRINTFORMW 朱鷺子は目をハートにしている。最早理性を失った发情期の獣のようだ。
		RETURN 1
	ENDIF
ENDIF
RETURN 1


;==================================================
;一日の終了时(AFTERTRAニーの前)、认识がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K101_31(ARG,ARG:1)
;口上主はTARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:自慰暴露で分岐入れてもよさそう
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;今日会っていない
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG
		IF TALENT:恋人 || TALENT:恋慕
			IF RAND:3 == 0
				PRINTFORML 「明日は会えるかな……」
				PRINTFORML 朱鷺子は一人でそう呟いた。
			ELSEIF RAND:2 == 0
				PRINTFORML 「%CALLNAME:MASTER%……今日は会えなかったな……」
				PRINTFORML 朱鷺子は一人でそう呟いた。
			ELSE
				PRINTFORML 「何処にいたんだろう…？」
				PRINTFORML 朱鷺子は一人でそう呟いた。
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
ENDIF
RETURN 1

