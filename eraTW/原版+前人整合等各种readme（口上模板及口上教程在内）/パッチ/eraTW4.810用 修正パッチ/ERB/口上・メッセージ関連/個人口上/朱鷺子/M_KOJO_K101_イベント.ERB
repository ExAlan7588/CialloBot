			ENDSELECT	
			ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「何だか少し紧张と言うか恥ずかしいわね……」
					PRINTFORMW 「いやまぁ全然良いんだけどね」
				CASE 1
					PRINTFORMW 「本とか沢山あるわよ！ 意味が分からないのも結構あるけど……」
				CASE 0
					PRINTFORMW 「あっ、好きな场所に座って良いわよ」
			ENDSELECT
			ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「くつろいでいってね」
				CASE 1
					PRINTFORMW 「本とか沢山あるわよ！ 意味が分からないのも結構あるけど……」
				CASE 0
					PRINTFORMW 「あっ、ここに座ってね」
			ENDSELECT
			ELSE
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「ゆっくりしていってね」
				CASE 1
					PRINTFORMW 「本とか沢山あるわよ！ 意味が分からないのも結構あるけど……」
				CASE 0
					PRINTFORMW 「あっ、ここに座ってね」
			ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;房间に诱われたのを断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
		IF TALENT:恋人
			SELECTCASE RAND:3
			CASE 2
				PRINTFORMW 「あっそうなの……」
				PRINTFORMDW %CALLNAME:MASTER%は謝りながら落ち込んでいる朱鷺子の头を优しく抚でた
				PRINTFORMW 「えへへ、优しいのね」
			CASE 1
				PRINTFORMW 「む～、まぁ无理なら仕方ないわね……」
			CASE 0
				PRINTFORMW 「じゃあ、また次回、房间に来てね」
			ENDSELECT	
			ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「あっ、そうなの……」
					PRINTFORMDW %CALLNAME:MASTER%は謝りながら落ち込んでいる朱鷺子の头を优しく抚でた
				CASE 1
					PRINTFORMW 「无理なら仕方ないわね……」
				CASE 0
					PRINTFORMW 「残念…またいつか诱うわ」
			ENDSELECT			
			ELSEIF TALENT:思慕
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「……少し寂しいけど仕方ないわね……」
				CASE 1
					PRINTFORMW 「う～ん、なら仕方ないわね……」
				CASE 0
					PRINTFORMW 「あらそうなの？」
					PRINTFORMW 「案外暇じゃないのね～」
			ENDSELECT
			ELSE
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「……今度また、来てね」
				CASE 1
					PRINTFORMW 「それなら仕方ないわね……」
				CASE 0
					PRINTFORMW 「……忙しいなら仕方ないわね」
			ENDSELECT		
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
