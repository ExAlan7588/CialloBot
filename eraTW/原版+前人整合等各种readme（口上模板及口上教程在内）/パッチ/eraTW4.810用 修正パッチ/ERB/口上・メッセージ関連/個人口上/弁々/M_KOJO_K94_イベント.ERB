;==================================================
@M_KOJO_EVENT_K94_29(ARG,ARG:1)
;口上主はTARGET
;ARGはTARGETの度胸で分岐
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;怒らせてしまった
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「……」
		PRINTFORMW 弁々はギロッと%CALLNAME:MASTER%を睨みつけている……
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;泣かせてしまった　[臆病]持ち
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;相手が推倒を解除
;==================================================
@M_KOJO_EVENT_K94_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORMW 「…さすがに疲れちゃったわ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「んー、气持ちよかった♪」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORMW 「んっ、もう…スケベ…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;坏れるまでしてあげる
		PRINTFORMW 「いいの？私、激しいわよ？」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;呼び捨て用関数
;==================================================
;-------------------------------------------------
@MASTER_CALLNAME_K94
;-------------------------------------------------
IF CFLAG:94:1900 == 1
	PRINTFORM お兄ちゃん
ELSEIF CFLAG:94:1901 == 1
	PRINTFORM 兄さん
ELSEIF CFLAG:94:1902 == 1
	PRINTFORM お姉ちゃん
ELSEIF CFLAG:94:1903 == 1
	PRINTFORM 姉さん
ELSE
	PRINTFORM %CALLNAME:MASTER%
ENDIF

