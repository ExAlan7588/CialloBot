@M_KOJO_EVENT_K29_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		SELECTCASE TCVAR:350
			;今日初めて会います
			CASE 0
				IF TALENT:恋人
					PRINTFORML 
					PRINTFORML 「……おや、これは%CALLNAME:MASTER%さん」
					PRINTFORML 「どうも……ふふ、今日は何をしますか？」
					PRINTFORMW 文は満面の笑みで訊ねてきた。
				ELSEIF TALENT:恋慕
					PRINTFORML 
					PRINTFORML 「……あ、%CALLNAME:MASTER%さん……」
					PRINTFORML 「……ど、どうも……」
					PRINTFORML 文は挨拶をしてきた。
					PRINTFORMW その颜は、何故かほんのりと红かった。
				ELSEIF TALENT:思慕
					PRINTFORML 
					PRINTFORML 「……おや、%CALLNAME:MASTER%さん」
					PRINTFORML 「……これは、どうも」
					PRINTFORMW どこかぎこちなく、文は言った。
				ELSE
					PRINTFORML 
					PRINTFORML 「……%CALLNAME:MASTER%さん？」
					PRINTFORML 「これはこれは、どうもどうも清く正しい射命丸です」
					PRINTFORMW 文は恭しく挨拶をしてきた。
				ENDIF
			;今日1～9回会いました
			CASE 1 TO 9
				IF TALENT:恋人
					PRINTFORML 
					PRINTFORML 「……あ、%CALLNAME:MASTER%さん」
					PRINTFORML 「また会っちゃいましたね……何、します？」
					PRINTFORML 文は笑みを浮かべて、ひらひらと手を振ってきて見せた。
				ELSEIF TALENT:恋慕
					PRINTFORML 
					PRINTFORML 「……おや、%CALLNAME:MASTER%さん」
					PRINTFORML 「ふふ、また会いましたね♪」
					PRINTFORML 嬉しそうに笑って、文はそう言った。
				ELSEIF TALENT:思慕
					PRINTFORML 
					PRINTFORML 「……あや、%CALLNAME:MASTER%さん」
					PRINTFORML 「どうも、ええと、また会いましたね」
					PRINTFORML 少し言叶を选ぶようにして、文は言った。
				ELSE
					PRINTFORML 
					PRINTFORML 「……あやや、%CALLNAME:MASTER%さん」
					PRINTFORML 「また会いましたねぇ、どうもどうも」
					PRINTFORML 妙に恭しく、文は挨拶してきた。
				ENDIF
			;今日10～19回会いました　結構会うよね的な感じ
			CASE 10 TO 19
				IF TALENT:恋人
					PRINTFORML 
					PRINTFORML 「……ふふ、よく会いますね」
					PRINTFORML 「そんなに会いたいんですか？　ふふっ」
					PRINTFORML 少し悪戯な笑みを浮かべて、文はこちらにすり寄ってきた。
				ELSEIF TALENT:恋慕
					PRINTFORML 
					PRINTFORML 「……あら、%CALLNAME:MASTER%さん」
					PRINTFORML 「今日はよく会いますね、ふふ……」
					PRINTFORML 横目で微笑みながら、文はそう言った。
				ELSEIF TALENT:思慕
					PRINTFORML 
					PRINTFORML 「……あや、また会いましたね」
					PRINTFORML 「……今日だけで、結構会ってますよね、私達……」
					PRINTFORML 「……いや……え、えぇ……？」
					PRINTFORML 一人何かを想像して、文は少し頬を红く染めた。
				ELSE
					PRINTFORML 
					PRINTFORML 「……よく会いますね」
					PRINTFORML 「ま、なんとも思いませんがね」
					PRINTFORML 我関せず、といった感じで、文はそう言い放った。
				ENDIF
			;会いすぎ　特定の人物と良く会う様になる异変でも起こったのかな？
			CASEELSE
				PRINTFORML 
				PRINTFORML 「……本当に、良く会いますね」
				PRINTFORML 少し困ったように、文は笑う。
				PRINTFORML 「……いや、本当に」
				PRINTFORML 何かを考える素振りをしてから、文は近くに寄ってきた。
		ENDSELECT
		;遭遇回数追加判定
		TCVAR:350 += 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE TCVAR:350
			CASE 0
				IF TALENT:恋人
					PRINTFORML 
					PRINTFORML 「……あ、%CALLNAME:MASTER%さん」
					PRINTFORML 「どうも、奇遇ですね……何かします？」
					PRINTFORMW 文は%CALLNAME:MASTER%を見とめるが否や、笑颜になって聞いてきた。
				ELSEIF TALENT:恋慕
					PRINTFORML 
					PRINTFORML 「……おや、%CALLNAME:MASTER%さん」
					PRINTFORML 「どうも……ここにいたんですね」
					PRINTFORMW 文は微笑んで声をかけてきた。
				ELSEIF TALENT:思慕
					PRINTFORML 
					PRINTFORML 「……おや、%CALLNAME:MASTER%さん」
					PRINTFORML 「……ど、どうも……」
					PRINTFORMW 少し视线を泳がせ、文は挨拶してきた。
				ELSE
					PRINTFORML 
					PRINTFORML 「……あや、これはこれは%CALLNAME:MASTER%さん」
					PRINTFORML 「どうもどうも、毎度お馴染み射命丸です」
					PRINTFORMW 文は少々おどけるようにして挨拶してきた。
				ENDIF
			CASE 1 TO 9
				IF TALENT:恋人
					PRINTFORML 
					PRINTFORML 「おや、%CALLNAME:MASTER%さん」
					PRINTFORML 「どうかしましたか？」
					PRINTFORML %CALLNAME:MASTER%が近寄ると、文は穏やかに笑んで声をかけてきた。
				ELSEIF TALENT:恋慕
					PRINTFORML 
					PRINTFORML 「……あや、%CALLNAME:MASTER%さん」
					PRINTFORML 「またお会いしましたね」
					PRINTFORML にこりと笑って、文は声をかけてきた。
				ELSEIF TALENT:思慕
					PRINTFORML 
					PRINTFORML 「……あ、%CALLNAME:MASTER%さん」
					PRINTFORML 「また会いましたね」
					PRINTFORML 文はこちらに手を振ってきた。
				ELSE
					PRINTFORML 
					PRINTFORML 「……あ、どうも、%CALLNAME:MASTER%さん」
					PRINTFORML 文はこちらに目を合わせることなく、そう言った。
				ENDIF
			CASE 10 TO 19
				IF TALENT:恋人
					PRINTFORML 
					PRINTFORML 「……今日は、よく会いますね」
					PRINTFORML 文は穏やかな笑みを浮かべている……。
				ELSEIF TALENT:恋慕
					PRINTFORML 
					PRINTFORML 「……あ、%CALLNAME:MASTER%さん」
					PRINTFORML 「……よく会いますね」
					PRINTFORML 文は微笑んで、そう声をかけてきた。
				ELSEIF TALENT:思慕
					PRINTFORML 
					PRINTFORML 「……おや、%CALLNAME:MASTER%さん」
					PRINTFORML 「今日、よく会いません？　結構会ってますよね……？」
					PRINTFORML 文は小さく笑んで、訊ねてきた。
				ELSE
					PRINTFORML 
					PRINTFORML 「……よく会いますね」
					PRINTFORML 文は上を向きながら、そう言った。
				ENDIF
			CASEELSE
				PRINTFORML 
				PRINTFORML 「……本当、よく会いますね」
				PRINTFORML 「偶然？　……それとも、会いに来てくれてるんですか……？」
					IF TALENT:恋慕
						PRINTFORML からかうような笑みを浮かべ、文は訊ねて来た。
					ELSE
						PRINTFORML いたって无表情で、文は訊ねて来た。
					ENDIF
		ENDSELECT
		;遭遇回数追加判定
		TCVAR:350 += 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて一绪に入る
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			PRINTFORML 
			PRINTFORML 「……おや、%CALLNAME:MASTER%さん」
			PRINTFORML 「%CALLNAME:MASTER%さんも入るのですか？」
			PRINTFORML 「……では、どうぞ」
			PRINTFORML 文は頬を红く染めながら、隣に座れるほどの场所を作った。
			PRINTFORMW 照れるならしなければいいのに、%CALLNAME:MASTER%はそう考えた。
		ELSE
			PRINTFORML 
			PRINTFORML 「……%CALLNAME:MASTER%さんですか」
			PRINTFORML 「……入るのですか？いえ、駄目というわけではありませんが……」
			PRINTFORMW 文は体を腕で隠しながら、そう言った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて出て行った
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 
		PRINTFORML 「――おや、これは、これは……」
		PRINTFORML 文は何でもないようにしているが、少し頬を红く染めている。
		PRINTFORML 「……失礼いたしました」
		PRINTFORMW ぴしゃりと戸を閉め、文はとてとてと向こうの方へ走っていった。
	RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて追い出された
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 
		PRINTFORML 「――――――」
		PRINTFORML 文は%CALLNAME:MASTER%の姿を見止め、押し黙っている。
		PRINTFORML 「――出てってください」
		PRINTFORML 文は颜を真っ赤にして、若干の杀气をにじませながら言う。
		PRINTFORMW %CALLNAME:MASTER%は最低限の动きで出て行った。
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K29_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕时陪睡中就寝时のみ？
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕无しでも入ってくる可能性有り
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝时TARGET起床はこちら（TARGET衰弱or陪睡中TARGET睡魔負け后そのままMASTERが起き続けた上で相手が起床した场合限定
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;共通
		IF TALENT:恋人
			PRINTFORML 
			PRINTFORML 「……おはよ、ござい、ます……」
			PRINTFORML 「……%CALLNAME:MASTER%、さんだぁ……」
			PRINTFORMW 文は寝ぼけながら、%CALLNAME:MASTER%に抱きついてきた。
		ELSEIF TALENT:恋慕
			PRINTFORML 
			PRINTFORML 「……ん、ぁ……」
			PRINTFORML 「……%CALLNAME:MASTER%、さん……？」
			PRINTFORML 「……っ、こ、これは、お見苦しいところを、えっと、えっと……」
			PRINTFORMW 文は飛び起きて、髪型を手で直した。
		ELSEIF TALENT:思慕
			PRINTFORML 
			PRINTFORML 「……ぅん……」
			PRINTFORML 「……%CALLNAME:MASTER%、さんですか……？」
			PRINTFORML 「……おはよう、ございます……」
			PRINTFORMW 文は目をこすりながら起床の挨拶をした。
		ELSE
			PRINTFORML 
			PRINTFORML 「……ん、ふぁ……」
			PRINTFORML 「……は、これは、おはよう、ございます」
			PRINTFORMW まだ寝ぼけている感じで、文は挨拶をしてきた。
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

