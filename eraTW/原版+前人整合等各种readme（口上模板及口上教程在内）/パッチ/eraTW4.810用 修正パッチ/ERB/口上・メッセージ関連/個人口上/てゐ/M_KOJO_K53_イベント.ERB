@M_KOJO_EVENT_K53_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;その日初めての遭遇
		IF TCVAR:53:350 == 0
			K53_合計绝顶数 = 0
			TCVAR:53:350 = 1
		ENDIF
		;发情中
		IF IsHeat(TARGET)
			PRINTFORML てゐは颜を赤らめて太腿を擦り合わせ、もじもじとしている…
			PRINTFORMW 「……んんっ……ふ…ふぅ………、………！……な、なに？」
		;欲求不満
		ELSEIF CFLAG:积攒度 >= 800
			IF ABL:自慰中毒 >= 4 && GET_TARGETNUM() == 1
				PRINTFORML %CALLNAME:MASTER%がてゐを视認するとバッと%CALLNAME:MASTER%の方を向き、股间から手を素早く離したように見えた
				PRINTFORMW 「な、なに！？………きゅ、急に入ってこないでよね………！」
			ELSE
				PRINTFORMW てゐはそわそわした様子で%CALLNAME:MASTER%の事をちらちらと气にかけている
			ENDIF
		ELSE
			;机嫌良い
			IF TALENT:机嫌 == 1	
				;販促
			;机嫌普通
			ELSEIF TALENT:机嫌 == 0
				;
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「ふふーん、今日はどんな恶作剧をしよっかな～」
						PRINTFORMW 「げ、%CALLNAME:MASTER%！…べ、べつに何も企んでないウサよ？」
					CASE 1
						PRINTFORML 「ここにこれを仕掛けて～………………あっ」
						PRINTFORMW 「……………な、なにウサ？别に何もしてないウサ」
				ENDSELECT
			;机嫌悪い
			ELSEIF TALENT:机嫌 == -1
				;无视
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「………………………」
						PRINTFORMW 明らかにてゐと目が合ったがこちらを一瞥すると、あなたに関心を示すことなくそっぽを向いた
					CASE 1
						PRINTFORML 「ん？………ああなんだ%CALLNAME:MASTER%か…」
						PRINTFORMW てゐは%CALLNAME:MASTER%を一瞥するとそっぽを向いた
				ENDSELECT
			
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;その日初めての遭遇
		IF TCVAR:53:351 == 0
			K53_合計绝顶数 = 0
			TCVAR:53:351 = 1
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて一绪に入る
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて出て行った
	;仲が進んでおらず、既成事实が1未満か、恋慕が付いていない异性の场合に出ていく（他でも共通の澡堂条件）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて追い出された
	;异性で弱みを握られている（パンツ发見or情事を見られた）场合に发生
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「おっふろ～おっふろ～♪………あら？」
		PRINTFORML 「ん、ちょっと%CALLNAME:MASTER%！何私より先にお澡堂はいってんの！」
		PRINTFORMW 「ほら私がお澡堂入りたいって言ってるんだから、出てって出てって！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラと约会中に遭遇した
	;ARG:1に约会的对象
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

