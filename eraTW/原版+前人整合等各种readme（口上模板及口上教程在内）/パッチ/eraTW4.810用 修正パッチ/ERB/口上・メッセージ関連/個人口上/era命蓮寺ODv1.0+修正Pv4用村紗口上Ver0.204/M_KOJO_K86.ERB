;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K86_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SIF TCVAR:353 == 1
		TCVAR:353 = 0
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕时陪睡中就寝时のみの筈
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕无しでも入ってくる可能性有り
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝时TARGET起床はこちら（TARGET衰弱or陪睡中TARGET睡魔負け后そのままMASTERが起き続けた上で相手が起床した场合限定
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF CFLAG:MASTER:陪睡中
				PRINTFORML 「…ん…… ふぁ…ぁ……っ…」
				PRINTFORMW 「あ…… おはよう%CALLNAME:MASTER%、ふふっ」
				;ムラサ私室以外の房间を私室に设定している(条件的に有り得ず
				;SIF CFLAG:MASTER:311 != 14
					;PRINTFORMW 「%CALLNAME:MASTER%が来てたのなら… もう少し早く起きていれば良かったかな」
				PRINTL
				PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%は温もりを惜しみつつ布団を抜け出した…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF CFLAG:MASTER:陪睡中
				PRINTFORML 「…ん…… ふぁ…ぁ……っ…」
				PRINTFORMW 「あ…… おはよう%CALLNAME:MASTER%、ふふっ」
				;ムラサ私室以外の房间を私室に设定している(条件的に有り得ず
				;SIF CFLAG:MASTER:311 != 14
					PRINTFORMW 「%CALLNAME:MASTER%が来てたのなら… もう少し早く起きていれば良かったかな」
				PRINTL
				PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%は温もりを惜しみつつ布団を抜け出した…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K86_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入スイッチ
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定时になって寝る）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF BASE:TARGET:体力 <= 500
			IF CFLAG:诶嘿嘿
				PRINTFORML 「はぁ… はぁ… もう、だめ……」
				PRINTFORMW %CALLNAME:TARGET%は力无く%CALLNAME:MASTER%にしなだれ掛かり、荒い呼吸と共に肩を上下させている…
				PRINTL
				PRINTFORML 「私の方が先にバテちゃうなんて、ね… ふふっ… 良かったわよ、%CALLNAME:MASTER%」
				PRINTFORMW そう言うと%CALLNAME:TARGET%は%CALLNAME:MASTER%の唇に优しく触れるだけのキスをする…
				PRINTL
			ELSE
				PRINTFORMW 「ん… やっぱりまだ疲れてるみたい」
			ENDIF
			PRINTFORMW 「…私は房间に戻って休むわね、おやすみなさい」
		ELSE
			PRINTFORMW 「ん… もうこんな时间。私は先に休ませてもらうわね」
			PRINTFORMW 「%CALLNAME:MASTER%もあんまり夜更かししちゃだめよ？ふふっ、おやすみなさい」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:陪睡中
			PRINTL
			PRINTL
			PRINTFORMW 「ふふっ……」
		ELSE
			PRINTFORMW 「ん… ふぁ…ぁ……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会话というよりは寝言？）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTL
		PRINTFORMW 「すー… すー… ん…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;恶作剧中
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「む… んー…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;衰弱中（恋慕时、房间まで運ぶ）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:诶嘿嘿
			PRINTFORMW 「はぁ… はぁ… もう、だめ……」
			PRINTFORMW %CALLNAME:TARGET%は力无く%CALLNAME:MASTER%にしなだれ掛かり、荒い呼吸と共に肩を上下させている…
			PRINTL
			PRINTFORMW 「ふふっ… すっごく、良かった。%CALLNAME:MASTER%の事、いっぱい感じられて幸せよ」
			PRINTFORMW そう言うと%CALLNAME:TARGET%は%CALLNAME:MASTER%の唇に优しく触れるだけのキスをする…
			PRINTFORMW そうしてしばらく逢瀬の余韻を乐しむと、%CALLNAME:MASTER%は%CALLNAME:TARGET%を房间まで送った…
		ELSE
			PRINTFORMW 「ん… やっぱりまだ疲れてるみたい」
			PRINTFORMW 「……送ってくれるの？ふふっ… ありがとう」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（房间まで運ばれる）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「あら… もうお休みかしら」
		PRINTFORMW 「仕方ないわね、お房间までエスコートしますわ」
		PRINTFORMW そう言うと%CALLNAME:TARGET%は、ひょいと%CALLNAME:MASTER%を抱き上げた…
		PRINTL
		PRINTFORMW 「ふふっ… 驚いた？こう見えても、私結構力あるのよ？」
		PRINTFORMW 「……それじゃあ、おやすみなさい。良い梦を」
		PRINTW
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

