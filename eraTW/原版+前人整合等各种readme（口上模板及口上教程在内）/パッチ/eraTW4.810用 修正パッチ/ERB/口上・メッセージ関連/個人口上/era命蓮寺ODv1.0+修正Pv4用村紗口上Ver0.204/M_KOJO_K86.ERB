;==================================================
;情事を目撃した
;==================================================
@M_KOJO_EVENT_K86_6(ARG,ARG:1)
;口上主はTARGET
;ARGに情事相手
;--------------------------------------------------
;记入スイッチ
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;情事を目撃した（馴れ合い度:低）
	;记入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;負けじと参加（馴れ合い度:高）
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 2
		;白蓮が居ない
		IF CFLAG:MASTER:当前位置 != CFLAG:55:当前位置
			PRINTFORMW 「…お乐しみのようね？」
			PRINTL
			PRINTFORMW %CALLNAME:ARG%の肩に背后からすうっと白い手が回される
			PRINTL
			PRINTFORML 「ふふ… そんなに驚いた颜しないの、可爱い声が響いていたわよ？」
			PRINTFORML 「とっても气持ち良さそうな… 甘くて切ない声。あんなの聴かされたら私まで参っちゃうわ」
			PRINTFORMW 「だから、私も仲间に入れて下さらない？ねぇ%CALLNAME:MASTER%…」
			PRINTL
			PRINTFORMW そう言って%CALLNAME:TARGET%は%CALLNAME:ARG%の肩越しに%CALLNAME:MASTER%に妖しく微笑んだ…
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;气まずそうに立ち去る
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 3
		PRINTFORML 「……お邪魔したようね？」
		PRINTFORMW 「そうね、私は失礼しますのでごゆっくりお乐しみを…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERに诱われて参加
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 4
		;白蓮が居る
		IF CFLAG:MASTER:当前位置 == CFLAG:55:当前位置
			;ムラサが恋人
			IF TALENT:TARGET:恋人
				PRINTFORML 「ふふっ… 御一绪できて光栄です、聖…」
				PRINTL
				PRINTFORMW そう言うと%CALLNAME:TARGET%は白蓮の手の甲に口付けた…
			ELSE
				PRINTFORMW 「私も一绪に…？」
				PRINTL
				PRINTFORMW %CALLNAME:TARGET%は戸惑うような表情で白蓮を見る
				PRINTL
				PRINTFORMW 「聖は… 構いませんか？」
				PRINTL
				PRINTFORML 「……そうですか、それなら喜んで」
				PRINTFORMW 「御一绪できて光栄です、と言うのも何か変ですよね」
				PRINTL
				PRINTFORMW そう言うと%CALLNAME:TARGET%は嬉しそうに微笑んだ…
			ENDIF
		ELSE
			PRINTFORML 「可爱がって貰ったみたいね、%CALLNAME:ARG%？一人占めなんてずるいわ」
			PRINTFORMW 「ふふ… そう膨れないの、一绪に乐しみましょう？」
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML 
ENDIF
RETURN 1

