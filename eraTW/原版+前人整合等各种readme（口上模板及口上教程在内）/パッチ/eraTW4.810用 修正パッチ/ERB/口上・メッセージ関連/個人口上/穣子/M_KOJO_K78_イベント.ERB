					CALL K78_HPH_PRINT,@"「言われたとおり生やしてるわよHPH どう？ うれしい？HPH」","L"
				ENDIF
				WAIT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;下着姿にする
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
