;-------------

IF LOCAL
	SIF 陷落状态() == 5
		GOTO NAKISATORIN
	IF ARG > 0 && ARG < 3
		;肛门射精
		SIF CFLAG:287 == 2
			SETBIT CFLAG:1494,0
		;膣内射精
		SIF CFLAG:286 == 2
			SETBIT CFLAG:1494,1
		;口内射精
		SIF CFLAG:288 == 2
			SETBIT CFLAG:1494,2
		;颜面射精
		SIF CFLAG:289 == 2
			SETBIT CFLAG:1494,3
		;手淫射精
		SIF CFLAG:290 == 2
			SETBIT CFLAG:1494,4
		FOR LOCAL:2,0,5
			SIF GETBIT (CFLAG:1494,LOCAL:2)
				LOCAL:3 += 1
		NEXT
		IF LOCAL:3 > 1
			FOR LOCAL:2,0,5
