@M_KOJO_EVENT_K49_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;诱い文句
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF (BASE:酒气 > (MAXBASE:酒气 / 10) * 4)
				CALL DRUNKER49(200)
			ELSEIF 陷落状态() >= 4
