	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…私も、ちょっとしつこかったわね。ごめん…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反抗刻印2>1レベルダウンイベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「…乐しかったわよ、また机会があるといいわね」
		RETURN 1
	ENDIF
	;-------------------------------------------------
