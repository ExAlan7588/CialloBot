;==================================================
;相手からデートのお诱い
;==================================================
@M_KOJO_EVENT_K44_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;诱い文句
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
			CASE 0
				LOCALS = 博丽神社
			CASE 1
				LOCALS = 命蓮寺
			CASE 2
				LOCALS = 人里
			CASE 3
				LOCALS = 雾の湖
			CASE 4
				LOCALS = 迷いの竹林
			;选ばれる事は无いんだけど一応
			CASE 5
				LOCALS = 魔法の森
			;デートで三途の川に诱われたら唖然とするよね、という事で
			CASE 6
				LOCALS = マヨヒガ
			CASE 7
				LOCALS = 妖怪の山のふもと
			CASE 8
				LOCALS = 妖怪の山
			CASE 9
				LOCALS = 地底
			CASE 10
				LOCALS = 月
			CASEELSE
				LOCALS = "これが出たらスレで報告してね"
			ENDSELECT
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「今日は二人で%LOCALS%にお出かけしましょっ%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1
				PRINTFORMW 「ねえ、二人でデートなんてどうかしら%UNICODE(0x2665)%//」
			;恋慕缚りが解除されたときのために
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、もし暇だったら一绪に%LOCALS%でも行かない？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「やった%UNICODE(0x2665)% それじゃあ行きましょっ%UNICODE(0x2665)%%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1
				PRINTFORMW 「やった%UNICODE(0x2665)%//」
			ELSE
				PRINTFORMW 「それじゃあ早速行きましょう%UNICODE(0x2665)%」
			ENDIF
			;デートに诱われたフラグを立てる
			TCVAR:356 = 1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「それじゃあ别の日にデートしましょうね%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「あやや、まあ%CALLNAME:MASTER%が乗り气じゃないならいいわ」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;房间に诱われた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「わたしの家でのんびりしましょう%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
				PRINTFORMW 「よかったらわたしの家に寄ってかない？//」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;房间に诱われたのを受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「やった%UNICODE(0x2665)% それじゃあ行きましょっ%UNICODE(0x2665)%%UNICODE(0x2665)%」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1
				PRINTFORMW 「やった%UNICODE(0x2665)%//」
			ELSE
				PRINTFORMW 「それじゃあ早速行きましょう%UNICODE(0x2665)%」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
