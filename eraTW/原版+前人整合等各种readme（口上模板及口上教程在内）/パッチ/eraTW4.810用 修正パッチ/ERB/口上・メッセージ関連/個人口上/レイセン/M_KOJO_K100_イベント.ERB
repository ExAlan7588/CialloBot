	IF !TCVAR:100:今日会ったか
		;-------------------------------------------------
		;小分岐
		;-------------------------------------------------
		;MASTERが入ってきた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 1
			CALL REISEN_ENCOUNTER
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが入ってきた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 2
			CALL REISEN_ENCOUNTER
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが澡堂に入ってきて一绪に入る
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 3
			IF TALENT:恋慕
				PRINTFORML 「あ、%MASTERNAME:100%。お澡堂入ってたの？」
				PRINTFORMDL %CALLNAME:MASTER%が湯舟でくつろいでいると、%CALLNAME:TARGET%が入ってきた
				PRINTFORMDL 『すぐに上がるから』%CALLNAME:MASTER%はそう言い立ち上がろうとするが……
				PRINTFORML 「背中流してあげるね♪」
				PRINTFORMDW 結局、二人で入ることになってしまった
			ELSE
				PRINTFORML 「あ…その……一绪に入っても、いいですか？」
				PRINTFORMDL まさか%CALLNAME:TARGET%がそんなことを言ってくるとは……
				PRINTFORMDW %CALLNAME:MASTER%は意外に思いながらも、彼女の為にスペースを空けた
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが澡堂に入ってきて出て行った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 4
			PRINTFORML 「きゃあっ！？す、すみませんっ！」
			PRINTFORMDL %CALLNAME:MASTER%が呼び止めるより早く%CALLNAME:TARGET%は戸を閉めてしまった
			PRINTFORMDW ……誰かが澡堂に入っているのに气が付かないとは、よほど疲れていたのだろうか
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGETが澡堂に入ってきて追い出された
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 5
			PRINTFORML 「あ、あの～」
			PRINTFORMDL %CALLNAME:MASTER%が澡堂でくつろいでいると、戸の向こうから%CALLNAME:TARGET%の声が聞こえてきた
			PRINTFORML 「后がつかえてるので…その、早めにお願いします」
			PRINTFORMDW どうやら待たせてしまっていたようだ。しょうがないので、%CALLNAME:MASTER%はさっさと澡堂から出た
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;外出先で遭遇した
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 6
			CALL REISEN_ENCOUNTER
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラと约会中に遭遇した
		;ARG:1に约会的对象
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 7
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MAIN_MAP上で一日最初の挨拶
		;注、このあと通常の遭遇口上、ARG==1 OR ARG==2に接続します
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 8
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;外出先で一日最初の挨拶
		;ARG:1に约会的对象、ARG:1が0の场合は非デート
		;注、このあと通常の外出先で遭遇した口上、ARG==6 OR ARG==7に接続します
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 9
			;ARG:1と约会中
			IF ARG:1
			;一人で外出
			ELSE
			ENDIF
			PRINTFORML 
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

