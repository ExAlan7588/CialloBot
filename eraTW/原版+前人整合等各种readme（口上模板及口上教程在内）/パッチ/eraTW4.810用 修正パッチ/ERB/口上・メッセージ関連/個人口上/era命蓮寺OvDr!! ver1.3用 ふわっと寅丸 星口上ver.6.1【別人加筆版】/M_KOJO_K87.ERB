;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"UPDATE判定"
;※※※※※※※※※※※※※※※※※※※※※※※※※※
@M_KOJO_UPDATE_K87
;口上独自のUPDATE用
;ENCOUNTER専用に独自分岐フラグを仕込んだ场合等にどうぞ
;UPDATE判定でCFLAGを使う场合、キャラ番号の指定が必要になります
IF FIRSTTIME("U87-1",0,87) && CFLAG:87:认识
	PRINTFORML 呼び名が変わってしまった场合の救済措置です
	PRINTFORML 呼び名が変わって居ない方には手数をおかけします、申し訳ありません
	MASTERNAME:87 = 
	PRINTFORMW 「では、あなたの名前を教えて下さい」
	PRINTL 名前を入力してください(直接入力するか、ボタンで选择してください)
	IF NAME:MASTER != "あなた"
		LOCALS = %NAME:MASTER%
	ELSE
		LOCALS = %CALLNAME:MASTER%
	ENDIF
	PRINTBUTTON @"[名前(%LOCALS%)を名乗る]", @"%LOCALS%"
	PRINTL 　
	;名前を入力するループ
	$INPUT_LOOP_ONAMAE
	INPUTS
	;/見事な響子口上の丸パクだと感心するがどこもおかしくはない
	; 行头行尾の空白文字削除
	LOCALS = %RESULTS%
	REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
	;10文字以上は受け付けない
	IF STRLENS(LOCALS) > 10
		CLEARLINE 1
		REUSELASTLINE 「…ちょっと长すぎますね。通称とかニックネームはお持ちですか？」
		GOTO INPUT_LOOP_ONAMAE
	;空白は受け付けない
	ELSEIF RESULTS == ""
		PRINTL 名前を入力してください
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		; 同姓同名
		IF FLAG:扮演 != 87 && STRCOUNT(LOCALS, "\(寅丸\)\*\\s\*星")
			CLEARLINE 1
			REUSELASTLINE 「…からかってますよね」
			GOTO INPUT_LOOP_ONAMAE
		; 白蓮が主人ではなく、白蓮というフレーズが含まれている。もしくは
		; 響子が主人ではなく、幽谷もしくは響子というフレーズが含まれている
		ELSEIF (FLAG:扮演 != 55 && STRCOUNT(LOCALS, "聖\*\\s\*白蓮")) || (FLAG:扮演 != 88 && STRCOUNT(LOCALS, "\(幽谷\)\*\\s\*響子\$"))
			CLEARLINE 1
			REUSELASTLINE 「…あのですねえ」
			GOTO INPUT_LOOP_ONAMAE
		; 命蓮寺勢はじき
		; うんそうだね多々良勝五郎とか教える人もいるかもしれないもんね
		ELSEIF (FLAG:扮演 != 39 && LOCALS == "ナズーリン") || (FLAG:扮演 != 40 && STRCOUNT(LOCALS, "\(多々良\)\*\\s\*小伞")) || (FLAG:扮演 != 85 && STRCOUNT(LOCALS, "\(雲居\)\*\\s\*一輪")) || (FLAG:扮演 != 86 && STRCOUNT(LOCALS, "\(\(村紗\)*\\s\*水蜜\|ムラサ\)"))
			CLEARLINE 1
			REUSELASTLINE 「…うちの者と紛らわしいので、出来れば他のに」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF (FLAG:扮演 != 41 && STRCOUNT(LOCALS, "\(封獣\)\*\\s\*ぬえ")) || (FLAG:扮演 != 69 && STRCOUNT(LOCALS, "\(二ツ岩\)\*\\s\*マミゾウ"))
			CLEARLINE 1
			REUSELASTLINE 「え…もしかしていたずらですか？」
			GOTO INPUT_LOOP_ONAMAE
		; 道教勢力はじき
		ELSEIF (FLAG:扮演 != 89 && STRCOUNT(LOCALS, "\(宮古\)\*\\s\*芳香")) || (FLAG:扮演 != 90 && STRCOUNT(LOCALS, "\^霍\*\\s\*青娥")) || (FLAG:扮演 != 91 && STRCOUNT(LOCALS, "\(蘇我\)\*\\s\*屠自古")) || (FLAG:扮演 != 92 && STRCOUNT(LOCALS, "\(物部\)\*\\s\*布都")) || (FLAG:扮演 != 56 && STRCOUNT(LOCALS, "\(\(丰聡耳\)\*\\s\*神子\|聖徳太子\)"))
			CLEARLINE 1
			REUSELASTLINE 
			GOTO INPUT_LOOP_ONAMAE
		; 博丽灵梦というフレーズが含まれてる。もしくは
		; 東风谷早苗というフレーズが含まれている
		ELSEIF STRCOUNT(LOCALS, "\(博丽\)\*\\s\*灵梦") || STRCOUNT(LOCALS, "\(東风谷\)\*\\s\*早苗")
			PRINTFORML 「あなたは見た感じ、彼女ほどはっちゃけてませんが…それでいいんですか？」
		; 雾雨魔理沙というフレーズが含まれてる。
		ELSEIF STRCOUNT(LOCALS, "\(雾雨\)\*\\s\*魔理沙")
			PRINTFORML 「まっすぐより曲がるレーザーのが强いんです！…それでいいんですか？」
		; 直接入力ならいけると思ったか？弾くぜ！
		ELSEIF FIRSTTIME("U87-1") && ABL:亲密 < 4 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(さん\|ちゃん\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「さすがにそれはちょっと…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U87-1") && ABL:亲密 < 7 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(様\|さま\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「さすがにそれはちょっと…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U87-1") && (ABL:顺从 < 7 || ABL:侍奉精神 < 7) && STRCOUNT(LOCALS, "\(\(御\|ご\)\(主人\|しゅじん\)\(様\|さま\)\)$")
			CLEARLINE 1
			REUSELASTLINE 「さすがにそれはちょっと…」
			GOTO INPUT_LOOP_ONAMAE
		ELSEIF FIRSTTIME("U87-1") && (!TALENT:恋人 || !TALENT:恋慕) && STRCOUNT(LOCALS, "\(\(旦那\|だんな\)\(様\|さま\)\)\$")
			CLEARLINE 1
			REUSELASTLINE 「さすがにそれはちょっと…」
			GOTO INPUT_LOOP_ONAMAE
		ELSE
			PRINTFORML 「%RESULTS%…ですね」
		ENDIF
		PRINTL [0] はい
		PRINTL [1] いいえ
		;入力した名前を確定するループ
		$INPUT_LOOP_KAKUTEI
		INPUT
		IF RESULT == 0
			;あなたの名前を%MASTERNAME:87%に格納
			MASTERNAME:87 = %RESULTS%
			PRINTFORML 
		ELSEIF RESULT == 1
			PRINTL 名前を入力してください
			GOTO INPUT_LOOP_ONAMAE
		ELSE
			PRINTL 名前を入力してください
			GOTO INPUT_LOOP_ONAMAE
		ENDIF
	ENDIF
ENDIF
SIF TALENT:87:プライド == -1
	TALENT:87:プライド = 1
IF TALENT:87:舌使い && ABL:87:舌 < 6
	TALENT:87:舌使い = 0
	TALENT:87:猫舌 = 1
ENDIF
SIF ABL:87:舌 == 6 && TALENT:87:舌使い == 0
	TALENT:87:舌使い = 1
RETURN 1

