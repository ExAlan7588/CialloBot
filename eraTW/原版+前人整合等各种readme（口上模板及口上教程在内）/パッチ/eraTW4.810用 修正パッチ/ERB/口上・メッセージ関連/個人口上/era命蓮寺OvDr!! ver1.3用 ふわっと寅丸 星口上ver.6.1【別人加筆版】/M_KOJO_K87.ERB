;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;记入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		SIF CFLAG:1501 == 0
		DRAWLINE
		PRINTFORMW 「ひぁっ！？　今、びりって…きました…」
		PRINTFORMW 「%MASTERNAME:87%…なに、したんですかぁ…？」
		RETURN 1
	ENDIF
	;基本セット
	;※CFLAG:1501 == 1でＧスポ責めしている时、0で选んだコマンドで継続して突いている时の口上
	;過激な恶作剧中
	IF CFLAG:恶作剧 && CFLAG:睡眠
		RETURN 1
	ELSE
		;插入Ｇスポ責め
		IF CFLAG:1501 == 1
			CFLAG:1501 = 0
			;Ｖ感觉1以下
			IF ABL:Ｖ感觉 <= 1
				;/Ｖ敏感！
				PRINTFORMW 「う、ぁ……%UNICODE(0x2665)%　これ、ダメです…っ」
				PRINTFORMW 「これ、觉えちゃったら…私っ……」
			;恋慕
			ELSEIF TALENT:恋慕
				;これでMASTERが射精
				IF NOWEX:PLAYER:11
					;これでTARGETがＶ绝顶する
					IF NOWEX:1
						PRINTFORML 「奥、きゅーって…あぁ……%UNICODE(0x2665)%　もう、きちゃ…」
						PRINTFORMW 「ふぁっ！？　膨らんで…！？　だめです！今そこに出されたら…っ」
					ELSE
						PRINTFORML 「%MASTERNAME:87%…出そう、なんですね…」
						PRINTFORMW 「私の中に、%MASTERNAME:87%の…いっぱい、ください%UNICODE(0x2665)%」
					ENDIF
				ELSE
					IF RAND:2 == 0
						PRINTFORML 「そ、こ……っ　痺れてるみたいに…っ%UNICODE(0x2665)%」
						PRINTFORMW 「ふぃい…%UNICODE(0x2665)%　强くしちゃ…うぁあ…%UNICODE(0x2665)%」
					ELSE
						PRINTFORML 「うあっ%UNICODE(0x2665)%！　また、すごいのきましたぁ……」
						PRINTFORMW 「ほんとに、癖に、なっちゃいます…　我慢できない子になっちゃいますぅ…%UNICODE(0x2665)%」
					ENDIF
				ENDIF
			;屈服3
			ELSEIF MARK:屈服刻印 == 3
				PRINTFORMW 「そこ…もっと擦ってください…%UNICODE(0x2665)%」
			;それ以外
			ELSE
				PRINTFORMW 「んっ…　じりじりして、气持ちいい…」
			ENDIF
		;连続
		ELSE
			DRAWLINE
			;正常位、対面立位
			IF TEQUIP:体位 == 1
				;MASTERについている
				IF TALENT:PLAYER:2 & 2
					PRINTFORMW %CALLNAME:TARGET%の膣内の感触を味わいながら、再び腰を动かし始めた…
				ELSE
					PRINTFORMW %CALLNAME:TARGET%の膣内から抜かずに、再び腰を动かし始めた…
				ENDIF
				PRINTFORML 　
				CALL M_KOJO_MESSAGE_COM_K87_60_1
			;后背位、背面立位
			ELSEIF TEQUIP:体位 == 2
				;MASTERについている
				IF TALENT:PLAYER:2 & 2
					PRINTFORMW %CALLNAME:TARGET%の膣内の感触を味わいながら、再び腰を动かし始めた…
				ELSE
					PRINTFORMW %CALLNAME:TARGET%の膣内から抜かずに、再び腰を动かし始めた…
				ENDIF
				PRINTFORML 　
				CALL M_KOJO_MESSAGE_COM_K87_61_1
			;骑乘位
			ELSEIF TEQUIP:体位 == 3
				PRINTFORMW %CALLNAME:TARGET%は插入されたままのペニスを締め付け、再び腰を动かし始めた…
				PRINTFORML 　
				CALL M_KOJO_MESSAGE_COM_K87_65_1
			;对面座位
			ELSEIF TEQUIP:体位 == 4
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:PLAYER%とキスをし、舌を絡めながら再び腰を揺すり始めた…
				PRINTFORML 　
				CALL M_KOJO_MESSAGE_COM_K87_67_1
			;背面座位
			ELSE
				PRINTFORMW %CALLNAME:TARGET%を背后から抱きしめ、再び%CALLNAME:TARGET%の身体を揺らし始めた…
				PRINTFORML 　
				CALL M_KOJO_MESSAGE_COM_K87_68_1
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
