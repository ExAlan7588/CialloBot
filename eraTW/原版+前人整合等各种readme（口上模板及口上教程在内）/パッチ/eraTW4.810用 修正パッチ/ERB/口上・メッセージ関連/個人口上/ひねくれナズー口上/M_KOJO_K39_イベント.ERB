		CLEARLINE 1
		REUSELASTLINE 「馬鹿か君は」
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTFORML 「%RESULTS%…ね」
	ENDIF
	PRINTL [0] はい
	PRINTL [1] いいえ
	;入力した名前を確定するループ
	$INPUT_LOOP_KAKUTEI_1
