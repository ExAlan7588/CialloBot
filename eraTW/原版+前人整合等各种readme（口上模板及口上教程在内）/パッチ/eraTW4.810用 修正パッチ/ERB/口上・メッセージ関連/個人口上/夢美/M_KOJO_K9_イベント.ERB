;==================================================
;相手が推倒を解除
;==================================================
@M_KOJO_EVENT_K9_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 「沢山%CALLNAME:MASTER%を味わったら疲れちゃったわ……ひと休みしましょ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORML 「んん～%CALLNAME:MASTER%成分の补給完了！満足、満足%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORML 「えっ%CALLNAME:MASTER%、まだ満足してなかったのね……いいわ、いらっしゃい%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;坏れるまでしてあげる
		PRINTFORML 「あら、%CALLNAME:MASTER%は补講がお望みなのかしら……？%UNICODE(0x2764)%
		PRINTFORML 「ふふっ%UNICODE(0x2764)%、いいわ、たっぷりしてあげる……けど、私の补講は厳しいわよ……？%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
ENDIF
RETURN 1


