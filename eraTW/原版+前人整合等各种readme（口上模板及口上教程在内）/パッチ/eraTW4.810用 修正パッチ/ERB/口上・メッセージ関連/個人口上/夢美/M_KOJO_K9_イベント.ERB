					PRINTFORML 「全部脱ぐよりなんだか背徳的で……その、興奮するわね」
				ENDIF
				RETURN 1
			ENDIF
		;-------------------------------------------------
		;パンツちょうだい
		;-------------------------------------------------
		CASE 3
			;记入チェック（=0, 非表示、1, 表示）
			LOCAL:1 = 1

			IF LOCAL:1
				SELECTCASE ARG:1
					;-------------------------------------------------
					;交换です
					;-------------------------------------------------
					CASE 0
						;记入チェック（=0, 非表示、1, 表示）
						LOCAL:1 = 1

						IF LOCAL:1
							PRINTFORML 「いいけど、私も%CALLNAME:MASTER%のが欲しいわ」
							PRINTFORML 「だから、交换しましょ？」
							RETURN 1
						ENDIF
					;-------------------------------------------------
					;OK（嬉々
					;-------------------------------------------------
