;==================================================
;相手からデートのお诱い
;==================================================
@M_KOJO_EVENT_K90_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;诱い文句
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「ねえ、デートしてくれないかしら、%CALLNAME:MASTER%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ねえ、%CALLNAME:MASTER%が良ければデートしてくださらない」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、今お暇でしたら…」
				PRINTFORMW 「ちょっと付き合ってほしいのだけれど…決して怪しいことをするわけではありませんわ」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、ちょっと付き合ってくださらない」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「ありがとう、さぁ、行きましょう%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ふふっ…ありがと」
			ELSEIF TALENT:思慕
				PRINTFORMW 「では参りましょう」
			ELSE
				PRINTFORMW 「お礼になにか奢ってあげますわ」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「残念、また诱うからその时はお願いしますわ%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「残念ですわね…」
			ELSEIF TALENT:思慕
				PRINTFORMW 「むぅ…残念ですわ」
			ELSE
				PRINTFORMW 「ケチ臭いですわねー」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;房间に诱われた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「%CALLNAME:MASTER%、私の家に来ないかしら？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%、私の家に来ませんこと？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、お暇でしたら私の家に来ません？」
			ELSE
				PRINTFORMW 「ねえ、もしかして暇だったりしません？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;房间に诱われたのを受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「今日は一日ゆったりとしていきましょう%UNICODE(0x2665) *1%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「あまりじろじろ見ないでくださいまし…変なものがありますので」
			ELSEIF TALENT:思慕
				PRINTFORMW 「…面白いものが見れるといいですわね♪」
			ELSE
				PRINTFORMW 「ふふっ后悔しても遅いですわよ%UNICODE(0x2665) *1%」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;房间に诱われたのを断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「では、また今度ということで…約束ですわよ？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「あら？残念ですわ…お茶も用意していたのに…」
			ELSEIF TALENT:思慕
				PRINTFORMW 「むぅ…折角の机会だと思いましたのに…」
			ELSE
				PRINTFORMW 「あら？意外と身持ちが固いのですわね%UNICODE(0x2665) *1%」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

;==================================================
;脱衣口上
;==================================================
@M_KOJO_EVENT_K90_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方选择
;ARG:1（ARG=3の场合のみあり）地の文の反応
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;时间停止时
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「あーん%UNICODE(0x2665) *1%いけませんわ%UNICODE(0x2665) *1%」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「あ～れ～♪」
				ELSE
					PRINTFORMW 「力づくでひん剥かれてしまいましたわ%UNICODE(0x2665) *1%一体何をされてしまうのかしら%UNICODE(0x2665) *1%」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;下着姿にする
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;时间停止时
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「いや～ん%UNICODE(0x2665) *1%下着だけにされてしまいましたわ%UNICODE(0x2665) *1%」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「あ～れ～♪」
				ELSE
					PRINTFORMW 「あらあら%UNICODE(0x2665) *1%こういうのがお好みですのね%UNICODE(0x2665) *1%」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱ぎ
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;时间停止时
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「いや～ん%UNICODE(0x2665) *1%こんなあられもない姿にされてしまいましたわ%UNICODE(0x2665) *1%」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「あ～れ～♪」
				ELSE
					PRINTFORMW 「あーん%UNICODE(0x2665) *1%恥ずかしいですわ%UNICODE(0x2665) *1%」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交换です
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「いいですわ、その代わり%CALLNAME:MASTER%のパンツをくださいませんこと？」
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「いいですわ、それで私のことを思い出してくださいまし%UNICODE(0x2665) *1%」
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「ま、まあ别に構いませんわ…でも他の人にはバレないようにしてくださいね」
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					PRINTFORMW 「えっ！？嫌よ」
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

