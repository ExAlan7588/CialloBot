;==================================================
;相手からデートのお诱い
;==================================================
@M_KOJO_EVENT_K26_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;诱い文句
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「ねえ、%CALLNAME:MASTER%、デートしない？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%、遊びに行かない？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、今暇？」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、夜遊びに付き合ってくれないかしら？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「乐しい夜になるといいわね、%CALLNAME:MASTER%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ふふっ、何かの异変と间違われないといいわね」
			ELSEIF TALENT:思慕
				PRINTFORMW 「灵梦には内绪よ？退治されてしまうもの」
			ELSE
				PRINTFORMW 「さぁ、幻视の夜を彷徨いましょう」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「ふふっ、じゃあまたの机会ね」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「あら、残念ね」
			ELSEIF TALENT:思慕
				PRINTFORMW 「あらら、口説き方、藍に教えてもらいましょうか」
			ELSE
				PRINTFORMW 「ざーんねん、フられてしまったわ」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;房间に诱われた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「折角だから、私の房间に来ないかしら？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「面白いものが見れるかもしれないけど、来ないかしら？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「家探ししてみたくはない？」
			ELSE
				PRINTFORMW 「少しお茶でもしない？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;房间に诱われたのを受けた
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「今日は一日、%CALLNAME:MASTER%と過ごせるわね♪」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「ふふっ后悔してもだめよ？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、怖いもの知らずねぇ」
			ELSE
				PRINTFORMW 「じゃあ、このスキマを通ってー」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;房间に诱われたのを断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「残念、でもいつでも来ていいわよ」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「あら？残念ね」
			ELSEIF TALENT:思慕
				PRINTFORMW 「めったにない机会なのにね」
			ELSE
				PRINTFORMW 「まあ、贤明ですわ」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

