;==================================================
;デート帰りに告白（恋人が付く）
;==================================================
@M_KOJO_无個性_SPEVENT_K1_2(ARG,ARG:1)
;口上主はTARGET
;地の文表示
;CALL SPEVENT_MESSAGE_2(ARG,ARG:1)
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;告白内容
	;この告白内容表示后に受けるかどうかの选择分が表示されます
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORMW %CALLNAME:MASTER%がデートを終えて帰ろうとすると、突然灵梦に呼び止められた
		PRINTFORMW 振り返ってみると、灵梦は颜を俯かせて立っていたが
		PRINTFORMW やがて意を決したように颜を上げると、まっすぐ%CALLNAME:MASTER%を見つめた
		PRINTFORMW 「私……あなたのことが好きになったのかもしれないわ」
		PRINTFORMW 「何か、あなたと一绪にいると落ち着くのよね。だから……」
		PRINTFORMW 「これからも一绪にいて欲しいの……駄目？」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;告白オッケー（恋人が付く）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTW
		PRINTW
		PRINTFORMW 不安そうに見つめてくる灵梦を、%CALLNAME:MASTER%はそっと抱きしめた
		PRINTFORMW 灵梦も%CALLNAME:MASTER%の背に手を回し、静かに身を預けてきたのだった……
		PRINTFORMW 「良かった……断られたらどうしようかって心配しちゃったわ」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;告白失敗
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTW
		PRINTW
		PRINTFORMW 「――そう」
		PRINTFORMW 「……いえ、分かってたわ」
		PRINTFORMW 「……いや、いいの。大丈夫だから……」
		PRINTFORMW 「……ん、ありがと……」
		PRINTFORMW 「……ええ、大丈夫よ。悲しいけど……」
		PRINTFORMW 「――私、あきらめないからね？」
		PRINTW
		RETURN 0
	ENDIF

ENDIF
RETURN 0

;==================================================
;デート帰り
;==================================================
@M_KOJO_无個性_SPEVENT_K1_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTW
			PRINTFORML 「今日は乐しかったわ」
			PRINTFORML 「また、行きましょう？」
			PRINTFORMW と、灵梦が幸せそうに言った。
			RETURN 0
		ELSEIF TALENT:恋慕
			PRINTW
			PRINTFORML 「今日は、乐しかったわよ」
			PRINTFORML 「その……今度また……」
			PRINTFORML %CALLNAME:MASTER%が頷くと、
			PRINTFORML 「本当？じゃあ、乐しみにしてるわね」
			PRINTFORMW と、灵梦が笑颜で言った。
		ELSEIF TALENT:思慕
			PRINTW
			PRINTFORML 「今日は……まあ乐しかったわよ」
			PRINTFORML 「また气が向いたら、ついて行っても良いわ」
			PRINTFORML 「……案外、直ぐに气が向くかもね」
			PRINTFORML と、灵梦は小さく言った。
			PRINTW
		ELSE
			PRINTW
			PRINTFORML 「はあ……疲れた」
			PRINTFORML 「やっぱり神社でゆっくりするのが一番よね～」
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;反抗刻印消去イベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORML 「――はぁ、まぁ良いわ」
			PRINTFORML 「分かった、許してあげる」
			PRINTFORML 「でも、あんまり酷いことするとまた怒るわよ？」
			PRINTFORMW 灵梦は颜に笑みを浮かべてそう言った。
			RETURN 0
		ELSE
			PRINTFORML 「――はぁ、しょうがないわね」
			PRINTFORML 「いいわよ、許してあげる」
			PRINTFORML 「一応、信用はできるみたいだし、ね」
			PRINTFORML 灵梦は颜をやわらかくして言った。
			PRINTW
		RETURN 0
		ENDIF
	ENDIF
	;-------------------------------------------------
	;反抗刻印2>1レベルダウンイベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORML 「……まぁ、ちょっと許してあげるわ」
			PRINTFORML 「いい？「ちょっと」よ？」
			PRINTFORML 「まだ全部許したわけじゃないんだから」
			PRINTFORMW 灵梦はまだ少し硬い颜で、そう言ってきた。
			RETURN 0
		ELSE
			PRINTFORML 「･･･ちょっとだけ、許してあげるわ」
			PRINTFORML 「でも、また調子に乗ったら、どうなるかは分かってるでしょ？」
			PRINTFORML 灵梦は颜を硬くして言った。
			PRINTW
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;反抗刻印3>2レベルダウンイベント
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORML 「あんまりやりすぎると、身を滅ぼすわよ？」
			PRINTFORML 「……でも、少しだけ許してあげる」
			PRINTFORML 「感謝なさいな」
			PRINTFORMW 灵梦は硬い表情のまま、%CALLNAME:MASTER%にそう言い放った。
			RETURN 0
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%、一つ聞きなさい」
			PRINTFORML 「私のことを甘く見てるのなら、止めておいたほうが良いわよ」
			PRINTFORML 「少しだけ、許してあげるわ」
			PRINTFORML 「それからどうするかは、%CALLNAME:MASTER%、あなた次第よ」
			PRINTW
		ENDIF
		RETURN 0
	ENDIF
	PRINTFORML
ENDIF
RETURN 0

