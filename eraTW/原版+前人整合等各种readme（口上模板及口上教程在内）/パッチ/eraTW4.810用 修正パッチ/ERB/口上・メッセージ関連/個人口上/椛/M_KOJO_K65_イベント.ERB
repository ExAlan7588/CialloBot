		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;我慢
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「明日は、%CALLNAME:MASTER%さんに会えるかな…」
				PRINTFORMW 椛はぽつりと呟いた。
			CASE 1
				PRINTFORML 布団の端がもぞもぞと动き、指先が物寂しそうに襦袢の裾を弄り回す。
				PRINTFORML 「………」
				PRINTFORMW しかし椛は弱弱しく首を横に振って、两腕を枕に静かな寝息を立て始めた…
		ENDSELECT
		PRINTFORML 
		RETURN 1
	ENDIF
	;その他
	;上の分岐を一切书かずに、ここに书くことで自慰の共通口上を表示することが出来ます
	PRINTFORML  
	RETURN 1
ENDIF
RETURN 1
