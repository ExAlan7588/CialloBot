@M_KOJO_EVENT_K130_17(ARG,ARG:1)
;口上主はTARGET
;排他と明记されていないモノは状况によっては连続で表示されるので注意
;ARGに射精状况
;ARG:1に射精状态
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;ARG=1～3は排他
	;謎の快感
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋慕
			PRINTFORMW 「……！！？」
			PRINTFORMW 八千慧は唐突な快感に身悶えしている。
			PRINTFORMW （何……？）
		ELSE
			PRINTFORMW 「……ん！」
			PRINTFORMW 八千慧はビクッと体を震わせ
			PRINTFORMW 「……何が……？」
			PRINTFORMW 困惑して辺りを見回している。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;绝顶
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕
			PRINTFORMW 「んんっ！？！？！？」
			PRINTFORMW 八千慧は一瞬でへたり込んでしまい何があったのか理解できないようだ。
			PRINTFORMW 「……何でもないです……」
			PRINTFORMW 八千慧は差し出された手を取るとぷるぷると震えながら立ち上がった。
		ELSE
			PRINTFORMW 「んっ！？」
			PRINTFORMW 「……何……？」
			PRINTFORMW 突然快乐を叩き込まれた八千慧はうずくまると混乱しながら周囲を見渡している。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;パンツ紛失
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「……！？」
		PRINTFORMW 下着を盗まれた八千慧はその感觉に違和感を觉えている。
		PRINTFORMW 「……な……ばっ……！？」
		PRINTFORMW 状况に气づいた八千慧は忙しなく周囲を見渡しながら裙子を押さえている。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【无自觉口内射精】
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;精液中毒低精饮
		IF ARG:1 == 1
			IF EXP:精液经验 == 0
				PRINTFORMW 「んん！？！！？」
				PRINTFORMW 八千慧は口の中に流し込まれた子种に驚いている。
				PRINTFORMW 「っ……ん……何なのですか……！」
				PRINTFORMW 八千慧は状况に対応できず、精液を飲み込んでしまった。
			ELSE
				PRINTFORMW 「！？」
				PRINTFORMW 八千慧は突然口を押さえ体を震わせながら崩れ落ちた。
				PRINTFORMW 「ん……うう……」
				PRINTFORMW 口内に残る子种をどうするか口を押さえながら考えこんだ后
				PRINTFORMW 「ん……んん……っ……」
				PRINTFORMW 飲み込むことにしたようだ。
				PRINTFORMW 「何よ……もう……！」
			ENDIF
		;精液中毒低吐き出し
		ELSEIF ARG:1 == 2
			PRINTFORMW 「ん！？」
			PRINTFORMW 「げほっ……げほっ……」
			PRINTFORMW 八千慧は突然口内に現れた漢汁の臭气に耐えかねている。
			PRINTFORMW その様子を%CALLNAME:MASTER%はじっと見つめていた。
		;精液中毒低手で覆う
		ELSEIF ARG:1 == 3
			PRINTFORMW 「んん！？」
			PRINTFORMW 八千慧は口腔のザーメンに耐えかねて口を押さえている。
		;精液中毒高精饮
		ELSEIF ARG:1 == 4
			PRINTFORMW 「……ん！」
			PRINTFORMW 八千慧は口腔内のザーメンに气づくと
			PRINTFORMW 「ん……悪くないですね……」
			PRINTFORMW 八千慧は口腔のザーメンを美味しそうに飲み込んだ。
		;濃厚精液＆精液中毒低吐き出し
		ELSEIF ARG:1 == 5
			PRINTFORMW 「……んんっ！？」
			PRINTFORMW 八千慧は口腔内の濃厚な漢汁に气づくと
			PRINTFORMW 「んんっ……ゴホッ……んうぇ……」
			PRINTFORMW 取り乱しながら吐き出した。
		;濃厚精液＆精液中毒高精饮
		ELSEIF ARG:1 == 6
			PRINTFORMW 「……んん！」
			PRINTFORMW 八千慧は口腔内の濃厚な漢汁に气づくと
			PRINTFORMW 「ん……んん……好きかもしれないですねこれ……%UNICODE(0x2665) *1%」
			PRINTFORMW 慣れたように飲み下した。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【无自觉颜面射精】
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		;无知持ち＆精液经验100未満
		IF ARG:1 == 1
		;通常ぬぐい取り
		ELSEIF ARG:1 == 2
			PRINTFORMW 「……っ！」
			PRINTFORMW 八千慧は颜面にへばりついた漢汁に气がつくと
			PRINTFORMW 「何なんですかもう！？」
			PRINTFORMW 取り乱しながら拭い取った。
		;颜射精液舐めとり
		ELSEIF ARG:1 == 3
			PRINTFORMW 「んっ！？」
			PRINTFORMW 八千慧は颜面にへばりついた漢汁に气がつくと
			PRINTFORMW 「……いい趣味してますね……」
			PRINTFORMW 美味しそうに舐めとった。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【无自觉手淫射精】
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;ぬぐい取り
		IF ARG:1 == 1
			PRINTFORMW 「……ちょっと……！」
			PRINTFORMW 八千慧は手にへばりついた漢汁に气がつくと
			PRINTFORMW 「痴漢ですかね……」
			PRINTFORMW 取り乱しながら拭い取った。
		;舐めとり
		ELSEIF ARG:1 == 2
			PRINTFORMW 「……ん……！」
			PRINTFORMW 八千慧は手にへばりついた漢汁に气がつくと
			PRINTFORMW 「……ん……悪くないですね」
			PRINTFORMW 妖艶に舐めとった。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【无自觉肛门射精】
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;Ａ感觉2以下
		IF ARG:1 == 1
			PRINTFORMW 「……っ！？」
			PRINTFORMW 八千慧は肛门に違和感を觉えたが、その正体まではわからないようだ。
		;ソレ以外
		ELSEIF ARG:1 == 2
			PRINTFORMW 「……っ！？」
			PRINTFORMW 八千慧は肛门の違和感に气づくと
			PRINTFORMW 「……气持ち悪い……」
			PRINTFORMW お尻を押さえながらこちらを睨みつけている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【无自觉处女丧失】
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
			PRINTFORMW 「……っ！？」
			PRINTFORMW 八千慧は下腹部の鈍痛に颜をしかめると
			PRINTFORMW 「……何……？」
			PRINTFORMW 悪寒に冷や汗をかいている。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【无自觉膣内射精】
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;非处女膣内射精口服避孕药なし
		IF ARG:1 == 1
			PRINTFORMW 「……！」
			PRINTFORMW 八千慧は膣内の違和感に气づくと
			PRINTFORMW 「……やだ……气持ち悪い……」
			PRINTFORMW 取り乱しながら下腹部を押さえている。
		;非处女膣内射精口服避孕药あり
		ELSEIF ARG:1 == 2
			PRINTFORMW 「……！？」
			PRINTFORMW 八千慧は下半身の違和感に气づくと
			PRINTFORMW 「口服避孕药は持っていたはずですが……」
			PRINTFORMW すぐさま避孕药を飲み安心している。
		;处女膣内射精
		ELSEIF ARG:1 == 3
			PRINTFORMW 「……！」
			PRINTFORMW 八千慧は下半身の鈍痛と違和感に气づくと
			PRINTFORMW 「最悪……」
			PRINTFORMW 取り乱しながら半泣きになっている。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;时间停止解除共通
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
ENDIF
RETURN 1
