;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;持っていた食物を食べられた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == -1
		PRINTFORMW 「お腹が減りましたね……」
		PRINTFORMW 「%CALLNAME:MASTER%、持っているものをくれませんか？」
		CALL ASK_YN("はい","はい")
		IF !RESULT
		ELSE
		ENDIF
		PRINTFORMW 「では、遠慮なく頂きます♪」
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

