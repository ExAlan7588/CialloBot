			;それ以外
			ELSE
				PRINTFORML 「これから……、私をどうするの？」
				PRINTFORMW 雛は不安そうな表情を浮かべている……。
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱ぎ
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋慕
				PRINTFORML 
				PRINTFORMW 
			ELSEIF TALENT:思慕
				PRINTFORMW 
			;それ以外
			ELSE
				PRINTFORMW 
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;パンツちょうだい
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交换です
				;★「手に入れた！」の后に表示
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF TALENT:恋慕
						PRINTFORML 
						PRINTFORML 「……ふふっ。あっ、%CALLNAME:MASTER%、いや、これは、その……」
						PRINTFORMW 雛は下着を持って笑みを浮かべていたが、%CALLNAME:MASTER%の视线に气づき慌てて取り繕った……。
					ELSEIF TALENT:思慕
						PRINTFORML 
						PRINTFORML 「その……、%CALLNAME:MASTER%に渡してばかりじゃ不公平でしょ？」
						PRINTFORMW %CALLNAME:MASTER%の下着を貰った雛はなぜか目を泳がせている……。
					;それ以外
					ELSE
						PRINTFORML 
						PRINTFORML 「……それじゃあ、これは貰っていくわね」
						PRINTFORMW そう言って雛は%CALLNAME:MASTER%の下着を懐にしまった……。
					ENDIF
					CFLAG:79:1012 += 1
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 0
				;-------------------------------------------------
				IF LOCAL:1
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 0
				;-------------------------------------------------
				IF LOCAL:1
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;记入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 0
				;-------------------------------------------------
				IF LOCAL:1
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

