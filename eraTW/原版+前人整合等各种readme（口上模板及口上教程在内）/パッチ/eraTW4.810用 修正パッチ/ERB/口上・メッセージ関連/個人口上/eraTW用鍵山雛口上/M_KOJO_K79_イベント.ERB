;==================================================
;相手が推倒を解除
;==================================================
@M_KOJO_EVENT_K79_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 
		PRINTFORML 「はぁっ、はぁっ……、ちょっと疲れたから……、はぁっ、ここまでね……」
		PRINTFORMW 雛は息も絶え絶えな様子で%CALLNAME:MASTER%を解放した……。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORML 
		PRINTFORML 「ふぅっ…、气持ちよかったわ……。ありがと、%CALLNAME:MASTER%？」
		PRINTFORMW 満足そうな表情を浮かべている雛は、肌が艶々と輝いている……。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 
				PRINTFORML 「えっ…？ %CALLNAME:MASTER%は満足できてないの……？
				PRINTFORML 　まあ、诱ったのは私からだし……、そうね。%CALLNAME:MASTER%が満足するまで付き合ってあげる」
				PRINTFORMW 雛は%CALLNAME:MASTER%に身体を預け、%CALLNAME:MASTER%の推倒を受け入れた……。
			CASE 1
				PRINTFORML 
				PRINTFORML 「あ、あれ…、もしかして%CALLNAME:MASTER%…、もっとえっちしたい……？
				PRINTFORML 　…分かったわ。それじゃあ、今日はいっぱいいっぱい、えっちしちゃいましょっ！」
				PRINTFORMW 満足していた雛も乗り气になったところで、%CALLNAME:MASTER%と雛は合图代わりに口づけを交わした……。
			CASE 2
				PRINTFORML 
				PRINTFORML 「ちょっ…、ちょっと待って、私イッたばかりで、敏感なのにっ……！ ひゃあぁんっ！」
				PRINTFORMW %CALLNAME:MASTER%は雛に覆いかぶさり、欲望の赴くままに雛を求め始めた……。
			CASE 3
				PRINTFORML 
				PRINTFORML 「そ、そんなっ……！ あ～～～れ～～～」
				PRINTFORML 良いではないか、良いではないか。
				PRINTFORMW 雛はわざとらしく声をあげ、クルクルと回りながら布団の上に倒れ伏した……。
			CASE 4
				PRINTFORML 
				PRINTFORML 「ふふっ、いいわよ%CALLNAME:MASTER%……。ふたりでいっしょに、气持ちよくなりましょ？」
				PRINTFORMW 元々%CALLNAME:MASTER%に押し倒されたかったのか、雛は嬉しそうにしている……。
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;坏れるまでしてあげる
		PRINTFORML 
		PRINTFORML 「あっ、そう……。それなら、%CALLNAME:MASTER%が倒れるまで、してあげるわっ……！」
		PRINTFORMW 雛は%CALLNAME:MASTER%の身体を强引に押さえつけ、更なる欲望を貪り始めた……。
		RETURN 1
	ENDIF
ENDIF
RETURN 1


