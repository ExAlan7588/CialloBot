	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF FIRSTTIME(SELECTCOM)
			PRINTFORML 「お兄様。何、するの？」
			PRINTFORMW 「プロレスごっこ？柔道ごっこ？そ・れ・と・も…」
			PRINTFORMW 「弾幕ごっこ？」
			RETURN 1
		ENDIF
		;死に分岐
		;まとまりが悪い。
		IF GETBIT (CFLAG:1500,4)
			IF 充填率(TARGET,1)
				PRINTFORML 「また孕め孕めするの？」
			ELSE
				PRINTFORML 「お兄様。孕め孕めしたいの？」
			ENDIF
		ELSEIF GETBIT (CFLAG:1500,3)
			IF CFLAG:TARGET:积攒度 / 10 > 750
				PRINTFORML 「%CALLNAME:TARGET%、なかだしごっこがいいわ」
			ELSE
				PRINTFORML 「またなかだしごっこするの？」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;逢瀬終了（ウフフ終了时）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;膣内射精经验がある
		IF GETBIT (CFLAG:1500,4)
			IF 充填率(TARGET,1)
			ELSE
			ENDIF
		;はじめてのなかだし
		ELSEIF 充填率(TARGET,1)
			PRINTFORMW 「ねえお兄様。『孕め』ってどんな意味？」
			PRINTFORMW 「好き？恋人の好きって意味なの？」
			PRINTFORML 
			PRINTFORMW 「ふぅん」
			PRINTFORMW 「ぱっちぇが言ってたのと違うのね」
			PRINTFORML 
			PRINTFORMW 「お兄様」
			PRINTFORMW 「%CALLNAME:TARGET%の好きも恋人の好きよ%UNICODE(0x2661)%」
			SETBIT CFLAG:1500,4
			RETURN 1
		;膣内射精した事无い分岐
		ELSE
			PRINTFORMW 「もういいの？お兄様」
			PRINTFORMW 「お兄様。%CALLNAME:TARGET%にはむぎゅむぎゅが足りないわ」
		ENDIF
	ENDIF
	;その他
	PRINTFORM 
ENDIF
RETURN 1

