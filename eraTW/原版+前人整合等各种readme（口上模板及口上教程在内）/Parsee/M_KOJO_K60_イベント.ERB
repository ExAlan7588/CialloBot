	;-------------------------------------------------
    IF LOCAL:1 && ARG == 1
        DEBUGPRINTFORML Call first greeting of day lines function
        CALL K60_FIRST_GREETING_OF_DAY_LINES(0)
        RETURN 1
    ENDIF
	;-------------------------------------------------
	;The character enters a room the player's in
	;TARGETが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
