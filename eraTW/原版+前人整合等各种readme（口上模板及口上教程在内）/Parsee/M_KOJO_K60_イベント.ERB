    SELECTCASE ARGS
        CASE "衝突"
		;-------------------------------------------------
		;场外に出てあなたがキャラと衝突した际の台詞
		;文字数制限はありません,泛用台詞もありません
		;-------------------------------------------------
            RESULTS = いたっ！ ちょっと、危ないじゃないのよ！
        CASE "そこだ！"
            RESULTS = %TEXTR("就是那里！/现在！")%
        CASE "近い"
            RESULTS = %TEXTR("很近了！很近了哦！/正在接近哦！/还差一点！")%
        CASE "远い"
            RESULTS = %TEXTR("还很远哦/还差得远呢～")%
        CASE "かなり远い"
            RESULTS = %TEXTR("完全还很远哦ー/还差得远着呢ー")%
        CASE "前"
            RESULTS = %TEXTR("在前面哦/就这样，就这样/那边那边")%
        CASE "少し前"
            RESULTS = %TEXTR("稍微靠前一点！/稍微ー靠前/再一步！")%
        CASE "后ろ"
            RESULTS = %TEXTR("朝后了！/反了，反了！/不是那边哦ー")%
        CASE "少し后ろ"
            RESULTS = %TEXTR("就在后面哦！/稍微ー靠后/退一步！")%
        CASE "右"
            RESULTS = %TEXTR("在右边哦/右边，右边/再往右一点")%
        CASE "少し右"
            RESULTS = %TEXTR("稍微靠右一点/还要往右边一点吗")%
        CASE "左"
            RESULTS = %TEXTR("在左边哦/左边，左边/再往左一点")%
        CASE "少し左"
            RESULTS = %TEXTR("稍微靠左一点/还要往左边一点吗")%
        CASE "结果"
		;-------------------------------------------------
		;结束后の一言
		;文字数制限はありません,泛用台詞もありません
		;TFLAG:193,
		;   -1=失败
		;    0=成功
		;    1=大成功
		;    2=木刀で大成功（BOUNS点）
		;ARG
		;    0=正直者
		;    1=ウソつき
		;-------------------------------------------------
            SELECTCASE TFLAG:193
                CASE -1
                    RESULTS = 真遗憾～
                CASE 0
                    RESULTS = 成功啦！
                CASE 1
                    RESULTS = 好厉害！
