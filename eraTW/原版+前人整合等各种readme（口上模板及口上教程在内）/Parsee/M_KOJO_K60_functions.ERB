    ;琪斯美/山女
    CASE 83, 84
        RETURNF @"%CALLNAME:LOCAL%酱"

    ;Koishi/Satori/Byakuren/Seija/Alice
    CASE 38, 49, 55, 97, 17
        ;No honorific because Parsee dislikes them
        RETURNF @"%CALLNAME:LOCAL%"

	;Everyone else: 勇仪 gets -san too because Parsee feels awkward around her crush
	CASEELSE
		RETURNF @"%CALLNAME:LOCAL%桑"
ENDSELECT


