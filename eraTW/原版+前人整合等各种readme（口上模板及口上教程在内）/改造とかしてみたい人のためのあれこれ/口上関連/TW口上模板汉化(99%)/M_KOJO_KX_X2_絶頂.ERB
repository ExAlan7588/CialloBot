		;[SKIPSTART]
		SELECTCASE NOWEX:二重绝顶
			;Ｃ＆Ｖ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｂ绝顶
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ绝顶
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｍ绝顶
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｍ绝顶
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ绝顶
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ绝顶
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ绝顶
		;超强绝顶Ｃ
		IF NOWEX:Ｃ绝顶 == 3
			PRINTFORMW 
		;强绝顶Ｃ
		ELSEIF NOWEX:Ｃ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ绝顶
		;超强绝顶Ｖ
		IF NOWEX:Ｖ绝顶 == 3
			PRINTFORMW 
		;强绝顶Ｖ
		ELSEIF NOWEX:Ｖ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ａ绝顶
		;超强绝顶Ａ
		IF NOWEX:Ａ绝顶 == 3
			PRINTFORMW 
		;强绝顶Ａ
		ELSEIF NOWEX:Ａ绝顶 == 2
			PRINTFORMW 
		;绝顶Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ绝顶
		;强绝顶Ｂ
		IF NOWEX:Ｂ绝顶 == 3
			PRINTFORMW 
		;强绝顶Ｂ
		ELSEIF NOWEX:Ｂ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｂ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｍ绝顶
		;超强绝顶Ｍ
		IF NOWEX:Ｍ绝顶 == 3
			PRINTFORMW 
		;强绝顶Ｍ
		ELSEIF NOWEX:Ｍ绝顶 == 2
			PRINTFORMW 
		;绝顶Ｍ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF

